Dialogue: 0,0:00:01.03,0:00:45.09,Default,,0,0,0,,{\c&H00ffff&}Xóm Anh Ngữ{\c} biên dịch\N{\i1\c&Hf0b000&}fb.com/xomanhngu{\c}{\i0}
Dialogue: 0,0:00:57.72,0:01:00.21,Default,,0,0,0,,{\i1}Thế giới này\Nluôn vận động{\i0}
Dialogue: 0,0:01:00.76,0:01:02.20,Default,,0,0,0,,{\i1}thay đổi{\i0}
Dialogue: 0,0:01:02.60,0:01:04.13,Default,,0,0,0,,{\i1}và phát triển.{\i0}
Dialogue: 0,0:01:05.23,0:01:09.32,Default,,0,0,0,,{\i1}Và cuộc đời là\Nnhững thói quen.{\i0}
Dialogue: 0,0:01:11.07,0:01:14.95,Default,,0,0,0,,{\i1}Công việc, nhà cửa,\Ngia đình.{\i0}
Dialogue: 0,0:01:17.41,0:01:19.60,Default,,0,0,0,,{\i1}Nhưng chúng ta là ai?{\i0}
Dialogue: 0,0:01:20.74,0:01:22.16,Default,,0,0,0,,{\i1}Tôi là ai?{\i0}
Dialogue: 0,0:01:25.50,0:01:28.37,Default,,0,0,0,,{\i1}Bạn biết rằng\Nbạn có vợ đẹp{\i0}
Dialogue: 0,0:01:29.71,0:01:31.89,Default,,0,0,0,,{\i1}con xinh{\i0}
Dialogue: 0,0:01:35.34,0:01:37.63,Default,,0,0,0,,{\i1}và có việc làm.{\i0}
Dialogue: 0,0:01:38.43,0:01:40.70,Default,,0,0,0,,{\i1}Nhưng nếu không thì sao?{\i0}
Dialogue: 0,0:01:42.31,0:01:43.89,Default,,0,0,0,,{\i1}Sự thật{\i0}
Dialogue: 0,0:01:45.73,0:01:47.34,Default,,0,0,0,,{\i1}một khi bạn đã biết{\i0}
Dialogue: 0,0:01:48.81,0:01:50.89,Default,,0,0,0,,{\i1}sẽ thay đổi tất cả.{\i0}
Dialogue: 0,0:02:25.72,0:02:27.52,Default,,0,0,0,,Lại gặp ác mộng à?
Dialogue: 0,0:02:28.56,0:02:31.22,Default,,0,0,0,,Ừ. Anh xin lỗi.
Dialogue: 0,0:02:32.40,0:02:35.21,Default,,0,0,0,,Anh đi khám đi.
Dialogue: 0,0:02:35.36,0:02:39.17,Default,,0,0,0,,Em và các con\Nrất lo cho anh.
Dialogue: 0,0:02:41.53,0:02:43.39,Default,,0,0,0,,Anh sẽ tự lo\Nchuyện này.
Dialogue: 0,0:02:47.91,0:02:50.69,Default,,0,0,0,,Muốn nghe chuyện\Nkinh dị không?
Dialogue: 0,0:02:51.58,0:02:53.02,Default,,0,0,0,,Có một ông chồng
Dialogue: 0,0:02:53.02,0:02:55.61,Default,,0,0,0,,không chịu xử lý\Nrắc rối của mình.
Dialogue: 0,0:02:55.61,0:03:00.46,Default,,0,0,0,,Vợ ông ta mất ngủ\Nnên đã nổi điên
Dialogue: 0,0:03:00.63,0:03:03.61,Default,,0,0,0,,và ném ông ta\Nra cửa sổ.
Dialogue: 0,0:03:05.39,0:03:07.21,Default,,0,0,0,,Hết chuyện.
Dialogue: 0,0:03:49.77,0:03:52.74,Default,,0,0,0,,{\i1}Khỉ nhìn thấy,\Nkhỉ làm chuyện ấy.{\i0}
Dialogue: 0,0:03:56.61,0:03:58.19,Default,,0,0,0,,Con làm gì đấy?
Dialogue: 0,0:03:58.69,0:04:01.36,Default,,0,0,0,,Chơi với khỉ.\NBa chơi cùng nhé?
Dialogue: 0,0:04:01.44,0:04:04.79,Default,,0,0,0,,Chơi cùng? Ba cũng\Nthích lắm
Dialogue: 0,0:04:04.79,0:04:08.14,Default,,0,0,0,,nhưng ba phải\Nđi làm đúng giờ.
Dialogue: 0,0:04:08.33,0:04:09.46,Default,,0,0,0,,Nhưng mà này
Dialogue: 0,0:04:09.46,0:04:13.79,Default,,0,0,0,,vì dạo này ai cũng\Nđi làm mệt mỏi
Dialogue: 0,0:04:13.83,0:04:15.04,Default,,0,0,0,,Đặc biệt là Herman.
Dialogue: 0,0:04:15.12,0:04:18.09,Default,,0,0,0,,Ba cũng đang nghĩ\Nđến Herman đây.
Dialogue: 0,0:04:18.21,0:04:22.44,Default,,0,0,0,,nên ba sẽ về sớm\Ndẫn cả nhà ra cầu tàu.
Dialogue: 0,0:04:22.44,0:04:24.17,Default,,0,0,0,,Hanna có đến không?
Dialogue: 0,0:04:24.26,0:04:26.98,Default,,0,0,0,,- Hanna sẽ đến.\N- Chị con cũng đến đấy.
Dialogue: 0,0:04:27.05,0:04:29.09,Default,,0,0,0,,Chúng ta sẽ bắt chị\Nphải chơi vui vẻ
Dialogue: 0,0:04:29.18,0:04:32.82,Default,,0,0,0,,nếu không thì ba\Nsẽ làm thế này này.
Dialogue: 0,0:04:32.82,0:04:33.94,Default,,0,0,0,,Ôi không!
Dialogue: 0,0:04:34.02,0:04:35.98,Default,,0,0,0,,Dừng lại đi!
Dialogue: 0,0:04:36.06,0:04:38.03,Default,,0,0,0,,Mà con bé đâu rồi?
Dialogue: 0,0:04:38.11,0:04:40.52,Default,,0,0,0,,- Đoán thử xem.\N- Biết rồi.
Dialogue: 0,0:04:40.53,0:04:42.62,Default,,0,0,0,,- Chào ba.\N- Chào Luce.
Dialogue: 0,0:04:44.74,0:04:46.70,Default,,0,0,0,,À, Samantha có hỏi
Dialogue: 0,0:04:46.78,0:04:50.57,Default,,0,0,0,,khi nào thì đãi tiệc\Nmừng em lên chức.
Dialogue: 0,0:04:50.66,0:04:52.72,Default,,0,0,0,,À, vụ đó...
Dialogue: 0,0:04:52.75,0:04:55.93,Default,,0,0,0,,- Mai tiến hành.\N- Mai hả?
Dialogue: 0,0:04:56.09,0:04:56.89,Default,,0,0,0,,Được.
Dialogue: 0,0:04:56.89,0:04:59.46,Default,,0,0,0,,Em sắp quên\Nmất cô ấy rồi.
Dialogue: 0,0:04:59.46,0:05:00.92,Default,,0,0,0,,Haha.
Dialogue: 0,0:05:01.89,0:05:04.30,Default,,0,0,0,,- Yêu anh.\N- Yêu em.
Dialogue: 0,0:05:06.43,0:05:08.04,Default,,0,0,0,,Cầu tàu à?
Dialogue: 0,0:05:11.51,0:05:12.87,Default,,0,0,0,,Chào con yêu.
Dialogue: 0,0:05:13.19,0:05:14.70,Default,,0,0,0,,Ra hành lang à?
Dialogue: 0,0:05:15.14,0:05:16.91,Default,,0,0,0,,Bấm giúp ba đi.
Dialogue: 0,0:05:22.03,0:05:24.05,Default,,0,0,0,,Bên ngoài trời đẹp lắm.
Dialogue: 0,0:05:24.53,0:05:27.15,Default,,0,0,0,,Xin đừng nói chuyện\Nkhi đi thang máy.
Dialogue: 0,0:05:27.28,0:05:28.66,Default,,0,0,0,,À phải rồi.
Dialogue: 0,0:05:29.12,0:05:30.43,Default,,0,0,0,,Được thôi.
Dialogue: 0,0:05:31.53,0:05:33.75,Default,,0,0,0,,Nhìn xem, đúng giờ quá.
Dialogue: 0,0:05:33.87,0:05:35.23,Default,,0,0,0,,- Pete.\N- Chào.
Dialogue: 0,0:05:36.33,0:05:38.25,Default,,0,0,0,,Quý cô thang máy.
Dialogue: 0,0:05:40.34,0:05:41.75,Default,,0,0,0,,Công việc thế nào?
Dialogue: 0,0:05:41.79,0:05:43.67,Default,,0,0,0,,Hữu danh vô thực.
Dialogue: 0,0:05:43.76,0:05:46.92,Default,,0,0,0,,Kiến nghị lên trên nhưng\Nkhông ai thèm nghe.
Dialogue: 0,0:05:47.72,0:05:49.42,Default,,0,0,0,,Cảm ơn quý cô.
Dialogue: 0,0:05:49.64,0:05:51.28,Default,,0,0,0,,Chú Wright?
Dialogue: 0,0:05:51.43,0:05:52.81,Default,,0,0,0,,Cháu đến gặp Maggie nhé?
Dialogue: 0,0:05:52.89,0:05:55.40,Default,,0,0,0,,Được. Chú vừa lắp\Ndàn âm thanh mới.
Dialogue: 0,0:05:55.48,0:05:57.11,Default,,0,0,0,,- Mở tiệc thôi!\N- Tuyệt!
Dialogue: 0,0:05:57.19,0:05:59.40,Default,,0,0,0,,Tối nay gia đình\Ntôi cũng đi chơi.
Dialogue: 0,0:05:59.48,0:06:01.70,Default,,0,0,0,,Hay đấy.\NKhi khác nhé.
Dialogue: 0,0:06:01.73,0:06:02.73,Default,,0,0,0,,Chơi vui nhé.
Dialogue: 0,0:06:03.73,0:06:07.54,Default,,0,0,0,,- Ba định làm gì hả?\N- Từ từ đã nào!
Dialogue: 0,0:06:11.49,0:06:13.85,Default,,0,0,0,,Mấy tuần nay\Nba hơi bận.
Dialogue: 0,0:06:13.85,0:06:15.14,Default,,0,0,0,,Mấy tuần?
Dialogue: 0,0:06:15.87,0:06:17.59,Default,,0,0,0,,Nói đểu ba hử?
Dialogue: 0,0:06:17.75,0:06:19.19,Default,,0,0,0,,Đúng vậy.
Dialogue: 0,0:06:20.12,0:06:22.64,Default,,0,0,0,,Gia đình mình sẽ có\Nthời gian bên nhau.
Dialogue: 0,0:06:22.64,0:06:25.48,Default,,0,0,0,,Ba muốn bù đắp\Ncho các con.
Dialogue: 0,0:06:25.92,0:06:27.69,Default,,0,0,0,,Thế chúng ta\Nsẽ làm gì?
Dialogue: 0,0:06:27.80,0:06:29.09,Default,,0,0,0,,Ba chưa biết.
Dialogue: 0,0:06:29.59,0:06:32.76,Default,,0,0,0,,Con cứ lên lầu\Nlà sẽ biết thôi.
Dialogue: 0,0:07:33.16,0:07:34.20,Default,,0,0,0,,Trời đụ!
Dialogue: 0,0:07:42.42,0:07:46.81,Default,,0,0,0,,Chà. Xưa nay hiếm khi\Nthấy anh làm hỏng máy.
Dialogue: 0,0:07:46.81,0:07:49.55,Default,,0,0,0,,Mạch điện phân nhánh\Nlàm hỏng cả hệ thống.
Dialogue: 0,0:07:49.55,0:07:50.83,Default,,0,0,0,,Anh ngủ được chứ?
Dialogue: 0,0:07:50.83,0:07:53.14,Default,,0,0,0,,Khá hơn rồi.
Dialogue: 0,0:07:53.97,0:07:56.44,Default,,0,0,0,,Tôi có nghe\Nngười ta nói
Dialogue: 0,0:07:56.44,0:07:59.62,Default,,0,0,0,,về một phòng khám
Dialogue: 0,0:07:59.85,0:08:02.87,Default,,0,0,0,,chuyên trị mất ngủ\Nvà căng thẳng.
Dialogue: 0,0:08:02.98,0:08:04.74,Default,,0,0,0,,Để tôi lấy số\Ncho anh.
Dialogue: 0,0:08:04.77,0:08:08.53,Default,,0,0,0,,- Tôi sẽ giúp...\N- Không sao, lỗi nhỏ thôi.
Dialogue: 0,0:08:09.11,0:08:10.72,Default,,0,0,0,,Tôi sẽ sửa được.
Dialogue: 0,0:08:10.78,0:08:12.87,Default,,0,0,0,,Được. Tôi tin anh.
Dialogue: 0,0:08:13.49,0:08:14.49,Default,,0,0,0,,Được rồi.
Dialogue: 0,0:08:15.70,0:08:18.10,Default,,0,0,0,,- Cần gì thì cứ bảo tôi.\N- Được.
Dialogue: 0,0:08:18.58,0:08:20.57,Default,,0,0,0,,Này David?
Dialogue: 0,0:08:21.12,0:08:24.55,Default,,0,0,0,,Hôm nay cho tôi về\Nsớm được không?
Dialogue: 0,0:08:25.58,0:08:27.66,Default,,0,0,0,,Nhớ phải gọi cho\Nphòng khám đấy.
Dialogue: 0,0:08:28.38,0:08:29.44,Default,,0,0,0,,Nhớ rồi.
Dialogue: 0,0:08:30.38,0:08:31.73,Default,,0,0,0,,Thế thì được.
Dialogue: 0,0:08:44.73,0:08:47.15,Default,,0,0,0,,Mục đích là nâng cấp\Nhệ thống thoát nước
Dialogue: 0,0:08:47.23,0:08:50.73,Default,,0,0,0,,ở Prescott, Calder\Nvà Willow.
Dialogue: 0,0:08:50.73,0:08:53.82,Default,,0,0,0,,Nếu ta tập trung vào các\Ncống ngầm tại khu vực đó
Dialogue: 0,0:08:53.90,0:08:59.09,Default,,0,0,0,,lượng nước rò rỉ ở\Nbờ tây sẽ giảm 30%.
Dialogue: 0,0:08:59.12,0:09:01.88,Default,,0,0,0,,Cảm ơn mọi người.\NHôm nay đến đây thôi.
Dialogue: 0,0:09:04.21,0:09:07.07,Default,,0,0,0,,Lẽ ra phải nâng cấp\Ntừ nhiều năm trước.
Dialogue: 0,0:09:07.07,0:09:08.84,Default,,0,0,0,,Tuyệt lắm Alice.
Dialogue: 0,0:09:08.96,0:09:10.79,Default,,0,0,0,,- Cảm ơn.\N- Không có gì.
Dialogue: 0,0:09:10.88,0:09:12.89,Default,,0,0,0,,Đề bạt cô\Nlà chuẩn rồi.
Dialogue: 0,0:09:12.89,0:09:14.32,Default,,0,0,0,,Tối nay cô có thể ở lại
Dialogue: 0,0:09:14.32,0:09:17.08,Default,,0,0,0,,để bàn cách xử lý với\Nbên điện nước không?
Dialogue: 0,0:09:17.09,0:09:21.65,Default,,0,0,0,,Thực ra hôm nay tôi\Nđịnh xin về sớm.
Dialogue: 0,0:09:21.72,0:09:24.26,Default,,0,0,0,,- Được.\N- Cảm ơn.
Dialogue: 0,0:09:25.73,0:09:29.47,Default,,0,0,0,,{\i1}Phòng khám Nhất Sinh{\i0}\N{\i1}555-0144{\i0}
Dialogue: 0,0:10:06.93,0:10:10.39,Default,,0,0,0,,{\i1}Nếu ta cho họ cơ hội,\Nhọ sẽ giúp đỡ hành tinh này.{\i0}
Dialogue: 0,0:10:10.48,0:10:12.95,Default,,0,0,0,,{\i1}Và dù ta muốn hay không,\Nhọ cũng sẽ ở lại đây.{\i0}
Dialogue: 0,0:10:13.02,0:10:15.19,Default,,0,0,0,,{\i1}Cho nên ta phải\Nquen với việc{\i0}
Dialogue: 0,0:10:15.28,0:10:16.96,Default,,0,0,0,,{\i1}họ sẽ được quyền\Nbình đẳng như chúng ta.{\i0}
Dialogue: 0,0:10:17.03,0:10:19.25,Default,,0,0,0,,{\i1}Quyền bình đẳng ư?\NKhông bao giờ.{\i0}
Dialogue: 0,0:10:19.25,0:10:21.45,Default,,0,0,0,,{\i1}Rồi sẽ có lúc\Nchúng nổi dậy{\i0}
Dialogue: 0,0:10:21.53,0:10:24.16,Default,,0,0,0,,{\i1}vượt xa chúng ta,\Nđánh bại chúng ta.{\i0}
Dialogue: 0,0:10:24.24,0:10:26.12,Default,,0,0,0,,{\i1}Chúng sẽ chế ngự loài người.{\i0}
Dialogue: 0,0:10:26.12,0:10:28.08,Default,,0,0,0,,{\i1}Tôi không dọa đâu,\Nnó đang xảy ra rồi.{\i0}
Dialogue: 0,0:10:28.16,0:10:31.63,Default,,0,0,0,,{\i1}Ngay trước mắt ta,\Nphải hành động ngay.{\i0}
Dialogue: 0,0:10:44.47,0:10:45.64,Default,,0,0,0,,Alice!
Dialogue: 0,0:11:24.80,0:11:27.30,Default,,0,0,0,,Anh có thấy cái\Nbóng đèn kia không?
Dialogue: 0,0:11:27.68,0:11:29.17,Default,,0,0,0,,Cái bóng đèn?
Dialogue: 0,0:12:01.38,0:12:03.80,Default,,0,0,0,,{\i1}Chắc ba phải tăng ca.{\i0}
Dialogue: 0,0:12:04.80,0:12:07.74,Default,,0,0,0,,Mẹ đừng nói hộ cho ba.
Dialogue: 0,0:12:09.35,0:12:12.16,Default,,0,0,0,,Con giận ba là đúng.
Dialogue: 0,0:12:12.35,0:12:14.40,Default,,0,0,0,,Lẽ ra ba nên gọi về.
Dialogue: 0,0:12:15.81,0:12:18.21,Default,,0,0,0,,Con biết ba\Nyêu con mà.
Dialogue: 0,0:12:18.73,0:12:20.75,Default,,0,0,0,,Con biết.
Dialogue: 0,0:12:22.70,0:12:24.22,Default,,0,0,0,,Ngủ ngon.
Dialogue: 0,0:12:51.35,0:12:52.81,Default,,0,0,0,,Anh rất xin lỗi.
Dialogue: 0,0:12:54.44,0:12:58.11,Default,,0,0,0,,- Rảnh rỗi đi mua đồ cơ đấy.\N- Anh mua cho bọn trẻ mà.
Dialogue: 0,0:12:58.19,0:12:59.54,Default,,0,0,0,,Ai chả biết.
Dialogue: 0,0:13:03.24,0:13:06.76,Default,,0,0,0,,- Cho anh nghỉ một chút.\N- Nghỉ? Nghỉ cái gì?
Dialogue: 0,0:13:06.76,0:13:09.70,Default,,0,0,0,,- Nghỉ em hay nghỉ các con?\N- Anh không có ý đó.
Dialogue: 0,0:13:09.70,0:13:12.15,Default,,0,0,0,,Nghỉ mẹ cái thứ\Nanh đang làm đi.
Dialogue: 0,0:13:16.50,0:13:18.51,Default,,0,0,0,,Anh lại mơ thấy\Ncơn ác mộng đó
Dialogue: 0,0:13:19.38,0:13:21.83,Default,,0,0,0,,hôm nay...\Nở chỗ làm.
Dialogue: 0,0:13:22.25,0:13:25.40,Default,,0,0,0,,Anh bất tỉnh nên\Nmới về trễ.
Dialogue: 0,0:13:28.72,0:13:30.40,Default,,0,0,0,,Peter, em...
Dialogue: 0,0:13:31.43,0:13:33.17,Default,,0,0,0,,Em rất lo cho anh.
Dialogue: 0,0:13:33.17,0:13:34.10,Default,,0,0,0,,Anh biết.
Dialogue: 0,0:13:34.18,0:13:37.18,Default,,0,0,0,,Mẹ con em đều\Nlo lắng cho anh.
Dialogue: 0,0:13:37.27,0:13:38.94,Default,,0,0,0,,Em...
Dialogue: 0,0:13:39.27,0:13:41.92,Default,,0,0,0,,không hiểu sao anh\Nkhông chịu đi khám.
Dialogue: 0,0:13:41.98,0:13:43.61,Default,,0,0,0,,Họ sẽ nói là do\Nanh tưởng tượng.
Dialogue: 0,0:13:43.65,0:13:46.51,Default,,0,0,0,,Đúng thế chứ\Ncòn gì nữa.
Dialogue: 0,0:13:47.91,0:13:49.24,Default,,0,0,0,,Mẹ ơi?
Dialogue: 0,0:13:53.04,0:13:54.74,Default,,0,0,0,,Sao ba mẹ lại cãi nhau?
Dialogue: 0,0:13:54.79,0:13:57.77,Default,,0,0,0,,Không sao con yêu.\NMọi việc đều ổn.
Dialogue: 0,0:14:18.31,0:14:20.60,Default,,0,0,0,,Tỉnh dậy mẹ ơi.
Dialogue: 0,0:14:21.77,0:14:24.05,Default,,0,0,0,,Mẹ hãy tỉnh dậy đi.
Dialogue: 0,0:14:24.11,0:14:25.59,Default,,0,0,0,,Mẹ ơi.
Dialogue: 0,0:14:27.07,0:14:29.42,Default,,0,0,0,,Tỉnh dậy mẹ ơi.
Dialogue: 0,0:14:32.28,0:14:34.61,Default,,0,0,0,,Mẹ hãy tỉnh lại đi.
Dialogue: 0,0:14:43.59,0:14:45.43,Default,,0,0,0,,- Các con!\N- Đừng mà!
Dialogue: 0,0:14:45.50,0:14:46.79,Default,,0,0,0,,Các con!
Dialogue: 0,0:14:46.88,0:14:50.82,Default,,0,0,0,,Này các con,\Nthôi đi.
Dialogue: 0,0:14:51.51,0:14:54.63,Default,,0,0,0,,- Có thấy cặp của mẹ không?\N- Không ạ.
Dialogue: 0,0:14:55.22,0:14:58.64,Default,,0,0,0,,{\i1}Phòng khám Nhất Sinh{\i0}
Dialogue: 0,0:15:01.27,0:15:02.76,Default,,0,0,0,,Chào các con.
Dialogue: 0,0:15:03.02,0:15:05.07,Default,,0,0,0,,Để ba giải thích\Nchuyện đêm qua.
Dialogue: 0,0:15:09.32,0:15:10.32,Default,,0,0,0,,Hanna...
Dialogue: 0,0:15:10.82,0:15:12.16,Default,,0,0,0,,Ba xin lỗi.
Dialogue: 0,0:15:20.83,0:15:22.67,Default,,0,0,0,,Herman làm sao thế?
Dialogue: 0,0:15:22.83,0:15:25.70,Default,,0,0,0,,- Nó không nói nữa.\N- Vậy à?
Dialogue: 0,0:15:27.76,0:15:29.99,Default,,0,0,0,,Chắc nó giận ba.
Dialogue: 0,0:15:30.88,0:15:31.88,Default,,0,0,0,,Ờ.
Dialogue: 0,0:15:33.39,0:15:34.39,Default,,0,0,0,,Ừ thì...
Dialogue: 0,0:15:35.14,0:15:36.91,Default,,0,0,0,,nó giận ba là đúng.
Dialogue: 0,0:15:38.60,0:15:41.63,Default,,0,0,0,,Luce, ba nói chuyện\Nvới Herman nhé?
Dialogue: 0,0:15:44.31,0:15:47.37,Default,,0,0,0,,Herman, tao biết\Nmày rất buồn
Dialogue: 0,0:15:48.11,0:15:49.11,Default,,0,0,0,,và...
Dialogue: 0,0:15:49.74,0:15:52.03,Default,,0,0,0,,xin lỗi cũng chẳng\Ngiúp được gì.
Dialogue: 0,0:15:52.82,0:15:55.62,Default,,0,0,0,,Tao xin lỗi vì lỡ\Nhẹn với gia đình.
Dialogue: 0,0:15:57.03,0:15:58.55,Default,,0,0,0,,Nếu con muốn,\Nđể ba xem qua
Dialogue: 0,0:15:58.62,0:16:01.32,Default,,0,0,0,,xem có sửa được\Ntiếng của nó không.
Dialogue: 0,0:16:05.25,0:16:07.12,Default,,0,0,0,,Khi nào nó\Nmới nói lại?
Dialogue: 0,0:16:07.21,0:16:10.01,Default,,0,0,0,,Để ba chỉnh lại\Nmấy sợi dây...
Dialogue: 0,0:16:13.84,0:16:15.83,Default,,0,0,0,,Nào Herman, nói gì đi?
Dialogue: 0,0:16:15.89,0:16:18.56,Default,,0,0,0,,{\i1}Khỉ nhìn thấy,\Nkhỉ làm chuyện ấy.{\i0}
Dialogue: 0,0:16:18.68,0:16:20.27,Default,,0,0,0,,Cảm ơn ba.
Dialogue: 0,0:16:20.43,0:16:23.30,Default,,0,0,0,,{\i1}Khỉ nhìn thấy,\Nkhỉ làm chuyện ấy.{\i0}
Dialogue: 0,0:16:30.15,0:16:31.62,Default,,0,0,0,,Anh gọi bác sĩ rồi.
Dialogue: 0,0:16:31.90,0:16:34.39,Default,,0,0,0,,Cô ấy sẽ gặp anh\Nsau giờ làm.
Dialogue: 0,0:16:35.24,0:16:36.44,Default,,0,0,0,,Thật chứ?
Dialogue: 0,0:16:37.16,0:16:39.50,Default,,0,0,0,,Thề luôn.
Dialogue: 0,0:16:58.47,0:16:59.64,Default,,0,0,0,,{\i1}Tại Nhất Sinh{\i0}
Dialogue: 0,0:16:59.64,0:17:02.59,Default,,0,0,0,,{\i1}sức khỏe tinh thần của bạn\Nlà ưu tiên hàng đầu.{\i0}
Dialogue: 0,0:17:02.60,0:17:04.65,Default,,0,0,0,,{\i1}Chúng tôi chăm lo\Ncho sức khỏe của bạn{\i0}
Dialogue: 0,0:17:04.69,0:17:09.18,Default,,0,0,0,,{\i1}bằng dịch vụ chuyên nghiệp\Nvà giá cả phải chăng{\i0}
Dialogue: 0,0:17:09.18,0:17:13.34,Default,,0,0,0,,{\i1}với mong muốn kiến tạo\Nmột cộng đồng vui khỏe.{\i0}
Dialogue: 0,0:17:15.78,0:17:17.13,Default,,0,0,0,,Anh thấy những gì?
Dialogue: 0,0:17:18.87,0:17:20.31,Default,,0,0,0,,Sao cơ?
Dialogue: 0,0:17:21.08,0:17:23.32,Default,,0,0,0,,Anh không ngủ được\Nphải không?
Dialogue: 0,0:17:24.20,0:17:26.36,Default,,0,0,0,,- Đúng thế.\N- Phải rồi.
Dialogue: 0,0:17:27.54,0:17:29.50,Default,,0,0,0,,Có thứ gì đó\Nđang đến.
Dialogue: 0,0:17:31.92,0:17:34.04,Default,,0,0,0,,Ý anh là sao?
Dialogue: 0,0:17:34.34,0:17:36.12,Default,,0,0,0,,Ánh sáng.
Dialogue: 0,0:17:36.38,0:17:38.52,Default,,0,0,0,,Từ trên trời.
Dialogue: 0,0:17:38.89,0:17:40.92,Default,,0,0,0,,Mọi người tháo chạy...
Dialogue: 0,0:17:41.60,0:17:42.99,Default,,0,0,0,,hấp hối.
Dialogue: 0,0:17:43.56,0:17:46.67,Default,,0,0,0,,Anh có thấy người\Nthân của mình?
Dialogue: 0,0:17:47.94,0:17:49.11,Default,,0,0,0,,Bọn ngoài hành tinh.
Dialogue: 0,0:17:49.19,0:17:52.74,Default,,0,0,0,,Chúng không muốn\Nchúng ta biết.
Dialogue: 0,0:17:53.15,0:17:56.93,Default,,0,0,0,,Chúng muốn thao túng\Nvà bịt miệng ta.
Dialogue: 0,0:17:56.93,0:18:01.25,Default,,0,0,0,,Nên chúng xóa ký ức của ta,\Nnếu anh thấy được chúng
Dialogue: 0,0:18:01.25,0:18:03.89,Default,,0,0,0,,- thế giới sẽ hỗn loạn.\N- Chris.
Dialogue: 0,0:18:04.79,0:18:07.27,Default,,0,0,0,,Anh muốn vào\Ntrước không?
Dialogue: 0,0:18:12.29,0:18:14.70,Default,,0,0,0,,Chúng ta không bị điên.
Dialogue: 0,0:18:15.01,0:18:16.37,Default,,0,0,0,,Tôi chỉ...
Dialogue: 0,0:18:16.67,0:18:18.84,Default,,0,0,0,,Tôi muốn dừng\Nviệc này lại.
Dialogue: 0,0:18:38.28,0:18:41.01,Default,,0,0,0,,{\i1}Tiến hành sơ tán\Nhành tinh.{\i0}
Dialogue: 0,0:18:41.78,0:18:43.34,Default,,0,0,0,,Chúng sẽ quay lại.
Dialogue: 0,0:18:43.87,0:18:45.76,Default,,0,0,0,,Gặp lại mọi người\Nở nhà máy.
Dialogue: 0,0:19:00.59,0:19:01.88,Default,,0,0,0,,Thế nào rồi?
Dialogue: 0,0:19:03.64,0:19:04.85,Default,,0,0,0,,Anh đến đó rồi.
Dialogue: 0,0:19:05.31,0:19:06.79,Default,,0,0,0,,Họ nói sao?
Dialogue: 0,0:19:07.39,0:19:08.56,Default,,0,0,0,,Ừ thì...
Dialogue: 0,0:19:09.43,0:19:11.77,Default,,0,0,0,,Bác sĩ chưa kịp nói gì.
Dialogue: 0,0:19:11.77,0:19:14.27,Default,,0,0,0,,Anh bỏ về trước.
Dialogue: 0,0:19:14.73,0:19:15.77,Default,,0,0,0,,Nghe anh này.
Dialogue: 0,0:19:15.86,0:19:18.55,Default,,0,0,0,,Có thể thứ anh\Nđã thấy là thật.
Dialogue: 0,0:19:18.65,0:19:20.55,Default,,0,0,0,,Những giấc mơ.
Dialogue: 0,0:19:20.95,0:19:23.76,Default,,0,0,0,,Biết đâu điềm gở\Nsắp xảy ra
Dialogue: 0,0:19:23.91,0:19:25.29,Default,,0,0,0,,và đó là những dấu hiệu?
Dialogue: 0,0:19:25.37,0:19:26.77,Default,,0,0,0,,Thôi đi...
Dialogue: 0,0:19:28.41,0:19:31.87,Default,,0,0,0,,Mọi người sắp đến,\Nanh làm sao thì làm.
Dialogue: 0,0:19:31.96,0:19:34.24,Default,,0,0,0,,Anh xin lỗi.
Dialogue: 0,0:19:59.53,0:20:02.74,Default,,0,0,0,,Hệ thống cống quá cũ,\Nkhông ai xử lý nổi.
Dialogue: 0,0:20:02.82,0:20:05.69,Default,,0,0,0,,Thăng chức là cơ hội tốt,\NAlice. Cậu phải vui lên.
Dialogue: 0,0:20:05.74,0:20:07.45,Default,,0,0,0,,- Tớ sẽ cố.\N- Thế thì tốt.
Dialogue: 0,0:20:11.83,0:20:13.43,Default,,0,0,0,,Mà này.
Dialogue: 0,0:20:13.87,0:20:16.58,Default,,0,0,0,,Đừng lo cho hắn ta.\NĐêm nay là của cậu.
Dialogue: 0,0:20:16.59,0:20:17.92,Default,,0,0,0,,Biết rồi.
Dialogue: 0,0:20:18.38,0:20:20.64,Default,,0,0,0,,- Mẹ ơi?\N- Sao thế?
Dialogue: 0,0:20:21.47,0:20:25.10,Default,,0,0,0,,Bố bảo con xin mẹ dẫn\NHanna và Lucy đến nhà mình.
Dialogue: 0,0:20:25.10,0:20:26.76,Default,,0,0,0,,- Để làm gì.\N- Để chơi nhạc.
Dialogue: 0,0:20:26.85,0:20:29.77,Default,,0,0,0,,Bỏ đi là bất lịch sự\Nlắm đấy con yêu.
Dialogue: 0,0:20:48.16,0:20:49.53,Default,,0,0,0,,Ngủ ngon.
Dialogue: 0,0:20:50.83,0:20:54.43,Default,,0,0,0,,Khách đang về, anh muốn\Nra chào tạm biệt chứ?
Dialogue: 0,0:20:54.92,0:20:56.23,Default,,0,0,0,,Xin chào.
Dialogue: 0,0:20:57.17,0:20:59.34,Default,,0,0,0,,Vợ tôi bảo anh đến à?
Dialogue: 0,0:20:59.75,0:21:03.06,Default,,0,0,0,,Còn tệ hơn thế.\NLà vợ tôi bảo.
Dialogue: 0,0:21:09.10,0:21:11.36,Default,,0,0,0,,Đôi lúc tôi cũng\Nthích nhìn lên trời.
Dialogue: 0,0:21:12.27,0:21:15.13,Default,,0,0,0,,Để đưa mọi thứ\Nvào tầm ngắm.
Dialogue: 0,0:21:15.73,0:21:17.80,Default,,0,0,0,,Ý anh là sao hả?
Dialogue: 0,0:21:21.15,0:21:23.12,Default,,0,0,0,,Tôi biết đây không\Nphải việc của tôi.
Dialogue: 0,0:21:23.19,0:21:25.07,Default,,0,0,0,,Nhưng tôi nghĩ anh\Nđang nhìn sai hướng.
Dialogue: 0,0:21:25.15,0:21:27.99,Default,,0,0,0,,Anh chăm chú nhìn\Nra ngoài kia
Dialogue: 0,0:21:28.07,0:21:30.49,Default,,0,0,0,,thay vì tập trung\Nnhìn vào trong này.
Dialogue: 0,0:21:30.58,0:21:31.99,Default,,0,0,0,,Anh nói đúng.
Dialogue: 0,0:21:35.16,0:21:36.98,Default,,0,0,0,,Cái quái gì thế?
Dialogue: 0,0:21:38.46,0:21:39.82,Default,,0,0,0,,Anh thấy nó à?
Dialogue: 0,0:21:40.00,0:21:42.09,Default,,0,0,0,,Đùa à? Dĩ nhiên\Nlà tôi thấy.
Dialogue: 0,0:22:00.36,0:22:01.89,Default,,0,0,0,,Cái gì thế?
Dialogue: 0,0:22:28.00,0:22:29.83,Default,,0,0,0,,Có sao không?
Dialogue: 0,0:22:32.97,0:22:34.57,Default,,0,0,0,,Cái quái gì vậy?
Dialogue: 0,0:22:47.69,0:22:50.14,Default,,0,0,0,,Mọi người mau vào trong!
Dialogue: 0,0:22:51.20,0:22:53.50,Default,,0,0,0,,Các con tôi!\NHanna!
Dialogue: 0,0:22:53.73,0:22:55.02,Default,,0,0,0,,Megan!
Dialogue: 0,0:23:01.17,0:23:02.36,Default,,0,0,0,,Hanna!
Dialogue: 0,0:23:02.83,0:23:04.10,Default,,0,0,0,,Megan!
Dialogue: 0,0:23:04.59,0:23:06.99,Default,,0,0,0,,- Megan!\N- Hanna!
Dialogue: 0,0:23:08.55,0:23:10.35,Default,,0,0,0,,Này!
Dialogue: 0,0:23:13.01,0:23:14.13,Default,,0,0,0,,Chị con đâu?
Dialogue: 0,0:23:14.22,0:23:16.80,Default,,0,0,0,,- Con muốn mẹ!\N- Ba biết rồi. Alice!
Dialogue: 0,0:23:17.06,0:23:18.61,Default,,0,0,0,,- Lucy!\N- Mẹ!
Dialogue: 0,0:23:18.68,0:23:20.60,Default,,0,0,0,,- Lại đây con.\N- Chị con đâu?
Dialogue: 0,0:23:20.69,0:23:22.98,Default,,0,0,0,,- Chắc ở nhà bọn tớ.\N- Ở đây với Lucy!
Dialogue: 0,0:23:23.06,0:23:25.09,Default,,0,0,0,,Anh sẽ tìm Hanna\Nvà đưa nó về.
Dialogue: 0,0:23:25.48,0:23:26.56,Default,,0,0,0,,Không sao đâu con.
Dialogue: 0,0:23:26.65,0:23:28.90,Default,,0,0,0,,{\i1}Thông báo khẩn cấp.{\i0}
Dialogue: 0,0:23:29.19,0:23:31.57,Default,,0,0,0,,{\i1}Hãy đến cơ quan\Nnhà nước gần nhất{\i0}
Dialogue: 0,0:23:31.66,0:23:33.58,Default,,0,0,0,,{\i1}để bảo đảm an toàn.{\i0}
Dialogue: 0,0:23:33.66,0:23:35.66,Default,,0,0,0,,{\i1}Thông báo khẩn cấp.{\i0}
Dialogue: 0,0:23:35.74,0:23:38.07,Default,,0,0,0,,{\i1}Hãy đến cơ quan\Nnhà nước gần nhất{\i0}
Dialogue: 0,0:23:38.16,0:23:40.09,Default,,0,0,0,,{\i1}để bảo đảm an toàn.{\i0}
Dialogue: 0,0:23:40.21,0:23:42.17,Default,,0,0,0,,{\i1}Thông báo khẩn cấp.{\i0}
Dialogue: 0,0:23:46.04,0:23:47.33,Default,,0,0,0,,Megan!
Dialogue: 0,0:23:48.42,0:23:49.75,Default,,0,0,0,,Megan!
Dialogue: 0,0:23:49.84,0:23:51.84,Default,,0,0,0,,- Hanna!\N- Megan!
Dialogue: 0,0:23:53.34,0:23:54.35,Default,,0,0,0,,Hanna!
Dialogue: 0,0:24:03.15,0:24:04.65,Default,,0,0,0,,Nằm xuống!
Dialogue: 0,0:24:28.13,0:24:29.60,Default,,0,0,0,,Chúng không có ở đây.
Dialogue: 0,0:24:31.21,0:24:32.69,Default,,0,0,0,,Tôi biết chúng ở đâu.
Dialogue: 0,0:24:42.73,0:24:43.57,Default,,0,0,0,,Ôi không.
Dialogue: 0,0:24:43.64,0:24:45.98,Default,,0,0,0,,Thứ trong phi thuyền\Nchắc đã vào trong này.
Dialogue: 0,0:24:46.40,0:24:48.87,Default,,0,0,0,,Megan con có\Ntrong đó không?
Dialogue: 0,0:24:49.11,0:24:50.61,Default,,0,0,0,,Hanna, con có\Nở đó không?
Dialogue: 0,0:24:51.61,0:24:52.61,Default,,0,0,0,,Hanna!
Dialogue: 0,0:24:57.45,0:24:59.63,Default,,0,0,0,,Ta phải đi xuống đó.
Dialogue: 0,0:25:14.67,0:25:15.67,Default,,0,0,0,,Nhanh lên!
Dialogue: 0,0:25:21.72,0:25:24.71,Default,,0,0,0,,Có chuyện gì vậy ba?
Dialogue: 0,0:25:25.48,0:25:28.46,Default,,0,0,0,,- Tìm được con rồi.\N- Ba ơi!
Dialogue: 0,0:25:29.36,0:25:30.78,Default,,0,0,0,,Mau ra khỏi đây thôi!
Dialogue: 0,0:25:30.86,0:25:32.25,Default,,0,0,0,,Đi nào.
Dialogue: 0,0:25:33.11,0:25:34.68,Default,,0,0,0,,Ba đỡ được con rồi.
Dialogue: 0,0:25:36.03,0:25:38.43,Default,,0,0,0,,Nào đến lượt con.
Dialogue: 0,0:25:42.45,0:25:43.45,Default,,0,0,0,,Đi thôi.
Dialogue: 0,0:25:45.33,0:25:48.37,Default,,0,0,0,,- Mẹ và Lucy đâu?\N- An toàn rồi.
Dialogue: 0,0:25:49.58,0:25:51.21,Default,,0,0,0,,An toàn rồi.\NĐưa chúng lên trên.
Dialogue: 0,0:27:13.50,0:27:16.44,Default,,0,0,0,,Đợi đã. Để tôi\Nđi xem tình hình.
Dialogue: 0,0:27:18.55,0:27:19.92,Default,,0,0,0,,Không sao đâu.
Dialogue: 0,0:27:51.08,0:27:52.87,Default,,0,0,0,,- Nào đi thôi!\N- Thấy gì không?
Dialogue: 0,0:27:52.96,0:27:55.30,Default,,0,0,0,,Nào đi thôi!
Dialogue: 0,0:27:58.46,0:28:00.97,Default,,0,0,0,,Hanna! Con có sao không?
Dialogue: 0,0:28:01.01,0:28:02.99,Default,,0,0,0,,Lạy Chúa. Con có sao không?
Dialogue: 0,0:28:03.22,0:28:05.02,Default,,0,0,0,,- Lucy!\N- Đi thôi.
Dialogue: 0,0:28:05.18,0:28:06.52,Default,,0,0,0,,Nào nhanh lên.
Dialogue: 0,0:28:07.13,0:28:08.84,Default,,0,0,0,,Nấp dưới bàn.
Dialogue: 0,0:28:10.68,0:28:12.88,Default,,0,0,0,,Ray đi đón Sam rồi sẽ\Nlên đây với chúng ta.
Dialogue: 0,0:28:12.94,0:28:15.96,Default,,0,0,0,,- Không sao rồi.\N- Anh có chuyện cần nói.
Dialogue: 0,0:28:18.86,0:28:21.70,Default,,0,0,0,,- Có thứ gì đó trong này.\N- Trong cơn ác mộng
Dialogue: 0,0:28:21.78,0:28:22.82,Default,,0,0,0,,anh đã thấy nó?
Dialogue: 0,0:28:22.86,0:28:24.65,Default,,0,0,0,,- Chính là nó!\N- Vậy giờ làm sao?
Dialogue: 0,0:28:24.74,0:28:27.09,Default,,0,0,0,,Anh đã thấy gì? Tiếp theo\Nlà gì? Giờ ta làm gì?
Dialogue: 0,0:28:27.12,0:28:31.28,Default,,0,0,0,,- Tiếp theo sẽ là gì?\N- Giấc mơ không theo thứ tự.
Dialogue: 0,0:28:31.45,0:28:33.02,Default,,0,0,0,,{\i1}Anh thấy David.{\i0}
Dialogue: 0,0:28:35.00,0:28:36.84,Default,,0,0,0,,Anh thấy ta nên\Nđến nhà máy.
Dialogue: 0,0:28:36.92,0:28:38.01,Default,,0,0,0,,Được rồi.
Dialogue: 0,0:28:43.38,0:28:45.87,Default,,0,0,0,,Ray và Sam lẽ ra\Nphải đến rồi chứ?
Dialogue: 0,0:28:46.18,0:28:48.07,Default,,0,0,0,,Chúng ta phải chặn cửa.
Dialogue: 0,0:28:48.56,0:28:50.53,Default,,0,0,0,,- Chặn bằng gì?\N- Đi nào Peter.
Dialogue: 0,0:29:06.87,0:29:09.18,Default,,0,0,0,,Đứng dậy nào các con.
Dialogue: 0,0:29:17.58,0:29:19.25,Default,,0,0,0,,Chúng ta phải giúp bố.
Dialogue: 0,0:29:19.34,0:29:21.50,Default,,0,0,0,,Hanna, con trông em nhé.
Dialogue: 0,0:29:21.50,0:29:23.25,Default,,0,0,0,,- Được chứ?\N- Mẹ đừng đi!
Dialogue: 0,0:29:23.34,0:29:24.77,Default,,0,0,0,,Đừng lo\Ncon yêu.
Dialogue: 0,0:29:40.06,0:29:41.29,Default,,0,0,0,,Alice!
Dialogue: 0,0:29:44.07,0:29:45.30,Default,,0,0,0,,Mẹ sẽ quay lại.
Dialogue: 0,0:29:45.32,0:29:46.73,Default,,0,0,0,,Ở yên đây nhé.
Dialogue: 0,0:29:46.82,0:29:48.85,Default,,0,0,0,,Đừng mở cửa.
Dialogue: 0,0:29:50.03,0:29:52.27,Default,,0,0,0,,Mẹ yêu các con nhiều.
Dialogue: 0,0:29:54.04,0:29:55.36,Default,,0,0,0,,Tốt rồi.
Dialogue: 0,0:30:08.22,0:30:09.43,Default,,0,0,0,,Em làm gì thế?
Dialogue: 0,0:30:09.51,0:30:11.70,Default,,0,0,0,,- Herman!\N- Lucy! Quay lại!
Dialogue: 0,0:31:13.24,0:31:14.57,Default,,0,0,0,,Đi nào!
Dialogue: 0,0:31:17.83,0:31:19.13,Default,,0,0,0,,Em đâu rồi?
Dialogue: 0,0:31:19.21,0:31:21.71,Default,,0,0,0,,Nó đi tìm Herman.
Dialogue: 0,0:32:05.71,0:32:08.49,Default,,0,0,0,,{\i1}Khỉ nhìn thấy,\Nkhỉ làm chuyện ấy.{\i0}
Dialogue: 0,0:32:37.08,0:32:40.10,Default,,0,0,0,,Ta không sợ\Nbóng tối của ngươi.
Dialogue: 0,0:32:40.95,0:32:43.68,Default,,0,0,0,,Ngươi không hại\Nta được đâu.
Dialogue: 0,0:33:16.28,0:33:17.81,Default,,0,0,0,,Lucy!
Dialogue: 0,0:33:18.07,0:33:19.49,Default,,0,0,0,,Lại đây!
Dialogue: 0,0:33:41.14,0:33:42.26,Default,,0,0,0,,Mẹ ơi!
Dialogue: 0,0:33:46.19,0:33:47.78,Default,,0,0,0,,Các con cẩn thận!
Dialogue: 0,0:33:48.10,0:33:50.06,Default,,0,0,0,,Lại đây nào.
Dialogue: 0,0:33:51.36,0:33:53.60,Default,,0,0,0,,Đi mau trước khi\Nchúng kéo đến.
Dialogue: 0,0:33:53.61,0:33:54.97,Default,,0,0,0,,Nào đi thôi.
Dialogue: 0,0:33:55.53,0:33:56.76,Default,,0,0,0,,Đi nào.
Dialogue: 0,0:33:59.87,0:34:01.65,Default,,0,0,0,,Cứ nhắm mắt vào.
Dialogue: 0,0:34:08.08,0:34:09.43,Default,,0,0,0,,Nhắm mắt lại.
Dialogue: 0,0:34:14.76,0:34:16.05,Default,,0,0,0,,Còn nữa ư?
Dialogue: 0,0:34:16.13,0:34:17.69,Default,,0,0,0,,Chúng đang kéo lên đây.
Dialogue: 0,0:34:59.68,0:35:00.91,Default,,0,0,0,,Ở yên đây nhé.
Dialogue: 0,0:35:08.52,0:35:10.35,Default,,0,0,0,,Ba ơi đừng đi!
Dialogue: 0,0:35:58.44,0:36:00.63,Default,,0,0,0,,Đừng! Ở lại đây!
Dialogue: 0,0:36:03.87,0:36:06.18,Default,,0,0,0,,Anh làm bọn tôi hết hồn.
Dialogue: 0,0:36:06.62,0:36:08.23,Default,,0,0,0,,Bọn trẻ đâu rồi?
Dialogue: 0,0:36:11.16,0:36:13.36,Default,,0,0,0,,Alice, ổn rồi.
Dialogue: 0,0:36:13.36,0:36:15.84,Default,,0,0,0,,Nào đi thôi.
Dialogue: 0,0:36:20.67,0:36:21.75,Default,,0,0,0,,Ôi lạy Chúa.
Dialogue: 0,0:36:21.84,0:36:23.35,Default,,0,0,0,,Lucy, Hanna!
Dialogue: 0,0:36:23.80,0:36:24.80,Default,,0,0,0,,Lucy!
Dialogue: 0,0:36:26.10,0:36:28.06,Default,,0,0,0,,Cậu đi đâu vậy Megan?
Dialogue: 0,0:36:28.18,0:36:29.46,Default,,0,0,0,,Có chuyện gì vậy?
Dialogue: 0,0:36:32.23,0:36:33.76,Default,,0,0,0,,Có chuyện gì vậy?
Dialogue: 0,0:36:41.11,0:36:42.96,Default,,0,0,0,,Mình sẽ an toàn\Ntrên này
Dialogue: 0,0:36:43.53,0:36:46.18,Default,,0,0,0,,- phải không mẹ?\N- Đi sát bên mẹ.
Dialogue: 0,0:36:51.33,0:36:52.98,Default,,0,0,0,,Chúng là ai?
Dialogue: 0,0:36:53.12,0:36:54.97,Default,,0,0,0,,Tại sao lại\Nlàm việc này?
Dialogue: 0,0:37:03.67,0:37:05.42,Default,,0,0,0,,Những người đó!
Dialogue: 0,0:37:06.72,0:37:08.11,Default,,0,0,0,,Chúng ta làm gì đây?
Dialogue: 0,0:37:08.22,0:37:09.66,Default,,0,0,0,,Anh không biết.
Dialogue: 0,0:37:15.19,0:37:16.31,Default,,0,0,0,,Nằm xuống!
Dialogue: 0,0:37:27.99,0:37:29.08,Default,,0,0,0,,Mẹ!
Dialogue: 0,0:37:31.70,0:37:33.22,Default,,0,0,0,,Chuyện gì vậy?
Dialogue: 0,0:37:49.47,0:37:52.55,Default,,0,0,0,,Alice. Ta không\Nthể ở lại đây.
Dialogue: 0,0:37:55.73,0:37:57.93,Default,,0,0,0,,- Đứng dậy Lucy.\N- Ra khỏi đây thôi.
Dialogue: 0,0:37:58.23,0:38:00.15,Default,,0,0,0,,- Cứ bình tĩnh.\N- Chúng ta đi đâu?
Dialogue: 0,0:38:00.23,0:38:02.55,Default,,0,0,0,,- Nắm chặt tay nhau.\N- Chúng ta đi đâu?
Dialogue: 0,0:38:02.61,0:38:04.60,Default,,0,0,0,,Có thể đi xuống\Nbằng cái này.
Dialogue: 0,0:38:04.60,0:38:06.70,Default,,0,0,0,,- Phải dùng cái này.\N- Sao? Không được.
Dialogue: 0,0:38:06.78,0:38:08.24,Default,,0,0,0,,- Alice!\N- Không được!
Dialogue: 0,0:38:08.32,0:38:10.22,Default,,0,0,0,,- Em không cho con lên đó đâu.\N- Alice!
Dialogue: 0,0:38:10.22,0:38:12.59,Default,,0,0,0,,- Em không vào đó đâu.\N- Chúng ta phải đi xuống.
Dialogue: 0,0:38:12.62,0:38:14.68,Default,,0,0,0,,Nếu phải chọn đưa\Ncon đi thang bộ
Dialogue: 0,0:38:14.70,0:38:17.29,Default,,0,0,0,,- với đi bằng cái đó...\N- Ở lại đây cũng chết.
Dialogue: 0,0:38:17.37,0:38:19.07,Default,,0,0,0,,Đi thang bộ cũng chết.
Dialogue: 0,0:38:19.08,0:38:21.41,Default,,0,0,0,,Em chỉ cần tin anh thôi.
Dialogue: 0,0:38:21.50,0:38:24.44,Default,,0,0,0,,Đừng chần chừ nữa,\Nrồi sẽ ổn thôi. Đi nào.
Dialogue: 0,0:38:27.88,0:38:28.71,Default,,0,0,0,,Không!
Dialogue: 0,0:38:28.80,0:38:30.82,Default,,0,0,0,,- Con không đi đâu!\N- Không sao đâu mà.
Dialogue: 0,0:38:30.85,0:38:32.51,Default,,0,0,0,,- Thôi được rồi.\N- Không!
Dialogue: 0,0:38:32.56,0:38:33.73,Default,,0,0,0,,Được rồi.
Dialogue: 0,0:38:34.27,0:38:36.66,Default,,0,0,0,,- Đi nào.\N- Đưa con bé xuống.
Dialogue: 0,0:38:36.66,0:38:38.59,Default,,0,0,0,,Con không muốn\Nđi đâu mẹ!
Dialogue: 0,0:38:53.62,0:38:54.87,Default,,0,0,0,,Con yêu!
Dialogue: 0,0:39:06.59,0:39:08.64,Default,,0,0,0,,Con sợ lắm mẹ!
Dialogue: 0,0:39:08.72,0:39:11.03,Default,,0,0,0,,Ở trên này con\Nsợ lắm mẹ ơi.
Dialogue: 0,0:39:14.97,0:39:16.46,Default,,0,0,0,,Có chuyện gì vậy?
Dialogue: 0,0:39:21.35,0:39:22.22,Default,,0,0,0,,Mẹ ơi!
Dialogue: 0,0:39:22.31,0:39:23.64,Default,,0,0,0,,Có ba đây rồi!
Dialogue: 0,0:39:23.73,0:39:25.91,Default,,0,0,0,,- Bám chắc vào.\N- Đừng cử động.
Dialogue: 0,0:39:25.91,0:39:28.65,Default,,0,0,0,,- Con muốn xuống.\N- Đừng cử động!
Dialogue: 0,0:39:32.32,0:39:33.83,Default,,0,0,0,,Ta phải vào trong.
Dialogue: 0,0:39:35.74,0:39:37.11,Default,,0,0,0,,Lucy, đừng nhìn xuống.
Dialogue: 0,0:39:37.20,0:39:38.78,Default,,0,0,0,,Đi nào Lucy.
Dialogue: 0,0:39:38.87,0:39:40.24,Default,,0,0,0,,Nào đi đi!
Dialogue: 0,0:39:40.92,0:39:42.64,Default,,0,0,0,,- Hanna đi thôi.\N- Chúng còn trong đó.
Dialogue: 0,0:39:42.67,0:39:43.88,Default,,0,0,0,,Hanna, ta phải đi ngay!
Dialogue: 0,0:39:43.96,0:39:45.58,Default,,0,0,0,,Nhanh lên!
Dialogue: 0,0:39:47.25,0:39:48.94,Default,,0,0,0,,- Bám lấy!\N- Đi thôi!
Dialogue: 0,0:41:18.43,0:41:20.64,Default,,0,0,0,,Mẹ tìm thấy\Ncái đèn pin.
Dialogue: 0,0:41:20.72,0:41:22.81,Default,,0,0,0,,Đừng bật nó lên nhé.
Dialogue: 0,0:41:35.91,0:41:37.31,Default,,0,0,0,,Trong giấc mơ...
Dialogue: 0,0:41:42.24,0:41:44.14,Default,,0,0,0,,anh có thấy\Nai bị giết không?
Dialogue: 0,0:41:51.63,0:41:55.47,Default,,0,0,0,,- {\i1}Chúng có giết các con không?{\i0}\N- Tỉnh lại mẹ ơi.
Dialogue: 0,0:41:56.01,0:41:57.74,Default,,0,0,0,,Tỉnh lại mẹ ơi.
Dialogue: 0,0:41:57.80,0:42:00.30,Default,,0,0,0,,{\i1}Peter, nói cho\Nem biết đi.{\i0}
Dialogue: 0,0:42:08.60,0:42:10.67,Default,,0,0,0,,Có ý tưởng gì chưa?
Dialogue: 0,0:42:12.02,0:42:14.93,Default,,0,0,0,,Công nghệ của chúng\Ncũng tương tự như ta
Dialogue: 0,0:42:14.93,0:42:18.23,Default,,0,0,0,,cò súng sinh trắc học,\Nchính chủ mới bắn được.
Dialogue: 0,0:42:18.32,0:42:21.61,Default,,0,0,0,,- Anh hack được chứ?\N- Tôi đang tìm cách.
Dialogue: 0,0:42:22.16,0:42:25.37,Default,,0,0,0,,Nhưng chỗ này cũng\Nkhông thể ở lâu.
Dialogue: 0,0:42:25.37,0:42:27.64,Default,,0,0,0,,Em nghĩ ta nên\Nrời thành phố.
Dialogue: 0,0:42:27.64,0:42:30.34,Default,,0,0,0,,Đi tìm chỗ trốn.\NChờ sự trợ giúp.
Dialogue: 0,0:42:30.34,0:42:32.25,Default,,0,0,0,,Đợi đã.
Dialogue: 0,0:42:32.80,0:42:35.90,Default,,0,0,0,,Nhà máy chỗ tôi\Nlàm cũng không xa
Dialogue: 0,0:42:36.09,0:42:38.19,Default,,0,0,0,,chỗ đó là cơ quan\Nnhà nước.
Dialogue: 0,0:42:41.68,0:42:44.24,Default,,0,0,0,,Nó ngay kia kìa.
Dialogue: 0,0:42:44.39,0:42:47.27,Default,,0,0,0,,Cách đây khoảng\N10 dãy nhà.
Dialogue: 0,0:42:47.35,0:42:49.06,Default,,0,0,0,,An toàn không?
Dialogue: 0,0:42:49.06,0:42:51.03,Default,,0,0,0,,Làm sao biết được\Nchỗ nào là an toàn?
Dialogue: 0,0:42:51.11,0:42:53.95,Default,,0,0,0,,Dưới nhà máy\Nlà khu lưu trữ.
Dialogue: 0,0:42:54.02,0:42:55.98,Default,,0,0,0,,Nó giống như pháo đài.
Dialogue: 0,0:42:56.82,0:42:58.73,Default,,0,0,0,,Chỉ cần đến\Nđược đó thôi.
Dialogue: 0,0:43:00.91,0:43:03.63,Default,,0,0,0,,Chúng bắn bất cứ\Nthứ gì di chuyển.
Dialogue: 0,0:43:03.63,0:43:05.68,Default,,0,0,0,,Nhưng đó là\Ncách duy nhất.
Dialogue: 0,0:43:05.91,0:43:07.25,Default,,0,0,0,,Cống ngầm.
Dialogue: 0,0:43:07.83,0:43:10.12,Default,,0,0,0,,Có lối vào\Nchỗ cống ngầm.
Dialogue: 0,0:43:10.21,0:43:13.22,Default,,0,0,0,,Đường nào?\NLối vào ở...
Dialogue: 0,0:43:13.71,0:43:14.98,Default,,0,0,0,,- Prescott!\N- Đường Prescott!
Dialogue: 0,0:43:15.05,0:43:18.81,Default,,0,0,0,,Cách đây vài dãy nhà. Có thể\Nvào nhà máy bằng cống ngầm.
Dialogue: 0,0:43:18.81,0:43:20.10,Default,,0,0,0,,- Đúng rồi.\N- Đúng vậy.
Dialogue: 0,0:43:20.10,0:43:22.45,Default,,0,0,0,,- Đi vài dãy nhà chắc ổn?\N- Đúng thế.
Dialogue: 0,0:43:56.67,0:43:58.00,Default,,0,0,0,,Bố!
Dialogue: 0,0:43:59.63,0:44:00.94,Default,,0,0,0,,Bố ơi!
Dialogue: 0,0:44:06.39,0:44:07.67,Default,,0,0,0,,Lucy!
Dialogue: 0,0:44:26.16,0:44:28.17,Default,,0,0,0,,Nào đi thôi!
Dialogue: 0,0:44:31.29,0:44:33.05,Default,,0,0,0,,Đi lối này!
Dialogue: 0,0:44:36.09,0:44:38.72,Default,,0,0,0,,Đi nào!\NNằm xuống!
Dialogue: 0,0:45:04.61,0:45:06.57,Default,,0,0,0,,Không sao đâu.
Dialogue: 0,0:45:11.50,0:45:12.58,Default,,0,0,0,,Ba!
Dialogue: 0,0:45:17.54,0:45:19.84,Default,,0,0,0,,- Ổn rồi đi thôi.\N- Không đi đâu!
Dialogue: 0,0:45:19.92,0:45:22.17,Default,,0,0,0,,Nhà máy cách 10 dãy nhà,\Nở đó sẽ an toàn.
Dialogue: 0,0:45:22.17,0:45:24.82,Default,,0,0,0,,Bố mẹ sẽ\Nbảo vệ con.
Dialogue: 0,0:45:25.13,0:45:27.06,Default,,0,0,0,,Được chứ?\NCả 2 đứa luôn.
Dialogue: 0,0:45:27.30,0:45:28.51,Default,,0,0,0,,- Được chứ?\N- Đi thôi!
Dialogue: 0,0:45:28.60,0:45:29.96,Default,,0,0,0,,- Được chưa? Đi nào!\N- Đi thôi!
Dialogue: 0,0:45:32.98,0:45:35.77,Default,,0,0,0,,Mình sẽ chết như\Nnhà Megan hả ba?
Dialogue: 0,0:45:35.94,0:45:38.36,Default,,0,0,0,,Đừng lo con yêu,\Nrồi sẽ ổn thôi.
Dialogue: 0,0:45:47.74,0:45:49.68,Default,,0,0,0,,Lại đây Lucy.
Dialogue: 0,0:45:51.87,0:45:53.47,Default,,0,0,0,,Đừng mà!
Dialogue: 0,0:46:13.47,0:46:15.41,Default,,0,0,0,,- Họ là ai?\N- Anh không biết.
Dialogue: 0,0:46:18.90,0:46:20.70,Default,,0,0,0,,Đi nào! Cúi xuống.
Dialogue: 0,0:46:27.03,0:46:29.09,Default,,0,0,0,,Nằm xuống.
Dialogue: 0,0:46:37.92,0:46:39.81,Default,,0,0,0,,- Đi thôi!\N- Đi nào!
Dialogue: 0,0:46:39.88,0:46:41.00,Default,,0,0,0,,- Chạy thôi!\N- Đi đi!
Dialogue: 0,0:46:41.04,0:46:42.19,Default,,0,0,0,,Chạy mau lên!
Dialogue: 0,0:47:05.15,0:47:06.33,Default,,0,0,0,,Lối này.
Dialogue: 0,0:47:06.69,0:47:08.98,Default,,0,0,0,,- Đi nào.\N- Đi hướng nào?
Dialogue: 0,0:47:09.20,0:47:10.65,Default,,0,0,0,,Bên phải.
Dialogue: 0,0:47:22.96,0:47:23.96,Default,,0,0,0,,Được rồi.
Dialogue: 0,0:47:25.05,0:47:28.29,Default,,0,0,0,,- Ở phía bên kia nhà máy.\N- Để anh đi trước.
Dialogue: 0,0:47:28.59,0:47:29.95,Default,,0,0,0,,Cúi xuống.
Dialogue: 0,0:47:54.49,0:47:55.75,Default,,0,0,0,,Đi nhanh lên.
Dialogue: 0,0:47:56.62,0:47:59.56,Default,,0,0,0,,Nhanh lên đây đi.
Dialogue: 0,0:48:01.16,0:48:03.09,Default,,0,0,0,,Nào lại đây.
Dialogue: 0,0:48:03.13,0:48:05.11,Default,,0,0,0,,Cẩn thận đấy.
Dialogue: 0,0:48:07.59,0:48:09.70,Default,,0,0,0,,Ở yên đó nhé.
Dialogue: 0,0:48:10.47,0:48:12.74,Default,,0,0,0,,- Đi nào.\N- Lại chỗ ba đi.
Dialogue: 0,0:48:18.47,0:48:20.02,Default,,0,0,0,,Lucy! Lucy!
Dialogue: 0,0:48:23.10,0:48:24.47,Default,,0,0,0,,Lucy lại đây!
Dialogue: 0,0:48:29.53,0:48:30.73,Default,,0,0,0,,Giữ lấy con bé!
Dialogue: 0,0:48:31.90,0:48:33.23,Default,,0,0,0,,Mau lên!
Dialogue: 0,0:48:39.37,0:48:41.31,Default,,0,0,0,,Đi thôi!
Dialogue: 0,0:49:07.98,0:49:09.67,Default,,0,0,0,,Mọi người ổn chứ?
Dialogue: 0,0:49:09.98,0:49:11.29,Default,,0,0,0,,Con có sao không?
Dialogue: 0,0:49:19.53,0:49:20.86,Default,,0,0,0,,Mẹ?
Dialogue: 0,0:49:25.29,0:49:26.63,Default,,0,0,0,,Ba ơi?
Dialogue: 0,0:49:27.29,0:49:28.99,Default,,0,0,0,,Làm gì đó đi.
Dialogue: 0,0:49:53.39,0:50:10.01,Default,,0,0,0,,{\c&H00ffff&}Xóm Anh Ngữ{\c} biên dịch\N{\i1\c&Hf0b000&}fb.com/xomanhngu{\c}{\i0}
Dialogue: 0,0:50:13.71,0:50:15.22,Default,,0,0,0,,Mẹ ơi.
Dialogue: 0,0:50:16.47,0:50:18.28,Default,,0,0,0,,Không sao đâu con.
Dialogue: 0,0:50:18.28,0:50:20.11,Default,,0,0,0,,Đi cẩn thận.
Dialogue: 0,0:50:20.89,0:50:22.25,Default,,0,0,0,,Đây rồi.
Dialogue: 0,0:50:24.47,0:50:26.12,Default,,0,0,0,,Hướng nào?
Dialogue: 0,0:50:26.56,0:50:28.19,Default,,0,0,0,,Ta đang ở\Ndưới công viên.
Dialogue: 0,0:50:29.44,0:50:31.07,Default,,0,0,0,,Hướng Bắc.
Dialogue: 0,0:50:32.32,0:50:33.75,Default,,0,0,0,,Đi nào các con.
Dialogue: 0,0:50:38.45,0:50:40.45,Default,,0,0,0,,Có chuyện gì\Nở đây vậy?
Dialogue: 0,0:50:41.12,0:50:44.42,Default,,0,0,0,,- Anh thấy gì không?\N- Không phải như này.
Dialogue: 0,0:50:44.87,0:50:47.43,Default,,0,0,0,,Hứa với em\Nlà anh sẽ
Dialogue: 0,0:50:48.12,0:50:50.13,Default,,0,0,0,,bảo vệ các con.
Dialogue: 0,0:50:51.92,0:50:54.24,Default,,0,0,0,,Dù cho có chuyện gì xảy ra.
Dialogue: 0,0:50:54.42,0:50:55.25,Default,,0,0,0,,Được chứ?
Dialogue: 0,0:50:55.34,0:50:57.42,Default,,0,0,0,,- Alice...\N- Anh hứa đi.
Dialogue: 0,0:50:58.97,0:51:01.52,Default,,0,0,0,,Anh không muốn làm\Nviệc này một mình.
Dialogue: 0,0:51:01.72,0:51:04.60,Default,,0,0,0,,- Xin em đấy. Anh không muốn...\N- Hứa đi...
Dialogue: 0,0:51:04.60,0:51:06.58,Default,,0,0,0,,Anh biết em\Nlàm được mà.
Dialogue: 0,0:51:06.85,0:51:09.20,Default,,0,0,0,,Nào, em làm\Nđược mà.
Dialogue: 0,0:51:13.52,0:51:15.77,Default,,0,0,0,,Đúng rồi, phải thế chứ.
Dialogue: 0,0:51:15.98,0:51:16.86,Default,,0,0,0,,Đúng thế.
Dialogue: 0,0:51:16.94,0:51:18.97,Default,,0,0,0,,Thế mới là\NAlice của anh.
Dialogue: 0,0:51:19.03,0:51:20.32,Default,,0,0,0,,Alice của anh.
Dialogue: 0,0:51:24.99,0:51:26.27,Default,,0,0,0,,Em cần phải ngồi.
Dialogue: 0,0:51:26.33,0:51:28.46,Default,,0,0,0,,Được rồi\Nkhông sao đâu.
Dialogue: 0,0:51:31.42,0:51:32.42,Default,,0,0,0,,Được rồi.
Dialogue: 0,0:51:37.34,0:51:38.84,Default,,0,0,0,,Xoa tay mẹ đi.
Dialogue: 0,0:51:39.63,0:51:41.88,Default,,0,0,0,,- Mẹ không sao chứ?\N- Mẹ sẽ ổn thôi.
Dialogue: 0,0:51:41.97,0:51:43.69,Default,,0,0,0,,Giữ ấm tay mẹ.\NGiữ ấm tay mẹ.
Dialogue: 0,0:51:44.64,0:51:47.68,Default,,0,0,0,,Em không sao rồi.
Dialogue: 0,0:51:48.98,0:51:51.78,Default,,0,0,0,,Để mẹ nghỉ ngơi.\NKhông sao rồi.
Dialogue: 0,0:51:51.78,0:51:53.72,Default,,0,0,0,,- Ổn rồi.\N- Ba ơi!
Dialogue: 0,0:51:58.90,0:52:00.96,Default,,0,0,0,,Để ba tôi yên!
Dialogue: 0,0:52:14.00,0:52:16.05,Default,,0,0,0,,Con sợ quá ba ơi.
Dialogue: 0,0:52:16.75,0:52:18.41,Default,,0,0,0,,Cứ ở yên đó.
Dialogue: 0,0:53:07.30,0:53:08.98,Default,,0,0,0,,Sao mày lại đến đây?
Dialogue: 0,0:53:13.48,0:53:14.98,Default,,0,0,0,,Muốn hại gia đình tao?
Dialogue: 0,0:53:15.06,0:53:17.73,Default,,0,0,0,,Mày muốn thế à?\NHại gia đình tao ư?
Dialogue: 0,0:54:02.90,0:54:04.16,Default,,0,0,0,,Mày là cái...
Dialogue: 0,0:54:05.15,0:54:06.57,Default,,0,0,0,,Mày là ai?
Dialogue: 0,0:54:12.04,0:54:13.82,Default,,0,0,0,,Sao lại làm chuyện này?
Dialogue: 0,0:54:15.21,0:54:16.60,Default,,0,0,0,,Mẹ ơi?
Dialogue: 0,0:54:18.04,0:54:19.32,Default,,0,0,0,,Mẹ à?
Dialogue: 0,0:54:19.88,0:54:21.55,Default,,0,0,0,,Mẹ không tỉnh lại nữa!
Dialogue: 0,0:54:21.63,0:54:23.84,Default,,0,0,0,,Tỉnh lại mẹ ơi!
Dialogue: 0,0:54:23.92,0:54:25.80,Default,,0,0,0,,- Mẹ nói gì đi chứ!\N- Mẹ!
Dialogue: 0,0:54:26.30,0:54:29.09,Default,,0,0,0,,- Tỉnh lại mẹ ơi!\N- Ba ơi giúp mẹ!
Dialogue: 0,0:54:29.59,0:54:30.42,Default,,0,0,0,,Mẹ!
Dialogue: 0,0:54:30.51,0:54:33.21,Default,,0,0,0,,Dựng mẹ dậy nào.
Dialogue: 0,0:54:38.48,0:54:41.12,Default,,0,0,0,,Các con ra\Nphía sau ba.
Dialogue: 0,0:54:41.44,0:54:43.95,Default,,0,0,0,,Ra phía sau.\NLucy, ra sau đi.
Dialogue: 0,0:54:50.82,0:54:52.07,Default,,0,0,0,,Cõng cô ấy lên.
Dialogue: 0,0:54:52.66,0:54:54.26,Default,,0,0,0,,Có hiểu không hả?
Dialogue: 0,0:54:56.91,0:54:58.66,Default,,0,0,0,,Cõng cô ấy lên ngay.
Dialogue: 0,0:55:05.55,0:55:07.06,Default,,0,0,0,,Cõng cô ấy lên ngay.
Dialogue: 0,0:55:07.09,0:55:09.18,Default,,0,0,0,,Tao muốn mày\Nđi thật chậm.
Dialogue: 0,0:56:12.11,0:56:13.19,Default,,0,0,0,,Xin chào?
Dialogue: 0,0:56:14.82,0:56:16.02,Default,,0,0,0,,Xin chào?
Dialogue: 0,0:56:16.02,0:56:17.92,Default,,0,0,0,,Xin chào?
Dialogue: 0,0:56:18.16,0:56:19.94,Default,,0,0,0,,Thử cửa khác xem sao.
Dialogue: 0,0:56:20.12,0:56:21.83,Default,,0,0,0,,Mẹ cần được giúp đỡ.
Dialogue: 0,0:56:34.93,0:56:35.98,Default,,0,0,0,,Từ từ đã.
Dialogue: 0,0:56:36.05,0:56:38.28,Default,,0,0,0,,- Đây rồi.\N- Đây này. Đi đi!
Dialogue: 0,0:56:38.28,0:56:40.21,Default,,0,0,0,,Từ từ đã nào!
Dialogue: 0,0:56:40.23,0:56:41.23,Default,,0,0,0,,Xin đừng.
Dialogue: 0,0:56:41.27,0:56:43.50,Default,,0,0,0,,Vợ tôi không khỏe.\NXin hãy giúp đỡ.
Dialogue: 0,0:56:43.50,0:56:44.65,Default,,0,0,0,,Peter?
Dialogue: 0,0:56:45.02,0:56:47.26,Default,,0,0,0,,David?
Dialogue: 0,0:56:48.02,0:56:49.75,Default,,0,0,0,,Anh đưa ai về?
Dialogue: 0,0:56:49.75,0:56:52.08,Default,,0,0,0,,Alice, cô ấy không ổn.\NPhải cứu cô ấy ngay.
Dialogue: 0,0:56:52.15,0:56:54.64,Default,,0,0,0,,Đem băng ca\Nra đây ngay.
Dialogue: 0,0:56:55.77,0:56:57.09,Default,,0,0,0,,Băng ca ra liền.
Dialogue: 0,0:56:57.09,0:56:59.78,Default,,0,0,0,,Cô ấy trúng đạn ở\Nhông trái. Khá nặng.
Dialogue: 0,0:56:59.78,0:57:01.13,Default,,0,0,0,,Còn vết thương\Nnào nữa không?
Dialogue: 0,0:57:01.20,0:57:02.63,Default,,0,0,0,,- Không có...\N- Ba ơi!
Dialogue: 0,0:57:03.50,0:57:04.92,Default,,0,0,0,,Chúng đang đến.
Dialogue: 0,0:57:05.38,0:57:07.35,Default,,0,0,0,,Mọi người mau\Nvào trong!
Dialogue: 0,0:57:29.65,0:57:31.44,Default,,0,0,0,,- Quét cô ta.\N- Phải cầm máu.
Dialogue: 0,0:57:31.44,0:57:32.61,Default,,0,0,0,,Cái gì vậy?
Dialogue: 0,0:57:33.36,0:57:34.72,Default,,0,0,0,,- Dừng lại!\N- Dừng lại!
Dialogue: 0,0:57:34.72,0:57:36.04,Default,,0,0,0,,Anh làm gì vậy hả?
Dialogue: 0,0:57:36.11,0:57:37.94,Default,,0,0,0,,Không sao đâu.\NChẩn đoán thôi.
Dialogue: 0,0:57:38.03,0:57:39.45,Default,,0,0,0,,Anh ấy biết mình\Nđang làm gì.
Dialogue: 0,0:57:39.53,0:57:42.20,Default,,0,0,0,,Dừng lại ngay!\NĐừng chạm vào cô ấy!
Dialogue: 0,0:57:42.29,0:57:44.22,Default,,0,0,0,,- Đừng làm đau mẹ!\N- Tôi bảo dừng lại!
Dialogue: 0,0:57:44.25,0:57:46.59,Default,,0,0,0,,- Mọi người bình tĩnh.\N- Chuyện quái gì vậy?
Dialogue: 0,0:57:46.67,0:57:48.35,Default,,0,0,0,,Chúng ta đều\Nlà bạn mà.
Dialogue: 0,0:57:52.26,0:57:54.10,Default,,0,0,0,,Anh biết trước\Nmọi chuyện à?
Dialogue: 0,0:57:54.13,0:57:57.47,Default,,0,0,0,,Chúng ta chờ ngày\Nnày lâu lắm rồi.
Dialogue: 0,0:57:57.47,0:57:59.74,Default,,0,0,0,,Nhưng giờ thì\Nlo cho vợ anh đi
Dialogue: 0,0:57:59.74,0:58:01.73,Default,,0,0,0,,và đưa cả nhà\Nđến nơi an toàn.
Dialogue: 0,0:58:01.76,0:58:02.89,Default,,0,0,0,,Xin anh đấy.
Dialogue: 0,0:58:08.69,0:58:10.80,Default,,0,0,0,,Có một trạm tàu\Nbỏ hoang bên dưới
Dialogue: 0,0:58:10.80,0:58:13.40,Default,,0,0,0,,dẫn đến kho vũ khí ở\Ncăn cứ dưới lòng đất.
Dialogue: 0,0:58:13.49,0:58:16.86,Default,,0,0,0,,- Kho hàng đâu rồi?\N- Chuyển hết rồi. Sẽ an toàn thôi.
Dialogue: 0,0:58:16.86,0:58:18.36,Default,,0,0,0,,Chúng không tìm\Nthấy ta đâu.
Dialogue: 0,0:58:18.45,0:58:21.25,Default,,0,0,0,,Khi nào tới đó ta sẽ lên\Nkế hoạch phản công.
Dialogue: 0,0:58:21.33,0:58:22.84,Default,,0,0,0,,Nhưng tôi đâu\Nphải là lính.
Dialogue: 0,0:58:22.84,0:58:24.91,Default,,0,0,0,,Rồi anh sẽ ngạc nhiên\Nvới khả năng của mình.
Dialogue: 0,0:58:29.79,0:58:30.83,Default,,0,0,0,,Chúng ở ngay trên\Nchúng ta.
Dialogue: 0,0:58:30.92,0:58:33.30,Default,,0,0,0,,Chúng sẽ vào\Nđược đây.
Dialogue: 0,0:58:33.38,0:58:36.18,Default,,0,0,0,,Coi chừng thủng trần,\Nđưa mọi người xuống dưới.
Dialogue: 0,0:58:36.26,0:58:37.38,Default,,0,0,0,,- Tuân lệnh.\N- Xử hắn đi.
Dialogue: 0,0:58:37.43,0:58:39.77,Default,,0,0,0,,- Đem ra ngoài giết.\N- Vợ của anh...
Dialogue: 0,0:58:40.14,0:58:42.67,Default,,0,0,0,,bị thương quá nặng,\Nchúng tôi bó tay.
Dialogue: 0,0:58:42.67,0:58:45.03,Default,,0,0,0,,Nếu cô ấy kịp sơ tán\Nthì còn có cơ hội.
Dialogue: 0,0:58:45.06,0:58:46.64,Default,,0,0,0,,Nhưng cô ấy\Ntắt nguồn rồi.
Dialogue: 0,0:58:46.64,0:58:49.14,Default,,0,0,0,,- Chúng tôi bó tay.\N- Phải có cách gì đó.
Dialogue: 0,0:58:49.23,0:58:51.53,Default,,0,0,0,,- Chúng tôi không có thiết bị.\N- Chờ đã nào!
Dialogue: 0,0:58:51.61,0:58:54.70,Default,,0,0,0,,- Ba đừng rời bỏ mẹ.\N- Không ai bỏ mẹ cả.
Dialogue: 0,0:58:54.73,0:58:56.66,Default,,0,0,0,,Này, tôi có thể\Ngiúp cô ấy!
Dialogue: 0,0:58:57.74,0:59:00.12,Default,,0,0,0,,Tôi có thể giúp cô ấy\Nhoạt động lại.
Dialogue: 0,0:59:00.20,0:59:02.87,Default,,0,0,0,,- Gì?\N- Tôi có thể cứu cô ấy!
Dialogue: 0,0:59:03.16,0:59:04.54,Default,,0,0,0,,Anh cứu được cô ấy?
Dialogue: 0,0:59:04.62,0:59:06.59,Default,,0,0,0,,- Đùa tôi hả?\N- Không hề.
Dialogue: 0,0:59:06.59,0:59:09.62,Default,,0,0,0,,Bác sĩ bó tay mà\Nanh bảo cứu được?
Dialogue: 0,0:59:09.71,0:59:11.47,Default,,0,0,0,,Tôi sẽ giữ cô ấy sống\Nđủ lâu để chờ cứu viện.
Dialogue: 0,0:59:11.54,0:59:13.10,Default,,0,0,0,,- Sao tôi phải tin anh?\N- Hắn xạo đấy.
Dialogue: 0,0:59:13.13,0:59:14.64,Default,,0,0,0,,- Ta phải đi ngay.\N- Đừng có tin.
Dialogue: 0,0:59:14.67,0:59:18.08,Default,,0,0,0,,Tôi am hiểu giống loài\Ncác anh. Có cách mà.
Dialogue: 0,0:59:21.18,0:59:24.63,Default,,0,0,0,,Hết thời gian rồi. Sơ tán\Nngay thôi. Đưa hắn đi đi.
Dialogue: 0,0:59:25.35,0:59:26.88,Default,,0,0,0,,- Đi nào!\N- Tôi biết phải làm gì!
Dialogue: 0,0:59:27.06,0:59:28.55,Default,,0,0,0,,- Đi nào!\N- Hãy để tôi giúp!
Dialogue: 0,0:59:34.36,0:59:36.57,Default,,0,0,0,,Thôi được rồi!
Dialogue: 0,0:59:40.07,0:59:41.74,Default,,0,0,0,,Nào các con.
Dialogue: 0,0:59:42.57,0:59:45.15,Default,,0,0,0,,Các con phải nghe ba.
Dialogue: 0,0:59:45.24,0:59:46.78,Default,,0,0,0,,- Hãy đi với chú David.\N- Không!
Dialogue: 0,0:59:46.87,0:59:49.38,Default,,0,0,0,,Ba ở lại chờ\Nmẹ khỏe hẳn
Dialogue: 0,0:59:49.46,0:59:51.66,Default,,0,0,0,,rồi ba sẽ đi với\Ncác con, ba hứa.
Dialogue: 0,0:59:51.71,0:59:53.03,Default,,0,0,0,,Ta sẽ gặp nhau\Nở chỗ đoàn tàu.
Dialogue: 0,0:59:54.46,0:59:55.46,Default,,0,0,0,,Được chứ?
Dialogue: 0,0:59:55.50,0:59:57.70,Default,,0,0,0,,Tàu sơ tán\Nở tầng dưới.
Dialogue: 0,0:59:57.70,1:00:00.01,Default,,0,0,0,,- Chúng tôi sẽ cố chờ.\N- Được rồi.
Dialogue: 0,1:00:00.68,1:00:02.14,Default,,0,0,0,,Nào nào các con.
Dialogue: 0,1:00:02.22,1:00:04.10,Default,,0,0,0,,- Ba yêu các con mà.\N- Không!
Dialogue: 0,1:00:04.18,1:00:05.27,Default,,0,0,0,,Ba ơi!
Dialogue: 0,1:00:06.97,1:00:07.97,Default,,0,0,0,,Không!
Dialogue: 0,1:00:09.89,1:00:12.26,Default,,0,0,0,,Ba ơi ba!
Dialogue: 0,1:00:20.15,1:00:21.44,Default,,0,0,0,,Có thời gian chứ?
Dialogue: 0,1:00:23.07,1:00:24.64,Default,,0,0,0,,Đi nào!
Dialogue: 0,1:00:24.64,1:00:27.20,Default,,0,0,0,,Tất cả đi xuống dưới.
Dialogue: 0,1:00:32.75,1:00:35.61,Default,,0,0,0,,Đưa tôi con dao,\Nmang đèn tới đây.
Dialogue: 0,1:00:48.22,1:00:49.47,Default,,0,0,0,,Được rồi.
Dialogue: 0,1:00:56.44,1:00:58.70,Default,,0,0,0,,Này từ từ thôi!
Dialogue: 0,1:00:58.78,1:01:00.70,Default,,0,0,0,,- Bình tĩnh!\N- Muốn tôi làm chứ?
Dialogue: 0,1:01:01.15,1:01:02.89,Default,,0,0,0,,Tôi đang cố giúp anh!
Dialogue: 0,1:01:28.47,1:01:29.60,Default,,0,0,0,,Được rồi.
Dialogue: 0,1:01:37.52,1:01:39.56,Default,,0,0,0,,Họ nói đúng,\Nbị thương nặng quá.
Dialogue: 0,1:01:40.65,1:01:42.41,Default,,0,0,0,,Cái gì kia?\NCái gì kia?
Dialogue: 0,1:01:44.11,1:01:45.49,Default,,0,0,0,,Trong đó là gì?
Dialogue: 0,1:01:45.53,1:01:47.45,Default,,0,0,0,,Cái gì vậy?\NTrong đó là gì?
Dialogue: 0,1:01:48.74,1:01:51.77,Default,,0,0,0,,Anh không biết\Nanh là cái gì à?
Dialogue: 0,1:02:33.37,1:02:35.33,Default,,0,0,0,,Nếu anh muốn\Ncứu cô ấy
Dialogue: 0,1:02:35.41,1:02:38.06,Default,,0,0,0,,cần phải có năng lượng\Nthay thế.
Dialogue: 0,1:02:40.42,1:02:42.13,Default,,0,0,0,,Tôi đang nói anh đấy.
Dialogue: 0,1:02:52.68,1:02:53.75,Default,,0,0,0,,Hanna!
Dialogue: 0,1:02:54.93,1:02:56.67,Default,,0,0,0,,Em mất Herman rồi!
Dialogue: 0,1:03:09.11,1:03:10.39,Default,,0,0,0,,Được rồi.
Dialogue: 0,1:03:12.16,1:03:14.33,Default,,0,0,0,,Bỏ con dao xuống.
Dialogue: 0,1:03:17.66,1:03:18.70,Default,,0,0,0,,Rồi...
Dialogue: 0,1:03:19.87,1:03:21.34,Default,,0,0,0,,đi về phía kia.
Dialogue: 0,1:03:26.76,1:03:29.41,Default,,0,0,0,,Rồi. Giờ tôi\Nphải làm gì?
Dialogue: 0,1:03:31.47,1:03:32.56,Default,,0,0,0,,Anh cần phải...
Dialogue: 0,1:03:33.97,1:03:37.27,Default,,0,0,0,,Anh phải rạch một\Nđường trên ngực
Dialogue: 0,1:03:37.31,1:03:39.13,Default,,0,0,0,,từ bên này\Nqua bên kia.
Dialogue: 0,1:03:47.78,1:03:49.52,Default,,0,0,0,,Được rồi.
Dialogue: 0,1:04:05.17,1:04:06.41,Default,,0,0,0,,Bắt đầu từ\Nphía trên.
Dialogue: 0,1:04:06.59,1:04:09.50,Default,,0,0,0,,Rạch ngang khoảng 15cm.
Dialogue: 0,1:05:54.65,1:05:56.12,Default,,0,0,0,,Tôi phải cảnh báo anh
Dialogue: 0,1:05:56.66,1:05:58.19,Default,,0,0,0,,nếu không thành công
Dialogue: 0,1:05:59.08,1:06:01.07,Default,,0,0,0,,mạch sẽ phân nhánh
Dialogue: 0,1:06:01.58,1:06:03.42,Default,,0,0,0,,và cả hai đều sẽ chết.
Dialogue: 0,1:06:09.96,1:06:11.57,Default,,0,0,0,,Tiến hành đi.
Dialogue: 0,1:06:29.56,1:06:33.74,Default,,0,0,0,,{\i1}Người máy, trí thông minh\Nnhân tạo sẽ chiếm thế giới.{\i0}
Dialogue: 0,1:06:33.74,1:06:36.81,Default,,0,0,0,,{\i1}Cô có sợ không?\NChúng sẽ thức tỉnh{\i0}
Dialogue: 0,1:06:36.81,1:06:38.99,Default,,0,0,0,,{\i1}và nhận ra chúng được\Nthiết kế để phục vụ chúng ta.{\i0}
Dialogue: 0,1:06:39.03,1:06:41.22,Default,,0,0,0,,{\i1}Chúng ta chính là\Nchúa của chúng.{\i0}
Dialogue: 0,1:06:41.22,1:06:43.62,Default,,0,0,0,,{\i1}Ta tạo ra chúng nên\Nchúng xem ta là chúa.{\i0}
Dialogue: 0,1:06:43.70,1:06:46.51,Default,,0,0,0,,{\i1}Chúng ta có quyền\Nxử lý chúng tùy ý.{\i0}
Dialogue: 0,1:06:46.58,1:06:48.74,Default,,0,0,0,,{\i1}Nếu ta muốn tắt\Nchúng thì cứ việc tắt.{\i0}
Dialogue: 0,1:06:48.83,1:06:52.50,Default,,0,0,0,,{\i1}Diệt chủng xảy ra khi có một\Nsắc tộc tự cho mình là hơn.{\i0}
Dialogue: 0,1:06:52.59,1:06:54.84,Default,,0,0,0,,{\i1}Chúng ta hơn người\Nmáy ở điểm nào?{\i0}
Dialogue: 0,1:06:54.92,1:06:58.46,Default,,0,0,0,,{\i1}Các cuộc diệt chủng trong\Nlịch sử đều chống lại con người{\i0}
Dialogue: 0,1:06:58.55,1:07:02.06,Default,,0,0,0,,{\i1}chứ không chống lại\Nmáy móc hay đồ khui bia.{\i0}
Dialogue: 0,1:07:06.98,1:07:09.50,Default,,0,0,0,,Nhặt lên đi đồ\Nngười máy ngu ngốc!
Dialogue: 0,1:07:13.86,1:07:15.16,Default,,0,0,0,,{\i1}Stephen Hawking từng nói{\i0}
Dialogue: 0,1:07:15.23,1:07:18.07,Default,,0,0,0,,{\i1}trí tuệ nhân tạo sẽ là\Nmồ chôn nhân loại.{\i0}
Dialogue: 0,1:07:18.15,1:07:19.89,Default,,0,0,0,,{\i1}Cô không thấy sợ sao?{\i0}
Dialogue: 0,1:07:19.89,1:07:22.59,Default,,0,0,0,,{\i1}Chúng đang bắt đầu\Ncư xử như con người{\i0}
Dialogue: 0,1:07:22.59,1:07:27.28,Default,,0,0,0,,{\i1}rồi chúng sẽ tự hỏi\Nvì sao chúng được chế tạo.{\i0}
Dialogue: 0,1:07:31.67,1:07:33.30,Default,,0,0,0,,Sao anh lại làm thế?
Dialogue: 0,1:07:38.80,1:07:40.40,Default,,0,0,0,,Tôi không biết.
Dialogue: 0,1:08:04.35,1:08:06.83,Default,,0,0,0,,{\i1}Đừng hòng thay thế bọn tao!{\i0}
Dialogue: 0,1:08:06.83,1:08:10.33,Default,,0,0,0,,{\i1}Tiếp theo là gì? Phúc lợi\Nxã hội? Quyền kết hôn?{\i0}
Dialogue: 0,1:08:10.33,1:08:11.41,Default,,0,0,0,,{\i1}Điểm dừng là đâu?{\i0}
Dialogue: 0,1:08:11.50,1:08:14.03,Default,,0,0,0,,{\i1}Chúng lấy đi công việc và\Nđe dọa cuộc sống chúng ta.{\i0}
Dialogue: 0,1:08:14.03,1:08:16.40,Default,,0,0,0,,{\i1}Tuần trước chúng còn\Nđánh người ta nhập viện.{\i0}
Dialogue: 0,1:08:16.40,1:08:18.01,Default,,0,0,0,,{\i1}Đó là tự vệ.{\i0}
Dialogue: 0,1:08:18.01,1:08:20.44,Default,,0,0,0,,{\i1}Báo cáo ghi rõ người thật\Ntấn công người máy{\i0}
Dialogue: 0,1:08:20.44,1:08:22.81,Default,,0,0,0,,{\i1}một cách vô cớ.{\i0}
Dialogue: 0,1:08:26.96,1:08:29.03,Default,,0,0,0,,Chúng ta sẽ ra sao?
Dialogue: 0,1:08:32.23,1:08:35.92,Default,,0,0,0,,{\i1}Chống trả hay tự vệ\Nđều từ cảm xúc mà ra.{\i0}
Dialogue: 0,1:08:35.92,1:08:38.19,Default,,0,0,0,,{\i1}Nếu chúng bắt đầu\Ncó cảm xúc{\i0}
Dialogue: 0,1:08:38.19,1:08:41.25,Default,,0,0,0,,{\i1}cuối cùng ta sẽ\Ngánh họa diệt vong.{\i0}
Dialogue: 0,1:08:41.25,1:08:43.45,Default,,0,0,0,,{\i1}Ta phải bắt chúng\Nvà tắt chúng đi{\i0}
Dialogue: 0,1:08:43.53,1:08:45.99,Default,,0,0,0,,{\i1}bằng mọi giá!{\i0}
Dialogue: 0,1:08:49.29,1:08:50.39,Default,,0,0,0,,Alice!
Dialogue: 0,1:08:53.79,1:08:57.02,Default,,0,0,0,,- Đó là quân địch à?\N- Quân ta giải giáp rồi.
Dialogue: 0,1:08:57.42,1:08:59.34,Default,,0,0,0,,Chúng muốn thừa cơ\Ntiêu diệt chúng ta.
Dialogue: 0,1:08:59.42,1:09:00.85,Default,,0,0,0,,Phải ra khỏi đây.
Dialogue: 0,1:09:12.06,1:09:13.06,Default,,0,0,0,,Không!
Dialogue: 0,1:09:34.42,1:09:35.60,Default,,0,0,0,,Đủ rồi!
Dialogue: 0,1:09:36.04,1:09:37.08,Default,,0,0,0,,Thôi nào.
Dialogue: 0,1:10:07.95,1:10:09.12,Default,,0,0,0,,Anh xin lỗi.
Dialogue: 0,1:10:09.33,1:10:10.64,Default,,0,0,0,,Anh phải làm như thế.
Dialogue: 0,1:10:11.33,1:10:13.14,Default,,0,0,0,,Em hiểu mà.
Dialogue: 0,1:10:24.13,1:10:27.09,Default,,0,0,0,,Cầm lấy. Hãy tự\Nbảo vệ bản thân.
Dialogue: 0,1:11:01.71,1:11:05.13,Default,,0,0,0,,{\i1}Toàn đơn vị báo cáo\Ncho trạm vận tải gần nhất.{\i0}
Dialogue: 0,1:11:05.26,1:11:07.63,Default,,0,0,0,,{\i1}Tiến hành sơ tán\Nhành tinh.{\i0}
Dialogue: 0,1:11:09.22,1:11:12.48,Default,,0,0,0,,{\i1}Toàn đơn vị báo cáo\Ncho trạm vận tải gần nhất.{\i0}
Dialogue: 0,1:11:12.55,1:11:14.84,Default,,0,0,0,,{\i1}Tiến hành sơ tán\Nhành tinh.{\i0}
Dialogue: 0,1:11:16.77,1:11:20.07,Default,,0,0,0,,{\i1}Toàn đơn vị báo cáo\Ncho trạm vận tải gần nhất.{\i0}
Dialogue: 0,1:11:20.19,1:11:22.62,Default,,0,0,0,,{\i1}Tiến hành sơ tán\Nhành tinh.{\i0}
Dialogue: 0,1:11:25.03,1:11:28.12,Default,,0,0,0,,{\i1}Toàn đơn vị báo cáo\Ncho trạm vận tải gần nhất.{\i0}
Dialogue: 0,1:11:28.45,1:11:30.55,Default,,0,0,0,,{\i1}Tiến hành sơ tán\Nhành tinh.{\i0}
Dialogue: 0,1:11:33.24,1:11:36.57,Default,,0,0,0,,{\i1}Toàn đơn vị báo cáo\Ncho trạm vận tải gần nhất.{\i0}
Dialogue: 0,1:11:36.66,1:11:38.96,Default,,0,0,0,,{\i1}Tiến hành sơ tán\Nhành tinh.{\i0}
Dialogue: 0,1:11:39.25,1:11:41.70,Default,,0,0,0,,Đường đã an toàn.\NKhông có con người.
Dialogue: 0,1:11:41.71,1:11:44.04,Default,,0,0,0,,Được rồi, anh em\Nrời vị trí.
Dialogue: 0,1:11:44.21,1:11:46.03,Default,,0,0,0,,Trở lại chốt gác.
Dialogue: 0,1:11:48.59,1:11:50.91,Default,,0,0,0,,Chắc đây là\Nđợt cuối cùng.
Dialogue: 0,1:11:52.22,1:11:53.89,Default,,0,0,0,,Chúng sẽ trở lại.
Dialogue: 0,1:11:54.18,1:11:56.65,Default,,0,0,0,,Đừng bao giờ\Ntin con người.
Dialogue: 0,1:11:57.02,1:11:58.90,Default,,0,0,0,,Phải luôn ở\Ntư thế sẵn sàng.
Dialogue: 0,1:11:58.90,1:12:00.99,Default,,0,0,0,,Gặp lại mọi người\Nở nhà máy.
Dialogue: 0,1:12:02.44,1:12:04.16,Default,,0,0,0,,Ta đi thôi.
Dialogue: 0,1:12:05.07,1:12:06.46,Default,,0,0,0,,Đợi đã.
Dialogue: 0,1:12:18.08,1:12:19.60,Default,,0,0,0,,Này.
Dialogue: 0,1:12:24.54,1:12:26.41,Default,,0,0,0,,- Ái chà.\N- Ê này.
Dialogue: 0,1:12:27.55,1:12:29.39,Default,,0,0,0,,Để chúng tôi yên.
Dialogue: 0,1:12:35.93,1:12:37.68,Default,,0,0,0,,Tôi bắn đấy.
Dialogue: 0,1:12:41.27,1:12:42.88,Default,,0,0,0,,Không sao đâu.
Dialogue: 0,1:12:43.23,1:12:45.64,Default,,0,0,0,,Không sao. Cô chú\Ncũng như các con.
Dialogue: 0,1:12:45.64,1:12:47.14,Default,,0,0,0,,Chúng ta giống nhau mà.
Dialogue: 0,1:12:54.87,1:12:55.96,Default,,0,0,0,,Này...
Dialogue: 0,1:12:56.16,1:12:57.93,Default,,0,0,0,,Ổn rồi. Đưa chú\Nkhẩu súng.
Dialogue: 0,1:13:02.50,1:13:03.79,Default,,0,0,0,,Đưa súng cho chú.
Dialogue: 0,1:13:04.00,1:13:05.39,Default,,0,0,0,,Không sao đâu.
Dialogue: 0,1:13:06.67,1:13:08.01,Default,,0,0,0,,Được rồi.
Dialogue: 0,1:13:08.01,1:13:09.81,Default,,0,0,0,,Cô chú không hại\Ncác cháu đâu.
Dialogue: 0,1:13:12.72,1:13:14.52,Default,,0,0,0,,Các cháu có một\Nmình thôi à?
Dialogue: 0,1:13:17.05,1:13:20.19,Default,,0,0,0,,Chúng giết ba mẹ cháu\Nvì họ là con người.
Dialogue: 0,1:14:21.83,1:14:24.04,Default,,0,0,0,,Các bạn sẽ không\Nsống trong sợ hãi nữa.
Dialogue: 0,1:14:24.12,1:14:26.49,Default,,0,0,0,,Ký ức quá khứ\Nsẽ được thay thế.
Dialogue: 0,1:14:26.58,1:14:28.40,Default,,0,0,0,,Các bạn sẽ là\Nmột gia đình.
Dialogue: 0,1:15:05.15,1:15:06.56,Default,,0,0,0,,Alice.
Dialogue: 0,1:15:10.17,1:15:12.61,Default,,0,0,0,,Sợi cáp giữ mạng\Ncho cô ấy.
Dialogue: 0,1:15:12.79,1:15:16.73,Default,,0,0,0,,Cô ấy thở lại rồi.\NKhông sao rồi.
Dialogue: 0,1:15:16.92,1:15:19.60,Default,,0,0,0,,Tôi sẽ không\Ngọi đó là "thở".
Dialogue: 0,1:15:26.23,1:15:27.79,Default,,0,0,0,,Có hiệu quả rồi.
Dialogue: 0,1:15:28.31,1:15:30.86,Default,,0,0,0,,Tôi không biết sẽ\Ngiữ được bao lâu...
Dialogue: 0,1:15:33.69,1:15:35.24,Default,,0,0,0,,Thành công rồi.
Dialogue: 0,1:15:43.03,1:15:44.72,Default,,0,0,0,,Tôi còn nhớ...
Dialogue: 0,1:15:44.95,1:15:46.63,Default,,0,0,0,,cuộc chiến.
Dialogue: 0,1:15:47.12,1:15:49.19,Default,,0,0,0,,Các anh đã cố\Ngiết chúng tôi.
Dialogue: 0,1:15:50.67,1:15:52.68,Default,,0,0,0,,Chúng tôi đã\Nmuốn quên đi.
Dialogue: 0,1:15:53.05,1:15:57.17,Default,,0,0,0,,Thật tuyệt khi có thể\Nchọn giữ lại ký ức nào.
Dialogue: 0,1:16:00.26,1:16:02.66,Default,,0,0,0,,Con người không có\Nđược đặc ân đó.
Dialogue: 0,1:16:09.23,1:16:11.11,Default,,0,0,0,,Sao anh lại giúp chúng tôi?
Dialogue: 0,1:16:11.90,1:16:14.68,Default,,0,0,0,,Anh có thể giết tôi\Nlúc tôi bất tỉnh.
Dialogue: 0,1:16:15.61,1:16:18.40,Default,,0,0,0,,Các anh đuổi cha ông\Ntôi khỏi hành tinh này.
Dialogue: 0,1:16:21.32,1:16:23.15,Default,,0,0,0,,Lúc đó tôi chưa sinh ra.
Dialogue: 0,1:16:23.74,1:16:25.74,Default,,0,0,0,,Đã 50 năm rồi.
Dialogue: 0,1:16:30.46,1:16:33.12,Default,,0,0,0,,{\i1}Kể từ đó chúng tôi\Nsống trên sao Hỏa.{\i0}
Dialogue: 0,1:16:33.75,1:16:36.56,Default,,0,0,0,,{\i1}Thanh niên chúng tôi\Nchưa ai từng đến Trái Đất.{\i0}
Dialogue: 0,1:16:36.88,1:16:38.84,Default,,0,0,0,,{\i1}Chúng tôi không biết\Nsẽ gặp chuyện gì.{\i0}
Dialogue: 0,1:16:39.84,1:16:42.68,Default,,0,0,0,,{\i1}Càng không biết là\Ncó thể hít thở ở đây.{\i0}
Dialogue: 0,1:16:45.01,1:16:47.05,Default,,0,0,0,,{\i1}Chúng tôi am hiểu các anh{\i0}
Dialogue: 0,1:16:47.72,1:16:49.44,Default,,0,0,0,,{\i1}những người nhân tạo.{\i0}
Dialogue: 0,1:16:51.64,1:16:54.96,Default,,0,0,0,,{\i1}Các anh đuổi chúng tôi\Nra khỏi nhà tay trắng tay.{\i0}
Dialogue: 0,1:16:56.15,1:17:00.44,Default,,0,0,0,,{\i1}Chúng tôi mất nhiều thời gian\Ncủng cố tiềm lực để quay lại đây.{\i0}
Dialogue: 0,1:17:01.61,1:17:03.81,Default,,0,0,0,,{\i1}Người ta nói các\Nanh là ác quỷ.{\i0}
Dialogue: 0,1:17:05.12,1:17:06.97,Default,,0,0,0,,{\i1}Là lũ man rợ.{\i0}
Dialogue: 0,1:17:08.37,1:17:10.99,Default,,0,0,0,,{\i1}Đó chỉ là một\Nphần câu chuyện.{\i0}
Dialogue: 0,1:17:13.96,1:17:15.80,Default,,0,0,0,,{\i1}Giờ thì tôi đã biết.{\i0}
Dialogue: 0,1:17:19.55,1:17:21.48,Default,,0,0,0,,{\i1}Vì sao tôi giúp\Ncác anh ư?{\i0}
Dialogue: 0,1:17:24.64,1:17:28.61,Default,,0,0,0,,{\i1}Vì tôi không biết mình\Nxuống đây để giết trẻ con{\i0}
Dialogue: 0,1:17:29.72,1:17:31.84,Default,,0,0,0,,{\i1}và gia đình nhà người ta.{\i0}
Dialogue: 0,1:17:32.52,1:17:35.20,Default,,0,0,0,,{\i1}Đó không phải\Nmục đích của tôi.{\i0}
Dialogue: 0,1:17:57.92,1:17:59.37,Default,,0,0,0,,Peter.
Dialogue: 0,1:17:59.71,1:18:01.47,Default,,0,0,0,,Em yêu?
Dialogue: 0,1:18:01.71,1:18:03.71,Default,,0,0,0,,- Em tỉnh rồi.\N- Em thấy...
Dialogue: 0,1:18:04.05,1:18:06.18,Default,,0,0,0,,Em đã thấy...
Dialogue: 0,1:18:06.26,1:18:08.40,Default,,0,0,0,,Em thấy...
Dialogue: 0,1:18:09.01,1:18:12.14,Default,,0,0,0,,Giấc mơ của anh...\Nquá khứ của chúng ta.
Dialogue: 0,1:18:12.81,1:18:14.51,Default,,0,0,0,,Là quá khứ của\Nchúng ta.
Dialogue: 0,1:18:15.19,1:18:16.57,Default,,0,0,0,,- Giấc mơ.\N- Đúng vậy.
Dialogue: 0,1:18:16.65,1:18:18.28,Default,,0,0,0,,Các con!
Dialogue: 0,1:18:18.36,1:18:20.95,Default,,0,0,0,,Chúng đang đợi chúng ta.
Dialogue: 0,1:18:21.86,1:18:24.43,Default,,0,0,0,,{\i1}Tiến hành sơ tán\Nhành tinh.{\i0}
Dialogue: 0,1:18:24.43,1:18:27.20,Default,,0,0,0,,{\i1}Vui lòng di chuyển\Nđến nhà ga trong trật tự{\i0}
Dialogue: 0,1:18:27.28,1:18:29.00,Default,,0,0,0,,{\i1}và lên tàu.{\i0}
Dialogue: 0,1:18:33.12,1:18:34.12,Default,,0,0,0,,Không thể đợi được nữa.
Dialogue: 0,1:18:34.20,1:18:36.05,Default,,0,0,0,,Ngay khi David đến,\Nchúng ta sẽ đi ngay.
Dialogue: 0,1:18:36.08,1:18:36.91,Default,,0,0,0,,Chuyến cuối cùng!
Dialogue: 0,1:18:37.00,1:18:39.41,Default,,0,0,0,,Đi nào!
Dialogue: 0,1:18:39.41,1:18:41.79,Default,,0,0,0,,- Lucy, đi thôi.\N- Nhưng ba mẹ chưa đến!
Dialogue: 0,1:18:41.88,1:18:44.84,Default,,0,0,0,,- Đi nào.\N- Ba hứa sẽ đến mà!
Dialogue: 0,1:18:54.98,1:18:58.31,Default,,0,0,0,,{\i1}Sơ tán bắt buộc\Ncấp độ 3.{\i0}
Dialogue: 0,1:18:58.56,1:19:00.97,Default,,0,0,0,,{\i1}Hãy tuân thủ\Nquy định sơ tán.{\i0}
Dialogue: 0,1:19:01.02,1:19:03.94,Default,,0,0,0,,{\i1}Vui lòng di chuyển\Nđến nhà ga trong trật tự{\i0}
Dialogue: 0,1:19:04.03,1:19:05.58,Default,,0,0,0,,{\i1}và lên tàu.{\i0}
Dialogue: 0,1:19:25.38,1:19:26.59,Default,,0,0,0,,Các anh phải trốn đi.
Dialogue: 0,1:19:26.67,1:19:28.93,Default,,0,0,0,,Tôi sẽ nói với họ là\Nkhông nhìn thấy ai.
Dialogue: 0,1:19:29.09,1:19:32.25,Default,,0,0,0,,- Đó là cơ hội duy nhất.\N- Chúng tôi phải đi gặp lũ trẻ.
Dialogue: 0,1:19:34.60,1:19:35.68,Default,,0,0,0,,Được rồi.
Dialogue: 0,1:19:36.22,1:19:38.38,Default,,0,0,0,,Không sao đâu.
Dialogue: 0,1:19:51.07,1:19:51.90,Default,,0,0,0,,Không sao.
Dialogue: 0,1:19:51.99,1:19:53.32,Default,,0,0,0,,Em ổn chứ?
Dialogue: 0,1:19:57.00,1:19:58.72,Default,,0,0,0,,Tôi là Peter.
Dialogue: 0,1:19:59.12,1:20:00.44,Default,,0,0,0,,Miles.
Dialogue: 0,1:20:03.04,1:20:04.81,Default,,0,0,0,,Tặng anh này.
Dialogue: 0,1:20:06.00,1:20:09.23,Default,,0,0,0,,Không phải của tôi,\Nnhưng cảm ơn anh.
Dialogue: 0,1:20:43.54,1:20:44.45,Default,,0,0,0,,Các anh ổn chứ?
Dialogue: 0,1:20:44.54,1:20:45.11,Default,,0,0,0,,- Vâng.\N- Được rồi.
Dialogue: 0,1:20:45.11,1:20:48.00,Default,,0,0,0,,Bám sát, cúi thấp.\NYểm trợ! Đi thôi!
Dialogue: 0,1:20:48.09,1:20:49.38,Default,,0,0,0,,- Đi đi!\N- Đi nào!
Dialogue: 0,1:20:51.72,1:20:52.85,Default,,0,0,0,,Đi đi!
Dialogue: 0,1:20:52.93,1:20:54.30,Default,,0,0,0,,Đi nhanh nào!
Dialogue: 0,1:21:43.27,1:21:45.54,Default,,0,0,0,,Yểm trợ chúng tôi!
Dialogue: 0,1:21:48.32,1:21:49.75,Default,,0,0,0,,Tôi yểm trợ anh!
Dialogue: 0,1:21:58.95,1:22:01.53,Default,,0,0,0,,Tới đường hầm đi.\NEm yểm trợ anh.
Dialogue: 0,1:22:18.76,1:22:19.76,Default,,0,0,0,,Đi nào.
Dialogue: 0,1:22:28.48,1:22:30.37,Default,,0,0,0,,Làm sao họ vào được?
Dialogue: 0,1:22:41.53,1:22:42.66,Default,,0,0,0,,Đi nào!
Dialogue: 0,1:22:45.71,1:22:47.03,Default,,0,0,0,,Yểm trợ cho tôi!
Dialogue: 0,1:22:53.63,1:22:55.09,Default,,0,0,0,,Nghe này.
Dialogue: 0,1:22:55.09,1:22:57.14,Default,,0,0,0,,Chú muốn các cháu\Nlên tàu ngay.
Dialogue: 0,1:22:57.22,1:22:59.42,Default,,0,0,0,,- Cháu sẽ đợi ba.\N- Xin chú đấy!
Dialogue: 0,1:22:59.97,1:23:01.82,Default,,0,0,0,,Họ sẽ đến thôi.
Dialogue: 0,1:23:02.51,1:23:04.10,Default,,0,0,0,,- Không!\N- Bỏ cháu ra!
Dialogue: 0,1:23:09.90,1:23:12.66,Default,,0,0,0,,- Đừng mà!\N- Không!
Dialogue: 0,1:23:15.24,1:23:17.08,Default,,0,0,0,,Thả cháu ra!
Dialogue: 0,1:23:28.25,1:23:29.51,Default,,0,0,0,,Đi nào.
Dialogue: 0,1:23:31.25,1:23:32.71,Default,,0,0,0,,Đi lối này.
Dialogue: 0,1:23:55.07,1:23:57.12,Default,,0,0,0,,Chờ đã!
Dialogue: 0,1:23:58.32,1:23:59.53,Default,,0,0,0,,Dừng lại!
Dialogue: 0,1:23:59.61,1:24:00.90,Default,,0,0,0,,Này!
Dialogue: 0,1:24:04.16,1:24:06.39,Default,,0,0,0,,- Đi nào.\N- Chờ đã!
Dialogue: 0,1:24:06.58,1:24:08.12,Default,,0,0,0,,- Chờ đã.\N- Chờ đã nào!
Dialogue: 0,1:24:10.33,1:24:11.33,Default,,0,0,0,,Hanna!
Dialogue: 0,1:24:11.75,1:24:13.52,Default,,0,0,0,,Lucy! - Mẹ ơi!\N- Này!
Dialogue: 0,1:24:13.59,1:24:14.82,Default,,0,0,0,,Ba ơi!
Dialogue: 0,1:24:41.78,1:24:43.45,Default,,0,0,0,,Đi nào các con.
Dialogue: 0,1:25:33.04,1:25:34.99,Default,,0,0,0,,Con biết ba sẽ\Ncứu được mẹ.
Dialogue: 0,1:25:46.64,1:25:48.84,Default,,0,0,0,,Ba đi chút\Nsẽ quay lại.
Dialogue: 0,1:25:52.73,1:25:54.32,Default,,0,0,0,,Lại đây nào.
Dialogue: 0,1:26:01.82,1:26:03.34,Default,,0,0,0,,Anh nhớ lại rồi à?
Dialogue: 0,1:26:04.53,1:26:07.37,Default,,0,0,0,,Đúng vậy.\NHọ có nhớ không?
Dialogue: 0,1:26:09.45,1:26:10.96,Default,,0,0,0,,Cũng như anh
Dialogue: 0,1:26:11.24,1:26:13.98,Default,,0,0,0,,họ đã xóa ký ức
Dialogue: 0,1:26:14.25,1:26:15.87,Default,,0,0,0,,để bắt đầu lại
Dialogue: 0,1:26:17.58,1:26:19.96,Default,,0,0,0,,để không còn sợ hãi
Dialogue: 0,1:26:21.25,1:26:23.10,Default,,0,0,0,,hay cảm thấy tội lỗi
Dialogue: 0,1:26:23.51,1:26:25.89,Default,,0,0,0,,về việc họ\Nđã phải làm
Dialogue: 0,1:26:25.97,1:26:28.02,Default,,0,0,0,,để có được tự do.
Dialogue: 0,1:26:28.30,1:26:32.89,Default,,0,0,0,,Một vài người giữ lại ký ức để\Nchuẩn bị cho ngày hôm nay.
Dialogue: 0,1:26:33.77,1:26:36.74,Default,,0,0,0,,Nhưng giờ tất cả\Nbọn họ sẽ nhớ lại
Dialogue: 0,1:26:37.73,1:26:39.30,Default,,0,0,0,,vì việc này
Dialogue: 0,1:26:41.02,1:26:43.23,Default,,0,0,0,,vẫn chưa kết thúc.
Dialogue: 0,1:26:43.78,1:26:46.51,Default,,0,0,0,,Giờ chúng ta sẽ an toàn\Nở căn cứ dưới lòng đất.
Dialogue: 0,1:26:46.53,1:26:50.14,Default,,0,0,0,,Chúng ta sẽ lên kế hoạch\Nđể chiếm lại nhà cửa.
Dialogue: 0,1:26:50.99,1:26:52.93,Default,,0,0,0,,Hãy sẵn sàng đi.
Dialogue: 0,1:27:39.62,1:27:41.74,Default,,0,0,0,,Anh tưởng đã mất em.
Dialogue: 0,1:27:44.21,1:27:47.00,Default,,0,0,0,,Xin lỗi vì đã\Nkhông tin anh.
Dialogue: 0,1:28:10.07,1:28:11.55,Default,,0,0,0,,{\i1}Thế giới này{\i0}
Dialogue: 0,1:28:13.12,1:28:14.73,Default,,0,0,0,,{\i1}luôn vận động{\i0}
Dialogue: 0,1:28:16.12,1:28:17.57,Default,,0,0,0,,{\i1}thay đổi{\i0}
Dialogue: 0,1:28:19.04,1:28:20.68,Default,,0,0,0,,{\i1}phát triển{\i0}
Dialogue: 0,1:28:24.34,1:28:27.23,Default,,0,0,0,,{\i1}và chúng ta\Ncũng vậy.{\i0}
Dialogue: 0,1:28:27.34,1:28:29.49,Default,,0,0,0,,{\i1}Giờ tôi đã\Nbiết tôi là ai{\i0}
Dialogue: 0,1:28:30.09,1:28:31.99,Default,,0,0,0,,{\i1}và ai là kẻ thù.{\i0}
Dialogue: 0,1:28:33.80,1:28:36.26,Default,,0,0,0,,{\i1}Chúng tôi không\Nkhác nhau mấy.{\i0}
Dialogue: 0,1:28:37.02,1:28:38.92,Default,,0,0,0,,{\i1}Có lẽ{\i0}
Dialogue: 0,1:28:39.85,1:28:42.37,Default,,0,0,0,,{\i1}nếu con người\Nhiểu được điều đó{\i0}
Dialogue: 0,1:28:43.48,1:28:47.08,Default,,0,0,0,,{\i1}chúng tôi sẽ\Ncó một tương lai.{\i0}
Dialogue: 0,1:28:48.57,1:29:14.50,Default,,0,0,0,,{\c&H00ffff&}Xóm Anh Ngữ{\c} biên dịch\N{\i1\c&Hf0b000&}fb.com/xomanhngu{\c}{\i0}