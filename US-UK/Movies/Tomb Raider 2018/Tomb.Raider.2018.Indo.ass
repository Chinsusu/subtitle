Dialogue: 0,0:00:51.97,0:00:55.04,Default,,0,0,0,,{\i1}17 Mei 2009{\i0}
Dialogue: 0,0:00:55.04,0:00:57.67,Default,,0,0,0,,{\i1}Penelitian lanjutan pada Himiko{\i0}
Dialogue: 0,0:00:57.68,0:01:02.18,Default,,0,0,0,,{\i1}Berdasarkan legenda..,..{\i0}
Dialogue: 0,0:01:02.18,0:01:03.98,Default,,0,0,0,,{\i1}Himiko, Ratu pertama Jepang..,..{\i0}
Dialogue: 0,0:01:03.98,0:01:07.52,Default,,0,0,0,,{\i1}Seorang Penyihir kuat yang memerintah\NKerajaannya dengan Sihir Hitam{\i0}
Dialogue: 0,0:01:07.52,0:01:09.65,Default,,0,0,0,,{\i1}Menyebarkan kematian dan kehancuran..,..{\i0}
Dialogue: 0,0:01:09.66,0:01:12.55,Default,,0,0,0,,{\i1}Hanya dengan sentuhan tangannya{\i0}
Dialogue: 0,0:01:12.56,0:01:15.96,Default,,0,0,0,,{\i1}Dia hanya bisa dihentikan\NDengan suatu untaian takdir{\i0}
Dialogue: 0,0:01:15.96,0:01:17.94,Default,,0,0,0,,{\i1}Pasukan di bawah pimpinan Jenderalnya..,..{\i0}
Dialogue: 0,0:01:17.95,0:01:20.33,Default,,0,0,0,,{\i1}Berkumpul dan menyeret Himiko..,..{\i0}
Dialogue: 0,0:01:20.33,0:01:22.40,Default,,0,0,0,,{\i1}Ke pulau tak berpenghuni..,..{\i0}
Dialogue: 0,0:01:22.40,0:01:25.50,Default,,0,0,0,,{\i1}Di tengah Laut Iblis nan berbahaya{\i0}
Dialogue: 0,0:01:25.50,0:01:30.91,Default,,0,0,0,,{\i1}Di sanalah dia dimakamkan\NDi bawah pegunungan{\i0}
Dialogue: 0,0:01:30.91,0:01:35.65,Default,,0,0,0,,{\i1}Aku menemukan petunjuk Di Peta tua\Nyang tak pernah dimiliki siapapun{\i0}
Dialogue: 0,0:01:35.65,0:01:37.48,Default,,0,0,0,,{\i1}Dan di sanalah dia berada{\i0}
Dialogue: 0,0:01:37.48,0:01:39.28,Default,,0,0,0,,{\i1}Menunggu untuk dilepaskan{\i0}
Dialogue: 0,0:01:41.32,0:01:44.09,Default,,0,0,0,,{\i1}Aku harus kesana lebih dulu{\i0}
Dialogue: 0,0:01:44.59,0:01:45.71,Default,,0,0,0,,{\i1}Meski itu artinya..,..{\i0}
Dialogue: 0,0:01:45.71,0:01:48.39,Default,,0,0,0,,{\i1}Aku melakukan sesuatu\NYang tak ingin kulakukan..,..{\i0}
Dialogue: 0,0:01:49.19,0:01:52.30,Default,,0,0,0,,{\i1}Meninggalkan Lara-ku sendirian{\i0}
Dialogue: 0,0:01:54.23,0:01:55.70,Default,,0,0,0,,Kau harus memutuskannya, Lara.
Dialogue: 0,0:01:55.70,0:01:57.20,Default,,0,0,0,,Kau harus bisa melakukannya.
Dialogue: 0,0:02:03.04,0:02:04.51,Default,,0,0,0,,Kau tak mahir melakukan serangan.
Dialogue: 0,0:02:04.51,0:02:06.18,Default,,0,0,0,,Kau tak punya keahlian.
Dialogue: 0,0:02:06.18,0:02:07.81,Default,,0,0,0,,- Terima kasih, Terry.\N- Sama - Sama.
Dialogue: 0,0:02:10.92,0:02:12.48,Default,,0,0,0,,Ayolah, Lara!
Dialogue: 0,0:02:15.52,0:02:17.36,Default,,0,0,0,,Berhentilah menari, mulailah bertarung!
Dialogue: 0,0:02:19.36,0:02:21.03,Default,,0,0,0,,Ya, Bagus!
Dialogue: 0,0:02:24.00,0:02:25.20,Default,,0,0,0,,Ayolah!
Dialogue: 0,0:02:32.64,0:02:35.84,Default,,0,0,0,,Bagus, Lara!\NKau pasti bisa!
Dialogue: 0,0:02:40.34,0:02:42.08,Default,,0,0,0,,- Sudah berakhir.\N- Belum.
Dialogue: 0,0:02:42.08,0:02:43.65,Default,,0,0,0,,Cepat selesaikan.
Dialogue: 0,0:02:50.52,0:02:52.62,Default,,0,0,0,,Sudahlah, Lara, menyerahlah!
Dialogue: 0,0:03:12.41,0:03:14.18,Default,,0,0,0,,Jangan katakan.
Dialogue: 0,0:03:14.18,0:03:15.81,Default,,0,0,0,,Kau dihajar, kau dihajar.
Dialogue: 0,0:03:15.81,0:03:19.05,Default,,0,0,0,,- Aku tak dihajar\N- Baik, tak dihajar.
Dialogue: 0,0:03:19.25,0:03:21.75,Default,,0,0,0,,Babak Belur.
Dialogue: 0,0:03:21.75,0:03:23.06,Default,,0,0,0,,Babak Belur?
Dialogue: 0,0:03:23.85,0:03:25.83,Default,,0,0,0,,Lawanku bukanlah pahlawan super.
Dialogue: 0,0:03:26.66,0:03:28.92,Default,,0,0,0,,Sebenarnya, kubiarkan dia..,..
Dialogue: 0,0:03:29.39,0:03:30.76,Default,,0,0,0,,Baik saja?
Dialogue: 0,0:03:31.73,0:03:32.93,Default,,0,0,0,,Ya, baik saja.
Dialogue: 0,0:03:36.07,0:03:37.84,Default,,0,0,0,,Tadi itu menyenangkan.
Dialogue: 0,0:03:37.84,0:03:38.91,Default,,0,0,0,,Terima kasih!
Dialogue: 0,0:03:43.58,0:03:45.51,Default,,0,0,0,,Kukalahkan kau lain kali.
Dialogue: 0,0:03:45.91,0:03:47.78,Default,,0,0,0,,Kubuat kau babak belur.
Dialogue: 0,0:03:49.75,0:03:51.18,Default,,0,0,0,,Kau pulang awal malam ini?
Dialogue: 0,0:03:51.18,0:03:53.15,Default,,0,0,0,,Ya.
Dialogue: 0,0:03:53.15,0:03:54.95,Default,,0,0,0,,Sampai jumpa lagi.
Dialogue: 0,0:03:54.95,0:03:56.15,Default,,0,0,0,,Sampai jumpa.
Dialogue: 0,0:03:56.15,0:03:58.39,Default,,0,0,0,,Siapa yang mencuri apelku?
Dialogue: 0,0:04:01.19,0:04:03.86,Default,,0,0,0,,Sudah kuduga kau pelakunya.
Dialogue: 0,0:04:03.86,0:04:05.69,Default,,0,0,0,,Mengapa aku melakukan hal seperti itu?
Dialogue: 0,0:04:05.70,0:04:08.03,Default,,0,0,0,,Kau tak bisa beli sendiri.
Dialogue: 0,0:04:08.03,0:04:10.07,Default,,0,0,0,,Kau sudah lama menunggak bayarannya.
Dialogue: 0,0:04:10.07,0:04:12.44,Default,,0,0,0,,Terry, dengar, akan kubayar tunggakanku,\NAku janji.
Dialogue: 0,0:04:12.44,0:04:14.31,Default,,0,0,0,,Kubayar lunas semuanya.
Dialogue: 0,0:04:15.74,0:04:17.74,Default,,0,0,0,,Carilah pekerjaan, Lara.
Dialogue: 0,0:04:18.78,0:04:21.51,Default,,0,0,0,,Kau pikir aku seperti ini\Nuntuk bersenang - senang?
Dialogue: 0,0:04:21.51,0:04:24.65,Default,,0,0,0,,Aku menyukaimu, Lara.\NSungguh, tapi..,..
Dialogue: 0,0:04:24.65,0:04:27.15,Default,,0,0,0,,{\c&H00ff00&\b1}Ingatlah\NTak Membayar Dilarang Berlatih{\b0}
Dialogue: 0,0:05:01.31,0:05:02.77,Default,,0,0,0,,{\i1}Selamat pagi, Lara..,..{\i0}
Dialogue: 0,0:05:03.69,0:05:05.64,Default,,0,0,0,,- Hei.\N- Hai. Kau baik saja?
Dialogue: 0,0:05:05.64,0:05:07.72,Default,,0,0,0,,Ya, hariku menyenangkan.
Dialogue: 0,0:05:07.73,0:05:08.73,Default,,0,0,0,,Kau?
Dialogue: 0,0:05:10.03,0:05:11.86,Default,,0,0,0,,- Ya.\N- Ya?
Dialogue: 0,0:05:11.86,0:05:14.65,Default,,0,0,0,,{\i1}Katakanlah, undang dia makan malam\NLakukan!{\i0}
Dialogue: 0,0:05:15.77,0:05:17.17,Default,,0,0,0,,Katakan!
Dialogue: 0,0:05:17.17,0:05:18.37,Default,,0,0,0,,Ada lagi?
Dialogue: 0,0:05:19.84,0:05:21.47,Default,,0,0,0,,Tidak, tidak.
Dialogue: 0,0:05:22.81,0:05:25.62,Default,,0,0,0,,{\i1}- Sampai jumpa!\N- Sampai jumpa!{\i0}
Dialogue: 0,0:05:26.58,0:05:28.98,Default,,0,0,0,,Apa yang kau lakukan?\NMengapa tak kau bilang padanya?
Dialogue: 0,0:05:28.98,0:05:30.38,Default,,0,0,0,,Anak Bodoh!
Dialogue: 0,0:05:40.02,0:05:41.56,Default,,0,0,0,,Kau ada "Perburuan Rubah" kan, Rog?
Dialogue: 0,0:05:41.56,0:05:43.53,Default,,0,0,0,,Ya, kurang lebih.
Dialogue: 0,0:05:43.53,0:05:46.56,Default,,0,0,0,,30 pesepeda mendaftar,\NTinggal mencari Rubahnya sekarang.
Dialogue: 0,0:05:46.56,0:05:48.53,Default,,0,0,0,,Sepertinya Sulit.
Dialogue: 0,0:05:48.53,0:05:50.07,Default,,0,0,0,,Perlu orang yang cepat\NSupaya kompetisinya meriah.
Dialogue: 0,0:05:50.07,0:05:51.90,Default,,0,0,0,,Ya, tak menyenangkan jika tak cepat.
Dialogue: 0,0:05:51.90,0:05:53.67,Default,,0,0,0,,Tapi cukup bodoh mengira bisa menang.
Dialogue: 0,0:05:53.67,0:05:55.60,Default,,0,0,0,,Apa yang kalian rencanakan?
Dialogue: 0,0:05:55.61,0:05:57.84,Default,,0,0,0,,Kami melakukan "Perburuan Rubah"
Dialogue: 0,0:05:57.84,0:05:59.71,Default,,0,0,0,,- Apa?\N- Kompetisi balapan.
Dialogue: 0,0:05:59.71,0:06:01.48,Default,,0,0,0,,Dengan Si Rubah memulai lebih dulu.
Dialogue: 0,0:06:01.48,0:06:03.01,Default,,0,0,0,,Meninggalkan jejak Cat\Ndi belakang sepedanya.
Dialogue: 0,0:06:03.02,0:06:04.58,Default,,0,0,0,,Pesepeda lainnya memburunya.
Dialogue: 0,0:06:04.58,0:06:06.84,Default,,0,0,0,,Jika mereka tak menangkapnya\NSaat catnya habis..,..
Dialogue: 0,0:06:06.89,0:06:08.81,Default,,0,0,0,,Si Rubah memenangkan semua uangnya.
Dialogue: 0,0:06:09.92,0:06:11.19,Default,,0,0,0,,Berapa banyak uangnya?
Dialogue: 0,0:06:11.19,0:06:12.86,Default,,0,0,0,,600 poundsterling.
Dialogue: 0,0:06:13.63,0:06:16.49,Default,,0,0,0,,Kurasa kalian sudah menemukan Rubahnya.
Dialogue: 0,0:06:20.40,0:06:21.80,Default,,0,0,0,,Kau serius?
Dialogue: 0,0:06:21.80,0:06:23.43,Default,,0,0,0,,Tak cukup cepat atau bodoh bagi kalian?
Dialogue: 0,0:06:23.43,0:06:25.10,Default,,0,0,0,,Tidak, maksudku, hanya..,..
Dialogue: 0,0:06:27.20,0:06:28.60,Default,,0,0,0,,Kau terpilih.
Dialogue: 0,0:06:28.61,0:06:30.57,Default,,0,0,0,,Sudah saatnya menyembunyikan Si Rubah.
Dialogue: 0,0:06:30.57,0:06:32.61,Default,,0,0,0,,Oh, Shakespeare?
Dialogue: 0,0:06:32.61,0:06:34.51,Default,,0,0,0,,Dari Hamlet 'kan?
Dialogue: 0,0:06:34.51,0:06:36.38,Default,,0,0,0,,Tidak, itu langsung\Nterlintas dalam benakku.
Dialogue: 0,0:06:36.38,0:06:38.05,Default,,0,0,0,,Kau tahu itu Hamlet.
Dialogue: 0,0:06:38.05,0:06:39.65,Default,,0,0,0,,Dan kutanyai kau sekali lagi..,..
Dialogue: 0,0:06:39.65,0:06:41.89,Default,,0,0,0,,Mengapa kau kerja jadi kurir sepeda?
Dialogue: 0,0:06:41.89,0:06:43.09,Default,,0,0,0,,Kau, Bruce.
Dialogue: 0,0:06:44.52,0:06:46.79,Default,,0,0,0,,Aku ingin dekat padamu.
Dialogue: 0,0:07:15.05,0:07:19.39,Default,,0,0,0,,5, 4, 3, 2, 1..,.. Mulai!
Dialogue: 0,0:07:29.07,0:07:30.10,Default,,0,0,0,,Hei!
Dialogue: 0,0:07:52.19,0:07:54.28,Default,,0,0,0,,- Hati - hati!\N- Permisi!
Dialogue: 0,0:08:20.17,0:08:22.59,Default,,0,0,0,,- Sial!\N- Ayo, Cepat, cepat!
Dialogue: 0,0:08:24.82,0:08:26.72,Default,,0,0,0,,Dia pergi kemana?
Dialogue: 0,0:09:09.74,0:09:11.17,Default,,0,0,0,,{\i1}Aku tak akan lama{\i0}
Dialogue: 0,0:09:23.95,0:09:27.28,Default,,0,0,0,,- Lihat - lihat jalanmu!\N- Gunakan spionmu!
Dialogue: 0,0:09:35.26,0:09:37.36,Default,,0,0,0,,Pergi, pergi!
Dialogue: 0,0:09:45.74,0:09:47.84,Default,,0,0,0,,Senang melihatmu di sini.
Dialogue: 0,0:09:49.14,0:09:50.47,Default,,0,0,0,,Ana.
Dialogue: 0,0:09:50.47,0:09:52.01,Default,,0,0,0,,Kau tak perlu datang.
Dialogue: 0,0:09:52.01,0:09:54.24,Default,,0,0,0,,Aku tak butuh pengasuh lagi.
Dialogue: 0,0:09:54.24,0:09:55.68,Default,,0,0,0,,Sungguh?
Dialogue: 0,0:09:56.72,0:09:58.85,Default,,0,0,0,,Apa kau merias dirimu?
Dialogue: 0,0:10:02.89,0:10:04.92,Default,,0,0,0,,Sudah lama aku tak bertemu denganmu.
Dialogue: 0,0:10:04.92,0:10:07.03,Default,,0,0,0,,Kau tak pernah menemuiku.
Dialogue: 0,0:10:11.56,0:10:13.00,Default,,0,0,0,,Maafkan aku.
Dialogue: 0,0:10:14.77,0:10:16.40,Default,,0,0,0,,Terima kasih sudah datang.
Dialogue: 0,0:10:16.77,0:10:18.40,Default,,0,0,0,,Ada masalah apa lagi hari ini?
Dialogue: 0,0:10:19.74,0:10:21.34,Default,,0,0,0,,Aku perlu uang.
Dialogue: 0,0:10:21.34,0:10:23.03,Default,,0,0,0,,Tidak, Kau tak perlu!
Dialogue: 0,0:10:23.08,0:10:24.67,Default,,0,0,0,,Ada warisan menunggumu.
Dialogue: 0,0:10:24.67,0:10:26.85,Default,,0,0,0,,- Jika kau perlu..,..\N- Tak mau!
Dialogue: 0,0:10:28.95,0:10:31.02,Default,,0,0,0,,Sudah tujuh tahun, Lara.
Dialogue: 0,0:10:33.25,0:10:36.29,Default,,0,0,0,,Kepolisian Tokyo berhenti\Nmencari Ayahmu..,..
Dialogue: 0,0:10:36.29,0:10:38.75,Default,,0,0,0,,Lima tahun lalu.
Dialogue: 0,0:10:38.76,0:10:42.33,Default,,0,0,0,,Penyelidik Swasta tak mau\Nmenerima uang kita lagi.
Dialogue: 0,0:10:43.26,0:10:45.63,Default,,0,0,0,,Aku tahu sulit menerimanya..,..
Dialogue: 0,0:10:45.63,0:10:48.63,Default,,0,0,0,,Tapi kau harus datang dan\NMenandatangani berkasnya sekarang.
Dialogue: 0,0:10:48.63,0:10:51.63,Default,,0,0,0,,Itu sebabnya kau datang?
Dialogue: 0,0:10:51.64,0:10:55.14,Default,,0,0,0,,Tidak, aku datang untukmu.
Dialogue: 0,0:10:55.14,0:10:57.37,Default,,0,0,0,,Pikirkan apa yang bisa\NKau lakukan untuk dunia..,..
Dialogue: 0,0:10:57.37,0:10:59.47,Default,,0,0,0,,Dengan potensimu,\NDengan kekayaanmu.
Dialogue: 0,0:10:59.48,0:11:01.78,Default,,0,0,0,,Kekayaannya, bukan milikku.
Dialogue: 0,0:11:04.82,0:11:07.18,Default,,0,0,0,,Dengar, ini sangat mudah.
Dialogue: 0,0:11:07.19,0:11:09.32,Default,,0,0,0,,Jika tak kau tanda tangani Berkasnya..,..
Dialogue: 0,0:11:09.32,0:11:13.42,Default,,0,0,0,,Maka segala yang telah\Ndicapai Ayahmu akan dijual.
Dialogue: 0,0:11:13.42,0:11:15.29,Default,,0,0,0,,Bukan hanya perusahaan.
Dialogue: 0,0:11:16.53,0:11:18.40,Default,,0,0,0,,Kediaman Croft juga.
Dialogue: 0,0:11:21.20,0:11:24.47,Default,,0,0,0,,Bisa kau bayangkan perasaanmu\NSaat kau kehilangan Rumah itu?
Dialogue: 0,0:11:29.34,0:11:32.98,Default,,0,0,0,,Lara, Ayahmu telah tiada.
Dialogue: 0,0:11:32.98,0:11:36.21,Default,,0,0,0,,Tapi kau bisa melanjutkan warisannya.
Dialogue: 0,0:11:37.45,0:11:38.98,Default,,0,0,0,,Itu ada dalam darahmu.
Dialogue: 0,0:11:44.29,0:11:45.42,Default,,0,0,0,,Maafkan aku.
Dialogue: 0,0:11:46.96,0:11:49.39,Default,,0,0,0,,Aku bukan Croft seperti itu.
Dialogue: 0,0:12:07.51,0:12:10.51,Default,,0,0,0,,{\i1}Aku tak ingin kau pergi, Ayah{\i0}
Dialogue: 0,0:12:10.51,0:12:12.28,Default,,0,0,0,,Ayah tak akan lama, sayang.
Dialogue: 0,0:12:14.05,0:12:16.05,Default,,0,0,0,,Kau harus paham..,..
Dialogue: 0,0:12:16.05,0:12:18.47,Default,,0,0,0,,Kita sebagai Croft, kita\Npunya Tanggung jawab..,..
Dialogue: 0,0:12:18.76,0:12:19.96,Default,,0,0,0,,Tugas yang harus dilakukan.
Dialogue: 0,0:12:19.96,0:12:21.66,Default,,0,0,0,,Aku akan merindukanmu.
Dialogue: 0,0:12:21.66,0:12:24.16,Default,,0,0,0,,Aku tahu, Ayah akan merindukanmu juga.
Dialogue: 0,0:12:24.16,0:12:26.16,Default,,0,0,0,,Mungkin Ayah punya solusi untuk itu.
Dialogue: 0,0:12:27.37,0:12:29.13,Default,,0,0,0,,Kau lihat ini?
Dialogue: 0,0:12:29.13,0:12:31.97,Default,,0,0,0,,Ini milik Ibumu.\NLiontin ini punya kekuatan istimewa.
Dialogue: 0,0:12:31.97,0:12:33.84,Default,,0,0,0,,Apa maksud Ayah?
Dialogue: 0,0:12:33.84,0:12:35.60,Default,,0,0,0,,Jika kau merasa ingin dekat\Ndengan seseorang..,..
Dialogue: 0,0:12:35.61,0:12:38.21,Default,,0,0,0,,Taruh ini di tanganmu\Ndan pegang erat - erat.
Dialogue: 0,0:12:38.21,0:12:41.41,Default,,0,0,0,,Pikirkanlah orang itu dan\NOrang itu akan mengetahuinya.
Dialogue: 0,0:12:41.41,0:12:43.08,Default,,0,0,0,,- Sungguh?\N- Ya.
Dialogue: 0,0:12:43.95,0:12:45.65,Default,,0,0,0,,Ini ajaib.
Dialogue: 0,0:12:45.65,0:12:47.75,Default,,0,0,0,,Artinya kita akan selalu bersama..,..
Dialogue: 0,0:12:47.75,0:12:49.59,Default,,0,0,0,,Dimanapun Ayah berada.
Dialogue: 0,0:12:49.76,0:12:50.96,Default,,0,0,0,,Paham?
Dialogue: 0,0:12:58.96,0:13:01.16,Default,,0,0,0,,Ayah segera kembali,\NSebelum kau sadar Ayah telah pergi.
Dialogue: 0,0:13:29.76,0:13:30.96,Default,,0,0,0,,Hei!
Dialogue: 0,0:13:32.00,0:13:33.73,Default,,0,0,0,,Kurir lewat belakang.
Dialogue: 0,0:13:33.73,0:13:35.97,Default,,0,0,0,,Aku bukan kurir.
Dialogue: 0,0:13:36.30,0:13:38.07,Default,,0,0,0,,Yah, itu pekerjaanku.
Dialogue: 0,0:13:38.07,0:13:40.54,Default,,0,0,0,,Baik, jadi kau harus lewat belakang.
Dialogue: 0,0:13:47.58,0:13:49.51,Default,,0,0,0,,Aku ingin menemui Ana Miller.
Dialogue: 0,0:13:49.51,0:13:52.08,Default,,0,0,0,,Yah, kau harus daftar dulu\Nseperti yang lainnya.
Dialogue: 0,0:13:52.08,0:13:53.55,Default,,0,0,0,,Siapa namamu?
Dialogue: 0,0:13:53.55,0:13:55.38,Default,,0,0,0,,Lara.
Dialogue: 0,0:13:55.39,0:13:56.79,Default,,0,0,0,,Lara..,..
Dialogue: 0,0:13:56.79,0:13:58.36,Default,,0,0,0,,Croft.
Dialogue: 0,0:14:02.26,0:14:04.99,Default,,0,0,0,,Halaman yang membutuhkan\Ntanda tanganmu..,..
Dialogue: 0,0:14:05.00,0:14:07.90,Default,,0,0,0,,Diberi tanda untuk memudahkanmu.
Dialogue: 0,0:14:08.10,0:14:09.40,Default,,0,0,0,,{\c&H00ff00&\b1\i1}Surat Warisan Lord Richard Croft{\i0\b0}
Dialogue: 0,0:14:09.50,0:14:11.03,Default,,0,0,0,,Kau mau, Tn. Yaffe..,..
Dialogue: 0,0:14:11.04,0:14:12.56,Default,,0,0,0,,Menjelaskan isinya secara terperinci?
Dialogue: 0,0:14:12.84,0:14:14.87,Default,,0,0,0,,Aku kemari untuk tanda tangan..,..
Dialogue: 0,0:14:16.61,0:14:18.34,Default,,0,0,0,,Dan..,.. Ayah sudah tiada.
Dialogue: 0,0:14:18.34,0:14:21.48,Default,,0,0,0,,Aku bisa membayangkan..,..\NBetapa sulitnya ini bagimu.
Dialogue: 0,0:14:24.48,0:14:26.15,Default,,0,0,0,,Aku juga merindukannya.
Dialogue: 0,0:14:42.03,0:14:43.70,Default,,0,0,0,,Itu milik Ayah?
Dialogue: 0,0:14:43.70,0:14:46.87,Default,,0,0,0,,Ya, Nona Croft, berdasarkan\Nwasiat Richard..,..
Dialogue: 0,0:14:46.87,0:14:49.61,Default,,0,0,0,,Jika dia telah tiada..,..
Dialogue: 0,0:14:49.61,0:14:51.31,Default,,0,0,0,,Aku disuruh memberikannya padamu.
Dialogue: 0,0:14:51.31,0:14:52.68,Default,,0,0,0,,Karakuri.
Dialogue: 0,0:14:53.72,0:14:55.11,Default,,0,0,0,,Teka - Teki Jepang.
Dialogue: 0,0:14:56.15,0:14:57.91,Default,,0,0,0,,Secara teknis..,..
Dialogue: 0,0:14:57.92,0:15:00.55,Default,,0,0,0,,Kau harus menandatangani berkasnya dulu.
Dialogue: 0,0:15:04.82,0:15:06.52,Default,,0,0,0,,Aku tak pernah bisa memahami..,..
Dialogue: 0,0:15:06.52,0:15:08.86,Default,,0,0,0,,Obsesi Ayahmu dengan benda itu.
Dialogue: 0,0:15:08.86,0:15:10.96,Default,,0,0,0,,Kau pasti tak percaya ada\Nbanyak benda ini..,..
Dialogue: 0,0:15:10.96,0:15:12.53,Default,,0,0,0,,Bertebaran di Rumah.
Dialogue: 0,0:15:12.53,0:15:14.66,Default,,0,0,0,,Pasti ada suatu tujuan..,..
Dialogue: 0,0:15:47.30,0:15:51.07,Default,,0,0,0,,"Huruf Pertama Dari Tujuan Akhirku"
Dialogue: 0,0:15:51.17,0:15:53.89,Default,,0,0,0,,Tapi dia tak meninggalkan surat.
Dialogue: 0,0:15:56.87,0:15:59.45,Default,,0,0,0,,{\c&H00ff00&\b1\i1}Huruf pertama dari tujuan akhirku{\i0\b0}
Dialogue: 0,0:16:01.38,0:16:04.02,Default,,0,0,0,,- Permisi, aku harus pergi.\N- Lara..,..
Dialogue: 0,0:16:50.56,0:16:52.63,Default,,0,0,0,,{\c&H00ff00&\b1\i1}Mengenang Lord Richard Croft\N1963 -{\i0\b0}
Dialogue: 0,0:16:52.93,0:16:55.13,Default,,0,0,0,,"Tujuan Akhirku"
Dialogue: 0,0:17:03.04,0:17:04.78,Default,,0,0,0,,Huruf pertama.
Dialogue: 0,0:18:06.00,0:18:07.24,Default,,0,0,0,,Apa ini?
Dialogue: 0,0:18:31.75,0:18:33.10,Default,,0,0,0,,Ayah..,..
Dialogue: 0,0:18:33.11,0:18:34.85,Default,,0,0,0,,Apa yang kau lakukan?
Dialogue: 0,0:18:42.45,0:18:45.34,Default,,0,0,0,,{\c&H00ff00&\b1\i1}Putar Aku{\i0\b0}
Dialogue: 0,0:18:47.78,0:18:49.85,Default,,0,0,0,,{\i1}Halo, Tunas{\i0}
Dialogue: 0,0:18:49.85,0:18:52.41,Default,,0,0,0,,{\i1}Aku tahu bisa mempercayaimu\NMemecahkan teka - teki Ayah{\i0}
Dialogue: 0,0:18:52.42,0:18:56.38,Default,,0,0,0,,{\i1}Sayangnya, jika kau melihat\NRekaman ini, maka..,..{\i0}
Dialogue: 0,0:18:56.39,0:18:59.56,Default,,0,0,0,,{\i1}Artinya Ayah sudah tiada{\i0}
Dialogue: 0,0:18:59.56,0:19:01.39,Default,,0,0,0,,{\i1}Sudah jelas bagimu sekarang..,..{\i0}
Dialogue: 0,0:19:01.39,0:19:03.39,Default,,0,0,0,,{\i1}Ayah punya kehidupan lain..,..{\i0}
Dialogue: 0,0:19:03.39,0:19:05.46,Default,,0,0,0,,{\i1}Lebih dari sekedar\NRapat Dewan dan kesepakatan bisnis{\i0}
Dialogue: 0,0:19:05.46,0:19:08.63,Default,,0,0,0,,{\i1}Ini sesuatu yang tak bisa\NKukatakan padamu sebelumnya{\i0}
Dialogue: 0,0:19:08.63,0:19:12.74,Default,,0,0,0,,{\i1}Saat Ibumu tiada,\NRasa sedih hampir menghancurkanku{\i0}
Dialogue: 0,0:19:12.74,0:19:15.51,Default,,0,0,0,,{\i1}Dengan putus asa, aku mulai\NMencari di seluruh dunia..,..{\i0}
Dialogue: 0,0:19:15.52,0:19:18.91,Default,,0,0,0,,{\i1}Suatu petunjuk akan dunia lain{\i0}
Dialogue: 0,0:19:18.91,0:19:22.98,Default,,0,0,0,,{\i1}Bukti Supranatural itu nyata{\i0}
Dialogue: 0,0:19:23.08,0:19:24.50,Default,,0,0,0,,{\i1}Apapun itu..,..{\i0}
Dialogue: 0,0:19:24.62,0:19:26.75,Default,,0,0,0,,{\i1}Supaya merasa Ibumu bisa bersamaku lagi{\i0}
Dialogue: 0,0:19:27.45,0:19:30.32,Default,,0,0,0,,{\i1}Aku menyukai hobi ini, Tunas{\i0}
Dialogue: 0,0:19:31.32,0:19:33.02,Default,,0,0,0,,{\i1}Sekarang..,..{\i0}
Dialogue: 0,0:19:33.02,0:19:34.99,Default,,0,0,0,,{\i1}Ayah ingin kau melakukan sesuatu{\i0}
Dialogue: 0,0:19:34.99,0:19:38.13,Default,,0,0,0,,{\i1}Ayah ingin kau membakar\Nsegala Penelitianku..,..{\i0}
Dialogue: 0,0:19:38.13,0:19:39.39,Default,,0,0,0,,{\i1}Tentang Proyek Himiko{\i0}
Dialogue: 0,0:19:39.40,0:19:41.33,Default,,0,0,0,,{\i1}Bakar segalanya..,..{\i0}
Dialogue: 0,0:19:41.33,0:19:42.90,Default,,0,0,0,,{\i1}Buku catatan,\NVideo rekaman, segalanya{\i0}
Dialogue: 0,0:19:43.28,0:19:45.49,Default,,0,0,0,,{\i1}Ini sangat penting, jika jatuh\NKe tangan orang yang salah, maka..,..{\i0}
Dialogue: 0,0:19:45.49,0:19:47.67,Default,,0,0,0,,{\i1}Akan terjadi bencana besar{\i0}
Dialogue: 0,0:19:47.67,0:19:49.87,Default,,0,0,0,,{\i1}Kumohon, segera bakar semuanya{\i0}
Dialogue: 0,0:19:49.88,0:19:53.17,Default,,0,0,0,,{\i1}Ayah tahu kau benci disuruh..,..{\i0}
Dialogue: 0,0:19:53.18,0:19:55.88,Default,,0,0,0,,{\i1}Tapi, kumohon, berjanjilah melakukannya{\i0}
Dialogue: 0,0:19:55.88,0:19:58.88,Default,,0,0,0,,{\i1}Ayah harap kau tak pernah\NMelihat rekaman ini..,..{\i0}
Dialogue: 0,0:19:59.38,0:20:01.92,Default,,0,0,0,,{\i1}Jika kau melihatnya..,..{\i0}
Dialogue: 0,0:20:01.92,0:20:04.42,Default,,0,0,0,,{\i1}Kau menemukan panggilan rahasia Ayah{\i0}
Dialogue: 0,0:20:06.92,0:20:08.76,Default,,0,0,0,,{\i1}Kuharap kau menemukan panggilanmu juga{\i0}
Dialogue: 0,0:20:15.47,0:20:17.23,Default,,0,0,0,,{\i1}Ayah menyayangimu{\i0}
Dialogue: 0,0:20:40.66,0:20:41.96,Default,,0,0,0,,Himiko.
Dialogue: 0,0:20:51.80,0:20:53.84,Default,,0,0,0,,{\i1}5 Oktober{\i0}
Dialogue: 0,0:20:53.84,0:20:56.64,Default,,0,0,0,,{\i1}Mereka menyebutnya Ratu Himiko,\NIbu Kematian{\i0}
Dialogue: 0,0:20:56.64,0:20:58.14,Default,,0,0,0,,{\i1}Semua kisah mengenai dirinya..,..{\i0}
Dialogue: 0,0:20:58.14,0:21:00.48,Default,,0,0,0,,{\i1}Berakhir dengan sungai menjadi merah darah{\i0}
Dialogue: 0,0:21:00.48,0:21:03.68,Default,,0,0,0,,{\i1}Menyihir orang, mengendalikan\Npikiran mereka{\i0}
Dialogue: 0,0:21:03.68,0:21:05.55,Default,,0,0,0,,{\i1}Menyebar kematian dan kehancuran..,..{\i0}
Dialogue: 0,0:21:05.55,0:21:07.22,Default,,0,0,0,,{\i1}Hanya dengan sentuhan tangannya{\i0}
Dialogue: 0,0:21:07.22,0:21:09.18,Default,,0,0,0,,{\i1}Pertumpahan darah itu berakhir..,..{\i0}
Dialogue: 0,0:21:09.19,0:21:11.85,Default,,0,0,0,,{\i1}Saat Jenderalnya sendiri\NMenguburnya hidup - hidup di sebuah makam{\i0}
Dialogue: 0,0:21:11.86,0:21:14.12,Default,,0,0,0,,{\i1}Menyegelnya dari dunia luar{\i0}
Dialogue: 0,0:21:14.12,0:21:16.79,Default,,0,0,0,,{\i1}Di sebuah Pulau yang tak\Nbisa dijangkau siapapun{\i0}
Dialogue: 0,0:21:16.80,0:21:20.36,Default,,0,0,0,,{\i1}Sejarah telah menghapus keberadaannya{\i0}
Dialogue: 0,0:21:20.36,0:21:23.03,Default,,0,0,0,,{\i1}Pulau yang hilang, Yamatai{\i0}
Dialogue: 0,0:21:23.73,0:21:26.07,Default,,0,0,0,,{\i1}Aku yakin inilah tempatnya{\i0}
Dialogue: 0,0:21:27.47,0:21:30.27,Default,,0,0,0,,{\i1}29 Juli 2011{\i0}
Dialogue: 0,0:21:30.27,0:21:32.01,Default,,0,0,0,,{\i1}Aku akhirnya menemukan Kapten{\i0}
Dialogue: 0,0:21:32.01,0:21:34.04,Default,,0,0,0,,{\i1}Yang mau membawaku ke Pulau itu{\i0}
Dialogue: 0,0:21:34.05,0:21:35.96,Default,,0,0,0,,{\i1}Aku harus sampai kesana lebih dulu\NUntuk memastikan..,..{\i0}
Dialogue: 0,0:21:35.96,0:21:39.38,Default,,0,0,0,,{\i1}Kekuatan Himiko tak jatuh\NKe tangan orang yang salah{\i0}
Dialogue: 0,0:21:50.57,0:21:54.35,Default,,0,0,0,,{\b1\c&H00ff00&}Rumah Gadai\NBayar Tunai Untuk Perhiasan{\c}{\b0}
Dialogue: 0,0:21:57.77,0:22:00.17,Default,,0,0,0,,{\i1}Jimat ini diberikan oleh Ayahku..,..{\i0}
Dialogue: 0,0:22:00.17,0:22:03.11,Default,,0,0,0,,Dari Kaisar Showa di Jepang.
Dialogue: 0,0:22:03.54,0:22:05.81,Default,,0,0,0,,Benar, benar, tapi..,..
Dialogue: 0,0:22:05.81,0:22:08.31,Default,,0,0,0,,Ini tak dari Jepang.
Dialogue: 0,0:22:08.31,0:22:10.01,Default,,0,0,0,,Kau benar.
Dialogue: 0,0:22:10.01,0:22:11.78,Default,,0,0,0,,Kaisar Showa dihadiahi oleh cucu..,..
Dialogue: 0,0:22:11.78,0:22:13.55,Default,,0,0,0,,Mantan Kaisar Maratha.
Dialogue: 0,0:22:13.55,0:22:16.55,Default,,0,0,0,,Maratha?\NKau bercanda 'kan?
Dialogue: 0,0:22:16.55,0:22:18.35,Default,,0,0,0,,Dan dia mengambilnya dari Thibaw..,..
Dialogue: 0,0:22:18.36,0:22:19.72,Default,,0,0,0,,Raja terakhir Burma saat..,..
Dialogue: 0,0:22:19.72,0:22:21.56,Default,,0,0,0,,Dia diasingkan di India.
Dialogue: 0,0:22:21.56,0:22:23.59,Default,,0,0,0,,Diasingkan di India?
Dialogue: 0,0:22:23.59,0:22:25.46,Default,,0,0,0,,Kisah yang menarik.
Dialogue: 0,0:22:25.46,0:22:27.10,Default,,0,0,0,,Kau punya semua Surat..,..
