Dialogue: 0,0:00:31.40,0:00:34.40,Default,,0,0,0,,{\i1}I amar prestar aen.{\i0}
Dialogue: 0,0:00:34.70,0:00:36.70,Default,,0,0,0,,{\i1}Thế giới đã thay đổi.{\i0}
Dialogue: 0,0:00:36.80,0:00:37.70,Default,,0,0,0,,{\i1}Han mathon ne nen.{\i0}
Dialogue: 0,0:00:37.90,0:00:40.00,Default,,0,0,0,,{\i1}Ta cảm thấy Nó trong nước.{\i0}
Dialogue: 0,0:00:40.10,0:00:41.40,Default,,0,0,0,,{\i1}Han mathon ne chae.{\i0}
Dialogue: 0,0:00:41.70,0:00:44.10,Default,,0,0,0,,{\i1}Ta cảm thấy Nó trong đất.{\i0}
Dialogue: 0,0:00:44.30,0:00:46.00,Default,,0,0,0,,{\i1}A han noston ned 'wilith.{\i0}
Dialogue: 0,0:00:46.20,0:00:49.10,Default,,0,0,0,,{\i1}Ta cảm thấy Nó trong không khí.{\i0}
Dialogue: 0,0:00:49.60,0:00:51.40,Default,,0,0,0,,{\i1}Nó đã biến mất...{\i0}
Dialogue: 0,0:00:51.70,0:00:53.20,Default,,0,0,0,,{\i1}rất nhiều lần.{\i0}
Dialogue: 0,0:00:54.00,0:00:56.90,Default,,0,0,0,,{\i1}Hẳn rằng không ai còn nhớ về Nó.{\i0}
Dialogue: 0,0:00:57.84,0:01:04.40,Default,,0,0,0,,CHÚA TỂ NHỮNG CHIẾC NHẪN
Dialogue: 0,0:01:09.90,0:01:14.10,Default,,0,0,0,,{\i1}Chuyện bắt đầu với việc\Nđúc thành những chiếc Nhẫn Vĩ Đại.{\i0}
Dialogue: 0,0:01:14.40,0:01:16.30,Default,,0,0,0,,{\i1}3 chiếc được đưa cho\Ntộc Tiên:{\i0}
Dialogue: 0,0:01:16.60,0:01:20.20,Default,,0,0,0,,{\i1}bất tử, thông thái\Nvà ngay thẳng nhất trong các loài.{\i0}
Dialogue: 0,0:01:21.70,0:01:24.00,Default,,0,0,0,,{\i1}7 chiếc được trao cho\Ncác lãnh chúa Người Lùn:{\i0}
Dialogue: 0,0:01:24.20,0:01:28.50,Default,,0,0,0,,{\i1}những người thợ mỏ và\Nthợ thủ công vĩ đại của những rặng núi lớn.{\i0}
Dialogue: 0,0:01:29.10,0:01:30.30,Default,,0,0,0,,{\i1}Và 9 cái...{\i0}
Dialogue: 0,0:01:30.60,0:01:33.40,Default,,0,0,0,,{\i1}9 chiếc Nhẫn\Nđược ban cho tộc Người...{\i0}
Dialogue: 0,0:01:33.80,0:01:37.50,Default,,0,0,0,,{\i1}là chủng loài\Nluôn khao khát sức mạnh.{\i0}
Dialogue: 0,0:01:38.70,0:01:43.70,Default,,0,0,0,,{\i1}Trong các chiếc Nhẫn chứa đựng\Nsức mạnh và ý chí kiềm chế các tộc.{\i0}
Dialogue: 0,0:01:45.50,0:01:48.50,Default,,0,0,0,,{\i1}Nhưng tất cả họ đều bị lừa.{\i0}
Dialogue: 0,0:01:48.90,0:01:50.90,Default,,0,0,0,,{\i1}Vì một chiếc Nhẫn khác\Nđã được tạo ra.{\i0}
Dialogue: 0,0:01:51.80,0:01:55.20,Default,,0,0,0,,{\i1}Tại vùng đất Mordor,\Ntrong ngọn lửa của núi Diệt Vong...{\i0}
Dialogue: 0,0:01:55.80,0:02:00.10,Default,,0,0,0,,{\i1}lãnh chúa Sauron bí mật\Ntôi luyện chiếc Nhẫn chúa...{\i0}
Dialogue: 0,0:02:00.50,0:02:02.40,Default,,0,0,0,,{\i1}để điều khiển tất cả\Nnhững chiếc Nhẫn khác.{\i0}
Dialogue: 0,0:02:02.70,0:02:06.50,Default,,0,0,0,,{\i1}Hắn cho vào chiếc Nhẫn đó\Nsự tàn bạo, nham hiểm...{\i0}
Dialogue: 0,0:02:06.90,0:02:10.90,Default,,0,0,0,,{\i1}và ý định thống trị\Ntất cả sự sống.{\i0}
Dialogue: 0,0:02:12.10,0:02:15.80,Default,,0,0,0,,{\i1}Một chiếc Nhẫn thống trị tất cả.{\i0}
Dialogue: 0,0:02:16.80,0:02:18.40,Default,,0,0,0,,{\i1}Lần lượt...{\i0}
Dialogue: 0,0:02:18.60,0:02:23.10,Default,,0,0,0,,{\i1}những vùng đất tự do của Trung Địa\Nbị khuất phục dưới sức mạnh của chiếc Nhẫn.{\i0}
Dialogue: 0,0:02:23.50,0:02:26.40,Default,,0,0,0,,{\i1}Nhưng vẫn còn một số chống cự.{\i0}
Dialogue: 0,0:02:27.10,0:02:31.50,Default,,0,0,0,,{\i1}Liên minh cuối cùng của tộc Người và Tiên\Nđã chống lại đội quân của Mordor.{\i0}
Dialogue: 0,0:02:31.90,0:02:36.40,Default,,0,0,0,,{\i1}Và trên những sườn dốc của ngọn Diệt Vong,\Nhọ chiến đấu cho tự do của vùng Trung Địa.{\i0}
Dialogue: 0,0:02:49.10,0:02:51.50,Default,,0,0,0,,Giữ vị trí!
Dialogue: 0,0:02:51.60,0:02:55.20,Default,,0,0,0,,Bắn tên!
Dialogue: 0,0:03:06.90,0:03:09.40,Default,,0,0,0,,{\i1}Chiến thắng đã gần kề.{\i0}
Dialogue: 0,0:03:14.20,0:03:16.50,Default,,0,0,0,,{\i1}Nhưng sức mạnh của chiếc Nhẫn...{\i0}
Dialogue: 0,0:03:16.80,0:03:18.60,Default,,0,0,0,,{\i1}vẫn không thể bị đánh bại.{\i0}
Dialogue: 0,0:03:42.90,0:03:44.70,Default,,0,0,0,,{\i1}Trong khoảnh khắc đó...{\i0}
Dialogue: 0,0:03:45.00,0:03:47.30,Default,,0,0,0,,{\i1}khi mọi hy vọng đều tắt...{\i0}
Dialogue: 0,0:03:47.60,0:03:51.50,Default,,0,0,0,,{\i1}thì Isildur, con trai đức vua,\Ncầm thanh gươm cha mình lên.{\i0}
Dialogue: 0,0:04:22.80,0:04:29.00,Default,,0,0,0,,{\i1}Sauron, kẻ thù của những người tự do\Nvùng Trung Địa, đã bị đánh bại.{\i0}
Dialogue: 0,0:04:36.20,0:04:38.30,Default,,0,0,0,,{\i1}Chiếc Nhẫn về tay Isildur,{\i0}
Dialogue: 0,0:04:38.60,0:04:42.60,Default,,0,0,0,,{\i1}người có cơ hội tiêu diệt\Nquỷ dữ mãi mãi.{\i0}
Dialogue: 0,0:04:43.50,0:04:45.50,Default,,0,0,0,,{\i1}Nhưng trái tim của con người...{\i0}
Dialogue: 0,0:04:45.80,0:04:48.20,Default,,0,0,0,,{\i1}lại dễ dàng hư hỏng.{\i0}
Dialogue: 0,0:04:48.60,0:04:53.00,Default,,0,0,0,,{\i1}Và chiếc Nhẫn quyền lực\Ncó ý chí của riêng nó.{\i0}
Dialogue: 0,0:05:18.40,0:05:20.90,Default,,0,0,0,,{\i1}Nó phản bội Isildur,{\i0}
Dialogue: 0,0:05:22.90,0:05:25.20,Default,,0,0,0,,{\i1}khiến ngài chết thảm.{\i0}
Dialogue: 0,0:05:29.50,0:05:33.20,Default,,0,0,0,,{\i1}Có những điều đáng ra\Nkhông được lãng quên...{\i0}
Dialogue: 0,0:05:33.50,0:05:35.70,Default,,0,0,0,,{\i1}thì lại biến mất.{\i0}
Dialogue: 0,0:05:36.60,0:05:38.80,Default,,0,0,0,,{\i1}Lịch sử trở thành truyền thuyết,{\i0}
Dialogue: 0,0:05:39.10,0:05:41.00,Default,,0,0,0,,{\i1}truyền thuyết trở thành huyền thoại.{\i0}
Dialogue: 0,0:05:41.30,0:05:44.00,Default,,0,0,0,,{\i1}Cho đến 2.500 năm sau,{\i0}
Dialogue: 0,0:05:44.40,0:05:48.00,Default,,0,0,0,,{\i1}không còn ai biết đến chiếc Nhẫn.{\i0}
Dialogue: 0,0:05:48.70,0:05:51.40,Default,,0,0,0,,{\i1}Cho đến khi cơ hội đến,{\i0}
Dialogue: 0,0:05:52.20,0:05:55.20,Default,,0,0,0,,{\i1}nó đã bẫy một nạn nhân mới.{\i0}
Dialogue: 0,0:05:56.70,0:06:01.30,Default,,0,0,0,,Nhẫn quý của ta.
Dialogue: 0,0:06:01.80,0:06:04.20,Default,,0,0,0,,{\i1}Chiếc Nhẫn tìm đến\Nvới sinh vật tên là Gollum,{\i0}
Dialogue: 0,0:06:04.50,0:06:08.20,Default,,0,0,0,,{\i1}hắn đem nó vào sâu trong những\Nđường hầm của dãy núi Sương Mù.{\i0}
Dialogue: 0,0:06:08.70,0:06:11.50,Default,,0,0,0,,{\i1}Và ở đó, nó huỷ hoại hắn.{\i0}
Dialogue: 0,0:06:11.80,0:06:13.70,Default,,0,0,0,,Nhẫn đã đến với ta.
Dialogue: 0,0:06:14.00,0:06:18.30,Default,,0,0,0,,Của ta, của ta.
Dialogue: 0,0:06:18.70,0:06:23.20,Default,,0,0,0,,Nhẫn quý của ta.
Dialogue: 0,0:06:27.00,0:06:30.30,Default,,0,0,0,,{\i1}Chiếc Nhẫn ban cho Gollum\Ncuộc sống dài lâu bất thường.{\i0}
Dialogue: 0,0:06:30.70,0:06:34.30,Default,,0,0,0,,{\i1}Trong hơn 500 năm,\Nnó đầu độc tâm trí hắn.{\i0}
Dialogue: 0,0:06:34.60,0:06:39.00,Default,,0,0,0,,{\i1}Và trong hang động tối tăm\Ncủa Gollum, nó chờ đợi.{\i0}
Dialogue: 0,0:06:39.40,0:06:42.60,Default,,0,0,0,,{\i1}Bóng tối tràn về những khu rừng\Ntrên thế giới.{\i0}
Dialogue: 0,0:06:43.00,0:06:45.70,Default,,0,0,0,,{\i1}Lời đồn về một thế lực bóng tối\Nở phương Đông dần lan ra,{\i0}
Dialogue: 0,0:06:46.00,0:06:48.70,Default,,0,0,0,,{\i1}thì thầm về\Nmột nỗi sợ không tên.{\i0}
Dialogue: 0,0:06:49.00,0:06:52.20,Default,,0,0,0,,{\i1}Và chiếc Nhẫn quyền năng nhận thấy...{\i0}
Dialogue: 0,0:06:52.60,0:06:55.20,Default,,0,0,0,,{\i1}thời điểm của nó đã đến.{\i0}
Dialogue: 0,0:06:57.80,0:07:00.30,Default,,0,0,0,,{\i1}Nó bỏ rơi Gollum.{\i0}
Dialogue: 0,0:07:01.10,0:07:05.30,Default,,0,0,0,,{\i1}Nhưng có chuyện đã xảy ra\Nngoài dự đoán của nó.{\i0}
Dialogue: 0,0:07:06.40,0:07:10.80,Default,,0,0,0,,{\i1}Nó được nhặt bởi một loài\Nkhông giống với các sinh vật khác.{\i0}
Dialogue: 0,0:07:11.20,0:07:12.20,Default,,0,0,0,,Cái gì đây?
Dialogue: 0,0:07:12.50,0:07:13.90,Default,,0,0,0,,{\i1}Một người lùn Hobbit.{\i0}
Dialogue: 0,0:07:14.00,0:07:16.70,Default,,0,0,0,,{\i1}Bilbo Baggins của vùng Shire.{\i0}
Dialogue: 0,0:07:17.10,0:07:18.80,Default,,0,0,0,,Một chiếc Nhẫn.
Dialogue: 0,0:07:19.00,0:07:20.80,Default,,0,0,0,,Mất rồi!
Dialogue: 0,0:07:22.30,0:07:25.20,Default,,0,0,0,,Ngày 22 tháng 9...
Dialogue: 0,0:07:23.60,0:07:26.90,Default,,0,0,0,,Nhẫn quí của ta mất rồi!
Dialogue: 0,0:07:27.30,0:07:29.50,Default,,0,0,0,,{\i1}Và sẽ sớm đến lúc...{\i0}
Dialogue: 0,0:07:29.90,0:07:34.80,Default,,0,0,0,,{\i1}người Hobbit quyết định\Nvận mệnh của tất cả.{\i0}
Dialogue: 0,0:07:43.00,0:07:46.10,Default,,0,0,0,,vào năm 1.400...
Dialogue: 0,0:07:46.50,0:07:48.70,Default,,0,0,0,,theo lịch vùng Shire.
Dialogue: 0,0:07:48.90,0:07:54.50,Default,,0,0,0,,Bag End, Bagshot Row.\NLàng Hobbiton, xứ Westfarthing,
Dialogue: 0,0:07:54.90,0:07:56.80,Default,,0,0,0,,vùng Shire,
Dialogue: 0,0:07:57.10,0:07:59.80,Default,,0,0,0,,Trung Địa.
Dialogue: 0,0:08:04.50,0:08:07.70,Default,,0,0,0,,Thời đại thứ ba của Thế giới.
Dialogue: 0,0:08:10.10,0:08:15.10,Default,,0,0,0,,HỘI BẰNG HỮU CỦA NHẪN
Dialogue: 0,0:08:19.50,0:08:22.00,Default,,0,0,0,,Hành trình và trở về.
Dialogue: 0,0:08:24.00,0:08:25.70,Default,,0,0,0,,Câu chuyện về người Hobbit.
Dialogue: 0,0:08:26.00,0:08:29.80,Default,,0,0,0,,Tác giả Bilbo Baggins.
Dialogue: 0,0:08:34.70,0:08:36.30,Default,,0,0,0,,Giờ thì...
Dialogue: 0,0:08:37.20,0:08:39.10,Default,,0,0,0,,nên bắt đầu từ đâu nhỉ?
Dialogue: 0,0:08:40.50,0:08:41.50,Default,,0,0,0,,À, phải rồi.
Dialogue: 0,0:08:46.00,0:08:47.80,Default,,0,0,0,,Chuyện về...
Dialogue: 0,0:08:49.00,0:08:50.50,Default,,0,0,0,,người Hobbit.
Dialogue: 0,0:08:51.70,0:08:55.10,Default,,0,0,0,,{\i1}Người Hobbit sinh sống và trồng trọt\Ntrên bốn cánh đồng Farthing của vùng Shire...{\i0}
Dialogue: 0,0:08:55.40,0:08:57.40,Default,,0,0,0,,{\i1}qua hàng trăm năm nay...{\i0}
Dialogue: 0,0:08:57.60,0:09:00.70,Default,,0,0,0,,{\i1}và không màng đến,\Ncũng như không bị biết đến...{\i0}
Dialogue: 0,0:09:00.80,0:09:03.80,Default,,0,0,0,,{\i1}bởi thế giới của những sinh vật\Nto lớn xung quanh.{\i0}
Dialogue: 0,0:09:04.30,0:09:09.70,Default,,0,0,0,,{\i1}Trung Địa là thế, đầy những\Nsinh vật lạ không kể hết...{\i0}
Dialogue: 0,0:09:10.10,0:09:13.20,Default,,0,0,0,,{\i1}và người Hobbit có vẻ như\Nchẳng quan trọng gì lắm.{\i0}
Dialogue: 0,0:09:13.50,0:09:17.30,Default,,0,0,0,,{\i1}Họ không có\Nnhững chiến binh vĩ đại...{\i0}
Dialogue: 0,0:09:17.70,0:09:21.70,Default,,0,0,0,,{\i1}và cũng chẳng phải là\Nnhững nhà thông thái.{\i0}
Dialogue: 0,0:09:26.20,0:09:29.10,Default,,0,0,0,,Frodo! Ai ngoài cửa kìa.
Dialogue: 0,0:09:30.40,0:09:32.90,Default,,0,0,0,,{\i1}Sự thật, người Hobbit\Nchỉ có hứng thú...{\i0}
Dialogue: 0,0:09:33.20,0:09:37.40,Default,,0,0,0,,{\i1}đối với thức ăn.{\i0}
Dialogue: 0,0:09:38.00,0:09:40.10,Default,,0,0,0,,{\i1}Lời nhận xét không chính xác lắm...{\i0}
Dialogue: 0,0:09:40.40,0:09:43.80,Default,,0,0,0,,{\i1}vì chúng tôi\Ncòn thích ủ rượu bia...{\i0}
Dialogue: 0,0:09:44.50,0:09:47.10,Default,,0,0,0,,{\i1}và hút thuốc trong tẩu.{\i0}
Dialogue: 0,0:09:47.70,0:09:50.40,Default,,0,0,0,,{\i1}Nhưng thật sự con tim chúng tôi\Nthuộc về...{\i0}
Dialogue: 0,0:09:50.70,0:09:52.70,Default,,0,0,0,,{\i1}đất mẹ bình yên, tĩnh lặng,{\i0}
Dialogue: 0,0:09:53.60,0:09:55.60,Default,,0,0,0,,{\i1}tốt bụng và màu mỡ.{\i0}
Dialogue: 0,0:09:55.90,0:10:00.90,Default,,0,0,0,,{\i1}Tất cả người Hobbit chia sẻ tình yêu\Nvề những gì nảy mầm từ đất.{\i0}
Dialogue: 0,0:10:04.20,0:10:08.40,Default,,0,0,0,,{\i1}Và có lẽ, đối với những loài khác,\Ncách sống của chúng tôi là kì quặc.{\i0}
Dialogue: 0,0:10:08.80,0:10:13.80,Default,,0,0,0,,{\i1}Nhưng hôm nay, tôi được về nhà.{\i0}
Dialogue: 0,0:10:14.60,0:10:19.28,Default,,0,0,0,,{\i1}Chẳng tội gì mà không ăn mừng\Ncho một cuộc sống giản đơn.{\i0}
Dialogue: 0,0:10:19.38,0:10:21.15,Default,,0,0,0,,Mừng sinh nhật Bilbo Baggins.
Dialogue: 0,0:10:24.70,0:10:27.80,Default,,0,0,0,,Frodo, mở cửa kìa!
Dialogue: 0,0:10:31.70,0:10:33.60,Default,,0,0,0,,Thằng nhỏ đâu rồi?
Dialogue: 0,0:10:33.80,0:10:35.50,Default,,0,0,0,,Frodo!
Dialogue: 0,0:10:41.30,0:10:44.70,Default,,0,0,0,,{\i1}Mọi chuyện bắt đầu\Ntừ dưới cánh cửa kia.{\i0}
Dialogue: 0,0:10:49.70,0:10:53.30,Default,,0,0,0,,{\i1}Và tôi phải đi theo\Nnếu có thể.{\i0}
Dialogue: 0,0:10:54.50,0:10:58.50,Default,,0,0,0,,{\i1}Con đường vẫn tiếp tục trải dài.{\i0}
Dialogue: 0,0:10:58.80,0:11:02.00,Default,,0,0,0,,{\i1}Mọi chuyện bắt đầu\Ntừ dưới cánh cửa kia.{\i0}
Dialogue: 0,0:11:02.50,0:11:07.20,Default,,0,0,0,,{\i1}Xa xa phía trước\Ncon đường chấm dứt.{\i0}
Dialogue: 0,0:11:08.10,0:11:11.70,Default,,0,0,0,,{\i1}Và tôi phải đi tiếp nếu có thể...\N- Ông đến trễ.{\i0}
Dialogue: 0,0:11:15.10,0:11:18.80,Default,,0,0,0,,Một pháp sư không bao giờ\Nđến trễ, Frodo Baggins.
Dialogue: 0,0:11:19.70,0:11:24.20,Default,,0,0,0,,Đến sớm cũng không.\NÔng ta đến đúng lúc cần.
Dialogue: 0,0:11:35.70,0:11:38.60,Default,,0,0,0,,Thật tuyệt khi được gặp ông, Gandalf!
Dialogue: 0,0:11:42.60,0:11:46.20,Default,,0,0,0,,Cậu nghĩ ta sẽ bỏ qua tiệc sinh nhật\Nông chú Bilbo của cậu chắc?
Dialogue: 0,0:11:48.00,0:11:50.10,Default,,0,0,0,,Ông già sao rồi?
Dialogue: 0,0:11:50.80,0:11:54.20,Default,,0,0,0,,Ta nghe nói sẽ là một bữa tiệc\Nrất hoành tráng mà.
Dialogue: 0,0:11:54.50,0:11:56.80,Default,,0,0,0,,Ông biết Bilbo mà,\Nchú ấy có cả một nơi đầy náo nhiệt.
Dialogue: 0,0:11:57.10,0:11:59.00,Default,,0,0,0,,Vậy thì lão ta sẽ hài lòng lắm.
Dialogue: 0,0:11:59.20,0:12:01.00,Default,,0,0,0,,Một nửa dân vùng Shire\Nđã được mời.
Dialogue: 0,0:12:01.40,0:12:03.90,Default,,0,0,0,,Và số còn lại đang đi đến đây.
Dialogue: 0,0:12:07.10,0:12:10.30,Default,,0,0,0,,{\i1}Và cuộc sống tại Shire\Nvẫn tiếp diễn...{\i0}
Dialogue: 0,0:12:10.70,0:12:13.60,Default,,0,0,0,,{\i1}giống như trong quá khứ.{\i0}
Dialogue: 0,0:12:13.90,0:12:19.20,Default,,0,0,0,,{\i1}Nhiều sự việc đã xảy ra,\Nvà mọi thứ chầm chậm thay đổi.{\i0}
Dialogue: 0,0:12:19.50,0:12:21.40,Default,,0,0,0,,{\i1}Nếu thật sự có thay đổi.{\i0}
Dialogue: 0,0:12:22.80,0:12:26.10,Default,,0,0,0,,{\i1}Tất cả những gì được làm ra\Ntại vùng đất Shire...{\i0}
Dialogue: 0,0:12:26.40,0:12:29.70,Default,,0,0,0,,{\i1}đều được truyền từ đời này\Nsang đời khác.{\i0}
Dialogue: 0,0:12:30.30,0:12:35.10,Default,,0,0,0,,{\i1}Sẽ luôn có người của gia đình Baggins\Nsống dưới ngọn đồi...{\i0}
Dialogue: 0,0:12:35.50,0:12:36.80,Default,,0,0,0,,{\i1}tại Bag End.{\i0}
Dialogue: 0,0:12:39.60,0:12:42.30,Default,,0,0,0,,Và sẽ luôn như thế.
Dialogue: 0,0:12:43.10,0:12:46.30,Default,,0,0,0,,Nói ông biết chuyện này nhé,\Nchú Bilbo gần đây hơi lập dị.
Dialogue: 0,0:12:47.70,0:12:49.90,Default,,0,0,0,,Ý cháu là khác hẳn thường ngày.
Dialogue: 0,0:12:51.00,0:12:52.80,Default,,0,0,0,,Chú ấy tự khoá mình trong phòng\Nđể nghiên cứu.
Dialogue: 0,0:12:54.50,0:12:58.50,Default,,0,0,0,,Bỏ hàng giờ liền nghiền ngẫm những tấm bản đồ\Ncũ rích, khi cho rằng cháu không để ý.
Dialogue: 0,0:13:11.40,0:13:13.00,Default,,0,0,0,,Nó đâu rồi?
Dialogue: 0,0:13:39.60,0:13:41.80,Default,,0,0,0,,Chú ấy đang toan tính gì đó.
Dialogue: 0,0:13:50.00,0:13:52.40,Default,,0,0,0,,- Được rồi, hãy giữ bí mật nhé.\N- Sao?
Dialogue: 0,0:13:52.70,0:13:54.30,Default,,0,0,0,,Nhưng cháu biết ông sẽ làm gì đó.
Dialogue: 0,0:13:54.70,0:13:55.70,Default,,0,0,0,,Chao ôi!
Dialogue: 0,0:13:56.20,0:13:58.80,Default,,0,0,0,,Trước khi ông đến, gia đình Baggins\Nđang được tiếng tốt.
Dialogue: 0,0:13:59.20,0:14:00.00,Default,,0,0,0,,Đúng thật.
Dialogue: 0,0:14:00.40,0:14:02.50,Default,,0,0,0,,Không bao giờ có cuộc phiêu lưu nào\Nhay làm điều gì khác thường.
Dialogue: 0,0:14:03.20,0:14:08.80,Default,,0,0,0,,Nếu cậu đang nói đến chuyện với loài rồng,\Nthì đúng là ta có liên quan.
Dialogue: 0,0:14:09.20,0:14:12.70,Default,,0,0,0,,Việc ta làm chỉ là đưa chú cậu\Nra khỏi nhà một tí thôi.
Dialogue: 0,0:14:13.00,0:14:17.00,Default,,0,0,0,,Gì cũng được, nhưng ông đã bị cho là\Nkẻ phá rối sự yên bình rồi đấy.
Dialogue: 0,0:14:17.60,0:14:19.40,Default,,0,0,0,,Thật vậy sao?
Dialogue: 0,0:14:25.10,0:14:27.00,Default,,0,0,0,,Gandalf! Gandalf!
Dialogue: 0,0:14:33.20,0:14:34.40,Default,,0,0,0,,Pháo hoa, Gandalf!
Dialogue: 0,0:14:37.50,0:14:40.00,Default,,0,0,0,,- Gandalf!\N- Pháo hoa, Gandalf!
Dialogue: 0,0:14:59.00,0:15:00.00,Default,,0,0,0,,Gandalf?
Dialogue: 0,0:15:00.60,0:15:04.50,Default,,0,0,0,,- Cháu rất vui là đã ông quay lại\N- Ta cũng vậy, chào nhóc.
Dialogue: 0,0:15:09.10,0:15:10.90,Default,,0,0,0,,Ta cũng vậy.
Dialogue: 0,0:15:37.79,0:15:39.77,Default,,0,0,0,,Miễn vào,\Nngoại trừ việc làm ăn.
Dialogue: 0,0:15:42.80,0:15:44.20,Default,,0,0,0,,Không, cám ơn!
Dialogue: 0,0:15:44.40,0:15:48.10,Default,,0,0,0,,Chúng tôi không muốn tiếp khách,\Nngười có thiện chí hay bà con xa nào hết.
Dialogue: 0,0:15:48.60,0:15:51.90,Default,,0,0,0,,Một người bạn cũ thì sao?
Dialogue: 0,0:15:55.30,0:15:56.90,Default,,0,0,0,,Gandalf?
Dialogue: 0,0:15:57.60,0:16:00.90,Default,,0,0,0,,- Bilbo Baggins.\N- Gandalf thân mến!
Dialogue: 0,0:16:01.30,0:16:06.20,Default,,0,0,0,,Gặp lại ông thật mừng! 111 tuổi!\NAi có thể tin được chứ?
Dialogue: 0,0:16:08.20,0:16:10.10,Default,,0,0,0,,Ông không già đi tí nào.
Dialogue: 0,0:16:16.80,0:16:18.70,Default,,0,0,0,,Thôi nào, vào đi!
Dialogue: 0,0:16:19.00,0:16:21.30,Default,,0,0,0,,Mời vào, mời vào!
Dialogue: 0,0:16:22.60,0:16:25.00,Default,,0,0,0,,À, vào đây.
Dialogue: 0,0:16:26.80,0:16:29.20,Default,,0,0,0,,Trà nhé, hay cái khác mạnh hơn?
Dialogue: 0,0:16:29.40,0:16:33.10,Default,,0,0,0,,Tôi còn vài chai Old Winyard\Ntừ năm 1296.
Dialogue: 0,0:16:33.40,0:16:37.20,Default,,0,0,0,,Lâu đấy, gần bằng tuổi tôi còn gì.
Dialogue: 0,0:16:38.60,0:16:42.90,Default,,0,0,0,,Được cha tôi ủ đấy.\NTa khui một chai nhé?
Dialogue: 0,0:16:43.30,0:16:45.80,Default,,0,0,0,,Chỉ trà thôi, cám ơn.
Dialogue: 0,0:16:52.40,0:16:53.90,Default,,0,0,0,,Tôi mong ông từ tuần trước cơ.
Dialogue: 0,0:16:54.20,0:16:57.40,Default,,0,0,0,,À không, ông đến và đi khi ông thích.\NĐã từng thế, và sẽ như vậy mãi.
Dialogue: 0,0:16:57.70,0:17:02.70,Default,,0,0,0,,Tôi chưa kịp chuẩn bị gì nhiều,\Nchỉ có gà lạnh và một ít dưa chua.
Dialogue: 0,0:17:03.10,0:17:06.30,Default,,0,0,0,,Có pho mát đây, nhưng chắc\Nông không cần đâu.
Dialogue: 0,0:17:06.70,0:17:10.50,Default,,0,0,0,,Tôi có mứt mâm xôi,\Nmột cái bánh táo...
Dialogue: 0,0:17:11.30,0:17:15.20,Default,,0,0,0,,Nhưng cũng không còn nhiều.\NÀ, đây rồi.
Dialogue: 0,0:17:15.50,0:17:18.20,Default,,0,0,0,,Tôi tìm thấy vài cái bánh xốp này.
Dialogue: 0,0:17:21.30,0:17:22.50,Default,,0,0,0,,Tôi có thể làm vài quả trứng\Nnếu ông thích...
Dialogue: 0,0:17:26.40,0:17:29.00,Default,,0,0,0,,- Chỉ trà thôi, cám ơn.\N- Ồ, được rồi.
Dialogue: 0,0:17:30.10,0:17:32.30,Default,,0,0,0,,- Ông không phiền nếu tôi ăn chứ.\N- Không, không sao đâu.
Dialogue: 0,0:17:33.60,0:17:35.40,Default,,0,0,0,,Bilbo! Bilbo Baggins!
Dialogue: 0,0:17:35.70,0:17:37.20,Default,,0,0,0,,Tôi không có nhà.
Dialogue: 0,0:17:43.30,0:17:45.80,Default,,0,0,0,,Đó là gia đình Sackville-Bagginses.
Dialogue: 0,0:17:46.10,0:17:48.10,Default,,0,0,0,,- Tôi biết ông ở đó mà.\N- Chúng muốn ngôi nhà này.
Dialogue: 0,0:17:48.80,0:17:51.20,Default,,0,0,0,,Chúng rủa tôi vì đã sống quá lâu.
Dialogue: 0,0:17:51.50,0:17:54.20,Default,,0,0,0,,Tôi phải thoát khỏi lũ họ hàng chết tiệt này,\Ncứ kêu cửa suốt ngày...
Dialogue: 0,0:17:54.40,0:17:56.30,Default,,0,0,0,,không cho tôi một phút yên bình.
Dialogue: 0,0:17:57.20,0:18:00.60,Default,,0,0,0,,Tôi muốn đến lại những ngọn núi.\NNhững ngọn núi, Gandalf!
Dialogue: 0,0:18:00.90,0:18:05.30,Default,,0,0,0,,Tìm nơi nào yên tĩnh để\Nhoàn thành cuốn sách này. A, trà!
Dialogue: 0,0:18:05.60,0:18:09.40,Default,,0,0,0,,- Ông đã có kế hoạch ra đi à?\N- Đúng, tất cả đã trong tầm tay.
Dialogue: 0,0:18:09.80,0:18:11.90,Default,,0,0,0,,Tất cả đã được sắp xếp xong.
Dialogue: 0,0:18:14.10,0:18:15.50,Default,,0,0,0,,Ồ, cám ơn.
Dialogue: 0,0:18:16.30,0:18:18.30,Default,,0,0,0,,Frodo đang nghi ngờ điều gì đó.
Dialogue: 0,0:18:18.60,0:18:21.00,Default,,0,0,0,,Dĩ nhiên, nó mang họ Baggins mà...
Dialogue: 0,0:18:21.30,0:18:23.80,Default,,0,0,0,,chứ đâu ngốc như Bracegirdle\Nở Hardbottle.
Dialogue: 0,0:18:24.50,0:18:27.20,Default,,0,0,0,,Ông sẽ nói với nó chứ?
Dialogue: 0,0:18:28.10,0:18:30.10,Default,,0,0,0,,- Vâng\N- Nó có vẻ hào hứng với ông.
Dialogue: 0,0:18:32.40,0:18:33.90,Default,,0,0,0,,Tôi biết.
Dialogue: 0,0:18:36.30,0:18:39.60,Default,,0,0,0,,Nó có thể sẽ đi với tôi\Nnếu tôi rủ nó.
Dialogue: 0,0:18:40.30,0:18:43.50,Default,,0,0,0,,Tôi nghĩ Frodo vẫn còn tình cảm\Nvới vùng Shire trong tim mình.
Dialogue: 0,0:18:45.20,0:18:47.50,Default,,0,0,0,,Những rừng cây, cánh đồng.
Dialogue: 0,0:18:47.80,0:18:50.00,Default,,0,0,0,,Những dòng sông nhỏ.
Dialogue: 0,0:18:54.60,0:18:57.20,Default,,0,0,0,,Tôi già rồi, Gandalf.
Dialogue: 0,0:18:58.70,0:19:03.70,Default,,0,0,0,,Tôi biết nhìn bề ngoài thì không,\Nnhưng tôi bắt đầu cảm thấy rồi.
Dialogue: 0,0:19:06.10,0:19:08.60,Default,,0,0,0,,Tôi cảm thấy yếu ớt.
Dialogue: 0,0:19:08.80,0:19:10.50,Default,,0,0,0,,Phần nào bị kéo căng...
Dialogue: 0,0:19:11.80,0:19:15.00,Default,,0,0,0,,như bơ bị trét lên bánh mì vậy.
Dialogue: 0,0:19:15.40,0:19:19.70,Default,,0,0,0,,Tôi cần một kỳ nghỉ,\Nmột kỳ nghỉ thật dài.
Dialogue: 0,0:19:20.00,0:19:23.10,Default,,0,0,0,,Và tôi không mong là sẽ trở về.
Dialogue: 0,0:19:25.50,0:19:28.60,Default,,0,0,0,,Tôi thực sự không muốn thế.
Dialogue: 0,0:19:31.30,0:19:35.20,Default,,0,0,0,,Old Toby. Loại lá thuốc\Ntốt nhất tại Nam Farthing.
Dialogue: 0,0:19:52.80,0:19:55.30,Default,,0,0,0,,Gandalf, ông bạn già của tôi,
Dialogue: 0,0:19:55.50,0:19:58.20,Default,,0,0,0,,sẽ là một đêm đáng nhớ đây.
Dialogue: 0,0:20:21.10,0:20:22.20,Default,,0,0,0,,Xin chào, xin chào.
Dialogue: 0,0:20:22.50,0:20:26.40,Default,,0,0,0,,Fatty Bolger, rất vui khi gặp ông.\NXin chào.
Dialogue: 0,0:20:27.60,0:20:30.00,Default,,0,0,0,,Đi nào Sam, mời Rosie nhảy đi.
Dialogue: 0,0:20:32.70,0:20:36.10,Default,,0,0,0,,- Tôi nghĩ tôi cần thêm bia.\N- Thôi nào, không cần đâu.
Dialogue: 0,0:20:37.30,0:20:39.10,Default,,0,0,0,,Lên đi!
Dialogue: 0,0:20:49.30,0:20:51.00,Default,,0,0,0,,Khi đó ông đành...
Dialogue: 0,0:20:51.30,0:20:55.30,Default,,0,0,0,,phó mặc cho\Nba tên khổng lồ độc ác.
Dialogue: 0,0:20:55.60,0:20:57.50,Default,,0,0,0,,Và chúng cãi nhau...
Dialogue: 0,0:20:57.70,0:21:00.00,Default,,0,0,0,,về việc sẽ nấu bọn ông thế nào.
Dialogue: 0,0:21:00.30,0:21:05.20,Default,,0,0,0,,Chặt ra làm đôi hay ngồi lên bọn ông\Ntừng đứa một, rồi ép thành nước.
Dialogue: 0,0:21:06.40,0:21:09.80,Default,,0,0,0,,Chúng cãi nhau nhiều giờ về\Ncách nấu ông...
Dialogue: 0,0:21:10.10,0:21:13.60,Default,,0,0,0,,đến khi ánh sánh mặt trời đầu tiên\Nvượt qua đỉnh những rặng cây cao...
Dialogue: 0,0:21:14.70,0:21:17.10,Default,,0,0,0,,biến tất cả bọn chúng thành đá.
Dialogue: 0,0:21:22.60,0:21:23.90,Default,,0,0,0,,Nhanh lên!
Dialogue: 0,0:21:28.10,0:21:30.10,Default,,0,0,0,,Chúng tới đây!
Dialogue: 0,0:21:33.00,0:21:34.30,Default,,0,0,0,,Không, không, cái to ấy.
Dialogue: 0,0:21:43.40,0:21:46.20,Default,,0,0,0,,Bà Bracegirdle, rất vui khi gặp bà.\NXin chào.
Dialogue: 0,0:21:46.40,0:21:48.30,Default,,0,0,0,,Tất cả lũ trẻ này của bà à?
Dialogue: 0,0:21:48.50,0:21:51.40,Default,,0,0,0,,Lạy chúa, bà năng suất thật.
Dialogue: 0,0:21:53.50,0:21:54.80,Default,,0,0,0,,Bilbo?
Dialogue: 0,0:21:56.50,0:21:58.20,Default,,0,0,0,,Sackville-Baggins!
Dialogue: 0,0:21:58.50,0:21:59.50,Default,,0,0,0,,Nhanh lên, trốn thôi!
Dialogue: 0,0:22:09.50,0:22:11.60,Default,,0,0,0,,Cám ơn, cháu trai.
Dialogue: 0,0:22:13.20,0:22:15.40,Default,,0,0,0,,Cháu là chàng trai tốt, Frodo.
Dialogue: 0,0:22:16.80,0:22:19.50,Default,,0,0,0,,Ta là người ích kỷ.
Dialogue: 0,0:22:20.00,0:22:23.40,Default,,0,0,0,,Phải, ta rất ích kỷ.
Dialogue: 0,0:22:23.90,0:22:26.00,Default,,0,0,0,,Ta không biết sao ta lại nhận nuôi cháu\Nkhi cha mẹ cháu mất...
Dialogue: 0,0:22:26.30,0:22:27.70,Default,,0,0,0,,nhưng đó không phải\Ntừ sự thương hại.
Dialogue: 0,0:22:28.40,0:22:29.70,Default,,0,0,0,,Ta nghĩ đó là vì...
Dialogue: 0,0:22:29.90,0:22:32.10,Default,,0,0,0,,trong số đông họ hàng của ta...
Dialogue: 0,0:22:32.50,0:22:35.90,Default,,0,0,0,,con thực sự có tinh thần\Ncủa nhà Baggins.
Dialogue: 0,0:22:36.40,0:22:38.10,Default,,0,0,0,,Bilbo, chú đã tới uống rượu của\Nnhà Gaffer phải không?
Dialogue: 0,0:22:38.40,0:22:40.00,Default,,0,0,0,,Không.
Dialogue: 0,0:22:40.30,0:22:42.80,Default,,0,0,0,,À, đúng, nhưng đó không là vấn đề.
Dialogue: 0,0:22:43.20,0:22:46.30,Default,,0,0,0,,Vấn đề là, Frodo...
Dialogue: 0,0:22:49.50,0:22:51.40,Default,,0,0,0,,cháu sẽ không sao.
Dialogue: 0,0:22:54.90,0:22:56.80,Default,,0,0,0,,- Xong!\N- Cậu phải cắm nó xuống đất chứ.
Dialogue: 0,0:22:57.10,0:22:58.60,Default,,0,0,0,,- Nó đã cắm xuống đất rồi.\N- Chưa mà!
Dialogue: 0,0:22:58.80,0:23:00.30,Default,,0,0,0,,Là ý của cậu đó.
Dialogue: 0,0:23:16.20,0:23:17.70,Default,,0,0,0,,Chú Bilbo!
Dialogue: 0,0:23:18.90,0:23:21.50,Default,,0,0,0,,Chú Bilbo, coi chừng con rồng!
Dialogue: 0,0:23:21.80,0:23:25.10,Default,,0,0,0,,Vô lý, không có con rồng nào\Ncả ngàn năm nay rồi.
Dialogue: 0,0:23:38.70,0:23:42.10,Default,,0,0,0,,- Hay quá!\N- Lấy thêm cái nữa nào!
Dialogue: 0,0:23:43.60,0:23:47.90,Default,,0,0,0,,Meriadoc Brandybuck\Nvà Peregrin Took.
Dialogue: 0,0:23:48.30,0:23:50.10,Default,,0,0,0,,Ta phải biết chứ.
Dialogue: 0,0:23:56.10,0:23:58.10,Default,,0,0,0,,Phát biểu đi, Bilbo!
Dialogue: 0,0:23:59.10,0:24:00.40,Default,,0,0,0,,Phát biểu!
Dialogue: 0,0:24:00.60,0:24:01.60,Default,,0,0,0,,Phát biểu!
Dialogue: 0,0:24:06.90,0:24:09.10,Default,,0,0,0,,Các gia đình Bagginses\Nvà Boffins,
Dialogue: 0,0:24:09.90,0:24:12.00,Default,,0,0,0,,Tooks và Brandybucks,
Dialogue: 0,0:24:12.30,0:24:14.70,Default,,0,0,0,,Grubbs, Chubbs,
Dialogue: 0,0:24:15.00,0:24:16.00,Default,,0,0,0,,Hornblowers,
Dialogue: 0,0:24:17.20,0:24:18.80,Default,,0,0,0,,Bolgers,
Dialogue: 0,0:24:19.30,0:24:20.80,Default,,0,0,0,,Bracegirdles...
Dialogue: 0,0:24:21.10,0:24:23.60,Default,,0,0,0,,... và Proudfoots thân mến.\N- Proudfeet chứ!
Dialogue: 0,0:24:25.90,0:24:28.90,Default,,0,0,0,,Hôm nay là sinh nhật\Nthứ 111 của tôi!
Dialogue: 0,0:24:30.80,0:24:32.30,Default,,0,0,0,,Chúc mừng sinh nhật!
Dialogue: 0,0:24:32.50,0:24:36.60,Default,,0,0,0,,Chao ôi, 111 năm vẫn là thời gian\Nquá ngắn để sống...
Dialogue: 0,0:24:37.00,0:24:39.80,Default,,0,0,0,,giữa những người Hobbit tuyệt vời\Nvà đáng ngưỡng mộ này.
Dialogue: 0,0:24:40.90,0:24:43.50,Default,,0,0,0,,Tôi biết là tôi vẫn còn\Nchưa hiểu rõ các bạn...
Dialogue: 0,0:24:43.80,0:24:48.20,Default,,0,0,0,,và tôi yêu thương các bạn\Nvẫn chưa đủ.
Dialogue: 0,0:24:58.40,0:25:00.60,Default,,0,0,0,,Nhưng tôi còn có việc phải làm.
Dialogue: 0,0:25:07.10,0:25:09.10,Default,,0,0,0,,Tôi đã quyết định điều này từ lâu rồi.
Dialogue: 0,0:25:13.10,0:25:16.90,Default,,0,0,0,,Tôi rất tiếc phải thông báo\Nđây là lần cuối cùng.
Dialogue: 0,0:25:18.20,0:25:19.30,Default,,0,0,0,,Tôi đi đây.
Dialogue: 0,0:25:20.50,0:25:23.30,Default,,0,0,0,,Tôi gửi đến các bạn\Nlời chào tạm biệt thân ái.
Dialogue: 0,0:25:25.70,0:25:26.90,Default,,0,0,0,,Tạm biệt.
Dialogue: 0,0:26:00.20,0:26:03.20,Default,,0,0,0,,Ông nghĩ như vậy là hay lắm sao?
Dialogue: 0,0:26:03.60,0:26:06.80,Default,,0,0,0,,Thôi nào, Gandalf.\NÔng nhìn thấy mặt họ không?
Dialogue: 0,0:26:07.10,0:26:09.90,Default,,0,0,0,,Có nhiều chiếc Nhẫn ma thuật\Ntrên đời này, Bilbo Baggins ạ,
Dialogue: 0,0:26:10.00,0:26:12.20,Default,,0,0,0,,và không nên sử dụng chúng bừa bãi.
Dialogue: 0,0:26:12.40,0:26:14.40,Default,,0,0,0,,Tôi chỉ đùa chút thôi mà.
Dialogue: 0,0:26:14.70,0:26:18.30,Default,,0,0,0,,Ôi, được rồi. Có lẽ ông đúng,\Nnhư mọi khi.
Dialogue: 0,0:26:19.80,0:26:23.20,Default,,0,0,0,,- Ông sẽ trông nom Frodo chứ?\N- Còn hơn cả trông nom.
Dialogue: 0,0:26:23.60,0:26:26.10,Default,,0,0,0,,Mỗi khi mà tôi rảnh rỗi.
Dialogue: 0,0:26:26.40,0:26:29.50,Default,,0,0,0,,- Tôi để tất cả lại cho nó.\N- Vậy chiếc Nhẫn của ông thì sao?
Dialogue: 0,0:26:29.80,0:26:33.00,Default,,0,0,0,,- Ông để lại luôn chứ?\N- Vâng, vâng.
Dialogue: 0,0:26:33.80,0:26:37.50,Default,,0,0,0,,Nó ở trong phong thư đằng kia\Ntrên bệ lò sưởi ấy.
Dialogue: 0,0:26:38.70,0:26:41.60,Default,,0,0,0,,Khoan, chờ đã, đây...
Dialogue: 0,0:26:44.20,0:26:46.80,Default,,0,0,0,,nó ở trong túi tôi.
Dialogue: 0,0:26:51.40,0:26:54.60,Default,,0,0,0,,Không phải nó hơi lạ sao?
Dialogue: 0,0:26:56.40,0:26:59.80,Default,,0,0,0,,Sau cùng, tại sao lại không chứ?
Dialogue: 0,0:27:01.40,0:27:06.10,Default,,0,0,0,,- Sao tôi không giữ nó nhỉ?\N- Tôi nghĩ ông nên để chiếc Nhẫn lại.
Dialogue: 0,0:27:06.70,0:27:08.80,Default,,0,0,0,,Việc đó khó thế sao?
Dialogue: 0,0:27:09.10,0:27:10.40,Default,,0,0,0,,À, không.
Dialogue: 0,0:27:12.90,0:27:14.30,Default,,0,0,0,,À, có chứ.
Dialogue: 0,0:27:16.30,0:27:19.30,Default,,0,0,0,,Giờ đã đến lúc,\Ntôi không muốn bỏ nó lại.
Dialogue: 0,0:27:19.70,0:27:21.40,Default,,0,0,0,,Nó là của tôi! Tôi đã tìm thấy nó!\NNó đến với tôi!
Dialogue: 0,0:27:21.70,0:27:25.70,Default,,0,0,0,,- Không cần tức giận như vậy đâu.\N- À, đó là lỗi của ông đấy.
Dialogue: 0,0:27:26.20,0:27:27.90,Default,,0,0,0,,Nó là của tôi.
Dialogue: 0,0:27:29.40,0:27:31.80,Default,,0,0,0,,Của tôi.
Dialogue: 0,0:27:32.20,0:27:36.00,Default,,0,0,0,,- Nhẫn quý của tôi.\N- Nhẫn quý?
Dialogue: 0,0:27:36.40,0:27:39.10,Default,,0,0,0,,Trước đây đã có người gọi như thế,\Nnhưng không phải ông.
Dialogue: 0,0:27:39.50,0:27:41.90,Default,,0,0,0,,Ông quan tâm gì đến việc\Ntôi làm với đồ của mình chứ?
Dialogue: 0,0:27:42.20,0:27:44.90,Default,,0,0,0,,Tôi nghĩ ông đã có nó khá lâu rồi.
Dialogue: 0,0:27:45.20,0:27:47.90,Default,,0,0,0,,- Ông muốn nó cho riêng mình!\N- Bilbo Baggins!
Dialogue: 0,0:27:48.20,0:27:52.30,Default,,0,0,0,,Đừng nghĩ tôi như những bọn ảo thuật\Nvới mánh mung rẻ tiền.
Dialogue: 0,0:27:52.70,0:27:56.20,Default,,0,0,0,,Tôi không cướp đoạt nó đâu.
Dialogue: 0,0:28:01.40,0:28:03.00,Default,,0,0,0,,Tôi đang cố giúp ông.
Dialogue: 0,0:28:11.50,0:28:14.70,Default,,0,0,0,,Ta đã là bạn nhiều năm rồi.
Dialogue: 0,0:28:15.20,0:28:17.30,Default,,0,0,0,,Hãy tin tôi như ông đã tin.
Dialogue: 0,0:28:19.30,0:28:21.00,Default,,0,0,0,,Hãy bỏ nó đi.
Dialogue: 0,0:28:22.20,0:28:24.50,Default,,0,0,0,,Ông đúng, Gandalf.
Dialogue: 0,0:28:26.70,0:28:29.30,Default,,0,0,0,,Chiếc Nhẫn phải theo Frodo.
Dialogue: 0,0:28:30.80,0:28:33.70,Default,,0,0,0,,Muộn rồi, đường còn xa lắm.
Dialogue: 0,0:28:34.00,0:28:36.40,Default,,0,0,0,,Vâng, tới lúc rồi.
Dialogue: 0,0:28:39.20,0:28:44.00,Default,,0,0,0,,Bilbo, chiếc Nhẫn vẫn còn\Ntrong túi của ông.
Dialogue: 0,0:29:19.00,0:29:21.90,Default,,0,0,0,,Tôi vừa nghĩ ra đoạn kết\Ncho cuốn sách của mình:
Dialogue: 0,0:29:23.60,0:29:28.40,Default,,0,0,0,,{\i1}"Và anh ta sống vui vẻ\Ncho đến cuối đời."{\i0}
Dialogue: 0,0:29:28.90,0:29:32.20,Default,,0,0,0,,Tôi chắc ông cũng vậy,\Nông bạn của tôi.
Dialogue: 0,0:29:33.30,0:29:34.70,Default,,0,0,0,,Tạm biệt, Gandalf.
Dialogue: 0,0:29:37.30,0:29:40.00,Default,,0,0,0,,Tạm biệt, ông bạn Bilbo.
Dialogue: 0,0:29:47.90,0:29:52.00,Default,,0,0,0,,{\i1}Con đường vẫn tiếp tục trải dài...{\i0}
Dialogue: 0,0:29:54.70,0:29:57.10,Default,,0,0,0,,đến khi chúng ta gặp lại.
Dialogue: 0,0:30:24.50,0:30:28.00,Default,,0,0,0,,{\i1}Là của ta, của ta.{\i0}
Dialogue: 0,0:30:28.30,0:30:31.70,Default,,0,0,0,,{\i1}Nhẫn quý của ta.{\i0}
Dialogue: 0,0:30:32.00,0:30:34.80,Default,,0,0,0,,Câu đố trong bóng tối.
Dialogue: 0,0:30:35.20,0:30:36.60,Default,,0,0,0,,Bilbo!
Dialogue: 0,0:30:36.80,0:30:38.20,Default,,0,0,0,,Bilbo!
Dialogue: 0,0:30:42.00,0:30:44.90,Default,,0,0,0,,Nhẫn quý của ta.
Dialogue: 0,0:30:46.50,0:30:48.60,Default,,0,0,0,,Nhẫn quý.
Dialogue: 0,0:30:48.90,0:30:51.30,Default,,0,0,0,,Chú Bilbo đi rồi phải không?
Dialogue: 0,0:30:54.30,0:30:57.20,Default,,0,0,0,,Chú ấy từ lâu đã nói về việc ra đi.
Dialogue: 0,0:30:57.50,0:31:00.10,Default,,0,0,0,,Cháu không nghĩ là\Nchú ấy sẽ làm thế thật.
Dialogue: 0,0:31:04.70,0:31:06.60,Default,,0,0,0,,Gandalf?
Dialogue: 0,0:31:14.90,0:31:16.50,Default,,0,0,0,,Nhẫn của Bilbo.
Dialogue: 0,0:31:16.90,0:31:19.20,Default,,0,0,0,,Ông ấy đã đi đến chỗ tộc Tiên.
Dialogue: 0,0:31:19.50,0:31:22.40,Default,,0,0,0,,Ông ấy để lại Bag End cho cháu.
Dialogue: 0,0:31:26.20,0:31:28.70,Default,,0,0,0,,Cùng với tất cả tài sản.
Dialogue: 0,0:31:28.90,0:31:31.70,Default,,0,0,0,,Chiếc Nhẫn giờ là của cháu.
Dialogue: 0,0:31:34.40,0:31:37.80,Default,,0,0,0,,- Hãy giấu nó ở nơi nào không ai thấy.\N- Ông sẽ đi đâu?
Dialogue: 0,0:31:38.00,0:31:40.20,Default,,0,0,0,,- Có vài thứ ta cần phải xem.\N- Là cái gì?
Dialogue: 0,0:31:40.50,0:31:44.60,Default,,0,0,0,,- Lại câu hỏi. Câu hỏi luôn cần câu trả lời.\N- Ông chỉ vừa tới thôi mà.
Dialogue: 0,0:31:45.70,0:31:48.20,Default,,0,0,0,,Cháu không hiểu.
Dialogue: 0,0:31:51.60,0:31:52.60,Default,,0,0,0,,Ta cũng thế.
Dialogue: 0,0:31:55.90,0:31:58.20,Default,,0,0,0,,Giữ bí mật nhé.
Dialogue: 0,0:31:58.40,0:32:00.20,Default,,0,0,0,,Hãy giữ nó an toàn.
Dialogue: 0,0:32:35.20,0:32:37.30,Default,,0,0,0,,{\i1}Ở Shire!{\i0}
Dialogue: 0,0:32:37.70,0:32:40.70,Default,,0,0,0,,{\i1}Nhà Baggins!{\i0}
Dialogue: 0,0:33:29.30,0:33:32.10,Default,,0,0,0,,{\i1}Năm 3.434 của của thời đại thứ hai.{\i0}
Dialogue: 0,0:33:32.40,0:33:36.60,Default,,0,0,0,,{\i1}Dựa theo ghi chép của Isildur,\Nđức vua cao quý của Gondor...{\i0}
Dialogue: 0,0:33:37.00,0:33:40.60,Default,,0,0,0,,{\i1}và cuộc tìm kiếm\Nchiếc Nhẫn quyền năng.{\i0}
Dialogue: 0,0:33:44.20,0:33:46.00,Default,,0,0,0,,{\i1}Nó đã đến với ta.{\i0}
Dialogue: 0,0:33:46.30,0:33:49.60,Default,,0,0,0,,{\i1}Chiếc Nhẫn chúa. Nó sẽ là vật\Ngia truyền của vương quốc ta.{\i0}
Dialogue: 0,0:33:49.90,0:33:53.20,Default,,0,0,0,,{\i1}Và tất cả ai thuộc dòng dõi ta\Nsẽ bị trói buộc với định mệnh của nó...{\i0}
Dialogue: 0,0:33:53.60,0:33:57.20,Default,,0,0,0,,{\i1}vì ta sẽ không mạo hiểm\Nlàm tổn thương chiếc Nhẫn.{\i0}
Dialogue: 0,0:33:57.60,0:34:00.00,Default,,0,0,0,,{\i1}Nó thật quý giá với ta...{\i0}
Dialogue: 0,0:34:01.00,0:34:03.50,Default,,0,0,0,,{\i1}dù ta có nó với một giá rất đắt.{\i0}
Dialogue: 0,0:34:06.70,0:34:10.70,Default,,0,0,0,,{\i1}Dấu hiệu trên chiếc Nhẫn\Ndần phai mờ.{\i0}
Dialogue: 0,0:34:11.20,0:34:15.70,Default,,0,0,0,,{\i1}Những dòng chữ, lúc đầu còn\Nsáng như lửa, giờ đã biến mất.{\i0}
Dialogue: 0,0:34:16.20,0:34:20.00,Default,,0,0,0,,{\i1}Một bí mật giờ chỉ có lửa mới biết.{\i0}
Dialogue: 0,0:34:36.80,0:34:39.70,Default,,0,0,0,,Shire.
Dialogue: 0,0:34:39.90,0:34:41.80,Default,,0,0,0,,Baggins.
Dialogue: 0,0:34:42.40,0:34:46.30,Default,,0,0,0,,Không có gia đình Baggins nào ở đây,\Nhọ đều ở trên vùng Hobbiton ấy.
Dialogue: 0,0:34:48.10,0:34:49.50,Default,,0,0,0,,Lối này.
Dialogue: 0,0:34:53.30,0:34:55.40,Default,,0,0,0,,{\i1}Nâng ly nào,{\i0}
Dialogue: 0,0:34:55.60,0:34:57.80,Default,,0,0,0,,{\i1}để chữa lành trái tim ta,\Nvà vùi tắt nỗi đau buồn.{\i0}
Dialogue: 0,0:34:58.00,0:35:00.30,Default,,0,0,0,,{\i1}Mưa có thể rơi, gió có thể thổi.{\i0}
Dialogue: 0,0:35:00.50,0:35:02.40,Default,,0,0,0,,{\i1}Nhưng vẫn còn...{\i0}
Dialogue: 0,0:35:02.70,0:35:04.00,Default,,0,0,0,,{\i1}nhiều dặm đường phải đi.{\i0}
Dialogue: 0,0:35:04.30,0:35:06.20,Default,,0,0,0,,{\i1}Vị ngọt là âm thanh của cơn mưa...{\i0}
Dialogue: 0,0:35:06.50,0:35:08.50,Default,,0,0,0,,{\i1}và dòng suối chảy từ đồi\Nxuống đồng bằng.{\i0}
Dialogue: 0,0:35:08.90,0:35:10.60,Default,,0,0,0,,{\i1}Tốt hơn mưa hay dòng suối lăn tăn...{\i0}
Dialogue: 0,0:35:10.90,0:35:13.20,Default,,0,0,0,,{\i1}là một vại bia tại nhà Took.{\i0}
Dialogue: 0,0:35:16.70,0:35:19.40,Default,,0,0,0,,Có vài câu chuyện cổ xưa\Nlạ lùng khắp vùng Shire này.
Dialogue: 0,0:35:19.90,0:35:23.60,Default,,0,0,0,,Người Dwarf và những loài khác\Ncòn ít hơn cả rau húng.
Dialogue: 0,0:35:24.30,0:35:25.70,Default,,0,0,0,,Chiến tranh như bia rượu.
Dialogue: 0,0:35:26.50,0:35:29.10,Default,,0,0,0,,Những ngọn núi thì\Nlúc nhúc đám Goblin.
Dialogue: 0,0:35:29.40,0:35:32.10,Default,,0,0,0,,Chấm dứt chuyện con nít này đi,\Nđủ rồi.
Dialogue: 0,0:35:32.20,0:35:36.70,Default,,0,0,0,,Nghe như lão già Bilbo Baggins ấy.\NLão ấy lẩn thẩn rồi.
Dialogue: 0,0:35:36.90,0:35:39.70,Default,,0,0,0,,Cậu Frodo trẻ đây,\Ncũng đang lẩn thẩn dần.
Dialogue: 0,0:35:40.10,0:35:41.50,Default,,0,0,0,,Và tôi tự hào về điều đó.
Dialogue: 0,0:35:41.60,0:35:44.10,Default,,0,0,0,,- Cụng ly nào, Gaffer.\N- Nâng ly.
Dialogue: 0,0:35:44.20,0:35:48.10,Default,,0,0,0,,Chúng ta không can hệ gì\Nđến những việc xảy ra bên ngoài.
Dialogue: 0,0:35:48.50,0:35:52.50,Default,,0,0,0,,Đừng nhúng mũi vào rắc rối,\Nnó sẽ không tìm đến bạn.
Dialogue: 0,0:35:56.10,0:35:58.20,Default,,0,0,0,,- Ngủ ngon nhé các anh.\N- Ngủ ngon.
Dialogue: 0,0:35:58.40,0:36:02.80,Default,,0,0,0,,Ngủ ngon, cô gái của bia vàng.
Dialogue: 0,0:36:03.10,0:36:05.40,Default,,0,0,0,,Hãy nhớ là cậu đang nói ngọt\Nvới ai đấy nhé.
Dialogue: 0,0:36:05.70,0:36:10.00,Default,,0,0,0,,Đừng lo Sam, Rosie biết\Nai là thằng ngốc mà.
Dialogue: 0,0:36:11.20,0:36:12.30,Default,,0,0,0,,Thế sao?
Dialogue: 0,0:36:13.00,0:36:15.90,Default,,0,0,0,,- Chúc ngủ ngon Sam.\N- Ngủ ngon, cậu Frodo.
Dialogue: 0,0:36:40.80,0:36:43.20,Default,,0,0,0,,Chiếc Nhẫn còn bí mật không?\NCòn an toàn không?
Dialogue: 0,0:36:52.20,0:36:54.20,Default,,0,0,0,,Ông làm gì thế?
Dialogue: 0,0:37:06.90,0:37:09.40,Default,,0,0,0,,Đưa tay đây, Frodo.
Dialogue: 0,0:37:09.70,0:37:10.80,Default,,0,0,0,,Nó nguội rồi đấy.
Dialogue: 0,0:37:14.30,0:37:16.00,Default,,0,0,0,,Cậu thấy gì?
Dialogue: 0,0:37:17.90,0:37:20.20,Default,,0,0,0,,Có thấy gì không?
Dialogue: 0,0:37:23.10,0:37:24.40,Default,,0,0,0,,Không.
Dialogue: 0,0:37:25.00,0:37:26.90,Default,,0,0,0,,Chẳng có gì cả.
Dialogue: 0,0:37:28.20,0:37:29.60,Default,,0,0,0,,Chờ đã.
Dialogue: 0,0:37:33.70,0:37:35.90,Default,,0,0,0,,Có vết chữ.
Dialogue: 0,0:37:37.50,0:37:40.80,Default,,0,0,0,,Một dạng chữ của tộc Tiên.\NCháu không đọc được.
Dialogue: 0,0:37:41.10,0:37:43.50,Default,,0,0,0,,Có ít người đọc được lắm.
Dialogue: 0,0:37:44.20,0:37:48.80,Default,,0,0,0,,Đây là ngôn ngữ Mordor,\Nvà ta không thể nói ở đây được.
Dialogue: 0,0:37:49.70,0:37:50.70,Default,,0,0,0,,Mordor?
Dialogue: 0,0:37:51.00,0:37:52.90,Default,,0,0,0,,Dòng chữ viết rằng:
Dialogue: 0,0:37:53.20,0:37:56.60,Default,,0,0,0,,{\i1}Một chiếc Nhẫn thống trị tất cả.{\i0}
Dialogue: 0,0:37:57.00,0:37:59.20,Default,,0,0,0,,{\i1}Một chiếc Nhẫn sẽ tìm đến họ.{\i0}
Dialogue: 0,0:37:59.50,0:38:01.60,Default,,0,0,0,,{\i1}Một chiếc Nhẫn mang cho họ tất cả...{\i0}
Dialogue: 0,0:38:02.00,0:38:04.90,Default,,0,0,0,,{\i1}và cũng giam cầm họ\Ntrong bóng tối.{\i0}
Dialogue: 0,0:38:06.70,0:38:08.60,Default,,0,0,0,,Đây là chiếc Nhẫn Chúa...
Dialogue: 0,0:38:08.80,0:38:13.90,Default,,0,0,0,,được rèn bởi Chúa tể Sauron\Ntrong lửa của ngọn Diệt Vong.
Dialogue: 0,0:38:14.40,0:38:18.20,Default,,0,0,0,,Isildur đã lấy nó từ chính tay Sauron.
Dialogue: 0,0:38:19.20,0:38:21.60,Default,,0,0,0,,Bilbo đã tìm thấy nó.
Dialogue: 0,0:38:22.10,0:38:24.00,Default,,0,0,0,,- Trong hang động Gollum.\N- Đúng vậy.
Dialogue: 0,0:38:24.30,0:38:28.71,Default,,0,0,0,,Hơn 60 năm nay, chiếc Nhẫn nằm yên\Ntrong tay của Bilbo...
Dialogue: 0,0:38:29.11,0:38:32.31,Default,,0,0,0,,kéo dài tuổi thọ và\Nlàm giảm sự già nua của ông ấy.
Dialogue: 0,0:38:33.11,0:38:37.31,Default,,0,0,0,,Nhưng hết rồi, Frodo.\NQuỷ dữ đang nổi lên tại Mordor.
Dialogue: 0,0:38:37.71,0:38:40.01,Default,,0,0,0,,Chiếc Nhẫn đã thức dậy.
Dialogue: 0,0:38:40.31,0:38:42.91,Default,,0,0,0,,Nó đã nghe được tiếng gọi\Ncủa chủ nhân nó.
Dialogue: 0,0:38:43.21,0:38:46.91,Default,,0,0,0,,Nhưng hắn đã bị tiêu diệt mà!\NSauron đã chết.
Dialogue: 0,0:38:52.81,0:38:54.81,Default,,0,0,0,,Không, Frodo.
Dialogue: 0,0:38:55.91,0:38:59.11,Default,,0,0,0,,Linh hồn của hắn vẫn tồn tại.
Dialogue: 0,0:38:59.41,0:39:04.51,Default,,0,0,0,,Sự sống của hắn gắn chặt với chiếc Nhẫn,\Nvà chiếc Nhẫn sống sót.
Dialogue: 0,0:39:05.01,0:39:07.11,Default,,0,0,0,,Sauron đã trở lại.
Dialogue: 0,0:39:08.21,0:39:10.11,Default,,0,0,0,,Đội quân Orc của hắn\Nđang lớn dần lên.
Dialogue: 0,0:39:10.51,0:39:14.31,Default,,0,0,0,,Pháo đài của hắn tại Baraddur\Nđược xây dựng lại trên đất Mordor.
Dialogue: 0,0:39:14.61,0:39:19.21,Default,,0,0,0,,Chỉ cần chiếc Nhẫn đó và Sauron sẽ\Nbao trùm bóng tối lên toàn bộ nơi này.
Dialogue: 0,0:39:19.61,0:39:21.61,Default,,0,0,0,,Hắn đang tìm nó.
Dialogue: 0,0:39:21.91,0:39:25.51,Default,,0,0,0,,Tìm kiếm nó.\NMọi suy nghĩ của hắn đều về nó.
Dialogue: 0,0:39:26.01,0:39:28.61,Default,,0,0,0,,Còn chiếc Nhẫn đang mong muốn...
Dialogue: 0,0:39:28.91,0:39:32.21,Default,,0,0,0,,quay về với chủ nhân của nó.
Dialogue: 0,0:39:32.61,0:39:34.31,Default,,0,0,0,,Chúng là một,
Dialogue: 0,0:39:34.41,0:39:37.41,Default,,0,0,0,,chiếc Nhẫn và Chúa tể bóng tối.
Dialogue: 0,0:39:37.81,0:39:38.81,Default,,0,0,0,,Frodo...
Dialogue: 0,0:39:39.31,0:39:41.11,Default,,0,0,0,,hắn không được tìm thấy nó.
Dialogue: 0,0:39:41.41,0:39:42.41,Default,,0,0,0,,Được rồi.
Dialogue: 0,0:39:42.81,0:39:47.01,Default,,0,0,0,,Hãy giữ nó thật xa, giấu nó,\Nkhông nói gì về nó.
Dialogue: 0,0:39:47.41,0:39:49.91,Default,,0,0,0,,Không ai biết được\Nnó ở đây, phải không?
Dialogue: 0,0:39:55.71,0:39:56.81,Default,,0,0,0,,Phải không, Gandalf?
Dialogue: 0,0:39:57.91,0:40:01.21,Default,,0,0,0,,Có một kẻ biết Bilbo có chiếc Nhẫn.
Dialogue: 0,0:40:02.61,0:40:05.41,Default,,0,0,0,,Ta đã tìm sinh vật tên Gollum\Nkhắp nơi.
Dialogue: 0,0:40:05.81,0:40:07.81,Default,,0,0,0,,Nhưng kẻ thù đã tìm ra hắn trước.
Dialogue: 0,0:40:09.71,0:40:12.11,Default,,0,0,0,,Ta không biết họ tra tấn hắn\Ntrong bao lâu.
Dialogue: 0,0:40:12.41,0:40:16.71,Default,,0,0,0,,Nhưng trong những tiếng la thét\Nvà bập bẹ vô nghĩa, chúng nghe được 2 từ:
Dialogue: 0,0:40:17.01,0:40:19.11,Default,,0,0,0,,Shire!
Dialogue: 0,0:40:19.41,0:40:20.91,Default,,0,0,0,,Baggins!
Dialogue: 0,0:40:21.21,0:40:23.01,Default,,0,0,0,,Shire.
Dialogue: 0,0:40:23.21,0:40:25.91,Default,,0,0,0,,Baggins.\NHai từ đó sẽ dẫn chúng tới đây!
Dialogue: 0,0:40:28.91,0:40:29.91,Default,,0,0,0,,Ai đấy?
Dialogue: 0,0:40:31.71,0:40:32.91,Default,,0,0,0,,Giữ nó đi, Gandalf!
Dialogue: 0,0:40:33.41,0:40:34.41,Default,,0,0,0,,- Giữ nó!\N- Không, Frodo.
Dialogue: 0,0:40:34.51,0:40:36.81,Default,,0,0,0,,- Ông phải giữ nó!\N- Cậu không thể đưa ta chiếc Nhẫn này!
Dialogue: 0,0:40:37.11,0:40:40.31,Default,,0,0,0,,- Cháu trả nó cho ông đấy.\N- Đừng cám dỗ ta, Frodo!
Dialogue: 0,0:40:42.01,0:40:44.11,Default,,0,0,0,,Ta không dám giữ nó.
Dialogue: 0,0:40:44.51,0:40:46.91,Default,,0,0,0,,Thậm chí không thể giữ nó an toàn.
Dialogue: 0,0:40:48.11,0:40:50.11,Default,,0,0,0,,Frodo, cậu phải hiểu rằng...
Dialogue: 0,0:40:50.81,0:40:54.31,Default,,0,0,0,,ta sẽ dùng nó với mục đích tốt.
Dialogue: 0,0:40:59.81,0:41:05.31,Default,,0,0,0,,Nhưng thông qua ta, nó sẽ có sức mạnh to lớn,\Nkhủng khiếp không thể tưởng tượng được.
Dialogue: 0,0:41:06.21,0:41:09.01,Default,,0,0,0,,- Nhưng nó không thể ở lại vùng Shire!\N- Đúng.
Dialogue: 0,0:41:11.01,0:41:12.51,Default,,0,0,0,,Nó không thể ở đây.
Dialogue: 0,0:41:17.91,0:41:19.31,Default,,0,0,0,,Cháu phải làm gì đây?
Dialogue: 0,0:41:21.01,0:41:24.31,Default,,0,0,0,,- Cậu phải rời khỏi đây thật nhanh.\N- Đi đâu? Cháu phải đi đâu?
Dialogue: 0,0:41:24.81,0:41:27.91,Default,,0,0,0,,Ra khỏi Shire, tới làng Bree.
Dialogue: 0,0:41:28.31,0:41:29.31,Default,,0,0,0,,Bree.
Dialogue: 0,0:41:29.61,0:41:32.21,Default,,0,0,0,,- Còn ông?\N- Ta sẽ đợi cháu...
Dialogue: 0,0:41:32.51,0:41:34.91,Default,,0,0,0,,tại quán trọ Prancing Pony.
Dialogue: 0,0:41:35.21,0:41:38.81,Default,,0,0,0,,- Chiếc Nhẫn sẽ an toàn ở đó chứ?\N- Ta không biết, Frodo.
Dialogue: 0,0:41:39.11,0:41:41.01,Default,,0,0,0,,Ta không có câu trả lời.
Dialogue: 0,0:41:42.61,0:41:47.91,Default,,0,0,0,,Ta phải gặp thủ lĩnh của ta, ông ấy\Nrất thông thái và mạnh mẽ. Tin ta đi Frodo.
Dialogue: 0,0:41:48.31,0:41:50.41,Default,,0,0,0,,Ông ấy biết phải làm gì.
Dialogue: 0,0:41:51.11,0:41:53.81,Default,,0,0,0,,Cháu nên bỏ họ Baggins đi.
Dialogue: 0,0:41:54.21,0:41:56.61,Default,,0,0,0,,Cái tên đó không an toàn\Nở ngoài vùng Shire.
Dialogue: 0,0:41:57.31,0:42:00.51,Default,,0,0,0,,Chỉ đi ban ngày,\Nvà tránh xa đường cái.
Dialogue: 0,0:42:01.31,0:42:04.41,Default,,0,0,0,,Cháu có thể đi qua vùng này\Nmột cách dễ dàng.
Dialogue: 0,0:42:07.01,0:42:08.31,Default,,0,0,0,,Ôi Frodo.
Dialogue: 0,0:42:08.61,0:42:11.61,Default,,0,0,0,,Người Hobbit thật sự\Nlà những sinh vật bất ngờ.
Dialogue: 0,0:42:11.81,0:42:14.61,Default,,0,0,0,,Bạn nghĩ có thể hiểu hết họ\Nchỉ trong một tháng.
Dialogue: 0,0:42:14.91,0:42:17.21,Default,,0,0,0,,Và sau 100 năm...
Dialogue: 0,0:42:17.51,0:42:19.41,Default,,0,0,0,,họ vẫn còn làm bạn bất ngờ.
Dialogue: 0,0:42:21.21,0:42:22.51,Default,,0,0,0,,Nằm xuống!
Dialogue: 0,0:42:34.71,0:42:38.01,Default,,0,0,0,,Quỷ tha ma bắt, Samwise Gamgee!\NCậu đang nghe lén à?
Dialogue: 0,0:42:38.41,0:42:39.81,Default,,0,0,0,,Tôi đâu nghe lén gì đâu, thật đấy.
Dialogue: 0,0:42:40.11,0:42:42.71,Default,,0,0,0,,Chỉ đang cắt cỏ\Ndưới cửa sổ ngoài kia thôi.
Dialogue: 0,0:42:43.11,0:42:45.41,Default,,0,0,0,,Hình như hơi muộn\Nđể cắt tỉa cỏ thì phải?
Dialogue: 0,0:42:45.71,0:42:48.51,Default,,0,0,0,,- Tôi nghe có người cao giọng.\N- Cậu nghe được gì? Nói mau!
Dialogue: 0,0:42:48.71,0:42:49.81,Default,,0,0,0,,Không có gì quan trọng cả.
Dialogue: 0,0:42:50.41,0:42:52.81,Default,,0,0,0,,Chỉ là câu chuyện về chiếc Nhẫn,\Nchúa tể bóng đêm,
Dialogue: 0,0:42:52.91,0:42:54.51,Default,,0,0,0,,rồi ngày tận thế, nhưng...
Dialogue: 0,0:42:54.61,0:42:59.21,Default,,0,0,0,,Ngài Gandalf, xin đừng đánh tôi,\Nđừng biến tôi thành thứ gì kì cục.
Dialogue: 0,0:43:00.21,0:43:01.71,Default,,0,0,0,,Không à?
Dialogue: 0,0:43:03.01,0:43:04.11,Default,,0,0,0,,Có lẽ không.
Dialogue: 0,0:43:05.41,0:43:08.71,Default,,0,0,0,,Ta sẽ dùng cậu\Nvào việc khác có ích hơn.
Dialogue: 0,0:43:10.51,0:43:13.41,Default,,0,0,0,,Đi nào, Samwise.\NTiếp tục đi.
Dialogue: 0,0:43:15.11,0:43:17.21,Default,,0,0,0,,Hai cậu phải cẩn thận đó.
Dialogue: 0,0:43:17.61,0:43:20.21,Default,,0,0,0,,Kẻ thù có rất nhiều tai mắt.
Dialogue: 0,0:43:20.51,0:43:23.31,Default,,0,0,0,,Loài chim, lũ thú.
Dialogue: 0,0:43:25.21,0:43:27.01,Default,,0,0,0,,Nó có an toàn không?
Dialogue: 0,0:43:28.71,0:43:30.01,Default,,0,0,0,,Đừng bao giờ đeo Nhẫn...
Dialogue: 0,0:43:30.31,0:43:33.61,Default,,0,0,0,,vì kẻ thù sẽ bị thu hút\Nbởi sức mạnh của nó.
Dialogue: 0,0:43:33.81,0:43:35.31,Default,,0,0,0,,Hãy luôn nhớ, Frodo...
Dialogue: 0,0:43:35.91,0:43:38.91,Default,,0,0,0,,chiếc Nhẫn đang cố quay về\Nvới chủ nhân của nó.
Dialogue: 0,0:43:39.51,0:43:41.51,Default,,0,0,0,,Nó muốn được tìm thấy.
Dialogue: 0,0:44:22.11,0:44:24.21,Default,,0,0,0,,Đây rồi.
Dialogue: 0,0:44:24.71,0:44:26.71,Default,,0,0,0,,Sao cơ?
Dialogue: 0,0:44:27.11,0:44:29.11,Default,,0,0,0,,Nếu tôi bước thêm một bước nữa...
Dialogue: 0,0:44:29.71,0:44:33.41,Default,,0,0,0,,đây sẽ là lần đầu tiên\Ntôi rời xa nhà đến vậy.
Dialogue: 0,0:44:37.91,0:44:39.31,Default,,0,0,0,,Thôi nào, Sam.
Dialogue: 0,0:44:47.81,0:44:49.61,Default,,0,0,0,,Còn nhớ chú Bilbo đã nói gì không:
Dialogue: 0,0:44:50.21,0:44:53.81,Default,,0,0,0,,{\i1}- Đó là một việc làm nguy hiểm, Frodo.\N- Bước ra khỏi cửa nhà.{\i0}
Dialogue: 0,0:44:54.11,0:44:58.41,Default,,0,0,0,,{\i1}Khi bước đi trên đường,\Nnếu không để ý dưới chân...{\i0}
Dialogue: 0,0:44:58.71,0:45:02.71,Default,,0,0,0,,{\i1}cháu sẽ không biết mình\Ncó thể bị rơi vào chỗ nào đâu.{\i0}
Dialogue: 0,0:45:16.81,0:45:18.51,Default,,0,0,0,,Sam!
Dialogue: 0,0:45:22.11,0:45:23.21,Default,,0,0,0,,Tiên rừng!
Dialogue: 0,0:45:36.91,0:45:40.31,Default,,0,0,0,,Họ đang đi tới bến cảng\Nphía bên kia Tháp Trắng.
Dialogue: 0,0:45:41.01,0:45:42.61,Default,,0,0,0,,Tới Cảng Xám.
Dialogue: 0,0:45:43.31,0:45:45.31,Default,,0,0,0,,Họ đang rời khỏi Trung Địa.
Dialogue: 0,0:45:45.51,0:45:47.61,Default,,0,0,0,,Không bao giờ trở lại.
Dialogue: 0,0:45:50.61,0:45:52.61,Default,,0,0,0,,Tôi không biết vì sao...
Dialogue: 0,0:45:53.51,0:45:55.81,Default,,0,0,0,,nhưng chuyện đó làm tôi buồn.
Dialogue: 0,0:45:57.91,0:46:02.31,Default,,0,0,0,,Nơi nào tôi nằm cũng bị\Nmấy cái rễ dơ dáy đâm vào lưng.
Dialogue: 0,0:46:03.31,0:46:05.01,Default,,0,0,0,,Nhắm mắt lại...
Dialogue: 0,0:46:05.21,0:46:07.81,Default,,0,0,0,,và tưởng tượng như\Nđang ngủ trên giường vậy...
Dialogue: 0,0:46:08.21,0:46:12.01,Default,,0,0,0,,với tấm nệm mềm mại\Nvà cái gối lông vũ đáng yêu.
Dialogue: 0,0:46:21.41,0:46:24.01,Default,,0,0,0,,Không được rồi, cậu Frodo.
Dialogue: 0,0:46:24.21,0:46:27.01,Default,,0,0,0,,Tôi không thể ngủ\Nở ngoài đây được.
Dialogue: 0,0:46:29.41,0:46:31.71,Default,,0,0,0,,Tôi cũng vậy, Sam.
Dialogue: 0,0:47:00.31,0:47:03.31,Default,,0,0,0,,{\i1}Khói bốc lên từ ngọn Diệt Vong.{\i0}
Dialogue: 0,0:47:03.61,0:47:05.21,Default,,0,0,0,,{\i1}Thời gian ngày càng trễ.{\i0}
Dialogue: 0,0:47:05.41,0:47:08.81,Default,,0,0,0,,{\i1}Và Gandalf Áo Xám\Nphi ngựa tiến về Isengard...{\i0}
Dialogue: 0,0:47:09.31,0:47:11.61,Default,,0,0,0,,{\i1}để kiếm tìm lời khuyên của ta.{\i0}
Dialogue: 0,0:47:11.91,0:47:15.71,Default,,0,0,0,,Có phải đó là lý do\Nông tới đây không?
Dialogue: 0,0:47:16.01,0:47:17.21,Default,,0,0,0,,Ông bạn già.
Dialogue: 0,0:47:17.61,0:47:19.11,Default,,0,0,0,,Saruman.
Dialogue: 0,0:47:24.01,0:47:26.91,Default,,0,0,0,,- Ông chắc chứ?\N- Không nghi ngờ gì.
Dialogue: 0,0:47:27.81,0:47:30.01,Default,,0,0,0,,Vậy chiếc Nhẫn quyền năng\Nđã được tìm thấy.
Dialogue: 0,0:47:30.31,0:47:33.31,Default,,0,0,0,,Từ nhiều năm rồi,\Nnó đã ở vùng Shire.
Dialogue: 0,0:47:33.71,0:47:36.81,Default,,0,0,0,,- Ngay trước mũi tôi.\N- Mà ông không thể thấy nó.
Dialogue: 0,0:47:37.51,0:47:41.71,Default,,0,0,0,,Hút nhiều thuốc đã làm chậm đi\Ntrí lực của ông.
Dialogue: 0,0:47:42.61,0:47:46.61,Default,,0,0,0,,Nhưng ta vẫn còn thời gian, đủ để\Nphản công Sauron, nếu hành động nhanh.
Dialogue: 0,0:47:47.01,0:47:48.41,Default,,0,0,0,,Thời gian?
Dialogue: 0,0:47:49.51,0:47:52.01,Default,,0,0,0,,Ông nghĩ ta còn bao lâu?
Dialogue: 0,0:47:52.51,0:47:56.21,Default,,0,0,0,,Sauron đã lấy lại gần hết\Nsức mạnh trước kia của hắn.
Dialogue: 0,0:47:56.61,0:47:58.71,Default,,0,0,0,,Mặc dù hắn\Nchưa có lại được thể xác...
Dialogue: 0,0:47:59.01,0:48:01.61,Default,,0,0,0,,nhưng linh hồn của hắn\Nvẫn còn nguyên sức mạnh.
Dialogue: 0,0:48:02.31,0:48:07.21,Default,,0,0,0,,Ẩn náu trong pháo đài,\Nlãnh chúa Mordor thấy tất cả.
Dialogue: 0,0:48:08.01,0:48:13.71,Default,,0,0,0,,Cái nhìn của hắn xuyên qua mây mù,\Nbóng tối, mặt đất và xác thịt.
Dialogue: 0,0:48:15.51,0:48:18.51,Default,,0,0,0,,Ông biết tôi nói về\Nđiều gì không, Gandalf?
Dialogue: 0,0:48:19.51,0:48:24.51,Default,,0,0,0,,Một con mắt vĩ đại, không mi mắt,\Nđược cuộn lại trong ngọn lửa.
Dialogue: 0,0:48:25.71,0:48:27.41,Default,,0,0,0,,Con mắt của Sauron.
Dialogue: 0,0:48:27.71,0:48:30.21,Default,,0,0,0,,Hắn đang tập trung tất cả quỷ dữ về.
Dialogue: 0,0:48:30.71,0:48:32.41,Default,,0,0,0,,Sớm thôi, hắn sẽ triệu tập\Nmột đội quân...
Dialogue: 0,0:48:32.71,0:48:35.51,Default,,0,0,0,,đủ lớn để tấn công vùng Trung Địa.
Dialogue: 0,0:48:35.91,0:48:38.21,Default,,0,0,0,,Ông biết chuyện đó à?
Dialogue: 0,0:48:38.81,0:48:40.31,Default,,0,0,0,,Bằng cách nào?
Dialogue: 0,0:48:40.91,0:48:42.51,Default,,0,0,0,,Tôi đã thấy điều đó.
Dialogue: 0,0:48:43.01,0:48:46.11,Default,,0,0,0,,Quả cầu tiên tri là\Nmột công cụ nguy hiểm, Saruman.
Dialogue: 0,0:48:47.01,0:48:48.41,Default,,0,0,0,,Tại sao?
Dialogue: 0,0:48:48.91,0:48:51.51,Default,,0,0,0,,Sao chúng ta lại sợ dùng nó?
Dialogue: 0,0:48:54.31,0:48:58.11,Default,,0,0,0,,Những hòn đá tiên tri,\Nchúng không đáng tin.
Dialogue: 0,0:48:58.41,0:49:01.51,Default,,0,0,0,,Chúng ta không biết còn ai khác\Nđang xem những hòn đá đó.
Dialogue: 0,0:49:04.21,0:49:06.91,Default,,0,0,0,,Tình hình đã muộn hơn ông tưởng đấy.
Dialogue: 0,0:49:07.11,0:49:09.41,Default,,0,0,0,,Lực lượng của Sauron\Nđã di chuyển rồi.
Dialogue: 0,0:49:11.71,0:49:14.41,Default,,0,0,0,,Bọn Chín Lãnh Chúa\Nđã rời Minas Morgul.
Dialogue: 0,0:49:14.81,0:49:15.81,Default,,0,0,0,,Chín tên đó ư?
Dialogue: 0,0:49:16.01,0:49:18.91,Default,,0,0,0,,Chúng đã băng qua sông Isen\Nvào ngày hạ chí...
Dialogue: 0,0:49:19.31,0:49:21.91,Default,,0,0,0,,dưới dạng những kị sĩ áo đen.
Dialogue: 0,0:49:22.21,0:49:26.01,Default,,0,0,0,,- Chúng đã đến vùng Shire?\N- Chúng sẽ tìm thấy chiếc Nhẫn.
Dialogue: 0,0:49:27.71,0:49:29.91,Default,,0,0,0,,Và giết người mang nó.
Dialogue: 0,0:49:30.41,0:49:31.81,Default,,0,0,0,,Frodo!
Dialogue: 0,0:49:44.61,0:49:48.81,Default,,0,0,0,,Ông nghĩ tên Hobbit đó có thể\Ngây chống lại ý chí của Sauron à?
Dialogue: 0,0:49:49.71,0:49:52.41,Default,,0,0,0,,Không ai có thể làm được điều đó.
Dialogue: 0,0:49:55.01,0:49:57.61,Default,,0,0,0,,Chống lại sức mạnh của Mordor...
Dialogue: 0,0:49:58.01,0:50:00.51,Default,,0,0,0,,thì sẽ thất bại thôi.
Dialogue: 0,0:50:02.61,0:50:05.61,Default,,0,0,0,,Chúng ta phải\Ngia nhập với hắn, Gandalf.
Dialogue: 0,0:50:07.01,0:50:09.21,Default,,0,0,0,,Ta phải nhập bọn với Sauron.
Dialogue: 0,0:50:12.01,0:50:14.51,Default,,0,0,0,,Đó là lựa chọn thông minh đấy,\Nông bạn.
Dialogue: 0,0:50:15.21,0:50:16.61,Default,,0,0,0,,Nói ta biết,
Dialogue: 0,0:50:16.81,0:50:18.31,Default,,0,0,0,,"ông bạn",
Dialogue: 0,0:50:18.81,0:50:23.21,Default,,0,0,0,,từ khi nào Saruman thông thái\Ntrở nên điên dại?
Dialogue: 0,0:51:02.91,0:51:04.51,Default,,0,0,0,,Ta đã cho ngươi cơ hội...
Dialogue: 0,0:51:05.71,0:51:08.31,Default,,0,0,0,,để giúp ta...
Dialogue: 0,0:51:08.61,0:51:12.61,Default,,0,0,0,,nhưng ngươi lại chọn\Ncon đường đau khổ!
Dialogue: 0,0:51:36.11,0:51:37.51,Default,,0,0,0,,Cậu Frodo?
Dialogue: 0,0:51:38.21,0:51:39.91,Default,,0,0,0,,Frodo? Frodo!
Dialogue: 0,0:51:41.41,0:51:44.51,Default,,0,0,0,,- Tôi cứ tưởng đã lạc cậu rồi.\N- Cậu đang nói gì vậy?
Dialogue: 0,0:51:44.71,0:51:47.61,Default,,0,0,0,,- Chỉ là Gandalf từng nói.\N- Nói gì?
Dialogue: 0,0:51:48.61,0:51:52.31,Default,,0,0,0,,{\i1}"Không rời khỏi cậu ấy, Samwise Gamgee."\NVà tôi sẽ không làm thế.{\i0}
Dialogue: 0,0:51:52.81,0:51:56.31,Default,,0,0,0,,Sam, ta vẫn còn ở Shire.\NChuyện gì có thể xảy ra chứ?
Dialogue: 0,0:51:59.31,0:52:00.31,Default,,0,0,0,,Frodo.
Dialogue: 0,0:52:00.61,0:52:02.81,Default,,0,0,0,,- Merry, đây là Frodo Baggins.\N- Chào, Frodo.
Dialogue: 0,0:52:03.11,0:52:04.41,Default,,0,0,0,,Tránh ra nào!
Dialogue: 0,0:52:05.01,0:52:06.41,Default,,0,0,0,,Cậu ổn chứ, Frodo?
Dialogue: 0,0:52:06.61,0:52:08.41,Default,,0,0,0,,Chuyện này là sao đây?
Dialogue: 0,0:52:08.91,0:52:11.91,Default,,0,0,0,,Các cậu đã trộm của ông Maggot à?
Dialogue: 0,0:52:13.21,0:52:15.31,Default,,0,0,0,,Các người trở lại đây!
Dialogue: 0,0:52:16.31,0:52:17.61,Default,,0,0,0,,Cút khỏi cánh đồng của tao!
Dialogue: 0,0:52:17.91,0:52:20.51,Default,,0,0,0,,Tao mà bắt được,\Ntụi bây sẽ biết tay!
Dialogue: 0,0:52:20.91,0:52:23.71,Default,,0,0,0,,Tôi không hiểu sao ông ta giận vậy.\NChỉ là vài củ cà rốt thôi mà.
Dialogue: 0,0:52:24.01,0:52:25.11,Default,,0,0,0,,Và vài củ cải bắp.
Dialogue: 0,0:52:25.31,0:52:28.21,Default,,0,0,0,,Và ba túi cà chua\Nchúng ta lấy tuần trước.
Dialogue: 0,0:52:28.41,0:52:32.31,Default,,0,0,0,,- Rồi nấm vào tuần trước nữa.\N- Phải, Pippin. Tôi nghĩ là...
Dialogue: 0,0:52:32.71,0:52:34.81,Default,,0,0,0,,ông ta phản ứng hơi quá.
Dialogue: 0,0:52:35.11,0:52:36.51,Default,,0,0,0,,Chạy!
Dialogue: 0,0:52:50.01,0:52:52.51,Default,,0,0,0,,Suýt chết.
Dialogue: 0,0:52:53.71,0:52:56.71,Default,,0,0,0,,Tôi nghĩ mình bị gãy cái gì rồi.
Dialogue: 0,0:52:57.81,0:53:00.11,Default,,0,0,0,,Không thể tin\Nbọn Brandybuck và Took!
Dialogue: 0,0:53:00.31,0:53:03.81,Default,,0,0,0,,Sao? Đó là đường ngắn đấy!\NĐường tắt!
Dialogue: 0,0:53:04.21,0:53:06.41,Default,,0,0,0,,- Đường tắt đến đâu?\N- Nấm kìa!
Dialogue: 0,0:53:15.21,0:53:16.61,Default,,0,0,0,,Của tôi đấy!
Dialogue: 0,0:53:19.41,0:53:21.01,Default,,0,0,0,,Tuyệt đấy, Merry.
Dialogue: 0,0:53:21.61,0:53:23.61,Default,,0,0,0,,Đây một cái nữa này, Sam.
Dialogue: 0,0:53:23.81,0:53:26.21,Default,,0,0,0,,Tôi nghĩ ta nên tránh xa đường cái.
Dialogue: 0,0:53:35.61,0:53:36.91,Default,,0,0,0,,Ra khỏi đây ngay!
Dialogue: 0,0:53:37.11,0:53:38.51,Default,,0,0,0,,Nhanh lên!
Dialogue: 0,0:53:51.31,0:53:52.71,Default,,0,0,0,,Im lặng!
Dialogue: 0,0:54:59.11,0:55:01.21,Default,,0,0,0,,Cái gì vậy?
Dialogue: 0,0:55:34.01,0:55:35.41,Default,,0,0,0,,Có gì không?
Dialogue: 0,0:55:36.21,0:55:37.61,Default,,0,0,0,,Không.
Dialogue: 0,0:55:38.41,0:55:40.61,Default,,0,0,0,,Chuyện gì đang xảy ra vậy?
Dialogue: 0,0:55:41.21,0:55:44.81,Default,,0,0,0,,Những Kị sĩ đen đó đang tìm kiếm\Nmột vật, hay người nào đó.
Dialogue: 0,0:55:46.31,0:55:47.71,Default,,0,0,0,,Frodo?
Dialogue: 0,0:55:48.51,0:55:49.51,Default,,0,0,0,,Nằm xuống!
Dialogue: 0,0:56:06.41,0:56:08.61,Default,,0,0,0,,Tôi phải rời khỏi vùng Shire.
Dialogue: 0,0:56:09.71,0:56:12.01,Default,,0,0,0,,Sam và tôi phải tới Bree.
Dialogue: 0,0:56:12.21,0:56:13.61,Default,,0,0,0,,Đúng.
Dialogue: 0,0:56:15.01,0:56:17.21,Default,,0,0,0,,Đến bến phà Bucklebury nào!\NTheo tôi!
Dialogue: 0,0:56:25.61,0:56:26.81,Default,,0,0,0,,Chạy đi!
Dialogue: 0,0:56:28.11,0:56:30.01,Default,,0,0,0,,Lối này! Theo tôi!
Dialogue: 0,0:56:33.81,0:56:34.71,Default,,0,0,0,,Chạy!
Dialogue: 0,0:56:48.01,0:56:49.71,Default,,0,0,0,,Lấy sợi dây đi, Sam!
Dialogue: 0,0:56:52.41,0:56:53.91,Default,,0,0,0,,Frodo!
Dialogue: 0,0:56:54.71,0:56:56.01,Default,,0,0,0,,Chạy đi, Frodo!
Dialogue: 0,0:56:56.51,0:56:57.51,Default,,0,0,0,,- Đi!\N- Nhanh lên!
Dialogue: 0,0:56:57.71,0:57:00.21,Default,,0,0,0,,Frodo, mau lên nào!
Dialogue: 0,0:57:01.31,0:57:03.31,Default,,0,0,0,,- Coi nào! Nhanh lên!\N- Nhảy!
Dialogue: 0,0:57:18.31,0:57:22.31,Default,,0,0,0,,- Chỗ qua sông gần nhất cách đây bao xa?\N- Cầu Brandywine, 20 dặm.
Dialogue: 0,0:57:37.11,0:57:38.61,Default,,0,0,0,,Đi nào!
Dialogue: 0,0:57:49.81,0:57:53.11,Default,,0,0,0,,- Các người muốn gì?\N- Chúng tôi đang đến Prancing Pony.
Dialogue: 0,0:57:56.71,0:57:59.51,Default,,0,0,0,,Người Hobbit. Bốn tên!
Dialogue: 0,0:57:59.71,0:58:03.51,Default,,0,0,0,,Các người nói không đi khỏi Shire cơ mà.\NChuyện gì đã đưa các người đến Bree này?
Dialogue: 0,0:58:04.21,0:58:07.21,Default,,0,0,0,,Chúng tôi muốn ở trọ.\NChuyện gì thì kệ bọn tôi.
Dialogue: 0,0:58:08.41,0:58:10.81,Default,,0,0,0,,Được thôi, cậu nhỏ.\NÝ ta là không mất lòng gì nhé.
Dialogue: 0,0:58:11.11,0:58:13.51,Default,,0,0,0,,Công việc của ta là phải hỏi khách\Nsau khi đêm xuống.
Dialogue: 0,0:58:13.91,0:58:16.71,Default,,0,0,0,,Có nhiều chuyện lạ ở ngoài mà.
Dialogue: 0,0:58:16.91,0:58:19.01,Default,,0,0,0,,Nhưng cũng không nên quá cẩn thận.
Dialogue: 0,0:58:38.81,0:58:42.11,Default,,0,0,0,,Tránh đường! Phải nhìn xem\Nmình đang đi đâu chứ, cậu nhỏ.
Dialogue: 0,0:59:06.31,0:59:07.81,Default,,0,0,0,,Xin lỗi!
Dialogue: 0,0:59:08.91,0:59:11.61,Default,,0,0,0,,Chào buổi tối, các cậu nhỏ.\NTôi có thể giúp gì?
Dialogue: 0,0:59:11.91,0:59:17.71,Default,,0,0,0,,Nếu muốn trọ, chúng tôi có vài phòng trống\Nấm cúng, vừa với người Hobbit đây.
Dialogue: 0,0:59:18.21,0:59:22.71,Default,,0,0,0,,Luôn sẵn lòng phục vụ\Nnhững người bé nhỏ, cậu...?
Dialogue: 0,0:59:23.91,0:59:25.91,Default,,0,0,0,,Underhill.\NTôi tên Underhill.
Dialogue: 0,0:59:27.31,0:59:29.71,Default,,0,0,0,,- Underhill, vâng.\N- Chúng tôi là bạn của Gandalf Áo Xám.
Dialogue: 0,0:59:30.01,0:59:33.01,Default,,0,0,0,,- Có thể nói cho ông ấy là chúng tôi đã tới?\N- Gandalf?
Dialogue: 0,0:59:33.51,0:59:35.21,Default,,0,0,0,,Gandalf.
Dialogue: 0,0:59:36.11,0:59:37.71,Default,,0,0,0,,Ồ, phải...
Dialogue: 0,0:59:38.01,0:59:40.01,Default,,0,0,0,,Tôi nhớ rồi, lão già đó hả?
Dialogue: 0,0:59:40.41,0:59:43.31,Default,,0,0,0,,Râu dài xám, đội mũ nhọn.
Dialogue: 0,0:59:43.71,0:59:45.51,Default,,0,0,0,,Không thấy ông ta\Nsáu tháng nay rồi.
Dialogue: 0,0:59:52.41,0:59:53.71,Default,,0,0,0,,Ta phải làm sao đây?
Dialogue: 0,1:00:00.91,1:00:02.11,Default,,0,0,0,,Sam,
Dialogue: 0,1:00:02.31,1:00:04.11,Default,,0,0,0,,ông ấy sẽ đến.
Dialogue: 0,1:00:07.01,1:00:09.01,Default,,0,0,0,,Tránh đường nào!
Dialogue: 0,1:00:11.51,1:00:13.11,Default,,0,0,0,,Gì thế?
Dialogue: 0,1:00:13.31,1:00:15.71,Default,,0,0,0,,Đây là một vại bia.
Dialogue: 0,1:00:16.11,1:00:17.61,Default,,0,0,0,,Người ta uống theo vại à?
Dialogue: 0,1:00:18.01,1:00:21.51,Default,,0,0,0,,- Tôi sẽ lấy một vại.\N- Cậu uống hơn một nửa rồi!
Dialogue: 0,1:00:28.01,1:00:31.71,Default,,0,0,0,,Cái gã kia cứ nhìn cậu\Ntừ khi chúng ta đến.
Dialogue: 0,1:00:35.51,1:00:36.91,Default,,0,0,0,,Xin lỗi.
Dialogue: 0,1:00:38.81,1:00:41.61,Default,,0,0,0,,Người đàn ông ở góc kia\Nlà ai vậy?
Dialogue: 0,1:00:43.01,1:00:47.21,Default,,0,0,0,,Hắn trong đội gác rừng, một kẻ\Nnguy hiểm, lang thang ở nơi hoang dã.
Dialogue: 0,1:00:47.51,1:00:51.91,Default,,0,0,0,,Tôi không biết tên thật của hắn là gì,\Nnhưng biệt danh của gã là Strider.
Dialogue: 0,1:00:54.41,1:00:55.61,Default,,0,0,0,,Strider.
Dialogue: 0,1:01:08.71,1:01:11.81,Default,,0,0,0,,{\i1}Baggins.{\i0}
Dialogue: 0,1:01:15.01,1:01:17.91,Default,,0,0,0,,{\i1}Baggins.{\i0}
Dialogue: 0,1:01:22.31,1:01:24.61,Default,,0,0,0,,{\i1}Baggins...{\i0}
Dialogue: 0,1:01:25.91,1:01:28.31,Default,,0,0,0,,{\i1}Baggins...{\i0}
Dialogue: 0,1:01:29.01,1:01:31.91,Default,,0,0,0,,Baggins?\NPhải, tôi biết Baggins.
Dialogue: 0,1:01:32.61,1:01:35.21,Default,,0,0,0,,Đằng kia kìa, Frodo Baggins.
Dialogue: 0,1:01:35.41,1:01:38.51,Default,,0,0,0,,Là anh họ thứ hai của tôi đấy,\Nnếu tính phía họ mẹ...
Dialogue: 0,1:01:38.61,1:01:40.81,Default,,0,0,0,,và anh họ thứ ba,\Nnếu tính bên...
Dialogue: 0,1:01:45.41,1:01:47.11,Default,,0,0,0,,- Pippin!\N- Bình tĩnh, Frodo.
Dialogue: 0,1:02:16.71,1:02:19.11,Default,,0,0,0,,Ngươi không thể trốn!
Dialogue: 0,1:02:22.11,1:02:25.11,Default,,0,0,0,,Ta đã thấy ngươi!
Dialogue: 0,1:02:27.01,1:02:29.61,Default,,0,0,0,,Không có sự sống nào...
Dialogue: 0,1:02:29.81,1:02:31.91,Default,,0,0,0,,trong nơi hư vô này.
Dialogue: 0,1:02:32.21,1:02:33.61,Default,,0,0,0,,Chỉ có...
Dialogue: 0,1:02:34.61,1:02:36.11,Default,,0,0,0,,cái chết!
Dialogue: 0,1:02:42.11,1:02:46.81,Default,,0,0,0,,Cậu thu hút nhiều sự chú ý quá,\Ncậu "Underhill."
Dialogue: 0,1:02:51.71,1:02:52.61,Default,,0,0,0,,Ông muốn gì?
Dialogue: 0,1:02:52.91,1:02:54.91,Default,,0,0,0,,Muốn cậu thận trọng hơn.\NThứ cậu đem theo không phải là nữ trang.
Dialogue: 0,1:02:55.41,1:02:58.31,Default,,0,0,0,,- Tôi có đem gì đâu.\N- Thật sao?
Dialogue: 0,1:02:58.71,1:03:01.11,Default,,0,0,0,,Tôi có thể tránh\Nkhông bị nhìn thấy nếu muốn...
Dialogue: 0,1:03:01.41,1:03:04.91,Default,,0,0,0,,nhưng biến mất hoàn toàn,\Nthì rất hiếm đấy.
Dialogue: 0,1:03:05.61,1:03:08.11,Default,,0,0,0,,- Ông là ai?\N- Cậu sợ à?
Dialogue: 0,1:03:09.51,1:03:10.41,Default,,0,0,0,,Phải.
Dialogue: 0,1:03:10.61,1:03:14.41,Default,,0,0,0,,Như vậy chưa là gì đâu.\NTôi biết thứ gì đang săn lùng cậu.
Dialogue: 0,1:03:18.51,1:03:20.91,Default,,0,0,0,,Để cậu ta đi!\NHoặc ta lấy mạng ngươi, Longshanks.
Dialogue: 0,1:03:22.81,1:03:26.71,Default,,0,0,0,,Dũng cảm đấy, anh bạn Hobbit,\Nnhưng dũng cảm không cứu được cậu đâu.
Dialogue: 0,1:03:27.11,1:03:29.91,Default,,0,0,0,,Đừng đợi lão pháp sư nữa, Frodo.
Dialogue: 0,1:03:30.21,1:03:31.21,Default,,0,0,0,,Chúng đang đến.
Dialogue: 0,1:04:54.91,1:04:56.31,Default,,0,0,0,,Chúng là cái gì vậy?
Dialogue: 0,1:04:58.51,1:05:00.61,Default,,0,0,0,,Chúng từng là con người.
Dialogue: 0,1:05:00.91,1:05:03.01,Default,,0,0,0,,Những vị vua vĩ đại của tộc Người.
Dialogue: 0,1:05:03.61,1:05:06.61,Default,,0,0,0,,Rồi Sauron Kẻ Lừa Gạt\Nđưa cho họ 9 chiếc Nhẫn quyền lực.
Dialogue: 0,1:05:07.41,1:05:10.41,Default,,0,0,0,,Bị tham lam làm mờ mắt,\Nhọ nhận lấy mà không do dự.
Dialogue: 0,1:05:10.71,1:05:13.91,Default,,0,0,0,,Lần lượt từng người\Nbị chìm vào bóng tối.
Dialogue: 0,1:05:14.21,1:05:16.61,Default,,0,0,0,,Giờ họ là nô lệ của hắn.
Dialogue: 0,1:05:19.11,1:05:20.71,Default,,0,0,0,,Họ là Nazgul.
Dialogue: 0,1:05:20.91,1:05:23.51,Default,,0,0,0,,Những hồn ma của chiếc Nhẫn.\NKhông sống cũng không chết.
Dialogue: 0,1:05:24.51,1:05:25.91,Default,,0,0,0,,Lúc nào chúng cũng cảm giác được...
Dialogue: 0,1:05:26.01,1:05:27.91,Default,,0,0,0,,sự hiện diện của chiếc Nhẫn.
Dialogue: 0,1:05:28.21,1:05:32.51,Default,,0,0,0,,Chúng bị thu hút bởi sức mạnh của nó,\Nchúng sẽ không ngừng săn tìm cậu.
Dialogue: 0,1:05:42.71,1:05:45.51,Default,,0,0,0,,- Ông sẽ đưa chúng tôi đi đâu?\N- Vào nơi hoang dã.
Dialogue: 0,1:05:53.91,1:05:57.71,Default,,0,0,0,,Làm sao chúng ta biết tên Strider này\Ncó phải là bạn của Gandalf không?
Dialogue: 0,1:05:58.11,1:06:00.91,Default,,0,0,0,,Tôi nghĩ tay chân của kẻ thù\Nsẽ hiểm ác hơn...
Dialogue: 0,1:06:01.31,1:06:05.11,Default,,0,0,0,,- và hôi thối hơn.\N- Ông ta cũng hôi lắm đấy.
Dialogue: 0,1:06:05.81,1:06:08.61,Default,,0,0,0,,Chúng ta không có sự lựa chọn nào khác\Nngoài việc tin ông ta.
Dialogue: 0,1:06:08.91,1:06:12.11,Default,,0,0,0,,- Nhưng hắn dẫn ta đi đâu?\N- Tới Rivendell, ngài Gamgee ạ.
Dialogue: 0,1:06:14.61,1:06:17.81,Default,,0,0,0,,- Nhà của Elrond.\N- Có nghe không? Rivendell.
Dialogue: 0,1:06:18.51,1:06:21.21,Default,,0,0,0,,Chúng ta sẽ gặp tộc Tiên.
Dialogue: 0,1:06:42.91,1:06:47.01,Default,,0,0,0,,- Ta sẽ không dừng lại trước khi đêm xuống.\N- Còn ăn sáng thì sao?
Dialogue: 0,1:06:47.31,1:06:50.01,Default,,0,0,0,,- Chúng ta mới ăn mà.\N- Chúng ta chỉ mới ăn một bữa.
Dialogue: 0,1:06:50.21,1:06:52.41,Default,,0,0,0,,Vậy bữa sáng thứ nhì thì sao?
Dialogue: 0,1:06:55.81,1:06:58.81,Default,,0,0,0,,Chắc ông ta không biết gì\Nvề bữa sáng thứ nhì đâu, Pip.
Dialogue: 0,1:06:59.31,1:07:04.31,Default,,0,0,0,,Vậy còn bữa 11 giờ? Bữa trưa?\NBữa trà chiều? Bữa tối? Bữa khuya?
Dialogue: 0,1:07:04.71,1:07:08.71,Default,,0,0,0,,- Hắn ta có biết không nhỉ?\N- Tôi cho là không.
Dialogue: 0,1:07:14.31,1:07:15.91,Default,,0,0,0,,Pippin!
Dialogue: 0,1:07:32.41,1:07:35.71,Default,,0,0,0,,Khi không có Hobbit\Nthì bọn chúng ăn cái gì nhỉ?
Dialogue: 0,1:07:51.21,1:07:54.51,Default,,0,0,0,,{\i1}Nàng Tinuviel xinh đẹp tộc Tiên{\i0}
Dialogue: 0,1:07:56.41,1:08:02.71,Default,,0,0,0,,{\i1}Tiên nữ đồng trinh\Nbất tử và thông thái{\i0}
Dialogue: 0,1:08:04.11,1:08:10.01,Default,,0,0,0,,{\i1}Với mái tóc đen tựa màn đêm{\i0}
Dialogue: 0,1:08:10.51,1:08:14.51,Default,,0,0,0,,{\i1}Và đôi tay tựa ánh bạc mờ ảo.{\i0}
Dialogue: 0,1:08:14.81,1:08:16.91,Default,,0,0,0,,Cô ấy là ai?
Dialogue: 0,1:08:17.71,1:08:20.11,Default,,0,0,0,,Người phụ nữ ông vừa hát ấy.
Dialogue: 0,1:08:23.61,1:08:26.01,Default,,0,0,0,,Là công nương xứ Luthien.
Dialogue: 0,1:08:26.21,1:08:31.21,Default,,0,0,0,,Là tiên nữ đồng trinh đem lòng\Nyêu Beren, một người bình thường.
Dialogue: 0,1:08:33.21,1:08:35.31,Default,,0,0,0,,Chuyện gì xảy ra với cô ấy?
Dialogue: 0,1:08:37.61,1:08:40.01,Default,,0,0,0,,Cô ấy đã chết.
Dialogue: 0,1:08:44.01,1:08:46.31,Default,,0,0,0,,Ngủ đi, Frodo.
Dialogue: 0,1:08:56.81,1:09:00.71,Default,,0,0,0,,{\i1}Sức mạnh của Isengard\Nsẽ dưới quyền của Ngài,{\i0}
Dialogue: 0,1:09:01.01,1:09:05.21,Default,,0,0,0,,{\i1}thưa Sauron, Chúa tể Trái Đất.{\i0}
Dialogue: 0,1:09:08.81,1:09:14.41,Default,,0,0,0,,{\i1}Hãy xây dựng cho ta một đội quân\Nxứng đáng với Mordor.{\i0}
Dialogue: 0,1:09:20.11,1:09:24.41,Default,,0,0,0,,Có mệnh lệnh gì từ Mordor, thưa ngài?\NCon Mắt ra lệnh gì?
Dialogue: 0,1:09:25.11,1:09:26.91,Default,,0,0,0,,Chúng ta có việc phải làm.
Dialogue: 0,1:10:09.81,1:10:14.21,Default,,0,0,0,,Đám cây rất mạnh,\Nrễ bám rất sâu.
Dialogue: 0,1:10:14.81,1:10:16.61,Default,,0,0,0,,Nhổ hết chúng đi.
Dialogue: 0,1:10:44.11,1:10:46.81,Default,,0,0,0,,Đây từng là tháp canh vĩ đại\Ncủa Amon Sul.
Dialogue: 0,1:10:48.61,1:10:51.11,Default,,0,0,0,,Chúng ta có thể nghỉ lại đây đêm nay.
Dialogue: 0,1:11:02.91,1:11:06.21,Default,,0,0,0,,Cho các cậu đây, giữ nó bên mình.
Dialogue: 0,1:11:06.61,1:11:09.01,Default,,0,0,0,,Tôi phải đi xem xét xung quanh.
Dialogue: 0,1:11:11.51,1:11:13.31,Default,,0,0,0,,Ở yên đây.
Dialogue: 0,1:11:15.91,1:11:18.11,Default,,0,0,0,,Cà chua của tớ cháy rồi.
Dialogue: 0,1:11:18.31,1:11:20.21,Default,,0,0,0,,Cho tớ vài miếng thịt lợn được không?
Dialogue: 0,1:11:20.31,1:11:22.31,Default,,0,0,0,,Cà chua không, Sam?
Dialogue: 0,1:11:22.71,1:11:26.31,Default,,0,0,0,,- Các cậu đang làm gì vậy?\N- Cà chua, nước sốt, thịt muối giòn.
Dialogue: 0,1:11:26.61,1:11:29.21,Default,,0,0,0,,- Chúng tôi để dành cho cậu đấy, Frodo.\N- Tắt đi, đồ ngốc!
Dialogue: 0,1:11:29.51,1:11:33.31,Default,,0,0,0,,- Dập lửa đi!\N- Coi chừng! Dơ hết cà chua của tôi!
Dialogue: 0,1:11:41.91,1:11:43.61,Default,,0,0,0,,Đi mau!
Dialogue: 0,1:12:37.21,1:12:38.61,Default,,0,0,0,,Cút đi, lũ quỷ dữ!
Dialogue: 0,1:14:02.11,1:14:03.31,Default,,0,0,0,,Frodo!
Dialogue: 0,1:14:08.01,1:14:09.21,Default,,0,0,0,,Ôi, Sam.
Dialogue: 0,1:14:46.31,1:14:47.31,Default,,0,0,0,,Strider!
Dialogue: 0,1:14:50.01,1:14:55.31,Default,,0,0,0,,- Giúp cậu ấy đi, Strider!\N- Cậu ấy bị đâm bởi gươm của Morgul.
Dialogue: 0,1:14:57.01,1:15:01.71,Default,,0,0,0,,Việc này ngoài khả năng của ta.\NCậu ấy cần thuốc của tộc Tiên.
Dialogue: 0,1:15:06.51,1:15:07.51,Default,,0,0,0,,Nhanh lên!
Dialogue: 0,1:15:07.81,1:15:11.61,Default,,0,0,0,,Chúng ta còn cách Rivendell 6 ngày đường!\NCậu ấy không chịu được đâu!
Dialogue: 0,1:15:11.91,1:15:13.71,Default,,0,0,0,,Cố lên, Frodo.
Dialogue: 0,1:15:14.31,1:15:16.31,Default,,0,0,0,,Gandalf!
Dialogue: 0,1:16:20.01,1:16:21.51,Default,,0,0,0,,Gwaihir, đi đi.
Dialogue: 0,1:17:54.51,1:17:57.41,Default,,0,0,0,,Nhìn này, Frodo.\NĐây là bọn khổng lồ của ông Bilbo.
Dialogue: 0,1:17:59.71,1:18:02.41,Default,,0,0,0,,Cậu Frodo?\NCậu ấy đang lạnh dần.
Dialogue: 0,1:18:05.41,1:18:07.11,Default,,0,0,0,,Cậu ấy sắp chết à?
Dialogue: 0,1:18:07.41,1:18:12.11,Default,,0,0,0,,Cậu ta đang chìm vào thế giới bóng tối.\NSẽ trở thành hồn ma giống như bọn chúng.
Dialogue: 0,1:18:17.41,1:18:21.11,Default,,0,0,0,,- Bọn chúng ở rất gần.\N- Sam, cậu có biết cây Athelas không?
Dialogue: 0,1:18:21.51,1:18:22.92,Default,,0,0,0,,- Athelas à?\N- Lá của nhà vua.
Dialogue: 0,1:18:23.12,1:18:24.12,Default,,0,0,0,,Vâng, nó là một loại cỏ dại.
Dialogue: 0,1:18:24.42,1:18:27.42,Default,,0,0,0,,Nó giúp làm chậm chất độc lại.\NMau lên!
Dialogue: 0,1:18:41.92,1:18:45.92,Default,,0,0,0,,Gì thế này?\NMột chiến binh mà mất cảnh giác ư?
Dialogue: 0,1:19:11.02,1:19:12.32,Default,,0,0,0,,Frodo.
Dialogue: 0,1:19:13.62,1:19:18.02,Default,,0,0,0,,Tôi là Arwen.\NTôi đến để giúp cậu.
Dialogue: 0,1:19:19.12,1:19:23.42,Default,,0,0,0,,Hãy lắng nghe tôi...\Ntrở về với ánh sáng.
Dialogue: 0,1:19:28.82,1:19:30.62,Default,,0,0,0,,Cô ấy là ai?
Dialogue: 0,1:19:31.32,1:19:33.02,Default,,0,0,0,,Frodo.
Dialogue: 0,1:19:33.32,1:19:36.72,Default,,0,0,0,,- Cô ấy là một người tộc Tiên.\N- Cậu ấy đang ngất dần.
Dialogue: 0,1:19:39.32,1:19:41.32,Default,,0,0,0,,Cậu ấy sẽ không qua được mất.
Dialogue: 0,1:19:41.62,1:19:44.12,Default,,0,0,0,,Phải đưa cậu ấy đến chỗ cha em.
Dialogue: 0,1:19:44.92,1:19:47.72,Default,,0,0,0,,- Em đã tìm anh hai ngày rồi.\N- Mấy người đưa cậu ta đi đâu vậy?
Dialogue: 0,1:19:48.02,1:19:53.92,Default,,0,0,0,,Năm tên hồn ma đang theo anh.\NBốn tên còn lại thì em không biết.
Dialogue: 0,1:19:55.02,1:19:57.82,Default,,0,0,0,,Ở lại với người Hobbit đi.\NAnh sẽ điều ngựa đến cho em.
Dialogue: 0,1:19:58.02,1:20:00.42,Default,,0,0,0,,Em cưỡi ngựa nhanh hơn,\Nem sẽ đưa cậu ta đi.
Dialogue: 0,1:20:00.72,1:20:01.92,Default,,0,0,0,,Đường đi rất nguy hiểm.
Dialogue: 0,1:20:02.02,1:20:03.12,Default,,0,0,0,,Họ đang nói gì thế?
Dialogue: 0,1:20:03.42,1:20:05.22,Default,,0,0,0,,Frodo đang chết dần.\NNếu em có thể băng qua sông,
Dialogue: 0,1:20:05.52,1:20:08.82,Default,,0,0,0,,sức mạnh của tộc Tiên\Nsẽ bảo vệ cậu ấy.
Dialogue: 0,1:20:09.32,1:20:10.82,Default,,0,0,0,,Em không sợ bọn chúng.
Dialogue: 0,1:20:14.52,1:20:16.02,Default,,0,0,0,,Vậy cứ theo ý em.
Dialogue: 0,1:20:20.82,1:20:22.22,Default,,0,0,0,,Arwen...
Dialogue: 0,1:20:22.32,1:20:25.42,Default,,0,0,0,,... hãy chạy nhanh!\NĐừng nhìn lại!
Dialogue: 0,1:20:30.62,1:20:34.82,Default,,0,0,0,,Ông làm gì vậy?\NBọn chúng còn ở ngoài đó mà!
Dialogue: 0,1:21:45.32,1:21:47.32,Default,,0,0,0,,Nhanh nào, Asfaloth!
Dialogue: 0,1:22:29.52,1:22:32.92,Default,,0,0,0,,Hãy bỏ cuộc đi, tộc Tiên.
Dialogue: 0,1:22:33.32,1:22:35.72,Default,,0,0,0,,Nếu các ngươi muốn cậu ấy,\Nhãy đến đây.
Dialogue: 0,1:22:47.62,1:22:49.02,Default,,0,0,0,,Hỡi nước trong dãy Sương Mù...
Dialogue: 0,1:22:49.02,1:22:50.42,Default,,0,0,0,,hãy lắng nghe lời ta nói!
Dialogue: 0,1:22:50.52,1:22:53.42,Default,,0,0,0,,Hãy dâng lên và cuốn đi bọn Ma Nhẫn!
Dialogue: 0,1:22:53.62,1:22:55.02,Default,,0,0,0,,Hỡi nước trong dãy Sương Mù...
Dialogue: 0,1:22:55.12,1:22:56.52,Default,,0,0,0,,hãy lắng nghe lời ta nói!
Dialogue: 0,1:22:56.52,1:23:00.42,Default,,0,0,0,,Hãy dâng lên và cuốn đi bọn Ma Nhẫn!
Dialogue: 0,1:23:29.72,1:23:32.12,Default,,0,0,0,,Không, không!
Dialogue: 0,1:23:35.42,1:23:37.62,Default,,0,0,0,,Frodo, không!
Dialogue: 0,1:23:37.92,1:23:40.12,Default,,0,0,0,,Frodo, đừng bỏ cuộc!
Dialogue: 0,1:23:40.42,1:23:42.32,Default,,0,0,0,,Không phải bây giờ!
Dialogue: 0,1:23:53.42,1:23:55.82,Default,,0,0,0,,Những ơn huệ gì đã được\Ntrao cho ta...
Dialogue: 0,1:23:56.52,1:23:58.32,Default,,0,0,0,,xin hãy truyền sang cho cậu ấy.
Dialogue: 0,1:23:58.62,1:24:00.72,Default,,0,0,0,,Hãy giải thoát cho cậu ấy.
Dialogue: 0,1:24:01.42,1:24:03.32,Default,,0,0,0,,Cứu cậu ấy.
Dialogue: 0,1:24:05.52,1:24:09.52,Default,,0,0,0,,Hãy nghe ta.\NHãy trở về với ánh sáng.
Dialogue: 0,1:24:15.82,1:24:20.82,Default,,0,0,0,,- Đây là đâu?\N- Cậu đang ở nhà của Elrond.
Dialogue: 0,1:24:21.42,1:24:27.62,Default,,0,0,0,,Và bây giờ là 10 giờ sáng\Nngày 24 tháng 10, nếu cậu muốn biết.
Dialogue: 0,1:24:29.92,1:24:33.52,Default,,0,0,0,,- Gandalf!\N- Phải, ta ở đây.
Dialogue: 0,1:24:35.42,1:24:38.32,Default,,0,0,0,,Cậu cũng thật may mắn mới ở đây.
Dialogue: 0,1:24:39.02,1:24:42.92,Default,,0,0,0,,Vài giờ trước đây cậu gần như\Nkhông thể chữa được.
Dialogue: 0,1:24:43.32,1:24:47.22,Default,,0,0,0,,Nhưng bên trong cậu vẫn còn sức mạnh,\Ncậu bé Hobbit à.
Dialogue: 0,1:24:55.02,1:24:57.42,Default,,0,0,0,,Chuyện gì đã xảy ra vậy, Gandalf?
Dialogue: 0,1:24:58.42,1:25:03.72,Default,,0,0,0,,- Sao ông không đến gặp chúng tôi?\N- Ta rất tiếc, Frodo.
Dialogue: 0,1:25:08.52,1:25:10.22,Default,,0,0,0,,Ta bị ngăn cản.
Dialogue: 0,1:25:11.32,1:25:15.32,Default,,0,0,0,,Tình bằng hữu với Saruman\Nkhông dễ gì chấm dứt được.
Dialogue: 0,1:25:19.62,1:25:24.12,Default,,0,0,0,,Sự trở mặt sẽ có cái giá của nó.\NTất cả đã chấm dứt.
Dialogue: 0,1:25:24.62,1:25:27.22,Default,,0,0,0,,Hoặc đi theo quyền lực của chiếc Nhẫn...
Dialogue: 0,1:25:27.92,1:25:30.32,Default,,0,0,0,,hoặc đi theo con đường diệt vong!
Dialogue: 0,1:25:32.72,1:25:36.42,Default,,0,0,0,,Nhẫn Chúa là duy nhất.
Dialogue: 0,1:25:36.92,1:25:40.72,Default,,0,0,0,,Chỉ có một người\Nsai khiến được nó.
Dialogue: 0,1:25:41.72,1:25:45.52,Default,,0,0,0,,Và hắn sẽ không chia sẻ\Nquyền lực của hắn.
Dialogue: 0,1:25:59.92,1:26:03.12,Default,,0,0,0,,Vậy là ngươi đã chọn... cái chết.
Dialogue: 0,1:26:13.42,1:26:17.02,Default,,0,0,0,,Gandalf? Cái gì vậy?
Dialogue: 0,1:26:18.22,1:26:19.92,Default,,0,0,0,,Không có gì, Frodo.
Dialogue: 0,1:26:20.62,1:26:22.52,Default,,0,0,0,,Frodo!
Dialogue: 0,1:26:23.92,1:26:26.12,Default,,0,0,0,,- Sam!\N- Ơn trời, cậu tỉnh rồi!
Dialogue: 0,1:26:26.52,1:26:29.12,Default,,0,0,0,,Sam luôn ở bên cạnh cậu.
Dialogue: 0,1:26:29.52,1:26:31.52,Default,,0,0,0,,Chúng tôi lo cho cậu quá,\Nphải không ông Gandalf?
Dialogue: 0,1:26:32.12,1:26:37.02,Default,,0,0,0,,Nhờ phép của ngài Elrond,\Ncậu đang dần bình phục.
Dialogue: 0,1:26:38.62,1:26:42.12,Default,,0,0,0,,Chào mừng đến với Rivendell,\NFrodo Baggins.
Dialogue: 0,1:27:30.32,1:27:33.42,Default,,0,0,0,,- Chú Bilbo!\N- Chào Frodo, cháu trai!
Dialogue: 0,1:27:33.82,1:27:35.52,Default,,0,0,0,,Bilbo!
Dialogue: 0,1:27:40.42,1:27:42.42,Default,,0,0,0,,{\i1}Hành trình và trở về:{\i0}
Dialogue: 0,1:27:42.42,1:27:45.12,Default,,0,0,0,,{\i1}Câu chuyện về người Hobbit.\NTác giả Bilbo Baggins.{\i0}
Dialogue: 0,1:27:48.32,1:27:51.72,Default,,0,0,0,,- Tuyệt thật!\N- Ta muốn trở lại...
Dialogue: 0,1:27:52.12,1:27:55.82,Default,,0,0,0,,lang thang trong rừng Mirkwood,
Dialogue: 0,1:27:56.42,1:27:58.62,Default,,0,0,0,,thăm Laketown,
Dialogue: 0,1:27:58.92,1:28:02.22,Default,,0,0,0,,nhìn thấy ngọn nói Cô Đơn lần nữa.
Dialogue: 0,1:28:02.92,1:28:07.52,Default,,0,0,0,,Nhưng tuổi tác xem ra đã bắt kịp ta.
Dialogue: 0,1:28:19.92,1:28:21.62,Default,,0,0,0,,Cháu nhớ vùng Shire.
Dialogue: 0,1:28:22.12,1:28:27.02,Default,,0,0,0,,Suốt thời thơ ấu, cháu đã tưởng tượng\Nrằng mình được đến một nơi khác.
Dialogue: 0,1:28:27.52,1:28:30.92,Default,,0,0,0,,Cùng với chú, trong một chuyến\Nphiêu lưu nào đó.
Dialogue: 0,1:28:33.92,1:28:37.42,Default,,0,0,0,,Nhưng chuyến đi của cháu lại khác hẳn.
Dialogue: 0,1:28:42.92,1:28:45.92,Default,,0,0,0,,Cháu không giống chú, Bilbo.
Dialogue: 0,1:28:47.32,1:28:49.42,Default,,0,0,0,,Cháu trai.
Dialogue: 0,1:28:53.72,1:28:57.42,Default,,0,0,0,,- Mình có quên gì không?\N- Cậu đã xếp đồ rồi à?
Dialogue: 0,1:28:58.32,1:29:00.02,Default,,0,0,0,,Chuẩn bị trước là tốt.
Dialogue: 0,1:29:00.32,1:29:02.82,Default,,0,0,0,,- Mình nghĩ cậu muốn gặp tộc Tiên mà.\N- Mình muốn chứ.
Dialogue: 0,1:29:03.12,1:29:05.82,Default,,0,0,0,,- Hơn bất cứ thứ gì.\N- Phải.
Dialogue: 0,1:29:06.22,1:29:08.12,Default,,0,0,0,,Chỉ là...
Dialogue: 0,1:29:08.92,1:29:11.42,Default,,0,0,0,,Chúng ta đã làm những gì Gandalf muốn,\Nphải không?
Dialogue: 0,1:29:11.72,1:29:15.22,Default,,0,0,0,,Chúng ta đem chiếc Nhẫn đến Rivendell.\NVà mình nghĩ...
Dialogue: 0,1:29:15.72,1:29:18.82,Default,,0,0,0,,khi nào cậu bình phục,\Nthì bọn mình sẽ đi ngay.
Dialogue: 0,1:29:19.12,1:29:20.72,Default,,0,0,0,,Về nhà.
Dialogue: 0,1:29:24.92,1:29:26.82,Default,,0,0,0,,Cậu nói phải, Sam.
Dialogue: 0,1:29:29.12,1:29:31.82,Default,,0,0,0,,Chúng ta đã làm xong\Nnhững gì được giao.
Dialogue: 0,1:29:32.42,1:29:34.92,Default,,0,0,0,,Chiếc Nhẫn sẽ an toàn tại Rivendell.
Dialogue: 0,1:29:39.12,1:29:41.42,Default,,0,0,0,,Tớ sẵn sàng về nhà rồi.
Dialogue: 0,1:29:42.22,1:29:44.12,Default,,0,0,0,,Cậu ta khoẻ lại rồi.
Dialogue: 0,1:29:44.42,1:29:49.92,Default,,0,0,0,,Vết thương sẽ không hồi phục hoàn toàn.\NNó sẽ theo cậu ấy suốt đời.
Dialogue: 0,1:29:50.82,1:29:53.22,Default,,0,0,0,,Cậu ấy còn có thể mang chiếc Nhẫn\Nđi xa như vậy...
Dialogue: 0,1:29:53.42,1:29:57.02,Default,,0,0,0,,cho thấy sự đề kháng tuyệt vời\Ncủa người Hobbit đối với ma thuật của Nhẫn.
Dialogue: 0,1:29:57.42,1:30:00.72,Default,,0,0,0,,Nó là gánh nặng mà lẽ ra\Ncậu ấy không phải gánh lấy.
Dialogue: 0,1:30:01.02,1:30:06.22,Default,,0,0,0,,- Chúng ta không thể đòi hỏi Frodo hơn nữa.\N- Gandalf, kẻ thù đã hành động.
Dialogue: 0,1:30:06.62,1:30:11.12,Default,,0,0,0,,Lực lượng của Sauron đang lớn mạnh ở phía Đông.\NÁnh mắt của hắn đang hướng về Rivendell.
Dialogue: 0,1:30:11.52,1:30:14.12,Default,,0,0,0,,Và Saruman, như ông nói,\Nđã phản bội chúng ta.
Dialogue: 0,1:30:14.52,1:30:17.62,Default,,0,0,0,,Đồng minh của ta đang ít lại.
Dialogue: 0,1:30:18.42,1:30:21.12,Default,,0,0,0,,Sự phản bội của hắn\Ncòn ghê gớm hơn ngài nghĩ.
Dialogue: 0,1:30:21.42,1:30:24.92,Default,,0,0,0,,Bằng cách dơ bẩn, Saruman\Nđã kết hợp loài Orc với Goblin.
Dialogue: 0,1:30:25.32,1:30:27.62,Default,,0,0,0,,Hắn ta nuôi dưỡng một đạo quân trong\Nhang động Isengard.
Dialogue: 0,1:30:28.02,1:30:32.42,Default,,0,0,0,,Một đạo quân có thể đi dưới mặt trời,\Nvà có thể chạy rất xa.
Dialogue: 0,1:30:32.82,1:30:35.12,Default,,0,0,0,,Saruman đang nhắm đến chiếc Nhẫn.
Dialogue: 0,1:30:35.62,1:30:39.52,Default,,0,0,0,,Sức mạnh của tộc Tiên cũng không thể\Nche giấu chiếc Nhẫn xấu xa này.
Dialogue: 0,1:30:39.92,1:30:44.12,Default,,0,0,0,,Chúng ta không đủ mạnh để chống lại\Ncả Mordor và Isengard!
Dialogue: 0,1:30:48.12,1:30:49.12,Default,,0,0,0,,Gandalf,
Dialogue: 0,1:30:50.52,1:30:52.92,Default,,0,0,0,,chiếc Nhẫn không thể ở đây.
Dialogue: 0,1:31:12.62,1:31:15.72,Default,,0,0,0,,Mối hoạ này thuộc về toàn Trung Địa.
Dialogue: 0,1:31:16.02,1:31:18.72,Default,,0,0,0,,Họ phải quyết định\Ncho số phận của nó.
Dialogue: 0,1:31:19.82,1:31:23.22,Default,,0,0,0,,Thời đại của tộc Tiên đã kết thúc.\NDân tôi đang rời khỏi nơi đây.
Dialogue: 0,1:31:24.02,1:31:27.52,Default,,0,0,0,,Ông sẽ trông cậy ai khi chúng tôi đi?\NTộc Người Lùn à?
Dialogue: 0,1:31:27.92,1:31:32.82,Default,,0,0,0,,Họ sống trong những dãy núi tìm kiếm của cải,\Nkhông quan tâm đến việc của người khác.
Dialogue: 0,1:31:34.52,1:31:37.32,Default,,0,0,0,,Chúng ta phải đặt hy vọng\Nvào tộc Người.
Dialogue: 0,1:31:37.82,1:31:39.42,Default,,0,0,0,,Tộc Người?
Dialogue: 0,1:31:40.42,1:31:43.42,Default,,0,0,0,,Tộc Người rất yếu đuối.\NDòng dõi của họ đã thất bại.
Dialogue: 0,1:31:43.82,1:31:48.02,Default,,0,0,0,,Dòng máu của Numenor đã hết rồi.\NNiềm tự hào, phẩm cách của nó đã bị quên lãng.
Dialogue: 0,1:31:48.52,1:31:51.92,Default,,0,0,0,,Chiếc Nhẫn còn tồn tại\Nchỉ vì loài Người.
Dialogue: 0,1:31:52.32,1:31:53.82,Default,,0,0,0,,Tôi đã ở đó, Gandalf.
Dialogue: 0,1:31:54.62,1:31:57.32,Default,,0,0,0,,Tôi đã ở đó 3.000 năm trước,
Dialogue: 0,1:32:04.22,1:32:06.52,Default,,0,0,0,,khi Isildur lấy chiếc Nhẫn.
Dialogue: 0,1:32:06.82,1:32:10.52,Default,,0,0,0,,Tôi đã ở đó, cái ngày mà\Nsức mạnh của tộc Người thất bại.
Dialogue: 0,1:32:13.22,1:32:15.72,Default,,0,0,0,,Isildur, nhanh lên!\NTheo tôi.
Dialogue: 0,1:32:17.42,1:32:20.92,Default,,0,0,0,,Tôi đã dẫn Isildur vào\Ntrung tâm của ngọn Diệt Vong...
Dialogue: 0,1:32:21.32,1:32:24.72,Default,,0,0,0,,nơi chiếc Nhẫn được luyện thành,\Nnơi duy nhất tiêu diệt được nó.
Dialogue: 0,1:32:24.92,1:32:27.42,Default,,0,0,0,,Ném nó vào lửa đi!
Dialogue: 0,1:32:32.72,1:32:36.32,Default,,0,0,0,,- Tiêu diệt nó!\N- Không.
Dialogue: 0,1:32:37.82,1:32:39.92,Default,,0,0,0,,Isildur!
Dialogue: 0,1:32:40.42,1:32:45.02,Default,,0,0,0,,Mọi chuyện đáng lẽ kết thúc vào ngày đó,\Nnhưng cái ác vẫn được cho tồn tại.
Dialogue: 0,1:32:49.32,1:32:53.42,Default,,0,0,0,,Isildur giữ chiếc Nhẫn.\NDòng dõi của các vị vua đã bị đứt đoạn.
Dialogue: 0,1:32:54.42,1:32:58.02,Default,,0,0,0,,Loài Người chẳng còn\Nchút sức mạnh nào.
Dialogue: 0,1:32:58.42,1:33:03.02,Default,,0,0,0,,- Họ phân tán, mâu thuẫn nhau, không người lãnh đạo.\N- Có một người có thể liên minh họ lại.
Dialogue: 0,1:33:03.42,1:33:06.62,Default,,0,0,0,,Người đó sẽ đòi lại\Nngai vàng ở Gondor.
Dialogue: 0,1:33:08.52,1:33:12.12,Default,,0,0,0,,Anh ta đã rời bỏ con đường đó\Ntừ lâu rồi.
Dialogue: 0,1:33:12.42,1:33:14.52,Default,,0,0,0,,Anh ta đã chọn lưu vong.
Dialogue: 0,1:33:45.62,1:33:47.52,Default,,0,0,0,,Anh không phải người tộc Tiên!
Dialogue: 0,1:33:47.82,1:33:49.82,Default,,0,0,0,,Tất cả con người ở phía Nam\Nđều được chào đón ở đây.
Dialogue: 0,1:33:50.52,1:33:53.92,Default,,0,0,0,,- Anh là ai?\N- Bạn của Gandalf áo xám.
Dialogue: 0,1:33:55.52,1:33:58.22,Default,,0,0,0,,Vậy chúng ta có chung mục đích,
Dialogue: 0,1:33:58.62,1:34:00.02,Default,,0,0,0,,anh bạn.
Dialogue: 0,1:34:15.42,1:34:18.22,Default,,0,0,0,,Những mảnh của thanh gươm Narsil.
Dialogue: 0,1:34:21.82,1:34:26.02,Default,,0,0,0,,Thanh gươm đã chém đứt tay Sauron.
Dialogue: 0,1:34:29.12,1:34:31.32,Default,,0,0,0,,Nó vẫn sắc bén.
Dialogue: 0,1:34:40.32,1:34:43.22,Default,,0,0,0,,Nhưng cũng chỉ là\Nmột di vật đã vỡ.
Dialogue: 0,1:35:10.22,1:35:13.22,Default,,0,0,0,,Sao anh lại sợ quá khứ?
Dialogue: 0,1:35:14.12,1:35:19.22,Default,,0,0,0,,Anh là dòng dõi của Isildur,\Nchứ không phải là ông ấy.
Dialogue: 0,1:35:19.72,1:35:23.02,Default,,0,0,0,,Anh không bị trói buộc\Nvới số phận của ông ấy.
Dialogue: 0,1:35:23.62,1:35:27.42,Default,,0,0,0,,Nhưng dòng máu đó\Nđang chảy trong anh.
Dialogue: 0,1:35:30.62,1:35:33.12,Default,,0,0,0,,Cùng một sự yếu đuối.
Dialogue: 0,1:35:35.92,1:35:38.62,Default,,0,0,0,,Thời của anh sẽ đến.
Dialogue: 0,1:35:38.92,1:35:43.12,Default,,0,0,0,,Anh sẽ đối mặt với cùng bọn quỷ dữ xưa.\NVà anh sẽ đánh bại chúng.
Dialogue: 0,1:35:46.22,1:35:49.12,Default,,0,0,0,,Bóng tối không thể\Nthống trị mãi được, Aragorn.
Dialogue: 0,1:35:49.62,1:35:53.42,Default,,0,0,0,,Không thể thống trị được anh, hoặc em.
Dialogue: 0,1:36:04.82,1:36:08.52,Default,,0,0,0,,Anh có nhớ lần đầu chúng ta gặp không?
Dialogue: 0,1:36:11.12,1:36:15.32,Default,,0,0,0,,Anh cứ nghĩ mình đã lạc vào giấc mơ.
Dialogue: 0,1:36:16.42,1:36:18.92,Default,,0,0,0,,Nhiều năm đã trôi qua.
Dialogue: 0,1:36:19.82,1:36:23.62,Default,,0,0,0,,Anh không phải lo toan\Nnhiều việc như bây giờ.
Dialogue: 0,1:36:25.82,1:36:27.92,Default,,0,0,0,,Còn nhớ em đã nói gì với anh không?
Dialogue: 0,1:36:33.82,1:36:36.42,Default,,0,0,0,,Em nói sẽ gắn liền đời em với anh,
Dialogue: 0,1:36:38.62,1:36:43.32,Default,,0,0,0,,từ bỏ cuộc sống bất tử\Ncủa người tộc Tiên.
Dialogue: 0,1:36:44.62,1:36:46.52,Default,,0,0,0,,Em vẫn giữ lời nói đó.
Dialogue: 0,1:36:46.72,1:36:50.22,Default,,0,0,0,,Em muốn sống cuộc đời này với anh...
Dialogue: 0,1:36:50.72,1:36:54.32,Default,,0,0,0,,còn hơn một mình trải qua\Nnhiều cuộc đời trên Thế giới này.
Dialogue: 0,1:37:03.22,1:37:06.52,Default,,0,0,0,,Em chọn cuộc sống\Ncủa một người bình thường.
Dialogue: 0,1:37:08.02,1:37:12.12,Default,,0,0,0,,- Em không thể đưa vật này cho anh!\N- Em sẽ trao nó cho người em chọn,
Dialogue: 0,1:37:15.52,1:37:17.92,Default,,0,0,0,,cũng như con tim em vậy.
Dialogue: 0,1:37:33.82,1:37:37.42,Default,,0,0,0,,Những người lạ từ phương xa đến,\Nhay những người bạn cũ,
Dialogue: 0,1:37:37.82,1:37:41.52,Default,,0,0,0,,các bạn được mời đến đây vì\Nđáp trả mối đe doạ từ Mordor.
Dialogue: 0,1:37:42.32,1:37:47.02,Default,,0,0,0,,Trung Địa đang đứng trên bờ vực huỷ diệt.\NKhông ai có thể thoát được.
Dialogue: 0,1:37:47.42,1:37:50.82,Default,,0,0,0,,Hoặc chúng ta liên kết lại,\Nhoặc thất bại.
Dialogue: 0,1:37:51.22,1:37:55.32,Default,,0,0,0,,Mỗi chủng tộc đều chung một số phận,\Nchung một sự diệt vong.
Dialogue: 0,1:37:57.32,1:38:00.62,Default,,0,0,0,,Mang chiếc Nhẫn lên đây, Frodo.
Dialogue: 0,1:38:14.92,1:38:16.52,Default,,0,0,0,,Vậy đây là sự thật.
Dialogue: 0,1:38:26.32,1:38:27.82,Default,,0,0,0,,{\i1}Chiếc nhẫn của quyền lực...{\i0}
Dialogue: 0,1:38:29.92,1:38:31.42,Default,,0,0,0,,{\i1}Sự sụp đổ của loài người...{\i0}
Dialogue: 0,1:38:34.02,1:38:36.02,Default,,0,0,0,,Trong mơ...
Dialogue: 0,1:38:37.82,1:38:40.92,Default,,0,0,0,,ta thấy bầu trời phía Đông\Ntrở nên u ám...
Dialogue: 0,1:38:41.22,1:38:42.92,Default,,0,0,0,,nhưng ở phía Tây, một luồng ánh sáng mờ\Nvẫn đang lan dài.
Dialogue: 0,1:38:43.72,1:38:45.52,Default,,0,0,0,,Một giọng nói vang lên:
Dialogue: 0,1:38:45.82,1:38:48.62,Default,,0,0,0,,"Sự diệt vong của các ngươi\Nđang gần kề.
Dialogue: 0,1:38:49.82,1:38:52.42,Default,,0,0,0,,Tai ương của Isildur\Nđã được tìm thấy."
Dialogue: 0,1:38:56.42,1:38:58.22,Default,,0,0,0,,Tai ương của Isildur.
Dialogue: 0,1:38:58.82,1:38:59.12,Default,,0,0,0,,Boromir!
Dialogue: 0,1:38:59.12,1:39:02.52,Default,,0,0,0,,Một chiếc nhẫn thống trị tất cả...
Dialogue: 0,1:39:02.82,1:39:06.32,Default,,0,0,0,,Một chiếc nhẫn để tìm ra...
Dialogue: 0,1:39:06.72,1:39:09.92,Default,,0,0,0,,Một chiếc nhẫn\Nđem tất cả lại với nhau...
Dialogue: 0,1:39:12.72,1:39:17.22,Default,,0,0,0,,và kìm giữ họ trong bóng tối.
Dialogue: 0,1:39:23.32,1:39:27.02,Default,,0,0,0,,Trước đây chưa bao giờ có giọng nói nào\Nthốt ra bằng ngôn ngữ này tại Imladris.
Dialogue: 0,1:39:27.42,1:39:30.92,Default,,0,0,0,,Tôi sẽ không xin lỗi ngài,\Nngài Elrond...
Dialogue: 0,1:39:31.22,1:39:33.22,Default,,0,0,0,,vì Ngôn ngữ Đen của Mordor...
Dialogue: 0,1:39:33.62,1:39:34.92,Default,,0,0,0,,có thể đã được nghe...
Dialogue: 0,1:39:35.22,1:39:38.02,Default,,0,0,0,,ở mọi ngóc ngách miền Tây!
Dialogue: 0,1:39:38.32,1:39:40.92,Default,,0,0,0,,Chiếc Nhẫn tập trung mọi sự tà ác.
Dialogue: 0,1:39:41.42,1:39:42.82,Default,,0,0,0,,Chiếc Nhẫn là món quà.
Dialogue: 0,1:39:43.62,1:39:45.52,Default,,0,0,0,,Món quà cho kẻ thù của Mordor.
Dialogue: 0,1:39:46.22,1:39:48.22,Default,,0,0,0,,Tại sao ta không sử dụng nó?
Dialogue: 0,1:39:48.52,1:39:51.22,Default,,0,0,0,,Đã từ lâu cha tôi,\Nquan nhiếp chính vùng Gondor...
Dialogue: 0,1:39:51.52,1:39:55.62,Default,,0,0,0,,đã giữ chân lực lượng Mordor tại vịnh.\NChính nhờ máu của dân tôi...
Dialogue: 0,1:39:56.12,1:39:58.22,Default,,0,0,0,,đất của các người\Nmới được an toàn.
Dialogue: 0,1:40:00.02,1:40:04.22,Default,,0,0,0,,Hãy giao cho Gondor vũ khí của kẻ thù.\NHãy sử dụng nó chống lại hắn ta.
Dialogue: 0,1:40:04.62,1:40:08.22,Default,,0,0,0,,Anh không thể mang chiếc Nhẫn.\NKhông ai trong chúng ta có thể cả.
Dialogue: 0,1:40:09.02,1:40:12.92,Default,,0,0,0,,Chiếc Nhẫn chỉ trả lời một mình Sauron thôi.\NNó sẽ không nghe ai khác.
Dialogue: 0,1:40:13.32,1:40:16.72,Default,,0,0,0,,Một kẻ du mục thì biết gì chuyện này?
Dialogue: 0,1:40:17.02,1:40:18.92,Default,,0,0,0,,Anh ấy không phải là du mục.
Dialogue: 0,1:40:20.02,1:40:23.52,Default,,0,0,0,,Anh ta là Aragorn,\Ncon trai của Arathorn.
Dialogue: 0,1:40:24.22,1:40:27.02,Default,,0,0,0,,Ngươi phải trung thành với anh ta.
Dialogue: 0,1:40:31.22,1:40:33.12,Default,,0,0,0,,Aragorn.
Dialogue: 0,1:40:34.92,1:40:37.72,Default,,0,0,0,,Dòng dõi của Isildur sao?
Dialogue: 0,1:40:40.12,1:40:42.82,Default,,0,0,0,,Và là người thừa kế ngai vàng\Ncủa Gondor.
Dialogue: 0,1:40:44.42,1:40:46.62,Default,,0,0,0,,Ngồi xuống đi, Legolas.
Dialogue: 0,1:40:49.72,1:40:52.52,Default,,0,0,0,,Gondor không có vua.
Dialogue: 0,1:40:54.52,1:40:56.12,Default,,0,0,0,,Và Gondor không cần vua.
Dialogue: 0,1:41:02.82,1:41:06.32,Default,,0,0,0,,Aragorn nói đúng.\NChúng ta không thể sử dụng nó.
Dialogue: 0,1:41:07.52,1:41:10.82,Default,,0,0,0,,Mọi người chỉ còn\Nmột sự lựa chọn duy nhất.
Dialogue: 0,1:41:11.12,1:41:13.42,Default,,0,0,0,,Chiếc Nhẫn phải bị phá huỷ.
Dialogue: 0,1:41:18.72,1:41:21.22,Default,,0,0,0,,Vậy thì còn chờ gì nữa?
Dialogue: 0,1:41:33.42,1:41:36.32,Default,,0,0,0,,Hỡi Gimli, con trai của Gloin,\Nchiếc Nhẫn không thể bị phá huỷ...
Dialogue: 0,1:41:36.72,1:41:40.82,Default,,0,0,0,,bằng bất kì vũ khí nào\Ncủa chúng ta.
Dialogue: 0,1:41:41.52,1:41:45.42,Default,,0,0,0,,Nó được tạo ra trong ngọn lửa\Ncủa núi Diệt Vong.
Dialogue: 0,1:41:45.92,1:41:49.52,Default,,0,0,0,,Chỉ tại đó nó mới bị tiêu diệt.
Dialogue: 0,1:41:50.82,1:41:53.22,Default,,0,0,0,,Phải tiến sâu vào Mordor...
Dialogue: 0,1:41:53.52,1:41:57.82,Default,,0,0,0,,và quăng nó xuống vực nham thạch\Nnơi nó được sinh ra.
Dialogue: 0,1:41:58.02,1:42:00.32,Default,,0,0,0,,Một chiếc nhẫn\Nthống trị tất cả...
Dialogue: 0,1:42:00.52,1:42:02.42,Default,,0,0,0,,Một trong số các bạn...
Dialogue: 0,1:42:03.02,1:42:05.12,Default,,0,0,0,,phải làm điều này.
Dialogue: 0,1:42:08.12,1:42:11.72,Default,,0,0,0,,Một người không thể dễ dàng\Nbước vào Mordor.
Dialogue: 0,1:42:12.72,1:42:16.22,Default,,0,0,0,,Những cánh cổng đen được canh giữ\Nkhông chỉ bởi lũ Orc.
Dialogue: 0,1:42:17.12,1:42:21.12,Default,,0,0,0,,Sự tà ác ở đó\Nkhông bao giờ nghỉ ngơi.
Dialogue: 0,1:42:21.52,1:42:25.62,Default,,0,0,0,,Và Con Mắt luôn luôn theo dõi.
Dialogue: 0,1:42:26.52,1:42:28.42,Default,,0,0,0,,Nó là một vùng đất cằn cỗi...
Dialogue: 0,1:42:28.82,1:42:32.12,Default,,0,0,0,,được che phủ bởi lửa,\Ntro tàn và cát bụi.
Dialogue: 0,1:42:32.52,1:42:36.32,Default,,0,0,0,,Trong không khí đầy chất độc.
Dialogue: 0,1:42:36.62,1:42:40.72,Default,,0,0,0,,10.000 người cũng không làm được.\NThật điên rồ!
Dialogue: 0,1:42:41.12,1:42:45.42,Default,,0,0,0,,Ngươi không nghe ngài Elrond sao?\NChiếc Nhẫn phải bị tiêu diệt.
Dialogue: 0,1:42:45.82,1:42:48.22,Default,,0,0,0,,Ngươi nghĩ ngươi sẽ\Nlàm được điều đó sao?
Dialogue: 0,1:42:48.52,1:42:53.02,Default,,0,0,0,,Và nếu ta thất bại thì sao? Điều gì xảy ra\Nkhi Sauron lấy lại quyền lực của hắn.
Dialogue: 0,1:42:53.42,1:42:58.52,Default,,0,0,0,,Ta thà chết còn hơn thấy chiếc Nhẫn\Ntrong tay bọn Tiên.
Dialogue: 0,1:43:00.72,1:43:03.12,Default,,0,0,0,,Không bao giờ tin bọn Tiên!
Dialogue: 0,1:43:04.42,1:43:10.02,Default,,0,0,0,,Các người không hiểu à? Trong lúc các người cãi nhau,\Nsức mạnh của Sauron đang tăng lên sao.
Dialogue: 0,1:43:10.52,1:43:13.32,Default,,0,0,0,,Không ai tránh khỏi được!\NTất cả sẽ bị tiêu diệt!
Dialogue: 0,1:43:32.12,1:43:34.12,Default,,0,0,0,,Tôi sẽ mang nó đi.
Dialogue: 0,1:43:36.42,1:43:38.92,Default,,0,0,0,,Tôi sẽ mang nó.
Dialogue: 0,1:43:44.72,1:43:47.72,Default,,0,0,0,,Tôi sẽ đem chiếc Nhẫn\Nđến Mordor.
Dialogue: 0,1:43:56.02,1:43:57.12,Default,,0,0,0,,Mặc dù...
Dialogue: 0,1:44:00.32,1:44:02.12,Default,,0,0,0,,tôi không biết đường.
Dialogue: 0,1:44:04.12,1:44:08.22,Default,,0,0,0,,Ta sẽ giúp cậu gánh trách nhiệm này,\NFrodo Baggins...
Dialogue: 0,1:44:08.62,1:44:11.92,Default,,0,0,0,,chừng nào cậu còn mang nó.
Dialogue: 0,1:44:13.12,1:44:16.12,Default,,0,0,0,,Tôi sẵn sàng hy sinh\Ntính mạng của mình...
Dialogue: 0,1:44:16.92,1:44:18.22,Default,,0,0,0,,để bảo vệ cậu.
Dialogue: 0,1:44:21.52,1:44:23.72,Default,,0,0,0,,Cậu có thanh gươm của tôi.
Dialogue: 0,1:44:25.62,1:44:27.62,Default,,0,0,0,,Và cung của tôi.
Dialogue: 0,1:44:28.32,1:44:30.62,Default,,0,0,0,,Cả rìu của ta nữa.
Dialogue: 0,1:44:37.82,1:44:40.62,Default,,0,0,0,,Anh bạn nhỏ, cậu gánh vác\Nsố phận tất cả chúng ta.
Dialogue: 0,1:44:42.82,1:44:46.32,Default,,0,0,0,,Nếu đây là ý nguyện của Hội Đồng...
Dialogue: 0,1:44:47.02,1:44:49.42,Default,,0,0,0,,thì Gondor sẽ nghe theo.
Dialogue: 0,1:44:52.02,1:44:56.62,Default,,0,0,0,,- Frodo sẽ không đi đâu nếu thiếu tôi.\N- Thật khó chia cắt các cậu...
Dialogue: 0,1:44:57.02,1:45:01.72,Default,,0,0,0,,cho dù cậu ta được triệu tập bởi Hội Đồng,\Ncòn cậu thì không.
Dialogue: 0,1:45:02.42,1:45:04.22,Default,,0,0,0,,Chúng tôi cũng theo nữa!
Dialogue: 0,1:45:06.42,1:45:09.12,Default,,0,0,0,,Trừ khi ông đưa chúng tôi về nhà\Nbằng cách cột vào bao bố.
Dialogue: 0,1:45:09.32,1:45:12.52,Default,,0,0,0,,Dù sao, ông luôn cần\Nngười thông minh để làm...
Dialogue: 0,1:45:13.02,1:45:14.22,Default,,0,0,0,,loại công việc như thế này.
Dialogue: 0,1:45:14.37,1:45:15.77,Default,,0,0,0,,Nhiệm vụ.
Dialogue: 0,1:45:16.27,1:45:17.27,Default,,0,0,0,,Gì cũng được.
Dialogue: 0,1:45:19.07,1:45:21.77,Default,,0,0,0,,Đủ rồi, Pip.
Dialogue: 0,1:45:23.47,1:45:25.27,Default,,0,0,0,,Chín người đồng hành.
Dialogue: 0,1:45:27.07,1:45:28.87,Default,,0,0,0,,Cứ vậy đi.
Dialogue: 0,1:45:29.27,1:45:32.27,Default,,0,0,0,,Các bạn sẽ là\NHội đồng hành của chiếc Nhẫn.
Dialogue: 0,1:45:32.97,1:45:34.77,Default,,0,0,0,,Tuyệt vời!
Dialogue: 0,1:45:35.17,1:45:37.27,Default,,0,0,0,,Chúng ta sẽ đi đâu?
Dialogue: 0,1:46:13.17,1:46:16.17,Default,,0,0,0,,Mẹ anh muốn bảo vệ con mình.
Dialogue: 0,1:46:17.47,1:46:20.87,Default,,0,0,0,,Bà ấy nghĩ anh sẽ an toàn ở Rivendell.
Dialogue: 0,1:46:23.87,1:46:27.27,Default,,0,0,0,,Trong tim mình, bà biết anh sẽ bị\Nsăn đuổi suốt cuộc đời.
Dialogue: 0,1:46:27.87,1:46:29.57,Default,,0,0,0,,Anh không thể thoát khỏi số phận.
Dialogue: 0,1:46:30.07,1:46:33.47,Default,,0,0,0,,Tộc Tiên có thể rèn lại lưỡi kiếm\Ncủa những vị vua...
Dialogue: 0,1:46:34.17,1:46:37.87,Default,,0,0,0,,nhưng chỉ anh mới có\Nsức mạnh sử dụng nó.
Dialogue: 0,1:46:39.07,1:46:41.47,Default,,0,0,0,,Tôi không muốn sức mạnh đó.
Dialogue: 0,1:46:41.77,1:46:44.37,Default,,0,0,0,,Chưa bao giờ muốn.
Dialogue: 0,1:46:45.67,1:46:48.17,Default,,0,0,0,,Anh là người cuối cùng của dòng tộc.\NKhông còn ai khác.
Dialogue: 0,1:46:52.87,1:46:55.07,Default,,0,0,0,,Thanh kiếm cũ của ta, Sting.
Dialogue: 0,1:46:55.47,1:46:56.57,Default,,0,0,0,,Đây, cầm lấy, cầm lấy!
Dialogue: 0,1:47:01.97,1:47:05.77,Default,,0,0,0,,- Nó nhẹ thật!\N- Phải, nó được làm bởi tộc Tiên đấy.
Dialogue: 0,1:47:06.57,1:47:10.57,Default,,0,0,0,,Thanh kiếm rực lên ánh sáng xanh\Nkhi bọn Orc ở gần.
Dialogue: 0,1:47:11.07,1:47:15.47,Default,,0,0,0,,Và vào lúc đó, cháu trai,\Ncháu phải cực kỳ cẩn thận.
Dialogue: 0,1:47:15.77,1:47:18.17,Default,,0,0,0,,Đây là thứ rất tuyệt.
Dialogue: 0,1:47:18.67,1:47:19.87,Default,,0,0,0,,Áo giáp Mithril.
Dialogue: 0,1:47:20.77,1:47:24.97,Default,,0,0,0,,Nhẹ như lông vũ,\Nvà cứng như vảy rồng.
Dialogue: 0,1:47:25.37,1:47:28.47,Default,,0,0,0,,Ta muốn thấy cháu mặc vào, mau lên.
Dialogue: 0,1:47:36.67,1:47:38.67,Default,,0,0,0,,Chiếc Nhẫn cũ của ta!
Dialogue: 0,1:47:39.97,1:47:43.77,Default,,0,0,0,,Ta rất... rất muốn...
Dialogue: 0,1:47:44.17,1:47:47.97,Default,,0,0,0,,được cầm nó, một lần cuối.
Dialogue: 0,1:48:05.47,1:48:08.97,Default,,0,0,0,,Ta xin lỗi đã để nó lại\Ncho cháu, cháu trai.
Dialogue: 0,1:48:09.47,1:48:13.37,Default,,0,0,0,,Ta xin lỗi khi để cháu\Nmang gánh nặng này.
Dialogue: 0,1:48:18.17,1:48:21.07,Default,,0,0,0,,Xin lỗi cháu về mọi việc!
Dialogue: 0,1:48:36.07,1:48:40.27,Default,,0,0,0,,Người mang Nhẫn có nhiệm vụ\Nđến ngọn Diệt Vong.
Dialogue: 0,1:48:41.67,1:48:44.17,Default,,0,0,0,,Những ai đi cùng cậu ấy,\Nkhông cần tuyên thệ,
Dialogue: 0,1:48:44.57,1:48:47.57,Default,,0,0,0,,không bị ràng buộc, hãy cố gắng\Nhỗ trợ cậu ấy hết mức có thể.
Dialogue: 0,1:48:51.07,1:48:53.47,Default,,0,0,0,,Tốt lắm, hãy nhớ mục đích của các bạn.
Dialogue: 0,1:48:53.97,1:48:57.77,Default,,0,0,0,,Những lời chúc từ tộc Tiên\Nvà tộc Người...
Dialogue: 0,1:48:58.57,1:49:01.07,Default,,0,0,0,,và từ tất cả cư dân tự do\Nsẽ đi cùng với các bạn.
Dialogue: 0,1:49:04.17,1:49:08.07,Default,,0,0,0,,Hội đồng hành\Nchờ đợi người mang Nhẫn.
Dialogue: 0,1:49:26.77,1:49:29.17,Default,,0,0,0,,Gandalf, Mordor ở bên phải hay trái?
Dialogue: 0,1:49:29.77,1:49:31.57,Default,,0,0,0,,Trái.
Dialogue: 0,1:50:51.27,1:50:55.37,Default,,0,0,0,,Chúng ta tiếp tục hướng này, về phía tây\Ncủa dãy Sương Mù, trong khoảng 40 ngày.
Dialogue: 0,1:50:55.87,1:50:59.37,Default,,0,0,0,,Nếu may mắn, Khe núi của Rohan\Nsẽ vẫn còn mở.
Dialogue: 0,1:50:59.87,1:51:03.37,Default,,0,0,0,,Từ đó, rẽ về phía đông tới Mordor.
Dialogue: 0,1:51:03.57,1:51:07.27,Default,,0,0,0,,Hai, một, năm.\NTốt. Rất tốt.
Dialogue: 0,1:51:09.27,1:51:10.27,Default,,0,0,0,,Di chuyển chân.
Dialogue: 0,1:51:10.47,1:51:11.77,Default,,0,0,0,,- Cậu trông khá lắm, Pippin.\N- Cám ơn.
Dialogue: 0,1:51:12.07,1:51:13.47,Default,,0,0,0,,Nhanh hơn!
Dialogue: 0,1:51:14.77,1:51:18.47,Default,,0,0,0,,Nếu có ai hỏi ý kiến tôi,\Nmà theo tôi thì chắc không có ai,
Dialogue: 0,1:51:18.77,1:51:21.37,Default,,0,0,0,,tôi sẽ nói rằng ta đang đi\Ncon đường vòng rất xa đấy.
Dialogue: 0,1:51:22.07,1:51:24.97,Default,,0,0,0,,Gandalf, ta có thể băng qua\Nkhu mỏ của Moria.
Dialogue: 0,1:51:25.67,1:51:29.37,Default,,0,0,0,,Anh họ tôi Balin sẽ đón ta\Nvới nghi thức hoàng gia.
Dialogue: 0,1:51:29.77,1:51:34.97,Default,,0,0,0,,Không, Gimli, ta sẽ không đi qua Moria,\Ntrừ phi không còn sự lựa chọn nào khác.
Dialogue: 0,1:51:44.47,1:51:45.57,Default,,0,0,0,,Xin lỗi!
Dialogue: 0,1:51:46.27,1:51:47.77,Default,,0,0,0,,Bắt lấy anh ta!
Dialogue: 0,1:51:49.27,1:51:51.57,Default,,0,0,0,,- Vì vùng Shire!\N- Giữ anh ấy, giữ anh ấy lại, Merry!
Dialogue: 0,1:51:52.27,1:51:54.47,Default,,0,0,0,,Các ngài, đủ rồi đấy.
Dialogue: 0,1:51:58.37,1:51:59.87,Default,,0,0,0,,Biết tay ta chưa!\NBiết tay ta chưa!
Dialogue: 0,1:52:00.37,1:52:02.97,Default,,0,0,0,,- Cái gì vậy?\N- Chẳng có gì, chỉ là đám mây nhỏ thôi mà.
Dialogue: 0,1:52:03.37,1:52:05.87,Default,,0,0,0,,Chúng đang di chuyển nhanh.
Dialogue: 0,1:52:06.27,1:52:08.37,Default,,0,0,0,,Ngược hướng gió.
Dialogue: 0,1:52:09.17,1:52:11.27,Default,,0,0,0,,- Quạ Crebain từ Dunland!\N- Trốn mau!
Dialogue: 0,1:52:11.57,1:52:14.17,Default,,0,0,0,,- Mau lên!\N- Nấp mau!
Dialogue: 0,1:52:47.77,1:52:49.37,Default,,0,0,0,,Gián điệp của Saruman.
Dialogue: 0,1:52:50.17,1:52:53.27,Default,,0,0,0,,Con đường xuống phía Nam\Nđã bị theo dõi.
Dialogue: 0,1:52:54.57,1:52:57.87,Default,,0,0,0,,Ta phải băng qua\Nđèo Caradhras thôi.
Dialogue: 0,1:53:10.47,1:53:12.07,Default,,0,0,0,,Frodo!
Dialogue: 0,1:53:31.17,1:53:33.37,Default,,0,0,0,,Boromir.
Dialogue: 0,1:53:33.67,1:53:37.87,Default,,0,0,0,,Thật kì lạ khi chúng ta phải\Nchịu đựng nỗi sợ hãi và nghi ngờ...
Dialogue: 0,1:53:38.07,1:53:41.07,Default,,0,0,0,,vì một vật nhỏ thế này.
Dialogue: 0,1:53:44.47,1:53:46.37,Default,,0,0,0,,Quả là nhỏ bé thật.
Dialogue: 0,1:53:46.67,1:53:48.07,Default,,0,0,0,,Boromir!
Dialogue: 0,1:53:48.77,1:53:51.27,Default,,0,0,0,,Trả chiếc Nhẫn cho Frodo.
Dialogue: 0,1:54:01.27,1:54:03.17,Default,,0,0,0,,Được rồi.
Dialogue: 0,1:54:03.97,1:54:05.97,Default,,0,0,0,,Ta không quan tâm đâu.
Dialogue: 0,1:54:41.37,1:54:45.87,Default,,0,0,0,,Gandalf, ngươi đang cố dẫn họ\Nbăng qua Caradhras.
Dialogue: 0,1:54:46.27,1:54:50.77,Default,,0,0,0,,Và nếu thất bại ngươi sẽ đi đâu?
Dialogue: 0,1:54:52.27,1:54:55.07,Default,,0,0,0,,Nếu dãy núi đánh bại các ngươi,
Dialogue: 0,1:54:55.37,1:54:59.67,Default,,0,0,0,,các ngươi có dám đi\Ncon đường khác nguy hiểm hơn không?
Dialogue: 0,1:55:04.97,1:55:11.97,Default,,0,0,0,,Hãy thức giấc,\Nhỡi ngọn Redhorn hung ác!
Dialogue: 0,1:55:12.27,1:55:14.97,Default,,0,0,0,,Có giọng nói trong không trung!
Dialogue: 0,1:55:15.07,1:55:16.17,Default,,0,0,0,,Và hãy nhuốm máu bọn chúng!
Dialogue: 0,1:55:16.27,1:55:17.67,Default,,0,0,0,,Là Saruman!
Dialogue: 0,1:55:26.67,1:55:31.07,Default,,0,0,0,,Hắn đang làm sập quả núi này!\NGandalf, chúng ta phải quay lại!
Dialogue: 0,1:55:31.47,1:55:33.27,Default,,0,0,0,,Không!
Dialogue: 0,1:55:35.37,1:55:41.37,Default,,0,0,0,,Ngủ yên hỡi dãy Caradhras,\Nhãy kiềm chế cơn thịnh nộ của mình!
Dialogue: 0,1:55:41.67,1:55:46.67,Default,,0,0,0,,Hãy thức giấc,\Nhỡi ngọn Redhorn hung ác!
Dialogue: 0,1:55:46.87,1:55:51.17,Default,,0,0,0,,Những chỏm núi hung tợn kia...
Dialogue: 0,1:55:51.37,1:55:54.87,Default,,0,0,0,,hãy rơi xuống đầu quân thù!
Dialogue: 0,1:56:39.77,1:56:41.57,Default,,0,0,0,,Ta phải rời khỏi ngọn núi!
Dialogue: 0,1:56:42.07,1:56:46.27,Default,,0,0,0,,Tới Khe núi của Rohan, đi về\Ncon đường phía Tây tới thành phố của tôi!
Dialogue: 0,1:56:46.67,1:56:49.87,Default,,0,0,0,,Khe núi Rohan ở quá gần Isengard!
Dialogue: 0,1:56:50.17,1:56:53.77,Default,,0,0,0,,Nếu không thể đi qua ngọn núi,\Nthì chúng ta hãy đi bên dưới nó.
Dialogue: 0,1:56:54.17,1:56:56.87,Default,,0,0,0,,Băng qua hầm mỏ Moria.
Dialogue: 0,1:56:58.77,1:57:02.37,Default,,0,0,0,,Moria.\NNgươi sợ đi trong hầm mỏ đó.
Dialogue: 0,1:57:03.27,1:57:08.07,Default,,0,0,0,,Người Lùn đã đào bới quá tham\Nvà quá sâu.
Dialogue: 0,1:57:09.17,1:57:13.87,Default,,0,0,0,,Ngươi biết họ sẽ đánh thức điều gì\Ntrong bóng tối của Khazad-dum:
Dialogue: 0,1:57:15.27,1:57:18.57,Default,,0,0,0,,Bóng tối và Lửa.
Dialogue: 0,1:57:22.17,1:57:24.77,Default,,0,0,0,,Hãy để người mang Nhẫn định đoạt.
Dialogue: 0,1:57:29.57,1:57:31.67,Default,,0,0,0,,Chúng ta không thể ở đây!
Dialogue: 0,1:57:31.87,1:57:34.57,Default,,0,0,0,,Đây sẽ là mồ chôn\Ncủa người Hobbit.
Dialogue: 0,1:57:34.87,1:57:36.47,Default,,0,0,0,,Frodo?
Dialogue: 0,1:57:39.57,1:57:42.27,Default,,0,0,0,,Chúng ta sẽ băng qua hầm mỏ.
Dialogue: 0,1:57:43.27,1:57:45.07,Default,,0,0,0,,Vậy thì đi.
Dialogue: 0,1:57:51.77,1:57:55.47,Default,,0,0,0,,Frodo, tới đây giúp ta nào.
Dialogue: 0,1:57:59.17,1:58:01.27,Default,,0,0,0,,Vai của cháu sao rồi?
Dialogue: 0,1:58:01.57,1:58:04.67,Default,,0,0,0,,- Đỡ hơn rồi.\N- Và chiếc Nhẫn?
Dialogue: 0,1:58:06.37,1:58:08.87,Default,,0,0,0,,Cháu cảm thấy sức mạnh của nó\Nđang gia tăng, phải không?
Dialogue: 0,1:58:09.37,1:58:12.17,Default,,0,0,0,,Ta cũng cảm thấy thế.\NCháu phải cẩn thận.
Dialogue: 0,1:58:12.77,1:58:16.47,Default,,0,0,0,,Quỷ dữ sẽ tìm đến cháu\Ntừ bên ngoài Hội đồng hành.
Dialogue: 0,1:58:16.78,1:58:19.28,Default,,0,0,0,,Và ta sợ nó cũng đến\Ntừ bên trong nữa.
Dialogue: 0,1:58:22.08,1:58:26.08,Default,,0,0,0,,- Vậy cháu nên tin ai?\N- Phải tin chính bản thân mình.
Dialogue: 0,1:58:26.38,1:58:29.08,Default,,0,0,0,,Tin chính sức mạnh của cháu.
Dialogue: 0,1:58:29.48,1:58:31.98,Default,,0,0,0,,- Ý ông là sao?\N- Có nhiều loại sức mạnh trên thế giới này,
Dialogue: 0,1:58:32.18,1:58:34.18,Default,,0,0,0,,có thể là tốt hoặc xấu.
Dialogue: 0,1:58:34.48,1:58:36.68,Default,,0,0,0,,Một số còn mạnh hơn cả ta.
Dialogue: 0,1:58:37.28,1:58:40.68,Default,,0,0,0,,Và một số khác\Nta chưa từng trải nghiệm.
Dialogue: 0,1:58:42.28,1:58:43.58,Default,,0,0,0,,Tường thành...
Dialogue: 0,1:58:44.38,1:58:46.38,Default,,0,0,0,,của Moria.
Dialogue: 0,1:58:52.18,1:58:55.08,Default,,0,0,0,,Tường thành của người Lùn\Nkhi đóng lại sẽ trở nên vô hình.
Dialogue: 0,1:58:55.38,1:58:59.58,Default,,0,0,0,,Vâng, Gimli, ngay cả người chủ\Ncũng không tìm ra nếu họ quên.
Dialogue: 0,1:59:00.18,1:59:02.58,Default,,0,0,0,,Tại sao tôi không ngạc nhiên nhỉ?
Dialogue: 0,1:59:10.58,1:59:13.78,Default,,0,0,0,,Chà, để xem nào.
Dialogue: 0,1:59:14.18,1:59:15.58,Default,,0,0,0,,Ithildin.
Dialogue: 0,1:59:16.28,1:59:19.48,Default,,0,0,0,,Nó chỉ phản chiếu ánh sáng\Ncủa trăng và sao.
Dialogue: 0,1:59:33.68,1:59:38.08,Default,,0,0,0,,Nó viết: "Cánh cửa của Durin,\NChúa tể của Moria."
Dialogue: 0,1:59:38.38,1:59:43.08,Default,,0,0,0,,- "Hãy nói, người bạn, và bước vào."\N- Ông nghĩ đó là gì?
Dialogue: 0,1:59:43.28,1:59:47.78,Default,,0,0,0,,Đơn giản, nếu là bạn, thì phải nói mật khẩu\Nvà cánh cửa sẽ mở.
Dialogue: 0,1:59:48.48,1:59:53.58,Default,,0,0,0,,Cổng của tộc Tiên,\Nhãy mở ra cho ta!
Dialogue: 0,2:00:04.28,2:00:08.18,Default,,0,0,0,,Hãy nghe lệnh ta.
Dialogue: 0,2:00:11.68,2:00:12.68,Default,,0,0,0,,Không có gì xảy ra cả.
Dialogue: 0,2:00:19.88,2:00:23.88,Default,,0,0,0,,Ta từng biết mọi câu thần chú\Ntrong ngôn ngữ tộc Tiên,
Dialogue: 0,2:00:24.38,2:00:27.08,Default,,0,0,0,,tộc Người và tộc Orc.
Dialogue: 0,2:00:27.38,2:00:28.58,Default,,0,0,0,,Ông định làm gì đây?
Dialogue: 0,2:00:28.78,2:00:30.98,Default,,0,0,0,,Đập đầu cậu vào cánh cửa,\NPeregrin Took!
Dialogue: 0,2:00:31.28,2:00:33.18,Default,,0,0,0,,Nếu cánh cửa không vỡ ra...
Dialogue: 0,2:00:33.48,2:00:36.08,Default,,0,0,0,,thì ít ra ta cũng thoát khỏi\Nnhững câu hỏi ngốc nghếch của cậu...
Dialogue: 0,2:00:36.38,2:00:39.68,Default,,0,0,0,,và ta sẽ tiếp tục tìm\Nnhững ngôn từ mở cửa.
Dialogue: 0,2:00:43.28,2:00:45.28,Default,,0,0,0,,Cổng của tộc Tiên...
Dialogue: 0,2:00:46.98,2:00:49.48,Default,,0,0,0,,... nghe lệnh ta...
Dialogue: 0,2:00:49.98,2:00:52.18,Default,,0,0,0,,... bậu cửa của người Lùn...
Dialogue: 0,2:00:52.68,2:00:55.08,Default,,0,0,0,,Hầm mỏ không có chỗ cho ngựa.
Dialogue: 0,2:00:55.48,2:00:59.28,Default,,0,0,0,,- Ngay cả Bill dũng cảm cũng vậy.\N- Tạm biệt, Bill.
Dialogue: 0,2:00:59.48,2:01:00.68,Default,,0,0,0,,Lối đi của người Lùn...
Dialogue: 0,2:01:00.78,2:01:03.38,Default,,0,0,0,,Đi đi, Bill, đi đi.
Dialogue: 0,2:01:04.08,2:01:07.58,Default,,0,0,0,,Đừng lo, Sam, nó biết đường về mà.
Dialogue: 0,2:01:14.48,2:01:16.88,Default,,0,0,0,,Đừng làm khuấy động mặt nước.
Dialogue: 0,2:01:17.18,2:01:19.08,Default,,0,0,0,,Ôi, chẳng có tác dụng gì.
Dialogue: 0,2:01:31.78,2:01:33.88,Default,,0,0,0,,Thật hóc búa.
Dialogue: 0,2:01:36.68,2:01:39.78,Default,,0,0,0,,"Hãy nói, người bạn, và bước vào."
Dialogue: 0,2:01:40.28,2:01:43.08,Default,,0,0,0,,Tiếng tộc Tiên nói từ "Bạn" thế nào?
Dialogue: 0,2:01:44.48,2:01:45.78,Default,,0,0,0,,Mellon.
Dialogue: 0,2:02:03.88,2:02:08.58,Default,,0,0,0,,Sớm thôi, ngài Tiên ạ, ngài sẽ thấy lòng\Nhiếu khách của người Lùn.
Dialogue: 0,2:02:09.18,2:02:14.68,Default,,0,0,0,,Lửa bùng cháy, bia mạch nha,\Nthịt chín rút xương.
Dialogue: 0,2:02:16.48,2:02:19.38,Default,,0,0,0,,Ông bạn, đây là nhà\Nngười anh họ tôi, Balin.
Dialogue: 0,2:02:19.98,2:02:22.28,Default,,0,0,0,,Vậy mà họ gọi đây là hầm mỏ.
Dialogue: 0,2:02:22.48,2:02:24.38,Default,,0,0,0,,Một hầm mỏ!
Dialogue: 0,2:02:25.58,2:02:28.68,Default,,0,0,0,,Không còn là hầm mỏ nữa.\NĐây là một ngôi mộ.
Dialogue: 0,2:02:34.98,2:02:36.48,Default,,0,0,0,,Không...
Dialogue: 0,2:02:37.78,2:02:39.38,Default,,0,0,0,,Không!
Dialogue: 0,2:02:42.18,2:02:43.58,Default,,0,0,0,,Lũ Goblin!
Dialogue: 0,2:02:47.58,2:02:51.68,Default,,0,0,0,,Chúng ta nên đi đến Khe núi Rohan,\Nchứ không nên đến đây.
Dialogue: 0,2:02:52.88,2:02:55.38,Default,,0,0,0,,Ra khỏi đây ngay.\NRa ngay!
Dialogue: 0,2:02:58.28,2:03:00.58,Default,,0,0,0,,- Frodo!\N- Cứu!
Dialogue: 0,2:03:00.98,2:03:02.48,Default,,0,0,0,,Strider!
Dialogue: 0,2:03:02.68,2:03:05.28,Default,,0,0,0,,- Cứu với!\N- Cứu cậu ấy!
Dialogue: 0,2:03:05.58,2:03:06.58,Default,,0,0,0,,Aragorn!
Dialogue: 0,2:03:17.08,2:03:18.08,Default,,0,0,0,,Frodo!
Dialogue: 0,2:03:44.38,2:03:45.48,Default,,0,0,0,,Vào trong hầm mỏ!
Dialogue: 0,2:03:45.68,2:03:48.78,Default,,0,0,0,,- Legolas!\N- Vào hang đi!
Dialogue: 0,2:03:53.18,2:03:54.88,Default,,0,0,0,,Chạy!
Dialogue: 0,2:04:11.88,2:04:15.38,Default,,0,0,0,,Chúng ta chỉ còn một lựa chọn.
Dialogue: 0,2:04:15.68,2:04:20.28,Default,,0,0,0,,Phải đối diện với bóng tối của Moria.
Dialogue: 0,2:04:20.78,2:04:22.68,Default,,0,0,0,,Hãy cảnh giác.
Dialogue: 0,2:04:22.98,2:04:26.28,Default,,0,0,0,,Có những thứ cổ xưa và\Nhôi thối hơn cả lũ Orc...
Dialogue: 0,2:04:26.68,2:04:29.98,Default,,0,0,0,,trong những nơi sâu thẳm\Ncủa thế giới.
Dialogue: 0,2:04:38.08,2:04:41.78,Default,,0,0,0,,Im lặng nào. Mất bốn ngày\Nđể tới được bên kia.
Dialogue: 0,2:04:42.28,2:04:45.88,Default,,0,0,0,,Hãy hy vọng chúng chưa biết\Nsự hiện diện của ta.
Dialogue: 0,2:05:31.78,2:05:35.38,Default,,0,0,0,,Của cải tại Moria không phải là vàng...
Dialogue: 0,2:05:35.78,2:05:38.08,Default,,0,0,0,,hay trang sức...
Dialogue: 0,2:05:38.28,2:05:41.28,Default,,0,0,0,,mà là Mithril.
Dialogue: 0,2:05:59.78,2:06:04.38,Default,,0,0,0,,Bilbo có tấm áo bằng sợi mithril\Nmà Thorin đưa cho.
Dialogue: 0,2:06:04.88,2:06:08.68,Default,,0,0,0,,- Ồ, một món quà hoàng gia.\N- Đúng vậy.
Dialogue: 0,2:06:08.88,2:06:10.78,Default,,0,0,0,,Ta chưa nói cho ông ấy biết rằng...
Dialogue: 0,2:06:11.08,2:06:15.78,Default,,0,0,0,,giá trị của nó còn lớn hơn\Ncả toàn vùng Shire.
Dialogue: 0,2:06:28.28,2:06:29.98,Default,,0,0,0,,Pippin!
Dialogue: 0,2:06:50.38,2:06:53.18,Default,,0,0,0,,Ta không còn ký ức gì về nơi này nữa.
Dialogue: 0,2:06:55.38,2:06:56.78,Default,,0,0,0,,- Chúng ta bị lạc à?\N- Không.
Dialogue: 0,2:06:57.08,2:06:59.48,Default,,0,0,0,,- Chắc vậy rồi.\N- Suỵt, Gandalf đang suy nghĩ.
Dialogue: 0,2:06:59.78,2:07:01.18,Default,,0,0,0,,- Merry?\N- Sao?
Dialogue: 0,2:07:01.48,2:07:03.28,Default,,0,0,0,,Tôi đói quá.
Dialogue: 0,2:07:14.78,2:07:18.98,Default,,0,0,0,,- Có gì ở dưới đó.\N- Là Gollum.
Dialogue: 0,2:07:19.38,2:07:23.28,Default,,0,0,0,,- Gollum?\N- Nó đã theo ta ba ngày nay.
Dialogue: 0,2:07:23.78,2:07:27.78,Default,,0,0,0,,- Hắn đã thoát khỏi ngục tù Barad-dur?\N- Thoát ư?
Dialogue: 0,2:07:28.58,2:07:30.18,Default,,0,0,0,,Được thả đi thì đúng hơn.
Dialogue: 0,2:07:30.78,2:07:34.28,Default,,0,0,0,,Giờ chiếc Nhẫn đã dẫn nó đến đây.
Dialogue: 0,2:07:36.98,2:07:39.68,Default,,0,0,0,,Nó sẽ không từ bỏ\Nlòng ham muốn với chiếc Nhẫn.
Dialogue: 0,2:07:40.08,2:07:45.48,Default,,0,0,0,,Hắn căm ghét và yêu thích chiếc Nhẫn,\Nnhư căm ghét và yêu thích chính bản thân mình.
Dialogue: 0,2:07:45.88,2:07:50.58,Default,,0,0,0,,Cuộc đời của Smeagol\Nlà một câu chuyện buồn.
Dialogue: 0,2:07:50.98,2:07:56.68,Default,,0,0,0,,Phải, Smeagol, hắn từng được gọi thế\Ntrước khi chiếc Nhẫn tìm đến.
Dialogue: 0,2:07:57.88,2:08:00.58,Default,,0,0,0,,Trước khi nó biến hắn thành điên dại.
Dialogue: 0,2:08:00.88,2:08:03.68,Default,,0,0,0,,Bilbo đã thương hại,\Nkhông giết hắn khi có cơ hội.
Dialogue: 0,2:08:03.98,2:08:05.08,Default,,0,0,0,,Thương hại à?
Dialogue: 0,2:08:06.18,2:08:09.08,Default,,0,0,0,,Đó là việc của Bilbo.
Dialogue: 0,2:08:10.08,2:08:15.08,Default,,0,0,0,,Nhiều người sống đáng ra nên chết đi,\Nmột số đáng sống lại phải chết.
Dialogue: 0,2:08:16.78,2:08:19.18,Default,,0,0,0,,Cậu có dám phán xử họ không, Frodo?
Dialogue: 0,2:08:21.68,2:08:25.48,Default,,0,0,0,,Đừng quá tự tin khi\Nphán quyết ai đó bằng cái chết.
Dialogue: 0,2:08:25.78,2:08:28.58,Default,,0,0,0,,Ngay cả người thông thái nhất\Ncũng không thể thấy hết mọi kết thúc.
Dialogue: 0,2:08:29.28,2:08:34.98,Default,,0,0,0,,Linh cảm cho ta biết Gollum có\Nvai trò nào đó, dù tốt hay xấu...
Dialogue: 0,2:08:36.08,2:08:38.78,Default,,0,0,0,,trước khi việc này chấm dứt.
Dialogue: 0,2:08:40.38,2:08:44.08,Default,,0,0,0,,Lòng thương hại của Bilbo có thể\Nthay đổi số phận rất nhiều người.
Dialogue: 0,2:08:50.98,2:08:54.18,Default,,0,0,0,,Cháu ước gì chiếc Nhẫn\Nđừng đến với cháu.
Dialogue: 0,2:08:55.68,2:08:57.68,Default,,0,0,0,,Cháu ước không có chuyện gì xảy ra.
Dialogue: 0,2:08:57.88,2:09:02.68,Default,,0,0,0,,Ai rồi cũng gặp những vấn đề.\NHọ không quyết định được chuyện đó.
Dialogue: 0,2:09:03.78,2:09:09.08,Default,,0,0,0,,Họ chỉ quyết định sẽ hành động ra sao\Ntrước những việc như thế.
Dialogue: 0,2:09:10.78,2:09:14.58,Default,,0,0,0,,Còn nhiều thế lực trên thế giới này,\NFrodo, bên cạnh quỷ dữ.
Dialogue: 0,2:09:15.18,2:09:17.58,Default,,0,0,0,,Số phận cho Bilbo tìm được chiếc Nhẫn.
Dialogue: 0,2:09:18.08,2:09:22.28,Default,,0,0,0,,Có nghĩa là Số phận giao nó cho cậu.
Dialogue: 0,2:09:22.68,2:09:25.68,Default,,0,0,0,,Đó là nghĩ theo hướng tích cực.
Dialogue: 0,2:09:30.68,2:09:32.58,Default,,0,0,0,,Là lối này.
Dialogue: 0,2:09:32.88,2:09:35.08,Default,,0,0,0,,- Ông ta đã nhớ rồi.\N- Không.
Dialogue: 0,2:09:35.38,2:09:38.98,Default,,0,0,0,,Chỉ là không khí dưới này\Nkhông có mùi dơ bẩn.
Dialogue: 0,2:09:39.28,2:09:44.28,Default,,0,0,0,,Nếu nghi ngờ, Meriadoc,\Nhãy nghe theo mũi cậu.
Dialogue: 0,2:09:59.38,2:10:03.38,Default,,0,0,0,,Liều thêm một tí ánh sáng nhé.
Dialogue: 0,2:10:08.18,2:10:10.18,Default,,0,0,0,,Phía trước...
Dialogue: 0,2:10:10.68,2:10:15.78,Default,,0,0,0,,là vương quốc vĩ đại của người Lùn\Nthành phố Dwarrowdelf.
Dialogue: 0,2:10:18.38,2:10:21.68,Default,,0,0,0,,Không thể nhầm được.
Dialogue: 0,2:10:46.78,2:10:48.48,Default,,0,0,0,,Gimli!
Dialogue: 0,2:10:55.88,2:10:57.48,Default,,0,0,0,,Không!
Dialogue: 0,2:11:00.08,2:11:03.18,Default,,0,0,0,,Ôi, không!
Dialogue: 0,2:11:05.08,2:11:06.28,Default,,0,0,0,,Không!
Dialogue: 0,2:11:13.68,2:11:15.58,Default,,0,0,0,,"Nơi đây an nghỉ Balin...
Dialogue: 0,2:11:16.08,2:11:18.08,Default,,0,0,0,,con trai của Fundin,
Dialogue: 0,2:11:18.48,2:11:20.98,Default,,0,0,0,,Lãnh chúa của Moria."
Dialogue: 0,2:11:22.08,2:11:23.68,Default,,0,0,0,,Ông ta chết rồi.
Dialogue: 0,2:11:25.58,2:11:27.18,Default,,0,0,0,,Như ta đã lo sợ.
Dialogue: 0,2:11:45.48,2:11:48.28,Default,,0,0,0,,Chúng ta phải đi tiếp,\Nkhông được chậm trễ.
Dialogue: 0,2:11:48.58,2:11:53.08,Default,,0,0,0,,"Chúng đã chiếm cây cầu và\Nsảnh thứ hai.
Dialogue: 0,2:11:54.38,2:11:56.88,Default,,0,0,0,,Chúng tôi cố thủ tại cổng...
Dialogue: 0,2:11:57.58,2:11:59.98,Default,,0,0,0,,nhưng không thể giữ được lâu.
Dialogue: 0,2:12:00.28,2:12:02.98,Default,,0,0,0,,Mặt đất rung chuyển.
Dialogue: 0,2:12:03.58,2:12:04.98,Default,,0,0,0,,Tiếng trống...
Dialogue: 0,2:12:05.48,2:12:08.68,Default,,0,0,0,,những tiếng trống từ dưới sâu.
Dialogue: 0,2:12:12.48,2:12:14.38,Default,,0,0,0,,Chúng tôi không thể thoát.
Dialogue: 0,2:12:16.48,2:12:20.18,Default,,0,0,0,,Một bóng đen xuất hiện trong đêm tối.
Dialogue: 0,2:12:21.98,2:12:24.48,Default,,0,0,0,,Chúng tôi không thể thoát
Dialogue: 0,2:12:27.58,2:12:29.38,Default,,0,0,0,,Chúng đang đến!"
Dialogue: 0,2:13:12.58,2:13:14.38,Default,,0,0,0,,Took ngu ngốc!
Dialogue: 0,2:13:14.68,2:13:18.38,Default,,0,0,0,,Lần sau hãy biến đi,\Nđừng gây phiền phức cho chúng ta nữa!
Dialogue: 0,2:13:44.58,2:13:46.18,Default,,0,0,0,,Frodo!
Dialogue: 0,2:13:48.58,2:13:49.98,Default,,0,0,0,,Bọn Orc!
Dialogue: 0,2:13:57.08,2:13:58.98,Default,,0,0,0,,Lùi lại!\NHãy ở gần Gandalf!
Dialogue: 0,2:14:06.28,2:14:07.98,Default,,0,0,0,,Chúng có Quỉ Khổng Lồ.
Dialogue: 0,2:14:23.78,2:14:24.98,Default,,0,0,0,,Cứ để chúng đến!
Dialogue: 0,2:14:25.38,2:14:28.78,Default,,0,0,0,,Vẫn còn Người Lùn tại Moria này!
Dialogue: 0,2:16:47.88,2:16:50.58,Default,,0,0,0,,Tôi nghĩ tôi lo được bọn này.
Dialogue: 0,2:16:55.38,2:16:57.08,Default,,0,0,0,,Frodo!
Dialogue: 0,2:17:30.58,2:17:32.38,Default,,0,0,0,,Aragorn! Aragorn!
Dialogue: 0,2:17:33.48,2:17:35.28,Default,,0,0,0,,Frodo!
Dialogue: 0,2:18:20.48,2:18:22.38,Default,,0,0,0,,Frodo!
Dialogue: 0,2:19:22.68,2:19:23.98,Default,,0,0,0,,Không!
Dialogue: 0,2:19:32.38,2:19:34.18,Default,,0,0,0,,Cậu ấy còn sống!
Dialogue: 0,2:19:36.78,2:19:39.58,Default,,0,0,0,,Tôi không sao,\Nkhông bị thương gì cả.
Dialogue: 0,2:19:39.88,2:19:41.98,Default,,0,0,0,,Cậu đáng lẽ đã chết!
Dialogue: 0,2:19:42.38,2:19:44.68,Default,,0,0,0,,Ngọn giáo đó đủ lấy mạng\Nmột con heo rừng.
Dialogue: 0,2:19:44.98,2:19:49.58,Default,,0,0,0,,Tôi nghĩ cậu Hobbit này\Nđặc biệt hơn vẻ bên ngoài đấy.
Dialogue: 0,2:19:55.58,2:19:56.68,Default,,0,0,0,,Áo giáp Mithril!
Dialogue: 0,2:19:59.48,2:20:01.98,Default,,0,0,0,,Bất ngờ đấy, cậu Baggins à!
Dialogue: 0,2:20:07.98,2:20:09.78,Default,,0,0,0,,Tới cây cầu Khazad-dum!
Dialogue: 0,2:20:24.98,2:20:26.58,Default,,0,0,0,,Lối này!
Dialogue: 0,2:21:37.08,2:21:39.88,Default,,0,0,0,,Lại yêu thuật gì nữa đây?
Dialogue: 0,2:21:56.08,2:21:58.88,Default,,0,0,0,,Một con Balrog.
Dialogue: 0,2:21:59.18,2:22:02.28,Default,,0,0,0,,Quái vật của thế giới cổ xưa.
Dialogue: 0,2:22:04.58,2:22:06.88,Default,,0,0,0,,Kẻ địch này là quá sức\Nvới tất cả chúng ta.
Dialogue: 0,2:22:08.18,2:22:09.68,Default,,0,0,0,,Chạy!
Dialogue: 0,2:22:16.98,2:22:18.48,Default,,0,0,0,,Nhanh lên!
Dialogue: 0,2:22:35.18,2:22:36.48,Default,,0,0,0,,Gandalf!
Dialogue: 0,2:22:37.08,2:22:38.58,Default,,0,0,0,,Dẫn dắt họ đi, Aragorn!
Dialogue: 0,2:22:40.08,2:22:42.58,Default,,0,0,0,,Gần tới cầu rồi!
Dialogue: 0,2:22:44.58,2:22:48.58,Default,,0,0,0,,Làm như ta bảo đi!\NCung kiếm không giúp gì được đâu!
Dialogue: 0,2:23:17.78,2:23:19.48,Default,,0,0,0,,Gandalf!
Dialogue: 0,2:23:37.88,2:23:39.08,Default,,0,0,0,,Merry! Pippin!
Dialogue: 0,2:23:50.28,2:23:51.68,Default,,0,0,0,,Sam!
Dialogue: 0,2:23:54.48,2:23:57.58,Default,,0,0,0,,Người Lùn không cần ai giúp.
Dialogue: 0,2:24:00.78,2:24:02.18,Default,,0,0,0,,Đừng nắm râu ta!
Dialogue: 0,2:24:15.48,2:24:17.38,Default,,0,0,0,,Bình tĩnh!
Dialogue: 0,2:24:19.38,2:24:20.98,Default,,0,0,0,,Đứng yên!
Dialogue: 0,2:24:42.18,2:24:43.38,Default,,0,0,0,,Bám chắc vào!
Dialogue: 0,2:24:54.08,2:24:55.78,Default,,0,0,0,,Ngả tới trước đi!
Dialogue: 0,2:24:58.88,2:24:59.78,Default,,0,0,0,,Giữ vững!
Dialogue: 0,2:25:01.18,2:25:02.28,Default,,0,0,0,,Cố lên!
Dialogue: 0,2:25:02.58,2:25:03.98,Default,,0,0,0,,Nhảy đi!
Dialogue: 0,2:25:21.18,2:25:22.88,Default,,0,0,0,,Qua cầu!
Dialogue: 0,2:25:23.18,2:25:24.68,Default,,0,0,0,,Nhanh lên!
Dialogue: 0,2:26:09.38,2:26:11.38,Default,,0,0,0,,Ngươi không thể qua đây!
Dialogue: 0,2:26:11.68,2:26:13.28,Default,,0,0,0,,Gandalf!
Dialogue: 0,2:26:17.58,2:26:22.28,Default,,0,0,0,,Ta là hầu cận của Ngọn lửa Bí mật,\Nngười nắm giữ Ngọn lửa của Anor.
Dialogue: 0,2:26:23.38,2:26:27.08,Default,,0,0,0,,Lửa đen sẽ không giúp gì ngươi đâu,\NNgọn lửa Udun!
Dialogue: 0,2:26:37.28,2:26:39.18,Default,,0,0,0,,Trở về với bóng tối đi!
Dialogue: 0,2:26:47.08,2:26:51.28,Default,,0,0,0,,Ngươi không thể qua!
Dialogue: 0,2:27:18.58,2:27:22.28,Default,,0,0,0,,- Không! Không!\N- Gandalf!
Dialogue: 0,2:27:30.08,2:27:32.48,Default,,0,0,0,,Đi đi, lũ ngốc!
Dialogue: 0,2:27:36.68,2:27:38.78,Default,,0,0,0,,Không!
Dialogue: 0,2:27:44.98,2:27:47.08,Default,,0,0,0,,Aragorn!
Dialogue: 0,2:28:44.58,2:28:47.28,Default,,0,0,0,,Legolas, đỡ họ dậy.
Dialogue: 0,2:28:51.38,2:28:53.18,Default,,0,0,0,,Cho họ nghỉ một lát đi!
Dialogue: 0,2:28:53.48,2:28:56.98,Default,,0,0,0,,Khi đêm đến, ngọn đồi này\Nsẽ đầy lũ Orc.
Dialogue: 0,2:28:57.38,2:29:00.48,Default,,0,0,0,,Ta phải đến được rừng Lothlorien.
Dialogue: 0,2:29:01.58,2:29:04.78,Default,,0,0,0,,Đi thôi, Boromir, Legolas.\NGimli, đỡ họ dậy.
Dialogue: 0,2:29:06.48,2:29:08.58,Default,,0,0,0,,Đứng dậy đi Sam.
Dialogue: 0,2:29:08.98,2:29:10.38,Default,,0,0,0,,Frodo?
Dialogue: 0,2:29:12.48,2:29:14.08,Default,,0,0,0,,Frodo!
Dialogue: 0,2:30:06.68,2:30:09.08,Default,,0,0,0,,Ở sát nhau nhé, các cậu Hobbit!
Dialogue: 0,2:30:09.98,2:30:13.98,Default,,0,0,0,,Nghe nói rằng có một phù thuỷ vĩ đại\Nsống trong khu rừng này.
Dialogue: 0,2:30:14.28,2:30:16.68,Default,,0,0,0,,Một phù thuỷ tộc Tiên...
Dialogue: 0,2:30:17.48,2:30:20.18,Default,,0,0,0,,có sức mạnh khủng khiểp.
Dialogue: 0,2:30:20.88,2:30:22.98,Default,,0,0,0,,Bất cứ ai nhìn bà ta...
Dialogue: 0,2:30:23.18,2:30:24.98,Default,,0,0,0,,sẽ gục ngã dưới câu thần chú của bà.
Dialogue: 0,2:30:25.28,2:30:26.48,Default,,0,0,0,,{\i1}Frodo!{\i0}
Dialogue: 0,2:30:28.78,2:30:30.78,Default,,0,0,0,,Và biến mất mãi mãi.
Dialogue: 0,2:30:30.98,2:30:35.28,Default,,0,0,0,,{\i1}Cậu đến với chúng tôi\Nvới những bước chân của diệt vong.{\i0}
Dialogue: 0,2:30:35.68,2:30:39.28,Default,,0,0,0,,{\i1}Cậu mang quỉ dữ đến đây,\Nngười mang Nhẫn.{\i0}
Dialogue: 0,2:30:39.68,2:30:41.28,Default,,0,0,0,,Cậu Frodo?
Dialogue: 0,2:30:46.78,2:30:52.18,Default,,0,0,0,,Có người Lùn ở đây, thì\Nbà ta không dễ dàng bẫy chúng ta đâu.
Dialogue: 0,2:30:52.48,2:30:56.88,Default,,0,0,0,,Ta có đôi mắt của diều hâu\Nvà đôi tai của cáo.
Dialogue: 0,2:31:04.68,2:31:09.08,Default,,0,0,0,,Người Lùn thở mạnh quá,\Nta có thể bắn trúng hắn trong bóng tối.
Dialogue: 0,2:31:13.38,2:31:16.58,Default,,0,0,0,,Chào mừng Legolas,\Ncon trai của Thranduil.
Dialogue: 0,2:31:17.28,2:31:18.88,Default,,0,0,0,,Những người bạn của tôi nợ ngài,
Dialogue: 0,2:31:18.98,2:31:20.78,Default,,0,0,0,,Haldir xứ Lorien.
Dialogue: 0,2:31:22.48,2:31:24.58,Default,,0,0,0,,Aragorn, dòng dõi Dunedain.
Dialogue: 0,2:31:24.68,2:31:27.08,Default,,0,0,0,,Chúng tôi biết anh.
Dialogue: 0,2:31:27.18,2:31:27.88,Default,,0,0,0,,Chào ngài, Haldir.
Dialogue: 0,2:31:27.98,2:31:29.58,Default,,0,0,0,,Nghi lễ của tộc Tiên\Nnhư thế là đủ rồi đấy.
Dialogue: 0,2:31:29.88,2:31:31.78,Default,,0,0,0,,Hãy nói ngôn ngữ nào\Ncho bọn tôi hiểu được đi!
Dialogue: 0,2:31:31.98,2:31:36.38,Default,,0,0,0,,Chúng tôi đã không hợp tác gì\Nvới người Lùn kể từ Những Ngày Đen Tối.
Dialogue: 0,2:31:36.78,2:31:39.18,Default,,0,0,0,,Thế anh biết tay người Lùn này\Nsẽ nói cái gì không?
Dialogue: 0,2:31:39.58,2:31:44.18,Default,,0,0,0,,Ta phỉ nhổ vào mộ các người!
Dialogue: 0,2:31:46.18,2:31:48.08,Default,,0,0,0,,Như thế là thiếu lịch sự.
Dialogue: 0,2:31:54.68,2:31:58.58,Default,,0,0,0,,Các người mang quỷ dữ đến đây.
Dialogue: 0,2:32:00.58,2:32:02.68,Default,,0,0,0,,Các người không thể đi xa hơn nữa.
Dialogue: 0,2:32:08.88,2:32:11.78,Default,,0,0,0,,Chúng tôi cần các bạn bảo vệ.\NĐường đi đã bị chiếm!
Dialogue: 0,2:32:14.68,2:32:15.98,Default,,0,0,0,,Chúng tôi muốn đi cùng các bạn.
Dialogue: 0,2:32:16.38,2:32:18.18,Default,,0,0,0,,Xin hãy hiểu cho,\Nchúng tôi cần các bạn giúp!
Dialogue: 0,2:32:19.58,2:32:22.38,Default,,0,0,0,,Aragorn!
Dialogue: 0,2:32:32.88,2:32:34.88,Default,,0,0,0,,Chúng tôi muốn đi cùng các bạn.
Dialogue: 0,2:32:37.58,2:32:39.08,Default,,0,0,0,,Đường đi rất nguy hiểm.
Dialogue: 0,2:32:40.08,2:32:42.58,Default,,0,0,0,,Cái chết của Gandalf\Nsẽ không vô nghĩa đâu.
Dialogue: 0,2:32:43.88,2:32:45.68,Default,,0,0,0,,Chúng ta không được từ bỏ hy vọng.
Dialogue: 0,2:32:47.28,2:32:50.48,Default,,0,0,0,,Frodo, cậu đã mang một gánh nặng rồi.
Dialogue: 0,2:32:51.58,2:32:54.58,Default,,0,0,0,,Đừng chất thêm gánh nặng\Nvề người đã chết nữa.
Dialogue: 0,2:32:57.48,2:32:59.48,Default,,0,0,0,,Các bạn theo tôi.
Dialogue: 0,2:33:11.28,2:33:13.18,Default,,0,0,0,,Cares Galeton.
Dialogue: 0,2:33:13.78,2:33:16.68,Default,,0,0,0,,Trái tim của vương quốc tộc Tiên\Ntrên trái đất.
Dialogue: 0,2:33:16.88,2:33:22.38,Default,,0,0,0,,Vương quốc của Ngài Celeborn\Nvà Galadriel, Quý bà Ánh sáng.
Dialogue: 0,2:34:41.78,2:34:44.68,Default,,0,0,0,,Kẻ địch biết các người đến đây.
Dialogue: 0,2:34:45.48,2:34:50.38,Default,,0,0,0,,Hành tung của các người đã bị lộ.
Dialogue: 0,2:34:52.38,2:34:56.58,Default,,0,0,0,,Chỉ có 8 người ở đây.\NCó 9 người đi từ Rivendell cơ mà!
Dialogue: 0,2:34:57.08,2:34:58.88,Default,,0,0,0,,Nói ta nghe, Gandalf ở đâu?
Dialogue: 0,2:34:59.08,2:35:02.08,Default,,0,0,0,,Ta rất mong nói chuyện với ông ấy.
Dialogue: 0,2:35:02.38,2:35:04.98,Default,,0,0,0,,Ta không còn nhìn thấy ông ấy nữa.
Dialogue: 0,2:35:06.08,2:35:10.98,Default,,0,0,0,,Gandalf áo xám không vượt qua được\Nbiên giới của vùng đất này.
Dialogue: 0,2:35:11.48,2:35:13.78,Default,,0,0,0,,Ông ấy đã rơi vào bóng tối.
Dialogue: 0,2:35:18.58,2:35:22.88,Default,,0,0,0,,Ông ấy bị bắt bởi Bóng tối và Lửa.
Dialogue: 0,2:35:24.78,2:35:28.28,Default,,0,0,0,,Quái vật Balrog của Morgoth.
Dialogue: 0,2:35:29.18,2:35:32.48,Default,,0,0,0,,Khi chúng tôi bất đắc dĩ\Nphải vào hầm mỏ Moria.
Dialogue: 0,2:35:34.68,2:35:37.78,Default,,0,0,0,,Gandalf chưa bao giờ làm việc gì vô ích.
Dialogue: 0,2:35:38.58,2:35:41.78,Default,,0,0,0,,Chúng ta vẫn chưa biết\Nmục đích cuối cùng của ông ấy.
Dialogue: 0,2:35:46.58,2:35:50.68,Default,,0,0,0,,Đừng để sự mất mát từ Khazad-dum\Nchất đầy trái tim mình,
Dialogue: 0,2:35:51.08,2:35:53.28,Default,,0,0,0,,hỡi Gimli, con trai của Gloin.
Dialogue: 0,2:35:54.38,2:35:57.78,Default,,0,0,0,,Thế giới đang tràn đầy nguy hiểm.
Dialogue: 0,2:35:59.38,2:36:01.98,Default,,0,0,0,,Và trên tất cả các mảnh đất...
Dialogue: 0,2:36:02.28,2:36:05.68,Default,,0,0,0,,giờ đây tình yêu hoà quyện\Ntrong nỗi đau buồn.
Dialogue: 0,2:36:18.38,2:36:21.08,Default,,0,0,0,,Hội đồng hành này sẽ ra sao đây?
Dialogue: 0,2:36:21.78,2:36:24.58,Default,,0,0,0,,Không có Gandalf,\Nhy vọng cũng không còn.
Dialogue: 0,2:36:27.68,2:36:30.88,Default,,0,0,0,,Sứ mệnh đang rất cam go.
Dialogue: 0,2:36:31.08,2:36:33.98,Default,,0,0,0,,Chỉ một chút sai lầm...
Dialogue: 0,2:36:34.98,2:36:37.68,Default,,0,0,0,,sẽ dẫn đến ngày tàn của thế giới.
Dialogue: 0,2:36:42.18,2:36:47.68,Default,,0,0,0,,Hy vọng vẫn còn khi nào Hội đồng hành\Ncòn tin tưởng nhau.
Dialogue: 0,2:36:49.48,2:36:53.98,Default,,0,0,0,,Đừng để lòng mình bị quấy rối,\Nhãy nghỉ ngơi đi...
Dialogue: 0,2:36:54.58,2:36:58.18,Default,,0,0,0,,vì các người đã kiệt sức với\Nnhững đau thương và vất vả.
Dialogue: 0,2:37:00.58,2:37:02.58,Default,,0,0,0,,Tối nay, các người sẽ được ngủ...
Dialogue: 0,2:37:02.98,2:37:06.28,Default,,0,0,0,,{\i1}Chào mừng Frodo của vùng Shire...{\i0}
Dialogue: 0,2:37:07.88,2:37:09.58,Default,,0,0,0,,{\i1}người đã từng nhìn thấy Con Mắt!{\i0}
Dialogue: 0,2:37:24.28,2:37:26.78,Default,,0,0,0,,Khúc hát dành cho Gandalf.
Dialogue: 0,2:37:34.18,2:37:35.58,Default,,0,0,0,,Hát gì về ông ấy vậy?
Dialogue: 0,2:37:36.08,2:37:38.48,Default,,0,0,0,,Tôi không có tâm trạng nói với cậu.
Dialogue: 0,2:37:41.48,2:37:43.68,Default,,0,0,0,,Với tôi, sự mất mát\Nnhư vừa mới đây.
Dialogue: 0,2:37:46.68,2:37:49.48,Default,,0,0,0,,Tôi cá là họ không nói về\Npháo hoa của ông ấy đâu.
Dialogue: 0,2:37:49.88,2:37:52.68,Default,,0,0,0,,Có lẽ nên có bài thơ về việc đó.
Dialogue: 0,2:37:56.18,2:37:58.08,Default,,0,0,0,,{\i1}Những ngọn pháo hoa\Nđẹp chưa từng thấy.{\i0}
Dialogue: 0,2:37:59.28,2:38:02.08,Default,,0,0,0,,{\i1}Cháy thành những vì sao\Nmàu trời và xanh lá.{\i0}
Dialogue: 0,2:38:03.18,2:38:08.18,Default,,0,0,0,,{\i1}Hay sau tia chớp..,\Nnhững hạt nước bạc...{\i0}
Dialogue: 0,2:38:08.98,2:38:11.69,Default,,0,0,0,,{\i1}Rơi xuống...\Nnhư cơn mưa đầy hoa...{\i0}
Dialogue: 0,2:38:12.09,2:38:15.79,Default,,0,0,0,,Ôi, sau một quãng đường dài thế,\Nthì thật không công bằng với họ.
Dialogue: 0,2:38:26.69,2:38:29.29,Default,,0,0,0,,Hãy nghỉ ngơi đi.
Dialogue: 0,2:38:29.59,2:38:32.19,Default,,0,0,0,,Bên ngoài được bảo vệ rất vững chắc.
Dialogue: 0,2:38:32.49,2:38:34.89,Default,,0,0,0,,Tôi không thể nghỉ ngơi được.
Dialogue: 0,2:38:37.89,2:38:40.69,Default,,0,0,0,,Giọng bà ấy vẫn vang lên trong đầu.
Dialogue: 0,2:38:41.19,2:38:44.69,Default,,0,0,0,,Bà ấy nói về cha tôi và\Nsự suy vong của Gondor.
Dialogue: 0,2:38:44.99,2:38:50.19,Default,,0,0,0,,Bà ấy nói rằng:\N"Ngay cả lúc này, hy vọng vẫn còn."
Dialogue: 0,2:38:51.89,2:38:54.49,Default,,0,0,0,,Nhưng tôi không thể thấy gì cả.
Dialogue: 0,2:38:55.99,2:38:58.39,Default,,0,0,0,,Đã từ rất lâu\Nchúng tôi không còn hy vọng gì.
Dialogue: 0,2:39:08.69,2:39:10.79,Default,,0,0,0,,Cha tôi là một người ưu tú.
Dialogue: 0,2:39:11.59,2:39:13.79,Default,,0,0,0,,Nhưng sự cai trị của ông đã thất bại,
Dialogue: 0,2:39:16.99,2:39:19.39,Default,,0,0,0,,dân chúng đang mất lòng tin.
Dialogue: 0,2:39:20.99,2:39:24.39,Default,,0,0,0,,Ông muốn tôi sửa chữa lại mọi thứ,\Nvà tôi sẽ làm.
Dialogue: 0,2:39:24.69,2:39:28.09,Default,,0,0,0,,Tôi muốn thấy sự huy hoàng\Ncủa Gondor được khôi phục.
Dialogue: 0,2:39:31.09,2:39:33.99,Default,,0,0,0,,Anh từng thấy Gondor chưa, Aragorn?
Dialogue: 0,2:39:34.69,2:39:36.59,Default,,0,0,0,,Toà tháp trắng Ecthelion,
Dialogue: 0,2:39:36.99,2:39:40.59,Default,,0,0,0,,lấp lánh như một ngọn tháp\Nbằng trân châu và bạc.
Dialogue: 0,2:39:41.39,2:39:44.79,Default,,0,0,0,,Trên đỉnh tháp, ngọn cờ cất cao\Ntrong làn gió ban mai.
Dialogue: 0,2:39:47.89,2:39:49.79,Default,,0,0,0,,Anh có bao giờ được gọi về nhà...
Dialogue: 0,2:39:49.99,2:39:53.79,Default,,0,0,0,,bằng tiếng nhạc trong trẻo của\Nkèn trumpet bạc?
Dialogue: 0,2:39:54.59,2:39:57.59,Default,,0,0,0,,Tôi đã nhìn thấy Thành phố Trắng...
Dialogue: 0,2:39:58.09,2:39:59.49,Default,,0,0,0,,từ lâu lắm rồi.
Dialogue: 0,2:40:00.59,2:40:05.39,Default,,0,0,0,,Rồi có ngày ta sẽ tới đó.
Dialogue: 0,2:40:06.49,2:40:09.29,Default,,0,0,0,,Và tháp canh sẽ kêu lên:
Dialogue: 0,2:40:09.99,2:40:13.09,Default,,0,0,0,,"Vua của Gondor đã trở về!"
Dialogue: 0,2:41:20.49,2:41:22.89,Default,,0,0,0,,Cậu sẽ nhìn vào tấm gương chứ?
Dialogue: 0,2:41:23.19,2:41:24.59,Default,,0,0,0,,Tôi sẽ thấy gì?
Dialogue: 0,2:41:27.69,2:41:29.99,Default,,0,0,0,,Người thông thái nhất\Ncũng không thể biết.
Dialogue: 0,2:41:30.89,2:41:32.69,Default,,0,0,0,,Vì tấm gương này...
Dialogue: 0,2:41:32.99,2:41:35.39,Default,,0,0,0,,chỉ ra rất nhiều điều.
Dialogue: 0,2:41:38.49,2:41:40.69,Default,,0,0,0,,Điều đã từng xảy ra,
Dialogue: 0,2:41:41.09,2:41:43.59,Default,,0,0,0,,điều đang xảy ra,
Dialogue: 0,2:41:44.39,2:41:46.19,Default,,0,0,0,,và vài điều...
Dialogue: 0,2:41:49.99,2:41:53.19,Default,,0,0,0,,vẫn còn chưa xuất hiện.
Dialogue: 0,2:43:15.79,2:43:18.59,Default,,0,0,0,,Ta biết cậu thấy gì.
Dialogue: 0,2:43:21.09,2:43:23.39,Default,,0,0,0,,Vì nó cũng hiện lên trong tâm trí ta.
Dialogue: 0,2:43:25.39,2:43:29.19,Default,,0,0,0,,{\i1}Đây là điều sẽ xảy ra\Nnếu cậu thất bại.{\i0}
Dialogue: 0,2:43:31.79,2:43:36.39,Default,,0,0,0,,{\i1}Hội đồng hành đang tan vỡ.\NMọi việc đã bắt đầu.{\i0}
Dialogue: 0,2:43:37.29,2:43:40.49,Default,,0,0,0,,{\i1}Hắn sẽ cố lấy chiếc Nhẫn.{\i0}
Dialogue: 0,2:43:40.79,2:43:42.69,Default,,0,0,0,,{\i1}Cậu biết ta đang nói về ai.{\i0}
Dialogue: 0,2:43:43.79,2:43:48.19,Default,,0,0,0,,{\i1}Lần lượt, nó sẽ\Nhuỷ hoại tất cả bọn họ.{\i0}
Dialogue: 0,2:43:49.99,2:43:52.29,Default,,0,0,0,,{\i1}Nếu bà yêu cầu tôi...{\i0}
Dialogue: 0,2:43:52.79,2:43:55.69,Default,,0,0,0,,{\i1}tôi sẽ đưa cho bà chiếc Nhẫn.{\i0}
Dialogue: 0,2:43:56.99,2:43:59.09,Default,,0,0,0,,Cậu thực lòng muốn đưa nó cho ta.
Dialogue: 0,2:44:03.59,2:44:07.39,Default,,0,0,0,,Ta không phủ nhận rằng ta cũng\Nmong muốn nó rất mãnh liệt.
Dialogue: 0,2:44:14.09,2:44:17.19,Default,,0,0,0,,Tại nơi của Chúa tể Bóng tối,\Nngươi sẽ có một nữ hoàng,
Dialogue: 0,2:44:17.69,2:44:21.29,Default,,0,0,0,,không đen tối mà xinh đẹp,\Nvà khủng khiếp như ánh bình minh!
Dialogue: 0,2:44:22.19,2:44:25.09,Default,,0,0,0,,Xảo trá như biển cả!
Dialogue: 0,2:44:25.59,2:44:30.39,Default,,0,0,0,,Quyền lực hơn cả\NĐấng sáng tạo ra trái đất!
Dialogue: 0,2:44:31.09,2:44:33.39,Default,,0,0,0,,Tất cả sẽ yêu mến ta...
Dialogue: 0,2:44:33.69,2:44:36.39,Default,,0,0,0,,và sẽ tuyệt vọng!
Dialogue: 0,2:44:50.59,2:44:54.79,Default,,0,0,0,,Ta đã vượt qua thử thách.\NTa sẽ thu mình lại...
Dialogue: 0,2:44:55.19,2:44:57.49,Default,,0,0,0,,và đi về phía Tây...
Dialogue: 0,2:44:57.89,2:45:02.19,Default,,0,0,0,,- và vẫn là Galadriel.\N- Tôi không thể làm điều này một mình.
Dialogue: 0,2:45:05.89,2:45:09.89,Default,,0,0,0,,Cậu là người mang Nhẫn, Frodo.\NĐể mang được chiếc nhẫn quyền lực,
Dialogue: 0,2:45:10.99,2:45:12.39,Default,,0,0,0,,cậu phải chịu cô độc.
Dialogue: 0,2:45:15.89,2:45:18.59,Default,,0,0,0,,Đây là Nenya,\NNhẫn của Adamant.
Dialogue: 0,2:45:18.89,2:45:20.39,Default,,0,0,0,,Và ta là người giữ nó.
Dialogue: 0,2:45:23.69,2:45:27.19,Default,,0,0,0,,Nhiệm vụ này đã chọn cậu.
Dialogue: 0,2:45:28.39,2:45:31.09,Default,,0,0,0,,Và nếu cậu không thể làm được...
Dialogue: 0,2:45:32.49,2:45:34.39,Default,,0,0,0,,thì không ai có thể làm.
Dialogue: 0,2:45:34.89,2:45:37.89,Default,,0,0,0,,Vậy thì tôi biết mình phải làm gì rồi.
Dialogue: 0,2:45:38.59,2:45:39.79,Default,,0,0,0,,Chỉ là...
Dialogue: 0,2:45:43.09,2:45:45.29,Default,,0,0,0,,tôi sợ phải làm điều đó.
Dialogue: 0,2:45:48.79,2:45:53.79,Default,,0,0,0,,Ngay cả người nhỏ bé nhất\Ncũng có thể thay đổi tương lai.
Dialogue: 0,2:45:59.39,2:46:04.89,Default,,0,0,0,,Các ngươi có biết loài Orc\Nlúc đầu thế nào không?
Dialogue: 0,2:46:05.89,2:46:08.99,Default,,0,0,0,,Chúng từng là người tộc Tiên.
Dialogue: 0,2:46:10.29,2:46:13.49,Default,,0,0,0,,Bị cám dỗ bởi sức mạnh đen tối,
Dialogue: 0,2:46:13.99,2:46:16.99,Default,,0,0,0,,bị tra tấn và hành hình..
Dialogue: 0,2:46:17.39,2:46:22.49,Default,,0,0,0,,Một thể sống bại hoại\Nvà kinh khủng.
Dialogue: 0,2:46:22.89,2:46:24.69,Default,,0,0,0,,Và bây giờ...
Dialogue: 0,2:46:26.29,2:46:28.09,Default,,0,0,0,,là hoàn hảo.
Dialogue: 0,2:46:28.89,2:46:31.89,Default,,0,0,0,,Những chiến binh Uruk-hai của ta,
Dialogue: 0,2:46:33.99,2:46:36.19,Default,,0,0,0,,các ngươi phục vụ ai?
Dialogue: 0,2:46:36.49,2:46:38.89,Default,,0,0,0,,Saruman!
Dialogue: 0,2:46:59.79,2:47:03.09,Default,,0,0,0,,Săn tìm chúng! Không ngừng lại\Ntrước khi tìm ra chúng.
Dialogue: 0,2:47:03.49,2:47:06.29,Default,,0,0,0,,Các ngươi không biết đau đớn.\NCác ngươi không biết sợ hãi.
Dialogue: 0,2:47:06.59,2:47:09.39,Default,,0,0,0,,Các ngươi sẽ được nếm vị thịt người!
Dialogue: 0,2:47:14.59,2:47:18.09,Default,,0,0,0,,Một tên trong bọn Hobbit mang theo\Nmột vật rất giá trị.
Dialogue: 0,2:47:18.49,2:47:22.49,Default,,0,0,0,,Bắt sống chúng về đây.
Dialogue: 0,2:47:24.59,2:47:25.79,Default,,0,0,0,,Và giết bọn còn lại!
Dialogue: 0,2:47:44.99,2:47:50.19,Default,,0,0,0,,Chưa bao giờ chúng tôi tự tay mình\Nchoàng áo của tộc cho người lạ.
Dialogue: 0,2:47:51.49,2:47:55.19,Default,,0,0,0,,Mong những tấm áo choàng này sẽ che chở\Ncác bạn trước những cặp mắt không thân thiện.
Dialogue: 0,2:48:02.49,2:48:04.09,Default,,0,0,0,,Lembas.
Dialogue: 0,2:48:04.39,2:48:05.99,Default,,0,0,0,,Bánh ăn đường của tộc Tiên.
Dialogue: 0,2:48:08.39,2:48:11.29,Default,,0,0,0,,Một miếng nhỏ cũng đủ no\Ncho một người trưởng thành.
Dialogue: 0,2:48:17.89,2:48:19.79,Default,,0,0,0,,Cậu đã ăn mấy cái vậy?
Dialogue: 0,2:48:19.99,2:48:21.59,Default,,0,0,0,,Bốn cái.
Dialogue: 0,2:48:27.49,2:48:29.99,Default,,0,0,0,,{\i1}Mỗi dặm các bạn đi,\Nmối nguy hiểm càng gia tăng.{\i0}
Dialogue: 0,2:48:30.29,2:48:34.19,Default,,0,0,0,,{\i1}Bọn Orc của Mordor đang kiểm soát\Nbờ biển phía Đông Anduin.{\i0}
Dialogue: 0,2:48:34.59,2:48:36.99,Default,,0,0,0,,{\i1}Bờ phía Tây cũng không an toàn.{\i0}
Dialogue: 0,2:48:37.29,2:48:40.99,Default,,0,0,0,,Những sinh vật lạ với dấu hiệu bàn tay trắng\Nđã xuất hiện tại biên giới.
Dialogue: 0,2:48:41.39,2:48:46.19,Default,,0,0,0,,Hiếm khi Orc hành quân dưới mặt trời,\Nnhưng bọn này lại làm như vậy.
Dialogue: 0,2:48:54.79,2:48:56.99,Default,,0,0,0,,Các người đã bị theo dõi.
Dialogue: 0,2:48:59.09,2:49:03.49,Default,,0,0,0,,Dọc bờ sông các người sẽ có cơ hội\Nthoát khỏi chúng để tới thác Rauros.
Dialogue: 0,2:49:20.99,2:49:25.59,Default,,0,0,0,,Quà của ta cho anh, Legolas,\Nlà cung tên của Galadhrim.
Dialogue: 0,2:49:25.99,2:49:29.29,Default,,0,0,0,,Xứng đáng với kỹ năng\Ncủa tộc Tiên ở trong rừng.
Dialogue: 0,2:49:34.89,2:49:37.29,Default,,0,0,0,,Đây là những đoản kiếm của Noldorin.
Dialogue: 0,2:49:37.59,2:49:41.39,Default,,0,0,0,,Chúng đã từng phục vụ\Ntrong chiến tranh.
Dialogue: 0,2:49:41.69,2:49:43.89,Default,,0,0,0,,Đừng sợ, cậu Peregrin Took.
Dialogue: 0,2:49:44.19,2:49:47.29,Default,,0,0,0,,Cậu sẽ tìm thấy sự dũng cảm của mình.
Dialogue: 0,2:49:49.29,2:49:51.29,Default,,0,0,0,,Và cho cậu, Samwise Gamgee,
Dialogue: 0,2:49:51.49,2:49:54.19,Default,,0,0,0,,dây thừng của tộc Tiên\Nđược làm từ hithlain.
Dialogue: 0,2:49:54.39,2:49:56.39,Default,,0,0,0,,Cám ơn quý bà.
Dialogue: 0,2:49:58.29,2:50:01.49,Default,,0,0,0,,Bà đã hết đoản kiếm rồi sao?
Dialogue: 0,2:50:07.99,2:50:10.89,Default,,0,0,0,,Một người Lùn có thể yêu cầu\Ntộc Tiên món quà gì đây?
Dialogue: 0,2:50:11.29,2:50:13.39,Default,,0,0,0,,Không cần gì cả.
Dialogue: 0,2:50:14.09,2:50:17.29,Default,,0,0,0,,Trừ việc được nhìn Quý bà\Ncủa Galadhrim lần cuối...
Dialogue: 0,2:50:17.99,2:50:22.99,Default,,0,0,0,,vì bà ấy xinh đẹp hơn\Ntất cả châu báu trên trái đất này.
Dialogue: 0,2:50:30.19,2:50:31.69,Default,,0,0,0,,Thực ra...
Dialogue: 0,2:50:31.99,2:50:33.59,Default,,0,0,0,,có một điều...
Dialogue: 0,2:50:34.49,2:50:36.99,Default,,0,0,0,,Không, tôi không thể.\NChuyện này hầu như là không thể.
Dialogue: 0,2:50:37.29,2:50:39.99,Default,,0,0,0,,Thật ngốc khi hỏi thế.
Dialogue: 0,2:50:42.69,2:50:44.69,Default,,0,0,0,,Ta chẳng có gì quý hơn...
Dialogue: 0,2:50:44.89,2:50:48.39,Default,,0,0,0,,món quà anh đang mang theo.
Dialogue: 0,2:50:49.39,2:50:51.19,Default,,0,0,0,,Vì tình yêu của cháu gái ta,
Dialogue: 0,2:50:51.89,2:50:56.79,Default,,0,0,0,,ta sợ phép thuật của Arwen Evenstar\Nsẽ giảm dần.
Dialogue: 0,2:50:58.09,2:51:02.59,Default,,0,0,0,,Đáng ra tôi phải để cô ấy\Nrời khỏi những bờ biển đó...
Dialogue: 0,2:51:02.99,2:51:06.19,Default,,0,0,0,,và về với dòng tộc của cô ấy.
Dialogue: 0,2:51:07.89,2:51:13.49,Default,,0,0,0,,Đáng ra tôi phải đưa cô ấy\Nlên tàu tới Valinor.
Dialogue: 0,2:51:13.89,2:51:16.79,Default,,0,0,0,,Đó là sự lựa chọn của cháu ta.
Dialogue: 0,2:51:17.69,2:51:20.49,Default,,0,0,0,,Anh có sự lựa chọn riêng\Ncủa mình, Aragorn.
Dialogue: 0,2:51:20.89,2:51:26.39,Default,,0,0,0,,Vươn lên trên tất cả tổ tiên\Ntừ thời Elendil...
Dialogue: 0,2:51:26.89,2:51:31.09,Default,,0,0,0,,hay rơi xuống vực sâu tối tăm\Ncùng với dòng tộc của mình.
Dialogue: 0,2:51:38.59,2:51:40.49,Default,,0,0,0,,Tạm biệt.
Dialogue: 0,2:51:43.99,2:51:47.79,Default,,0,0,0,,Anh vẫn còn nhiều việc phải làm.
Dialogue: 0,2:51:49.49,2:51:57.99,Default,,0,0,0,,Chúng ta sẽ không gặp lại nữa, Elessar.
Dialogue: 0,2:51:58.89,2:52:01.99,Default,,0,0,0,,Tạm biệt, Frodo Baggins.
Dialogue: 0,2:52:02.19,2:52:05.49,Default,,0,0,0,,Ta tặng cho cậu ánh sáng của Earendil,
Dialogue: 0,2:52:06.49,2:52:08.69,Default,,0,0,0,,ngôi sao chúng ta yêu mến nhất.
Dialogue: 0,2:52:17.89,2:52:22.09,Default,,0,0,0,,Cầu cho ánh sáng này sẽ đi cùng cậu\Ntrong những nơi tối tăm...
Dialogue: 0,2:52:22.49,2:52:26.49,Default,,0,0,0,,khi những ánh sáng khác\Nđã lụi tàn.
Dialogue: 0,2:52:35.59,2:52:38.59,Default,,0,0,0,,Trong chuyến đi này ta đã bị\Nvết thương tệ nhất...
Dialogue: 0,2:52:39.39,2:52:43.39,Default,,0,0,0,,nhưng cuối cùng lại được\Nmột món quà đẹp nhất.
Dialogue: 0,2:52:44.39,2:52:47.59,Default,,0,0,0,,Từ nay chẳng còn cái gì là đẹp\Nkhi so với món quà của bà ấy.
Dialogue: 0,2:52:47.99,2:52:49.89,Default,,0,0,0,,Bà ấy đưa ông quà gì?
Dialogue: 0,2:52:50.19,2:52:54.89,Default,,0,0,0,,Ta xin bà ấy một sợi tóc vàng.
Dialogue: 0,2:52:55.79,2:52:58.59,Default,,0,0,0,,Bà ấy đưa đến ba sợi.
Dialogue: 0,2:54:40.89,2:54:44.19,Default,,0,0,0,,Là Gollum.\NHắn theo chúng ta từ Moria.
Dialogue: 0,2:54:48.09,2:54:50.99,Default,,0,0,0,,Tôi cứ tưởng đã\Nđánh lạc hướng hắn trên sông.
Dialogue: 0,2:54:51.39,2:54:54.39,Default,,0,0,0,,Nhưng hắn lại là một tay\Nthông thạo đường thuỷ.
Dialogue: 0,2:54:55.39,2:54:58.59,Default,,0,0,0,,Nếu hắn báo động cho kẻ địch biết\Nvị trí của chúng ta...
Dialogue: 0,2:54:58.79,2:55:00.89,Default,,0,0,0,,thì đường đi sẽ càng nguy hiểm hơn.
Dialogue: 0,2:55:01.19,2:55:03.19,Default,,0,0,0,,Ăn gì không, cậu Frodo?
Dialogue: 0,2:55:03.49,2:55:06.49,Default,,0,0,0,,- Thôi khỏi Sam ạ!\N- Cậu không ăn gì cả ngày rồi.
Dialogue: 0,2:55:06.89,2:55:09.79,Default,,0,0,0,,Cũng không ngủ nghỉ gì,\Nđừng nghĩ tôi không biết.
Dialogue: 0,2:55:10.79,2:55:13.69,Default,,0,0,0,,- Cậu Frodo.\N- Tôi ổn mà.
Dialogue: 0,2:55:13.99,2:55:15.09,Default,,0,0,0,,Không ổn đâu!
Dialogue: 0,2:55:15.49,2:55:17.99,Default,,0,0,0,,Tôi ở đây để giúp cậu.
Dialogue: 0,2:55:18.19,2:55:19.79,Default,,0,0,0,,Tôi đã hứa với Gandalf là sẽ giúp cậu.
Dialogue: 0,2:55:25.89,2:55:28.39,Default,,0,0,0,,Sam, cậu không thể giúp được đâu.
Dialogue: 0,2:55:30.79,2:55:32.39,Default,,0,0,0,,Không phải lúc này.
Dialogue: 0,2:55:35.39,2:55:37.39,Default,,0,0,0,,Hãy ngủ đi.
Dialogue: 0,2:55:43.79,2:55:45.89,Default,,0,0,0,,Minas Tirith là con đường an toàn hơn.
Dialogue: 0,2:55:46.49,2:55:49.09,Default,,0,0,0,,Anh biết thế mà.\NTừ đó ta có thể nhóm họp lại.
Dialogue: 0,2:55:49.89,2:55:51.99,Default,,0,0,0,,Tấn công Mordor\Ntừ một nơi có sức mạnh.
Dialogue: 0,2:55:52.39,2:55:55.09,Default,,0,0,0,,Chẳng có sức mạnh nào tại Gondor\Ncó thể giúp chúng ta.
Dialogue: 0,2:55:55.79,2:55:57.69,Default,,0,0,0,,Anh quá vội tin vào tộc Tiên.
Dialogue: 0,2:55:59.59,2:56:02.39,Default,,0,0,0,,Anh còn chút lòng tin nào\Nvới dân của mình không?
Dialogue: 0,2:56:02.59,2:56:05.39,Default,,0,0,0,,Phải, họ yếu đuối. Họ nhu nhược.
Dialogue: 0,2:56:05.79,2:56:08.99,Default,,0,0,0,,Nhưng vẫn còn lòng dũng cảm,\Nvà niềm kiêu hãnh trong tộc Người.
Dialogue: 0,2:56:09.29,2:56:11.39,Default,,0,0,0,,Nhưng anh sẽ chẳng thấy nó đâu.
Dialogue: 0,2:56:11.89,2:56:13.19,Default,,0,0,0,,Anh đang sợ!
Dialogue: 0,2:56:13.59,2:56:15.99,Default,,0,0,0,,Suốt đời anh chỉ biết\Ntrốn trong bóng tối.
Dialogue: 0,2:56:16.39,2:56:19.69,Default,,0,0,0,,Sợ bản thân mình,\Nsợ thân phận của mình.
Dialogue: 0,2:56:22.29,2:56:26.49,Default,,0,0,0,,Tôi sẽ không để chiếc Nhẫn\Nvào gần thành phố của anh đâu.
Dialogue: 0,2:56:38.49,2:56:39.79,Default,,0,0,0,,Frodo!
Dialogue: 0,2:56:41.69,2:56:43.09,Default,,0,0,0,,Tượng Argonath!
Dialogue: 0,2:56:46.69,2:56:50.19,Default,,0,0,0,,Từ lâu rồi ta đã khao khát\Nđược trông thấy những vị vua cổ xưa.
Dialogue: 0,2:56:51.09,2:56:52.89,Default,,0,0,0,,Tổ tiên của ta.
Dialogue: 0,2:58:12.09,2:58:14.09,Default,,0,0,0,,Chúng ta sẽ vượt sông khi đêm xuống.
Dialogue: 0,2:58:14.49,2:58:16.89,Default,,0,0,0,,Giấu thuyền và tiếp tục đi bộ.
Dialogue: 0,2:58:17.19,2:58:20.49,Default,,0,0,0,,- Ta sẽ đến Mordor từ hướng Bắc.\N- Thế à?
Dialogue: 0,2:58:21.09,2:58:24.79,Default,,0,0,0,,Chỉ cần đơn giản là tìm đường\Nbăng qua Emyn Muil...
Dialogue: 0,2:58:25.09,2:58:28.19,Default,,0,0,0,,một mê cung với những phiến đá sắc nhọn\Nkhông thể qua được!
Dialogue: 0,2:58:28.49,2:58:31.49,Default,,0,0,0,,Sau đó, có lẽ sẽ tốt đẹp hơn.
Dialogue: 0,2:58:31.99,2:58:36.69,Default,,0,0,0,,Một vùng đầm lầy thật hôi hám,\Nrộng lớn quá tầm mắt.
Dialogue: 0,2:58:37.19,2:58:38.49,Default,,0,0,0,,Đó là con đường của chúng ta.
Dialogue: 0,2:58:40.09,2:58:43.19,Default,,0,0,0,,Ta đề nghị ngài nên nghỉ ngơi\Nlấy lại sức đi, ngài Người Lùn.
Dialogue: 0,2:58:43.49,2:58:45.09,Default,,0,0,0,,Lấy lại gì chứ?
Dialogue: 0,2:58:48.29,2:58:50.09,Default,,0,0,0,,- Ta nên đi ngay.\N- Không.
Dialogue: 0,2:58:50.39,2:58:54.59,Default,,0,0,0,,Lũ Orc đang tuần tra dọc bờ phía Đông.\NTa phải chờ đến đêm.
Dialogue: 0,2:58:54.99,2:58:58.39,Default,,0,0,0,,Tôi không lo lắng chuyện bờ phía Đông.
Dialogue: 0,2:58:58.69,2:59:02.49,Default,,0,0,0,,Một bóng đen và một mối đe doạ\Nđang hiện lên trong tâm trí tôi.
Dialogue: 0,2:59:03.19,2:59:07.19,Default,,0,0,0,,Có cái gì đó đang tới gần...\NTôi cảm nhận được nó.
Dialogue: 0,2:59:09.39,2:59:14.09,Default,,0,0,0,,Người Lùn không cần lấy sức!\NCậu Hobbit trẻ, đừng để ý chuyện này.
Dialogue: 0,2:59:14.89,2:59:16.49,Default,,0,0,0,,Frodo đâu?
Dialogue: 0,2:59:42.19,2:59:44.09,Default,,0,0,0,,Không ai được đi một mình.
Dialogue: 0,2:59:45.39,2:59:47.49,Default,,0,0,0,,Nhất là cậu đó.
Dialogue: 0,2:59:47.79,2:59:50.09,Default,,0,0,0,,Có nhiều người phụ thuộc vào cậu lắm.
Dialogue: 0,2:59:51.29,2:59:53.19,Default,,0,0,0,,Frodo?
Dialogue: 0,3:00:00.39,3:00:02.89,Default,,0,0,0,,Tôi biết vì sao\Ncậu đi đến nơi vắng vẻ này.
Dialogue: 0,3:00:03.69,3:00:06.99,Default,,0,0,0,,Cậu đang phải chịu đựng.\NTa thấy nó ngày qua ngày.
Dialogue: 0,3:00:07.89,3:00:10.89,Default,,0,0,0,,Cậu có chắc rằng\Nmình không chịu đựng vô ích?
Dialogue: 0,3:00:12.59,3:00:15.19,Default,,0,0,0,,Vẫn còn những cách khác, Frodo.
Dialogue: 0,3:00:15.49,3:00:19.19,Default,,0,0,0,,- Còn những con đường khác cho chúng ta.\N- Tôi biết anh muốn nói gì.
Dialogue: 0,3:00:19.49,3:00:23.09,Default,,0,0,0,,Nghe hay đấy nhưng trong lòng tôi\Nđã cảnh báo việc này.
Dialogue: 0,3:00:23.49,3:00:26.49,Default,,0,0,0,,Cảnh báo? Về cái gì?
Dialogue: 0,3:00:26.79,3:00:28.79,Default,,0,0,0,,Chúng ta đều sợ, Frodo.
Dialogue: 0,3:00:29.09,3:00:32.59,Default,,0,0,0,,Nhưng để nỗi sợ đó điều khiển mình,\Nhuỷ diệt hy vọng của mình...
Dialogue: 0,3:00:32.99,3:00:36.59,Default,,0,0,0,,- cậu không thấy nó điên rồ sao?\N- Không còn cách nào khác!
Dialogue: 0,3:00:38.59,3:00:41.69,Default,,0,0,0,,Ta chỉ yêu cầu sức mạnh\Nđể bảo vệ dân ta.
Dialogue: 0,3:00:43.49,3:00:45.39,Default,,0,0,0,,- Nếu cậu đưa ta chiếc Nhẫn...\N- Không.
Dialogue: 0,3:00:46.69,3:00:50.19,Default,,0,0,0,,- Sao cậu lại lùi? Ta đâu phải kẻ cướp.\N- Anh không còn là chính mình nữa!
Dialogue: 0,3:00:52.29,3:00:55.39,Default,,0,0,0,,Cậu nghĩ cậu có cơ hội à?
Dialogue: 0,3:00:55.69,3:01:00.19,Default,,0,0,0,,Chúng sẽ tìm thấy cậu!\NChúng sẽ lấy chiếc Nhẫn...
Dialogue: 0,3:01:00.59,3:01:04.19,Default,,0,0,0,,và cậu sẽ van xin được chết\Ntrước khi mọi chuyện kết thúc!
Dialogue: 0,3:01:06.49,3:01:08.49,Default,,0,0,0,,Ngu ngốc!
Dialogue: 0,3:01:08.69,3:01:12.59,Default,,0,0,0,,Nó không phải của ngươi, chỉ xui mà có được.\NNó đáng ra là của ta!
Dialogue: 0,3:01:12.89,3:01:15.59,Default,,0,0,0,,Nó phải là của ta!\NĐưa đây!
Dialogue: 0,3:01:15.89,3:01:17.09,Default,,0,0,0,,- Đưa cho ta!\N- Không!
Dialogue: 0,3:01:17.39,3:01:19.19,Default,,0,0,0,,- Đưa nó cho ta!\N- Không.
Dialogue: 0,3:01:26.89,3:01:28.79,Default,,0,0,0,,Ta biết ngươi nghĩ gì.
Dialogue: 0,3:01:29.19,3:01:31.99,Default,,0,0,0,,Ngươi sẽ đưa nó cho Sauron!
Dialogue: 0,3:01:32.29,3:01:34.09,Default,,0,0,0,,Ngươi sẽ phản bội chúng ta!
Dialogue: 0,3:01:34.39,3:01:37.49,Default,,0,0,0,,Ngươi sẽ chết,\Nvà tất cả chúng ta cũng chết theo!
Dialogue: 0,3:01:37.99,3:01:41.49,Default,,0,0,0,,Ta nguyền rủa ngươi!\NVà nguyền rủa tất cả bọn Hobbit!
Dialogue: 0,3:01:48.69,3:01:50.19,Default,,0,0,0,,Frodo?
Dialogue: 0,3:01:54.49,3:01:56.09,Default,,0,0,0,,Frodo?
Dialogue: 0,3:02:00.09,3:02:01.99,Default,,0,0,0,,Ta đã làm gì thế này?
Dialogue: 0,3:02:02.29,3:02:03.99,Default,,0,0,0,,Làm ơn, Frodo!
Dialogue: 0,3:02:04.29,3:02:06.79,Default,,0,0,0,,Frodo, ta xin lỗi!
Dialogue: 0,3:02:37.19,3:02:40.69,Default,,0,0,0,,Bọn chúng sẽ gục ngã.
Dialogue: 0,3:02:55.49,3:02:56.79,Default,,0,0,0,,Frodo?
Dialogue: 0,3:02:58.09,3:03:01.09,Default,,0,0,0,,- Chiếc Nhẫn đã xâm chiếm Boromir.\N- Chiếc Nhẫn đâu?
Dialogue: 0,3:03:01.49,3:03:03.19,Default,,0,0,0,,Tránh ra!
Dialogue: 0,3:03:03.59,3:03:05.19,Default,,0,0,0,,Frodo!
Dialogue: 0,3:03:07.49,3:03:11.19,Default,,0,0,0,,- Ta đã thề sẽ bảo vệ cậu.\N- Anh có bảo vệ tôi khỏi chính anh được không?
Dialogue: 0,3:03:21.09,3:03:23.39,Default,,0,0,0,,Anh sẽ phá huỷ nó chứ?
Dialogue: 0,3:03:28.59,3:03:30.39,Default,,0,0,0,,{\i1}Aragorn.{\i0}
Dialogue: 0,3:03:33.89,3:03:36.09,Default,,0,0,0,,{\i1}Aragorn.{\i0}
Dialogue: 0,3:03:38.09,3:03:39.79,Default,,0,0,0,,{\i1}Elessar.{\i0}
Dialogue: 0,3:03:47.49,3:03:50.69,Default,,0,0,0,,Ta sẽ đi với cậu đến tận cùng.
Dialogue: 0,3:03:51.49,3:03:54.29,Default,,0,0,0,,Vào trong ngọn lửa của Mordor.
Dialogue: 0,3:03:57.79,3:03:58.89,Default,,0,0,0,,Tôi biết.
Dialogue: 0,3:04:01.29,3:04:06.59,Default,,0,0,0,,Hãy chăm sóc người khác, nhất là Sam.\NCậu ấy sẽ không hiểu được.
Dialogue: 0,3:04:10.59,3:04:12.19,Default,,0,0,0,,Đi nào, Frodo!
Dialogue: 0,3:04:15.29,3:04:16.79,Default,,0,0,0,,Chạy đi!
Dialogue: 0,3:04:16.99,3:04:18.59,Default,,0,0,0,,Chạy mau!
Dialogue: 0,3:04:43.19,3:04:44.79,Default,,0,0,0,,Cậu Frodo!
Dialogue: 0,3:04:54.29,3:04:55.89,Default,,0,0,0,,Tìm tên Hobbit!
Dialogue: 0,3:04:57.59,3:04:59.99,Default,,0,0,0,,Tìm tên Hobbit!
Dialogue: 0,3:05:03.69,3:05:05.29,Default,,0,0,0,,Elendil!
Dialogue: 0,3:05:11.79,3:05:13.29,Default,,0,0,0,,Aragorn, đi mau!
Dialogue: 0,3:05:34.99,3:05:36.29,Default,,0,0,0,,Frodo!
Dialogue: 0,3:05:36.89,3:05:38.49,Default,,0,0,0,,Trốn qua đây, mau lên!
Dialogue: 0,3:05:39.19,3:05:40.59,Default,,0,0,0,,Mau lên!
Dialogue: 0,3:05:43.09,3:05:45.09,Default,,0,0,0,,Cậu ấy làm gì vậy?
Dialogue: 0,3:05:49.29,3:05:51.29,Default,,0,0,0,,Cậu ấy đang bỏ đi.
Dialogue: 0,3:05:56.19,3:05:58.99,Default,,0,0,0,,- Không!\N- Pippin!
Dialogue: 0,3:06:03.99,3:06:06.19,Default,,0,0,0,,Chạy đi, Frodo, chạy đi!
Dialogue: 0,3:06:06.89,3:06:09.79,Default,,0,0,0,,- Ê, tụi bay!\N- Ở đây này!
Dialogue: 0,3:06:10.39,3:06:11.89,Default,,0,0,0,,- Ở đây!\N- Lối này!
Dialogue: 0,3:06:22.89,3:06:25.09,Default,,0,0,0,,- Hiệu quả rồi!\N- Tớ biết nó hiệu quả mà! Chạy đi!
Dialogue: 0,3:07:20.49,3:07:23.39,Default,,0,0,0,,- Tiếng tù và của Gondor!\N- Boromir!
Dialogue: 0,3:08:06.69,3:08:07.69,Default,,0,0,0,,Chạy đi!
Dialogue: 0,3:11:39.39,3:11:40.89,Default,,0,0,0,,Không!
Dialogue: 0,3:11:44.99,3:11:47.89,Default,,0,0,0,,- Chúng bắt mấy cậu nhỏ rồi.\N- Yên nào.
Dialogue: 0,3:11:48.19,3:11:50.19,Default,,0,0,0,,Frodo! Frodo đâu?
Dialogue: 0,3:11:52.19,3:11:53.59,Default,,0,0,0,,Tôi để Frodo đi rồi.
Dialogue: 0,3:11:54.29,3:11:56.79,Default,,0,0,0,,Vậy anh đã làm được\Nviệc mà tôi không thể.
Dialogue: 0,3:11:57.89,3:12:01.09,Default,,0,0,0,,Tôi đã cố lấy chiếc Nhẫn\Ntừ tay Frodo.
Dialogue: 0,3:12:01.69,3:12:05.79,Default,,0,0,0,,- Chiếc Nhẫn giờ đã ngoài tầm của chúng ta.\N- Thứ lỗi cho tôi.
Dialogue: 0,3:12:06.19,3:12:08.39,Default,,0,0,0,,Tôi đã không hiểu.
Dialogue: 0,3:12:08.69,3:12:11.79,Default,,0,0,0,,- Tôi đã làm mọi người thất vọng.\N- Không, Boromir.
Dialogue: 0,3:12:12.39,3:12:14.79,Default,,0,0,0,,Anh chiến đấu rất dũng cảm!
Dialogue: 0,3:12:15.19,3:12:17.69,Default,,0,0,0,,Anh đã giữ được\Ndanh dự của mình.
Dialogue: 0,3:12:18.49,3:12:20.19,Default,,0,0,0,,Bỏ đi!
Dialogue: 0,3:12:20.69,3:12:22.69,Default,,0,0,0,,Đã hết rồi.
Dialogue: 0,3:12:23.99,3:12:29.09,Default,,0,0,0,,Thế giới loài Người sẽ sụp đổ,\Nmọi thứ sẽ chìm vào bóng tối...
Dialogue: 0,3:12:29.69,3:12:32.49,Default,,0,0,0,,và thành phố của tôi sẽ bị tàn phá.
Dialogue: 0,3:12:36.69,3:12:39.39,Default,,0,0,0,,Tôi không biết sức mạnh nào\Nđang có trong dòng máu của mình...
Dialogue: 0,3:12:39.59,3:12:43.49,Default,,0,0,0,,nhưng tôi thề với anh\Nsẽ không để Thành phố Trắng...
Dialogue: 0,3:12:44.59,3:12:46.99,Default,,0,0,0,,hoặc người dân của ta gục ngã!
Dialogue: 0,3:12:47.99,3:12:49.39,Default,,0,0,0,,Người dân của chúng ta.
Dialogue: 0,3:12:53.29,3:12:55.29,Default,,0,0,0,,Người dân của chúng ta.
Dialogue: 0,3:13:17.09,3:13:19.99,Default,,0,0,0,,Tôi sẽ phục vụ anh, người anh em.
Dialogue: 0,3:13:21.69,3:13:23.59,Default,,0,0,0,,Người Thủ lĩnh.
Dialogue: 0,3:13:26.39,3:13:28.19,Default,,0,0,0,,Đức vua của tôi!
Dialogue: 0,3:13:47.89,3:13:49.99,Default,,0,0,0,,Hãy yên nghỉ...
Dialogue: 0,3:13:50.59,3:13:52.39,Default,,0,0,0,,hỡi người con của Gondor.
Dialogue: 0,3:14:08.89,3:14:12.09,Default,,0,0,0,,Tại Tháp Trắng, họ sẽ\Nmong chờ anh ấy.
Dialogue: 0,3:14:12.79,3:14:15.69,Default,,0,0,0,,Nhưng anh ấy sẽ không quay về nữa.
Dialogue: 0,3:14:39.09,3:14:40.99,Default,,0,0,0,,Frodo!
Dialogue: 0,3:15:00.49,3:15:03.69,Default,,0,0,0,,{\i1}Cháu ước gì chiếc Nhẫn\Nkhông đến với mình.{\i0}
Dialogue: 0,3:15:04.99,3:15:07.79,Default,,0,0,0,,{\i1}Cháu ước không có chuyện gì xảy ra.{\i0}
Dialogue: 0,3:15:10.99,3:15:13.69,Default,,0,0,0,,{\i1}Ai rồi cũng gặp những vấn đề...{\i0}
Dialogue: 0,3:15:13.99,3:15:17.29,Default,,0,0,0,,{\i1}nhưng họ không quyết định\Nđược chuyện đó.{\i0}
Dialogue: 0,3:15:17.69,3:15:20.19,Default,,0,0,0,,{\i1}Họ chỉ quyết định sẽ hành động ra sao...{\i0}
Dialogue: 0,3:15:20.49,3:15:25.09,Default,,0,0,0,,{\i1}trước những việc như thế.{\i0}
Dialogue: 0,3:15:53.99,3:15:55.99,Default,,0,0,0,,Frodo, không!
Dialogue: 0,3:15:56.79,3:15:57.99,Default,,0,0,0,,Frodo!
Dialogue: 0,3:15:58.69,3:16:01.29,Default,,0,0,0,,- Cậu Frodo!\N- Đừng, Sam.
Dialogue: 0,3:16:04.99,3:16:06.89,Default,,0,0,0,,Quay về đi, Sam!
Dialogue: 0,3:16:07.49,3:16:10.89,Default,,0,0,0,,- Tôi sẽ tới Mordor một mình.\N- Đương nhiên cậu sẽ đi.
Dialogue: 0,3:16:11.19,3:16:13.89,Default,,0,0,0,,Và tôi sẽ đi với cậu!
Dialogue: 0,3:16:14.59,3:16:16.89,Default,,0,0,0,,Cậu không biết bơi mà Sam!
Dialogue: 0,3:16:20.69,3:16:22.39,Default,,0,0,0,,Sam!
Dialogue: 0,3:16:27.99,3:16:29.39,Default,,0,0,0,,Sam!
Dialogue: 0,3:17:10.29,3:17:12.99,Default,,0,0,0,,Tôi đã hứa rồi cậu Frodo.
Dialogue: 0,3:17:13.39,3:17:17.49,Default,,0,0,0,,Một lời hứa! "Không rời khỏi\Ncậu ấy, Samwise Gamgee."
Dialogue: 0,3:17:18.79,3:17:21.29,Default,,0,0,0,,Và tôi sẽ làm thế!
Dialogue: 0,3:17:21.69,3:17:23.79,Default,,0,0,0,,Tôi sẽ làm thế.
Dialogue: 0,3:17:25.59,3:17:27.29,Default,,0,0,0,,Sam!
Dialogue: 0,3:17:42.29,3:17:43.89,Default,,0,0,0,,Đi thôi nào.
Dialogue: 0,3:18:12.30,3:18:16.70,Default,,0,0,0,,Nhanh lên! Frodo và Sam\Nđã qua bờ đông rồi.
Dialogue: 0,3:18:31.80,3:18:36.70,Default,,0,0,0,,- Anh định không theo họ à?\N- Số phận của Frodo không gắn với bọn ta được!
Dialogue: 0,3:18:40.20,3:18:44.90,Default,,0,0,0,,Tất cả thật vô ích!\NHội đồng hành đã tan rã.
Dialogue: 0,3:18:56.90,3:19:00.00,Default,,0,0,0,,Không đâu, nếu chúng ta\Nvẫn còn tin tưởng nhau.
Dialogue: 0,3:19:01.50,3:19:05.80,Default,,0,0,0,,Ta sẽ không để mặc Merry và Pippin\Nbị hành hạ và chết...
Dialogue: 0,3:19:06.90,3:19:12.00,Default,,0,0,0,,khi ta vẫn còn chút sức lực.\NĐể lại hết những thứ dư thừa đi.
Dialogue: 0,3:19:13.50,3:19:15.40,Default,,0,0,0,,Chúng ta sẽ đi\Nvới thật ít hành trang.
Dialogue: 0,3:19:15.70,3:19:17.60,Default,,0,0,0,,Đi săn bọn Orc nào!
Dialogue: 0,3:19:20.60,3:19:22.60,Default,,0,0,0,,Có thế chứ!
Dialogue: 0,3:19:41.60,3:19:43.00,Default,,0,0,0,,Mordor.
Dialogue: 0,3:19:43.70,3:19:46.90,Default,,0,0,0,,Hy vọng bọn họ sẽ tìm được\Ncon đường an toàn hơn.
Dialogue: 0,3:19:48.00,3:19:50.50,Default,,0,0,0,,Aragorn sẽ lo cho họ.
Dialogue: 0,3:19:51.60,3:19:54.80,Default,,0,0,0,,Tôi không nghĩ là sẽ gặp lại họ nữa.
Dialogue: 0,3:19:55.30,3:19:57.60,Default,,0,0,0,,Chúng ta có thể, cậu Frodo.
Dialogue: 0,3:19:57.80,3:19:59.30,Default,,0,0,0,,Chúng ta có thể.
Dialogue: 0,3:20:01.50,3:20:02.60,Default,,0,0,0,,Sam,
Dialogue: 0,3:20:06.20,3:20:08.70,Default,,0,0,0,,tôi mừng vì có cậu đi cùng.