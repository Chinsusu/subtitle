Dialogue: 0,0:00:03.00,0:00:09.00,Default,,0,0,0,,VAV STUDIO .:. VIETTORRENT.VN\Nwww.facebook.com/VAV.AudioViet
Dialogue: 0,0:00:09.01,0:00:16.00,Default,,0,0,0,,VAV STUDIO\N© Copyright 2013, All rights reserved
Dialogue: 0,0:00:16.01,0:00:23.00,Default,,0,0,0,,Cảm ơn các nhà tài trợ đã donate để VAV\Ncó kinh phí thực hiện Thuyết minh phim.
Dialogue: 0,0:00:23.01,0:00:30.00,Default,,0,0,0,,Việc chia sẻ trong cộng đồng\Nphải được sự đồng ý của VAV.
Dialogue: 0,0:00:53.35,0:00:55.35,Default,,0,0,0,,Phụ đề dịch bởi Emma - Subteam 2pi.vn
Dialogue: 0,0:00:55.35,0:00:59.41,Default,,0,0,0,,VAV STUDIO Trân Trọng Giới thiệu\NBộ phim: Mật danh - Phần II (2013)\N{\i1}[Tập 16]{\i0}
Dialogue: 0,0:01:16.47,0:01:17.97,Default,,0,0,0,,Ngồi xuống đi.
Dialogue: 0,0:01:26.87,0:01:28.96,Default,,0,0,0,,Anh lại có ý đồ gì đây?
Dialogue: 0,0:01:33.43,0:01:35.76,Default,,0,0,0,,Không có gì đâu.
Dialogue: 0,0:01:36.72,0:01:39.82,Default,,0,0,0,,Giống khi tôi ở Nhật...
Dialogue: 0,0:01:39.82,0:01:44.17,Default,,0,0,0,,tôi nghĩ cô cũng đang trăn trở về bản thân.
Dialogue: 0,0:01:44.17,0:01:47.71,Default,,0,0,0,,Bước tiếp theo mình nên làm gì...
Dialogue: 0,0:01:47.71,0:01:50.89,Default,,0,0,0,,và nên oán trách ai.
Dialogue: 0,0:01:53.75,0:01:56.87,Default,,0,0,0,,Cô nên ăn đi trước khi nguội.
Dialogue: 0,0:02:31.88,0:02:36.11,Default,,0,0,0,,Tôi cũng không muốn hại những người vô tội.
Dialogue: 0,0:02:36.17,0:02:39.31,Default,,0,0,0,,Tôi không biết thông tin này có ích không...
Dialogue: 0,0:02:39.31,0:02:43.86,Default,,0,0,0,,nhưng muốn biết danh tính của Ngài\NBlack, anh nên tìm hiểu quá khứ.
Dialogue: 0,0:02:47.10,0:02:51.73,Default,,0,0,0,,5 nhà vật lý nguyên tử mà\NBaek San ra lệnh ám sát...
Dialogue: 0,0:02:51.87,0:02:54.99,Default,,0,0,0,,Tôi đã từng nghe Rey nói...
Dialogue: 0,0:02:55.68,0:03:01.50,Default,,0,0,0,,Ngài Black là một trong số họ,...
Dialogue: 0,0:03:01.50,0:03:05.14,Default,,0,0,0,,người duy nhất sống sót.
Dialogue: 0,0:03:23.39,0:03:25.45,Default,,0,0,0,,Anh đã biết thêm được gì?
Dialogue: 0,0:03:25.47,0:03:27.91,Default,,0,0,0,,Tôi cần đến Cơ quan Lưu trữ Quốc\Ngia để tìm hiểu vài việc.
Dialogue: 0,0:03:27.92,0:03:29.27,Default,,0,0,0,,Cơ quan Lưu trữ Quốc gia ư?
Dialogue: 0,0:03:29.27,0:03:31.65,Default,,0,0,0,,{\i1}Tôi cần xem tài liệu về Cục\NTình báo Trung ương (CIS).{\i0}
Dialogue: 0,0:03:31.65,0:03:34.43,Default,,0,0,0,,{\i1}Những tài liệu mật từ năm 1979.{\i0}
Dialogue: 0,0:03:34.43,0:03:36.11,Default,,0,0,0,,Tôi có thể làm gì giúp anh?
Dialogue: 0,0:03:36.11,0:03:39.77,Default,,0,0,0,,Tôi cần giấy tờ cho phép tiếp cận\Ndữ liệu và dụng cụ để sao chụp.
Dialogue: 0,0:03:39.84,0:03:44.39,Default,,0,0,0,,{\i1}Được. Tôi sẽ cho người chuẩn bị\Nđầy đủ tại tủ đồ ở sảnh.{\i0}
Dialogue: 0,0:03:44.39,0:03:47.54,Default,,0,0,0,,{\i1}Tôi sẽ gửi mã số tủ cho anh sau.{\i0}
Dialogue: 0,0:03:49.59,0:03:51.26,Default,,0,0,0,,Việc tìm bắt gián điệp thế nào rồi?
Dialogue: 0,0:03:51.44,0:03:54.57,Default,,0,0,0,,Tôi sẽ thẩm vấn cả Yoon\NShi Hyuk và Seo Hyun Woo.
Dialogue: 0,0:03:54.57,0:03:55.88,Default,,0,0,0,,{\i1}Seo Hyun Woo ư?{\i0}
Dialogue: 0,0:03:55.88,0:03:58.06,Default,,0,0,0,,Không thể nào, tôi không\Nnghĩ đó là Seo Hyun Woo.
Dialogue: 0,0:03:58.06,0:04:01.47,Default,,0,0,0,,Tôi cũng thế, nhưng vẫn\Nphải tiến hành thẩm vấn.
Dialogue: 0,0:04:01.47,0:04:02.97,Default,,0,0,0,,Tôi sẽ liên lạc sau.
Dialogue: 0,0:04:14.23,0:04:15.91,Default,,0,0,0,,{\i1}Là tôi đây ạ.{\i0}
Dialogue: 0,0:04:15.98,0:04:20.21,Default,,0,0,0,,Tôi xác nhận là Phó Cục trưởng Choi\NMin có liên lạc với Jung Yoo Gun.
Dialogue: 0,0:04:20.60,0:04:22.14,Default,,0,0,0,,Tôi có nên bắt cô ta bây giờ không?
Dialogue: 0,0:04:22.14,0:04:24.85,Default,,0,0,0,,{\i1}Không, cứ vờ như anh không biết gì.{\i0}
Dialogue: 0,0:04:25.56,0:04:30.43,Default,,0,0,0,,{\i1}Chúng ta cần bằng chứng thuyết\Nphục để diệt gọn cô ta.{\i0}
Dialogue: 0,0:04:31.67,0:04:35.55,Default,,0,0,0,,Vâng, tôi sẽ tiếp tục giám sát cô ta.
Dialogue: 0,0:04:39.41,0:04:43.92,Default,,0,0,0,,IRIS luôn có gián điệp bên trong NSS.
Dialogue: 0,0:04:44.30,0:04:49.27,Default,,0,0,0,,Rey lấy thông tin nội bộ từ đó.
Dialogue: 0,0:04:49.68,0:04:51.09,Default,,0,0,0,,Họ là những ai?
Dialogue: 0,0:04:51.09,0:04:53.27,Default,,0,0,0,,Tôi cũng không được biết.
Dialogue: 0,0:04:55.68,0:04:57.85,Default,,0,0,0,,Mà chúng ta đang đi đâu vậy?
Dialogue: 0,0:04:59.21,0:05:01.69,Default,,0,0,0,,Tôi đưa cô đến nơi khác an toàn hơn.
Dialogue: 0,0:05:05.02,0:05:06.67,Default,,0,0,0,,{\i1}[Cơ quan Lưu trữ Quốc gia]{\i0}
Dialogue: 0,0:05:14.01,0:05:15.34,Default,,0,0,0,,{\i1}[Tủ số 17, mã số 8857]{\i0}
Dialogue: 0,0:05:59.60,0:06:00.60,Default,,0,0,0,,{\i1}[Thẻ cho khách]{\i0}
Dialogue: 0,0:06:00.88,0:06:03.82,Default,,0,0,0,,Tôi cần vào phòng lưu trữ thông tin mật.
Dialogue: 0,0:06:03.92,0:06:05.26,Default,,0,0,0,,Xin đợi một chút.
Dialogue: 0,0:06:22.58,0:06:23.80,Default,,0,0,0,,Của anh đây.
Dialogue: 0,0:06:25.06,0:06:26.13,Default,,0,0,0,,Cảm ơn.
Dialogue: 0,0:07:54.17,0:07:56.68,Default,,0,0,0,,{\i1}Giám đốc CIS, Kim Jung Yong lệnh cho...{\i0}
Dialogue: 0,0:07:56.68,0:07:58.73,Default,,0,0,0,,{\i1}một đặc vụ, Yoo Sang Joon,\Nthực hiện ám sát...{\i0}
Dialogue: 0,0:07:58.73,0:08:04.85,Default,,0,0,0,,{\i1}5 nhà khoa học tham gia nghiên\Ncứu chương trình hạt nhân.{\i0}
Dialogue: 0,0:08:16.33,0:08:22.36,Default,,0,0,0,,{\i1}Kim Huey So, Lee Jung Nan và Choi Yi Goon\Nđều bị ám sát bởi Yoo Sang Joon.{\i0}
Dialogue: 0,0:08:22.55,0:08:26.71,Default,,0,0,0,,{\i1}Yoo Jong Won người đã biến mất sau khi phát\Nhiện bị liệt bán thân do đạn bắn...{\i0}
Dialogue: 0,0:08:26.71,0:08:32.45,Default,,0,0,0,,{\i1}bị giết bởi Baek San, Cục\Ntrưởng mới của NSS vào năm 2009.{\i0}
Dialogue: 0,0:08:32.56,0:08:37.63,Default,,0,0,0,,{\i1}Anh trai của ông ta, Yoo Jong\NChul hiện không rõ tung tích.{\i0}
Dialogue: 0,0:08:44.19,0:08:47.55,Default,,0,0,0,,Người đàn ông này đang ở trong phải không?
Dialogue: 0,0:08:47.55,0:08:49.22,Default,,0,0,0,,Vâng.
Dialogue: 0,0:09:04.59,0:09:06.61,Default,,0,0,0,,Tôi đã tìm thấy Jung Yoo Gun.
Dialogue: 0,0:09:06.63,0:09:09.01,Default,,0,0,0,,Phải bắt cho được anh ta. Đừng để thoát.
Dialogue: 0,0:09:09.01,0:09:10.13,Default,,0,0,0,,Vâng.
Dialogue: 0,0:10:27.27,0:10:29.00,Default,,0,0,0,,Chúng ta mất dấu anh ta rồi.
Dialogue: 0,0:12:19.29,0:12:21.15,Default,,0,0,0,,Không thể...
Dialogue: 0,0:12:22.69,0:12:24.55,Default,,0,0,0,,Không thể nào là anh.
Dialogue: 0,0:12:37.23,0:12:42.93,Default,,0,0,0,,Xin lỗi. Có phải anh đã tới quá muộn không?
Dialogue: 0,0:13:57.62,0:14:03.62,Default,,0,0,0,,Giờ nhìn con, ta thấy con rất giống mẹ.
Dialogue: 0,0:14:06.37,0:14:11.12,Default,,0,0,0,,Có lẽ bà ấy chịu đựng được\Nngần ấy năm là nhờ có con.
Dialogue: 0,0:14:14.43,0:14:19.68,Default,,0,0,0,,Trước kia, ta cứ sống không suy nghĩ gì...
Dialogue: 0,0:14:19.68,0:14:21.85,Default,,0,0,0,,vì không thể thay đổi được sự thật.
Dialogue: 0,0:14:23.87,0:14:28.88,Default,,0,0,0,,Ta nghĩ hối hận chỉ là lãng phí thời gian.
Dialogue: 0,0:14:29.70,0:14:32.21,Default,,0,0,0,,Nhưng...
Dialogue: 0,0:14:32.21,0:14:37.12,Default,,0,0,0,,Khi biết ta khiến Jung Soo Min phải đánh\Nđổi lấy thân phận Jung Ji Young...
Dialogue: 0,0:14:37.12,0:14:41.58,Default,,0,0,0,,và việc ta đã sống mà không biết\Nđến sự tồn tại của 2 mẹ con con.
Dialogue: 0,0:14:43.20,0:14:45.95,Default,,0,0,0,,Điều đó khiến ta day dứt.
Dialogue: 0,0:14:47.45,0:14:51.30,Default,,0,0,0,,Con hẳn... vô cùng oán trách ta.
Dialogue: 0,0:14:55.47,0:15:00.12,Default,,0,0,0,,Được, con cứ căm hận ta.\NHãy để ta chịu mọi lỗi lầm.
Dialogue: 0,0:15:01.78,0:15:03.93,Default,,0,0,0,,Nhưng...
Dialogue: 0,0:15:07.45,0:15:12.64,Default,,0,0,0,,xin con đừng truy đuổi IRIS nữa.
Dialogue: 0,0:15:13.69,0:15:19.93,Default,,0,0,0,,Hãy để ta lo mọi việc từ đây.
Dialogue: 0,0:15:21.23,0:15:28.03,Default,,0,0,0,,Oán hận... Báo thù Yoo Joong Won\Nvà IRIS, là nhiệm vụ của tôi.
Dialogue: 0,0:15:28.46,0:15:33.02,Default,,0,0,0,,Ông đừng nhúng tay vào.
Dialogue: 0,0:15:35.20,0:15:38.35,Default,,0,0,0,,Con không thể làm gì được đâu.
Dialogue: 0,0:15:38.35,0:15:41.34,Default,,0,0,0,,Ông biết những gì về Ngài Black?
Dialogue: 0,0:15:42.40,0:15:47.38,Default,,0,0,0,,Tất cả mọi thứ bắt đầu từ lúc đó.
Dialogue: 0,0:15:47.38,0:15:54.07,Default,,0,0,0,,Từ lúc ông nhận nhiệm vụ ám\Nsát 5 nhà vật lý hạt nhân.
Dialogue: 0,0:15:57.61,0:15:59.21,Default,,0,0,0,,Con đang nói gì vậy?
Dialogue: 0,0:15:59.31,0:16:05.09,Default,,0,0,0,,Yoo Jong Chul, ngiười duy nhất ông đã\Nkhông thể giết, chính là Ngài Black.
Dialogue: 0,0:16:08.32,0:16:14.83,Default,,0,0,0,,Thật phi lý. Chính ông ra yêu\Ncầu ta giết Yoo Jong Won.
Dialogue: 0,0:16:17.48,0:16:19.75,Default,,0,0,0,,Tại sao ông ta lại muốn\Ngiết anh trai mình?
Dialogue: 0,0:16:19.75,0:16:24.57,Default,,0,0,0,,Người ra lệnh ám sát Kwon Young Chan...
Dialogue: 0,0:16:24.57,0:16:26.99,Default,,0,0,0,,chính là anh trai ông ta, Kwon Young Choon.
Dialogue: 0,0:16:28.08,0:16:30.66,Default,,0,0,0,,Lịch sử tái diễn.
Dialogue: 0,0:16:30.66,0:16:34.97,Default,,0,0,0,,Một cái là thảm kịch còn\Nmột là màn tấu hài đặc sắc.
Dialogue: 0,0:16:34.97,0:16:39.26,Default,,0,0,0,,Ông đã lựa chọn khởi xướng\Ntấn bi kịch thì tôi chọn...
Dialogue: 0,0:16:39.26,0:16:41.49,Default,,0,0,0,,đứng ở bờ đối diện.
Dialogue: 0,0:16:41.65,0:16:47.96,Default,,0,0,0,,Giờ là lúc... tôi đặt dấu chấm hết cho tất cả.
Dialogue: 0,0:16:57.24,0:16:59.82,Default,,0,0,0,,Xin con đừng tiếp tục như thế.
Dialogue: 0,0:17:00.87,0:17:03.58,Default,,0,0,0,,Tôi có thể thỉnh cầu một việc không?
Dialogue: 0,0:17:04.91,0:17:08.42,Default,,0,0,0,,Trước khi tôi tìm thấy Yoo\NJoong Won và Ngài Black...
Dialogue: 0,0:17:09.60,0:17:12.81,Default,,0,0,0,,ông đứng để bị chúng bắt được.
Dialogue: 0,0:18:18.00,0:18:20.39,Default,,0,0,0,,Anh đã tìm thấy gì ở\NCơ quan Lưu trữ Quốc gia?
Dialogue: 0,0:18:32.98,0:18:35.73,Default,,0,0,0,,Tôi đã xác nhận việc ám\Nsát 5 nhà khoa học...
Dialogue: 0,0:18:35.73,0:18:39.54,Default,,0,0,0,,tham gia vào chương trình hạt\Nnhân thời kì chính phủ thứ 4.
Dialogue: 0,0:18:39.54,0:18:44.37,Default,,0,0,0,,3 người trong số họ đã bị giết\Nbởi 1 đặc vụ CIS, Yoo Sang Joon.
Dialogue: 0,0:18:44.37,0:18:47.43,Default,,0,0,0,,1 trong 2 người còn sống sót,\NYoo Jong Won...
Dialogue: 0,0:18:47.43,0:18:51.47,Default,,0,0,0,,đã bị Baek San giết năm 2009.
Dialogue: 0,0:18:52.13,0:18:59.07,Default,,0,0,0,,Tôi nghĩ nhà khoa học cuối cùng,\NYoo Jong Chul chính là Ngài Black.
Dialogue: 0,0:18:59.23,0:19:01.03,Default,,0,0,0,,Yoo Jong Chul?
Dialogue: 0,0:19:05.52,0:19:10.32,Default,,0,0,0,,Đội trưởng Jung... cha anh... không...
Dialogue: 0,0:19:11.04,0:19:14.36,Default,,0,0,0,,Anh có tin gì từ Baek San không?
Dialogue: 0,0:19:16.04,0:19:18.67,Default,,0,0,0,,- Nếu ông ta liên lạc với anh...\N- Tôi hiểu mà.
Dialogue: 0,0:19:18.67,0:19:23.34,Default,,0,0,0,,Chúng ta cần tìm ra... quả bom còn lại.
Dialogue: 0,0:19:25.28,0:19:30.65,Default,,0,0,0,,Còn nữa... anh ở cùng Kim\NYun Hwa không nguy hiểm chứ?
Dialogue: 0,0:19:32.18,0:19:34.18,Default,,0,0,0,,Tôi sẽ tự lo được.
Dialogue: 0,0:19:56.29,0:20:00.66,Default,,0,0,0,,Vẫn ở đây à?\NCô được tự do rồi mà.
Dialogue: 0,0:20:03.30,0:20:05.10,Default,,0,0,0,,Tôi đói.
Dialogue: 0,0:20:05.86,0:20:07.90,Default,,0,0,0,,Trong tủ lạnh có đồ ăn đấy.
Dialogue: 0,0:20:08.31,0:20:10.04,Default,,0,0,0,,Anh ăn gì chưa?
Dialogue: 0,0:20:11.38,0:20:13.95,Default,,0,0,0,,Sao đột nhiên cô nói năng nhã nhặn vậy?
Dialogue: 0,0:20:17.21,0:20:19.69,Default,,0,0,0,,Anh lớn tuổi hơn tôi còn gì.
Dialogue: 0,0:20:32.69,0:20:33.92,Default,,0,0,0,,Này.
Dialogue: 0,0:20:51.99,0:20:53.95,Default,,0,0,0,,Giờ anh định làm gì?
Dialogue: 0,0:20:53.95,0:20:55.63,Default,,0,0,0,,Tôi cần tìm Yoon Joong Won.
Dialogue: 0,0:20:55.63,0:20:57.70,Default,,0,0,0,,Gặp anh ta rồi, anh định làm gì?
Dialogue: 0,0:20:58.59,0:21:00.74,Default,,0,0,0,,Anh sẽ giết anh ta sao?
Dialogue: 0,0:21:54.47,0:21:56.92,Default,,0,0,0,,Kết quả tốt hơn tôi nghĩ.
Dialogue: 0,0:21:56.92,0:22:00.86,Default,,0,0,0,,Cô định hình kế hoạch tiếp theo thế nào?
Dialogue: 0,0:22:00.86,0:22:04.63,Default,,0,0,0,,Đầu tiên cần tìm ra quả bom\Nthứ 5 mà Baek San đang giữ.
Dialogue: 0,0:22:04.63,0:22:07.56,Default,,0,0,0,,Hiện NSS đang nỗ lực hết\Nsức để truy lùng ông ta.
Dialogue: 0,0:22:07.63,0:22:10.38,Default,,0,0,0,,Tôi muốn bàn với cô về số\Nbom hạt nhân ta đang giữ.
Dialogue: 0,0:22:10.38,0:22:13.37,Default,,0,0,0,,Trước tiên tôi muốn biết ý kiến\Ncủa cô về việc xử lý chúng.
Dialogue: 0,0:22:13.37,0:22:18.00,Default,,0,0,0,,Tôi nghĩ để tiếp tục việc phi hạt\Nnhân hóa Bán đảo Triều tiên...
Dialogue: 0,0:22:18.00,0:22:23.05,Default,,0,0,0,,nên thông báo cho các nước đồng minh về sự tồn\Ntại của chúng và thực hiện giải trừ số bom.
Dialogue: 0,0:22:23.06,0:22:27.63,Default,,0,0,0,,Phía Bắc đã sở hữu công\Nnghệ hạt nhân từ lâu.
Dialogue: 0,0:22:27.63,0:22:30.67,Default,,0,0,0,,Vậy cô nghĩ duy trì việc phi\Nhạt nhân hóa là sáng suốt à?
Dialogue: 0,0:22:31.38,0:22:32.74,Default,,0,0,0,,Theo như Cơ quan Phát triển Quốc phòng...
Dialogue: 0,0:22:32.74,0:22:36.49,Default,,0,0,0,,họ có thể cho lắp đặt những đầu\Nđạn hạt nhân vào một số tên lửa.
Dialogue: 0,0:22:36.54,0:22:39.18,Default,,0,0,0,,Phạm vi của tên lửa có thể bao\Nphủ toàn bộ lãnh thổ Bắc Hàn.
Dialogue: 0,0:22:39.18,0:22:43.71,Default,,0,0,0,,Giải trừ hay đầu tư phát triển số bom...
Dialogue: 0,0:22:43.71,0:22:46.77,Default,,0,0,0,,Lựa chọn nào lý tưởng\Nhơn cho lợi ích quốc gia?
Dialogue: 0,0:22:46.98,0:22:52.04,Default,,0,0,0,,Thưa Tổng thống, nếu là trước kia...
Dialogue: 0,0:22:52.04,0:22:54.53,Default,,0,0,0,,hẳn Ngài đã đồng tình với đề xuất của tôi.
Dialogue: 0,0:22:56.51,0:23:00.25,Default,,0,0,0,,Hòa bình không phải tự nhiên mà có.
Dialogue: 0,0:23:01.21,0:23:03.55,Default,,0,0,0,,Cho đến khi nào còn chênh lệch vũ trang...
Dialogue: 0,0:23:03.55,0:23:06.23,Default,,0,0,0,,thì rất khó khăn để ngăn chặn chiến tranh.
Dialogue: 0,0:23:06.23,0:23:08.83,Default,,0,0,0,,Vì hòa bình quốc gia...
Dialogue: 0,0:23:08.83,0:23:12.15,Default,,0,0,0,,sẽ tốt hơn nếu ta được\Ntrang bị vũ khí hạt nhân.
Dialogue: 0,0:23:15.69,0:23:17.38,Default,,0,0,0,,Và nữa...
Dialogue: 0,0:23:17.38,0:23:19.55,Default,,0,0,0,,Tôi đã cho triệu tập Cục\Ntrưởng Kang Chul Hwan...
Dialogue: 0,0:23:19.74,0:23:21.30,Default,,0,0,0,,để bàn về vấn đề vũ trang hạt nhân...
Dialogue: 0,0:23:21.30,0:23:24.47,Default,,0,0,0,,và việc hợp nhất NSS và NIS.
Dialogue: 0,0:23:24.47,0:23:28.12,Default,,0,0,0,,Chúng ta sẽ lập một đội chuyên\Nquản lý vũ khí hạt nhân.
Dialogue: 0,0:23:41.36,0:23:43.08,Default,,0,0,0,,{\i1}Choi Min.\NChuyện gì vậy?{\i0}
Dialogue: 0,0:23:43.50,0:23:45.50,Default,,0,0,0,,Cô đã có thêm thông tin\Ngì về Anthony Choi chưa?
Dialogue: 0,0:23:45.52,0:23:48.11,Default,,0,0,0,,{\i1}Theo những gì tôi điều tra được\Nthì cũng không có gì đặc biệt.{\i0}
Dialogue: 0,0:23:48.11,0:23:51.71,Default,,0,0,0,,{\i1}Ông ta di cư vào Mỹ khoảng\Ngiữa năm 1979, 1980.{\i0}
Dialogue: 0,0:23:52.05,0:23:54.10,Default,,0,0,0,,Ông ta nhanh chóng được cấp thẻ xanh...
Dialogue: 0,0:23:54.10,0:23:57.47,Default,,0,0,0,,{\i1}chứng tỏ ông ta phải được giúp đỡ từ\Nchính phủ hoặc một tập đoàn lớn.{\i0}
Dialogue: 0,0:23:58.16,0:24:00.45,Default,,0,0,0,,Lĩnh vực kinh doanh chính của Jeron là gì?
Dialogue: 0,0:24:00.46,0:24:04.12,Default,,0,0,0,,{\i1}Jeron xếp thứ 5 trong ngành công\Nnghiệp vũ khí toàn thế giới.{\i0}
Dialogue: 0,0:24:04.12,0:24:06.30,Default,,0,0,0,,Máy bay chiến đấu tàng hình\Nđược cho định kỳ sơn lại ...
Dialogue: 0,0:24:06.30,0:24:08.45,Default,,0,0,0,,{\i1}sử dụng loại sơn hấp\Nthụ sóng điện của họ.{\i0}
Dialogue: 0,0:24:08.45,0:24:10.83,Default,,0,0,0,,{\i1}Trên thực tế, Jeron đang\Nnắm độc quyền thị trường.{\i0}
Dialogue: 0,0:24:11.74,0:24:14.38,Default,,0,0,0,,{\i1}Trông thần sắc cô tệ quá.\NKhông sao chứ?{\i0}
Dialogue: 0,0:24:14.38,0:24:16.44,Default,,0,0,0,,Jenny, cô hãy giúp tôi bắt liên lạc...
Dialogue: 0,0:24:16.44,0:24:18.49,Default,,0,0,0,,với người phụ trách khu vực Đông Á của NSA.
Dialogue: 0,0:24:18.61,0:24:21.23,Default,,0,0,0,,Vì sao? Cô muốn tới Lầu Năm Góc à?
Dialogue: 0,0:24:21.23,0:24:24.53,Default,,0,0,0,,Không phải thế... những cô có thể...
Dialogue: 0,0:24:24.53,0:24:28.10,Default,,0,0,0,,{\i1}Được rồi, khi nào có thông tin tôi\Nsẽ cho cô hay. Giữ sức khỏe nhé.{\i0}
Dialogue: 0,0:24:28.10,0:24:29.67,Default,,0,0,0,,- Tạm biệt.\N- Tạm biệt.
Dialogue: 0,0:24:46.95,0:24:48.57,Default,,0,0,0,,Ngài nghĩ sao?
Dialogue: 0,0:24:48.59,0:24:50.95,Default,,0,0,0,,Nếu là sự thật...
Dialogue: 0,0:24:50.95,0:24:54.30,Default,,0,0,0,,chỉ trong thời gian ngắn chúng ta có\Nthể làm chủ công nghệ hạt nhân.
Dialogue: 0,0:24:54.47,0:24:58.62,Default,,0,0,0,,Nhật Bản, Đài Loan và\Ncác nước Đông Á khác...
Dialogue: 0,0:24:58.62,0:25:00.81,Default,,0,0,0,,cũng sẽ góp mặt trong cuộc\Nchạy đua vũ trang hạt nhân.
Dialogue: 0,0:25:00.81,0:25:04.18,Default,,0,0,0,,Đó chính là lý do việc giải trừ hạt\Nnhân trên Bán đảo Hàn Quốc là thiêt yếu.
Dialogue: 0,0:25:04.18,0:25:06.82,Default,,0,0,0,,Ý cô là chúng ta nên phá hủy chúng\Nsau khi thông báo cho thế giới?
Dialogue: 0,0:25:06.82,0:25:09.03,Default,,0,0,0,,Tôi nghĩ đó là cách giải quyết hợp lý nhất.
Dialogue: 0,0:25:09.85,0:25:11.78,Default,,0,0,0,,Ý kiến của Ngài thế nào, thưa Tổng thống?
Dialogue: 0,0:25:11.78,0:25:14.19,Default,,0,0,0,,Đã xác nhận các đầu đạn hạt\Nnhân này có thể được lắp đặt...
Dialogue: 0,0:25:14.24,0:25:18.35,Default,,0,0,0,,vào những tên lửa có phạm vi\Nhoạt động bao phủ Bắc Hàn.
Dialogue: 0,0:25:24.37,0:25:26.46,Default,,0,0,0,,Như đã đề cập với Phó Cục trưởng Choi Min...
Dialogue: 0,0:25:26.46,0:25:29.72,Default,,0,0,0,,để duy trì hòa bình và\Nngăn chặn chiến tranh...
Dialogue: 0,0:25:29.72,0:25:32.74,Default,,0,0,0,,tôi định nhân cơ hội này thúc đẩy phát\Ntriển vũ trang hạt nhân ở nước ta.
Dialogue: 0,0:25:32.74,0:25:35.15,Default,,0,0,0,,Việc đó sẽ gây tổn hại đến mối quan\Nhệ hữu nghĩ với các nước đồng minh.
Dialogue: 0,0:25:35.85,0:25:38.84,Default,,0,0,0,,Nhưng nếu việc trang bị vũ khí hạt nhân\Nđược hoàn thành trong thời gian ngắn...
Dialogue: 0,0:25:38.84,0:25:40.95,Default,,0,0,0,,thì cũng đáng.
Dialogue: 0,0:25:41.45,0:25:42.92,Default,,0,0,0,,Ngài Tổng thống.
Dialogue: 0,0:25:43.72,0:25:46.63,Default,,0,0,0,,Tôi muốn Cục trưởng Kang\NChul Hwan trở lại NSS...
Dialogue: 0,0:25:46.63,0:25:50.79,Default,,0,0,0,,thành lập một đội nghiên cứu\Nvà quản lý vũ khí hạt nhân.
Dialogue: 0,0:25:50.79,0:25:52.72,Default,,0,0,0,,Tôi sẽ lập tức chấp hành.
Dialogue: 0,0:26:03.73,0:26:07.03,Default,,0,0,0,,Vậy là nay Nam Hàn đã có\Ntrong tay vũ khí hạt nhân.
Dialogue: 0,0:26:07.03,0:26:08.99,Default,,0,0,0,,Vũ trang hạt nhân không phải là\Ncách để duy trì hòa bình...
Dialogue: 0,0:26:08.99,0:26:10.57,Default,,0,0,0,,mà chỉ góp phần mở đầu sự sụp đổ.
Dialogue: 0,0:26:10.57,0:26:11.79,Default,,0,0,0,,Chúng ta phải dừng lại.
Dialogue: 0,0:26:13.93,0:26:15.39,Default,,0,0,0,,Bằng cách nào?
Dialogue: 0,0:26:45.66,0:26:47.69,Default,,0,0,0,,Hãy đi theo chúng tôi.
Dialogue: 0,0:26:47.69,0:26:49.95,Default,,0,0,0,,Có người muốn gặp ông.
Dialogue: 0,0:27:30.50,0:27:31.68,Default,,0,0,0,,Yoo Joong Won.
Dialogue: 0,0:27:31.68,0:27:34.22,Default,,0,0,0,,Đáng nhẽ phải lịch sự hơn.
Dialogue: 0,0:27:34.22,0:27:36.28,Default,,0,0,0,,Rất xin lỗi về sự lỗ mãng này.
Dialogue: 0,0:27:36.28,0:27:38.35,Default,,0,0,0,,Các người đang âm mưu gì đây?
Dialogue: 0,0:27:39.42,0:27:42.17,Default,,0,0,0,,Chắc NSS đang truy lùng tôi ráo riết.
Dialogue: 0,0:27:43.92,0:27:48.98,Default,,0,0,0,,Tôi chắc các người băn khoăn không biết\Ntại sao tôi tiếp tục ở lại Nam Hàn.
Dialogue: 0,0:27:49.77,0:27:56.91,Default,,0,0,0,,Đôi khi... người đời không nhận ra đâu\Nmới thực sự là những cá nhân yêu nước.
Dialogue: 0,0:27:58.04,0:27:59.86,Default,,0,0,0,,Chỉ đến khi thời gian trôi qua...
Dialogue: 0,0:27:59.86,0:28:03.49,Default,,0,0,0,,họ mới biết ai đã hi sinh vì Tổ quốc.
Dialogue: 0,0:28:03.49,0:28:06.70,Default,,0,0,0,,Anh muốn nói anh là người yêu nước?
Dialogue: 0,0:28:07.20,0:28:11.94,Default,,0,0,0,,Không, tôi đang nói đến Ngài.
Dialogue: 0,0:28:12.84,0:28:16.05,Default,,0,0,0,,Không giống những thằng ngu\Nthích lải nhải về lý tưởng...
Dialogue: 0,0:28:16.05,0:28:18.98,Default,,0,0,0,,Ngài có con mắt biết nhìn xa trông rộng.
Dialogue: 0,0:28:19.47,0:28:24.27,Default,,0,0,0,,Tôi tin đó là lý do Ngài phản\Nđối cuộc đàm thoại hòa bình.
Dialogue: 0,0:28:24.27,0:28:26.03,Default,,0,0,0,,Nói thẳng ra đi.
Dialogue: 0,0:28:28.64,0:28:33.05,Default,,0,0,0,,Phải chăng đã đến lúc cần gánh lấy\Ntrọng trách mà đất nước trao cho Ngài?
Dialogue: 0,0:28:33.91,0:28:40.67,Default,,0,0,0,,Cùng với tôi cũng tức là Ngài đã gia nhập\Nvào lực lượng bảo vệ an ninh đất nước này.
Dialogue: 0,0:28:42.51,0:28:45.53,Default,,0,0,0,,Kẻ đang đe dọa sự an toàn của\Nquốc gia này chính là anh.
Dialogue: 0,0:28:45.53,0:28:53.32,Default,,0,0,0,,Không. Điểm cốt yếu của an ninh\Nchính là cân bằng lực lượng.
Dialogue: 0,0:28:54.73,0:28:56.61,Default,,0,0,0,,Ông có biết Tổng thống và\NPhó Cục trưởng Choi Min...
Dialogue: 0,0:28:56.61,0:29:01.16,Default,,0,0,0,,gần đây đang giấu giếm một\Nlượng vũ khí hạt nhân không?
Dialogue: 0,0:29:02.90,0:29:07.49,Default,,0,0,0,,Nếu cô nàng Choi Min vẫn còn lởn vởn,\Ntình hình sẽ không bao giờ thay đổi.
Dialogue: 0,0:29:07.49,0:29:11.65,Default,,0,0,0,,Ý tôi là chuyện ông mãi sẽ sẽ\Nlà con rối để người ta chơi đùa.
Dialogue: 0,0:29:13.96,0:29:16.38,Default,,0,0,0,,Để tôi nói rõ hơn.
Dialogue: 0,0:29:16.38,0:29:19.26,Default,,0,0,0,,Tôi không thích chiến tranh.
Dialogue: 0,0:29:19.64,0:29:23.94,Default,,0,0,0,,Tôi chỉ muốn cân bằng được duy trì.\NĐể làm được điều đó...
Dialogue: 0,0:29:24.88,0:29:30.03,Default,,0,0,0,,cần những nhà yêu nước\Nhoạt động thầm lặng.
Dialogue: 0,0:29:41.57,0:29:43.67,Default,,0,0,0,,Kang Chul Hwan...
Dialogue: 0,0:29:43.67,0:29:45.26,Default,,0,0,0,,Anh nghĩ ông ta có cắn câu không?
Dialogue: 0,0:29:45.26,0:29:47.59,Default,,0,0,0,,Ông ta là loại yêu nước\Ntheo chủ nghĩa anh hùng.
Dialogue: 0,0:29:49.34,0:29:51.10,Default,,0,0,0,,Ông ta sẽ sớm liên lạc với chúng ta thôi.
Dialogue: 0,0:29:51.10,0:29:53.39,Default,,0,0,0,,Tôi vừa nhận được điện từ Park Tae Hee.
Dialogue: 0,0:29:53.39,0:29:55.97,Default,,0,0,0,,Ji Soon Yun đang tìm người lãnh\Nđạo nhóm tị nạn Bắc Hàn.
Dialogue: 0,0:29:56.20,0:29:59.30,Default,,0,0,0,,Tôi nghĩ cô ta đang dò tìm chúng ta.
Dialogue: 0,0:30:01.10,0:30:04.72,Default,,0,0,0,,Gọi cho Park Tae Hee và dặn\Nđừng rời mắt khỏi cô ta.
Dialogue: 0,0:30:04.72,0:30:06.42,Default,,0,0,0,,Vâng.
Dialogue: 0,0:30:16.81,0:30:18.66,Default,,0,0,0,,Tôi tìm Ngài vì có vài điều cần hỏi.
Dialogue: 0,0:30:18.66,0:30:19.91,Default,,0,0,0,,Anh nói đi.
Dialogue: 0,0:30:19.91,0:30:25.03,Default,,0,0,0,,Ngài đã hứa sẽ bảo đảm an toàn cho mẹ tôi nhưng\Nbà ấy lại bị đưa đến Bộ Tư lệnh An ninh...
Dialogue: 0,0:30:25.03,0:30:27.44,Default,,0,0,0,,và bị xử tử.
Dialogue: 0,0:30:29.08,0:30:30.66,Default,,0,0,0,,Điều đó có đúng không?
Dialogue: 0,0:30:31.11,0:30:34.26,Default,,0,0,0,,Tôi đã định nói với anh.
Dialogue: 0,0:30:34.36,0:30:37.96,Default,,0,0,0,,Khi tôi nối được liên lạc\Nthì mọi chuyện đã quá trễ.
Dialogue: 0,0:30:38.07,0:30:40.11,Default,,0,0,0,,Vậy là Ngài đã biết từ lâu?
Dialogue: 0,0:30:41.03,0:30:43.82,Default,,0,0,0,,Chúng ta còn có thể làm gì? Chẳng gì cả.
Dialogue: 0,0:30:44.78,0:30:46.67,Default,,0,0,0,,Hãy nghĩ rằng đó là ý Chúa.
Dialogue: 0,0:30:46.67,0:30:51.76,Default,,0,0,0,,Từ giờ anh chỉ còn mục tiêu duy\Nnhất là lợi ích đất nước.
Dialogue: 0,0:30:54.00,0:30:55.88,Default,,0,0,0,,Đúng, tôi nên như thế.
Dialogue: 0,0:30:59.48,0:31:02.10,Default,,0,0,0,,Tôi chắc rằng cái chết của mẹ tôi...
Dialogue: 0,0:31:02.10,0:31:09.01,Default,,0,0,0,,là sự hi sinh anh dũng cho nước Cộng Hòa.
Dialogue: 0,0:31:12.19,0:31:17.07,Default,,0,0,0,,Chỉ là... tôi muốn bà được an táng hẳn hoi.
Dialogue: 0,0:31:17.07,0:31:19.25,Default,,0,0,0,,Đừng ủy mị.
Dialogue: 0,0:31:19.25,0:31:21.44,Default,,0,0,0,,Tôi đã nói anh cần phải rũ\Nbỏ những tình cảm cá nhân.
Dialogue: 0,0:31:22.22,0:31:24.21,Default,,0,0,0,,Chúng ta còn nhiều việc phải làm.
Dialogue: 0,0:34:15.20,0:34:18.41,Default,,0,0,0,,Không phải chính cô là người...
Dialogue: 0,0:34:19.86,0:34:21.78,Default,,0,0,0,,đã muốn tôi hoạt động bí mật sao?
Dialogue: 0,0:34:22.95,0:34:24.80,Default,,0,0,0,,Đúng là thế.
Dialogue: 0,0:34:26.48,0:34:32.82,Default,,0,0,0,,Cô bảo tôi theo dõi Ji Soo Yun...\Nvà lần theo Baek San.
Dialogue: 0,0:34:32.82,0:34:40.06,Default,,0,0,0,,Cô cũng tiết lộ cho tôi về thiết bị\Ndò tìm cấy vào người Lee Soo Jin.
Dialogue: 0,0:34:44.02,0:34:46.67,Default,,0,0,0,,Nếu tôi là gián điệp...
Dialogue: 0,0:34:47.18,0:34:50.73,Default,,0,0,0,,thì cô sẽ phải chịu trách\Nnhiệm tất cả những việc đó.
Dialogue: 0,0:34:53.43,0:34:55.72,Default,,0,0,0,,Song Young Min đã chết.
Dialogue: 0,0:34:57.59,0:34:59.97,Default,,0,0,0,,- Anh không biết sao?\N- Ý cô là gì?
Dialogue: 0,0:34:59.97,0:35:01.84,Default,,0,0,0,,Ui Sang Chul...
Dialogue: 0,0:35:01.84,0:35:03.21,Default,,0,0,0,,có phải cũng là do anh giết?
Dialogue: 0,0:35:03.21,0:35:05.84,Default,,0,0,0,,Cô chính là người ra lệnh\Ncho tôi tìm Ui Sang Chul.
Dialogue: 0,0:35:05.84,0:35:10.72,Default,,0,0,0,,Tôi bảo anh tìm chứ chứ\Nkhông phải hạ sát ông ta.
Dialogue: 0,0:35:11.13,0:35:12.73,Default,,0,0,0,,Tôi không giết ông ta.
Dialogue: 0,0:35:12.73,0:35:16.10,Default,,0,0,0,,súng của anh đang được kiểm tra\Nđể biết lần cuối cùng khai hỏa.
Dialogue: 0,0:35:17.10,0:35:20.11,Default,,0,0,0,,Chúng tôi sẽ so sánh với những viên\Nđạn từ thi thể Song Young Min...
Dialogue: 0,0:35:20.11,0:35:22.40,Default,,0,0,0,,với rãnh súng.
Dialogue: 0,0:35:23.30,0:35:25.81,Default,,0,0,0,,Tìm ra ai là thủ phạm chỉ là vấn đề thời gian.
Dialogue: 0,0:35:31.19,0:35:32.26,Default,,0,0,0,,Đợi chút.
Dialogue: 0,0:35:33.17,0:35:36.40,Default,,0,0,0,,Đây là hồ sơ nhận dạng của những thành\Nviên IRIS tham gia sự kiện ở nhà thờ.
Dialogue: 0,0:35:38.57,0:35:41.07,Default,,0,0,0,,Chúng đều đội lốt những\Nkẻ đào tẩu khỏi Bắc Hàn.
Dialogue: 0,0:35:41.07,0:35:44.34,Default,,0,0,0,,Phần lớn khi còn ở phía Bắc đều\Nđã từng ở lực lượng đặc chủng.
Dialogue: 0,0:35:44.34,0:35:50.80,Default,,0,0,0,,Vậy... chúng không thuộc IRIS\Nmà được Yoo Joong Won tập hợp?
Dialogue: 0,0:35:51.65,0:35:55.05,Default,,0,0,0,,Hầu hết dấu vân tay thành\Nviên IRIS đều bị xóa.
Dialogue: 0,0:35:55.61,0:35:57.32,Default,,0,0,0,,Nhưng lần này thì không.
Dialogue: 0,0:35:58.85,0:36:03.18,Default,,0,0,0,,Mà... việc của Hyun Woo thế nào rồi?
Dialogue: 0,0:36:04.62,0:36:06.13,Default,,0,0,0,,Vẫn...
Dialogue: 0,0:36:07.32,0:36:09.95,Default,,0,0,0,,Vẫn chưa có tin gì tốt cả.
Dialogue: 0,0:36:11.14,0:36:12.86,Default,,0,0,0,,Tôi chắc chúng ta sẽ sớm được biết thôi.
Dialogue: 0,0:36:18.30,0:36:20.72,Default,,0,0,0,,Anh có giết Ui Sang Chul không?
Dialogue: 0,0:36:20.94,0:36:21.98,Default,,0,0,0,,Không.
Dialogue: 0,0:36:24.40,0:36:27.42,Default,,0,0,0,,Người giết Song Young\NMin có phải anh không?
Dialogue: 0,0:36:28.66,0:36:30.05,Default,,0,0,0,,Không.
Dialogue: 0,0:36:36.08,0:36:39.13,Default,,0,0,0,,Anh có phải thành viên IRIS không?
Dialogue: 0,0:36:42.22,0:36:44.22,Default,,0,0,0,,Không.
Dialogue: 0,0:36:54.74,0:36:56.45,Default,,0,0,0,,Mang Seo Hyun Woo đến.
Dialogue: 0,0:36:56.97,0:36:58.25,Default,,0,0,0,,Vâng.
Dialogue: 0,0:37:04.45,0:37:06.32,Default,,0,0,0,,{\i1}Tên anh có phải là Seo Hyun Woo?{\i0}
Dialogue: 0,0:37:07.33,0:37:08.99,Default,,0,0,0,,Đúng.
Dialogue: 0,0:37:10.43,0:37:12.11,Default,,0,0,0,,{\i1}Anh thích bóng đá không?{\i0}
Dialogue: 0,0:37:14.42,0:37:15.71,Default,,0,0,0,,Có.
Dialogue: 0,0:37:16.62,0:37:18.33,Default,,0,0,0,,Anh lớn lên ở đâu?
Dialogue: 0,0:37:18.40,0:37:19.51,Default,,0,0,0,,Tôi lớn lên tại Mokdong, Seoul.
Dialogue: 0,0:37:19.51,0:37:21.35,Default,,0,0,0,,{\i1}Anh có người yêu không?{\i0}
Dialogue: 0,0:37:25.08,0:37:28.30,Default,,0,0,0,,{\i1}Hiện nay... anh có đang yêu ai không?{\i0}
Dialogue: 0,0:37:34.63,0:37:36.05,Default,,0,0,0,,Không.
Dialogue: 0,0:37:36.18,0:37:40.41,Default,,0,0,0,,{\i1}Có phải anh giết Song Young Min không?{\i0}
Dialogue: 0,0:37:42.05,0:37:43.52,Default,,0,0,0,,Không.
Dialogue: 0,0:37:47.49,0:37:51.22,Default,,0,0,0,,Anh có từng tiết lộ thông tin cho IRIS không?
Dialogue: 0,0:37:51.83,0:37:53.54,Default,,0,0,0,,Tôi chưa bao giờ làm điều đó.
Dialogue: 0,0:38:21.38,0:38:25.54,Default,,0,0,0,,Tất cả các đặc vụ Bắc Hàn có mặt tại\Nnhà thờ hôm đó đều có một điểm chung.
Dialogue: 0,0:38:26.08,0:38:29.81,Default,,0,0,0,,Khi sang Nam Hàn, họ đều nhận được\Nsự trợ giúp từ Xơ Jung Mary.
Dialogue: 0,0:38:29.81,0:38:30.89,Default,,0,0,0,,Tất cả bọn họ ư?
Dialogue: 0,0:38:30.89,0:38:33.24,Default,,0,0,0,,Thời gian lưu trú có khác nhau...
Dialogue: 0,0:38:33.24,0:38:35.31,Default,,0,0,0,,nhưng tất cả bọn họ đều có ít\Nnhất một lần liên lạc với cô ta.
Dialogue: 0,0:38:35.73,0:38:38.58,Default,,0,0,0,,Cảm ơn nhé. Tôi sẽ tới gặp cô ấy.
Dialogue: 0,0:38:38.58,0:38:39.84,Default,,0,0,0,,Một mình cô sao?
Dialogue: 0,0:38:40.52,0:38:41.61,Default,,0,0,0,,Tôi không sao đâu.
Dialogue: 0,0:39:08.15,0:39:09.53,Default,,0,0,0,,Cảm ơn.
Dialogue: 0,0:39:13.69,0:39:17.59,Default,,0,0,0,,Tôi nghe nói cô đã giúp đỡ rất\Nnhiều dân tị nạn Bắc Hàn.
Dialogue: 0,0:39:18.84,0:39:21.90,Default,,0,0,0,,Vì trợ cấp của chính phủ thôi không đủ.
Dialogue: 0,0:39:23.06,0:39:25.51,Default,,0,0,0,,Cô có...
Dialogue: 0,0:39:28.82,0:39:30.56,Default,,0,0,0,,quen những người này quen không?
Dialogue: 0,0:39:36.30,0:39:38.25,Default,,0,0,0,,Vâng, tôi biết họ.
Dialogue: 0,0:39:39.82,0:39:45.01,Default,,0,0,0,,Vài năm trước họ từng ở đây một thời gian ngắn.
Dialogue: 0,0:39:45.01,0:39:47.64,Default,,0,0,0,,Có chuyện gì sao?
Dialogue: 0,0:39:49.49,0:39:54.16,Default,,0,0,0,,Có khả năng họ là gián điệp\NTriều Tiên giả danh là dân tị nạn.
Dialogue: 0,0:39:54.75,0:39:56.63,Default,,0,0,0,,Việc này đã được chứng thực chưa?
Dialogue: 0,0:39:58.83,0:40:03.95,Default,,0,0,0,,Dân tị nạn Bắc Hàn hay\Nbị nghi oan là điệp viên.
Dialogue: 0,0:40:04.11,0:40:07.92,Default,,0,0,0,,Họ thường bị đối xử bất công do\Ncon mắt thành kiến của mọi người.
Dialogue: 0,0:40:08.79,0:40:13.92,Default,,0,0,0,,Vâng, tôi cũng thấy điều đó đáng buồn.
Dialogue: 0,0:40:14.01,0:40:16.90,Default,,0,0,0,,Nhưng lần này...
Dialogue: 0,0:40:17.47,0:40:19.64,Default,,0,0,0,,không có sự bất công.
Dialogue: 0,0:40:26.52,0:40:31.97,Default,,0,0,0,,Cô đợi một chút được chứ? Để tôi xem\Nhọ có để lại thông tin liên lạc không.
Dialogue: 0,0:40:32.90,0:40:33.97,Default,,0,0,0,,Vâng.
Dialogue: 0,0:41:13.54,0:41:15.10,Default,,0,0,0,,Anh!
Dialogue: 0,0:41:15.39,0:41:20.20,Default,,0,0,0,,Anh!
Dialogue: 0,0:41:23.79,0:41:24.87,Default,,0,0,0,,Anh ơi...
Dialogue: 0,0:41:31.22,0:41:33.01,Default,,0,0,0,,Anh.
Dialogue: 0,0:41:33.01,0:41:34.18,Default,,0,0,0,,Đợi một chút.
Dialogue: 0,0:41:35.58,0:41:36.91,Default,,0,0,0,,Xin đợi một chút thôi.
Dialogue: 0,0:41:46.97,0:41:50.45,Default,,0,0,0,,Ji Soo Yun của NSS đang ở đây.
Dialogue: 0,0:41:50.45,0:41:52.03,Default,,0,0,0,,{\i1}Đợi chút.{\i0}
Dialogue: 0,0:41:57.47,0:41:58.69,Default,,0,0,0,,Tôi là Yoo Joong Won.
Dialogue: 0,0:42:00.27,0:42:03.18,Default,,0,0,0,,Hãy giữ chân cô ta càng lâu càng tốt.\NPark Tae Hee đang trên đường đến.
Dialogue: 0,0:42:03.18,0:42:04.39,Default,,0,0,0,,{\i1}Vâng, tôi hiểu rồi.{\i0}
Dialogue: 0,0:42:15.65,0:42:16.87,Default,,0,0,0,,Phó Cục trưởng.
Dialogue: 0,0:42:16.90,0:42:19.71,Default,,0,0,0,,Rãnh súng của Yoo Shi Hyuk\Ncó khớp với viên đạn không?
Dialogue: 0,0:42:19.71,0:42:22.13,Default,,0,0,0,,Không.
Dialogue: 0,0:42:22.69,0:42:23.76,Default,,0,0,0,,Sao cơ?
Dialogue: 0,0:42:23.76,0:42:27.32,Default,,0,0,0,,Súng của Yoon Shi Hyuk đã không\Nđược sử dụng trong 20 giờ và...
Dialogue: 0,0:42:27.32,0:42:29.16,Default,,0,0,0,,hình dạng của rãnh súng không khớp.
Dialogue: 0,0:42:29.16,0:42:32.84,Default,,0,0,0,,Súng của Yoon Shi Hyuk không phải là\Nkhẩu súng đã giết Song Young Min.
Dialogue: 0,0:42:45.74,0:42:46.95,Default,,0,0,0,,Vâng, thưa Phó Cục trưởng.
Dialogue: 0,0:42:46.95,0:42:51.20,Default,,0,0,0,,{\i1}Việc xét nghiệm súng của\NYoon Shi Hyuk không có kêt quả.{\i0}
Dialogue: 0,0:42:51.76,0:42:54.54,Default,,0,0,0,,{\i1}Cô có tìm ra nghi vấn gì\Nvề Seo Hyun Woo không?{\i0}
Dialogue: 0,0:42:54.91,0:42:57.69,Default,,0,0,0,,Tôi không thấy gì đáng nghi.
Dialogue: 0,0:42:57.91,0:43:01.50,Default,,0,0,0,,Kết quả kiểm tra nói dối cũng\Nkhông kết luận được gì nhiều.
Dialogue: 0,0:43:01.64,0:43:03.74,Default,,0,0,0,,{\i1}Thôi được, cứ tạm như vậy đã.{\i0}
Dialogue: 0,0:43:04.12,0:43:05.38,Default,,0,0,0,,Vâng.
Dialogue: 0,0:43:37.94,0:43:40.77,Default,,0,0,0,,Kết quả so sánh mẫu súng đã có chưa?
Dialogue: 0,0:43:41.80,0:43:44.27,Default,,0,0,0,,Tôi đã cho người lục soát nhà cậu.
Dialogue: 0,0:43:44.27,0:43:47.18,Default,,0,0,0,,Rất có thể cậu đã sử\Ndụng một khẩu súng khác.
Dialogue: 0,0:43:48.21,0:43:51.16,Default,,0,0,0,,Dừng lại được rồi đấy.
Dialogue: 0,0:43:52.76,0:43:58.48,Default,,0,0,0,,Chẳng phải cô chỉ cần làm như với\NBaek San và Lee Soo Jin thôi sao.
Dialogue: 0,0:44:01.01,0:44:04.92,Default,,0,0,0,,Cấy máy dò rồi thả tôi đi.
Dialogue: 0,0:44:05.02,0:44:07.32,Default,,0,0,0,,Ai biết đấy.
Dialogue: 0,0:44:07.88,0:44:11.64,Default,,0,0,0,,Có thể tôi sẽ chạy đến\Nchỗ Yoo Joong Won thì sao.
Dialogue: 0,0:44:17.06,0:44:20.04,Default,,0,0,0,,Chuyện khác tôi không biết nhưng...
Dialogue: 0,0:44:21.33,0:44:28.70,Default,,0,0,0,,tôi không vô nhân tính đến độ giết\Nhại chính cộng sự của mình.
Dialogue: 0,0:44:33.55,0:44:38.13,Default,,0,0,0,,Tôi tốt đẹp hơn nhiều hạng người...
Dialogue: 0,0:44:38.13,0:44:41.17,Default,,0,0,0,,lợi dụng Đội trưởng Jung dù biết rõ\Ntình trạng tinh thần bất ổn của anh ta.
Dialogue: 0,0:44:49.12,0:44:51.29,Default,,0,0,0,,Đã tìm ra Jung Yoo Gun chưa?
Dialogue: 0,0:44:51.29,0:44:54.55,Default,,0,0,0,,Chúng tôi chạm mặt ở Cơ quan Lưu trữ\NQuốc gia nhưng anh ta đã chạy mất.
Dialogue: 0,0:44:54.97,0:44:58.23,Default,,0,0,0,,Việc anh ta thâm nhập được vào đó...
Dialogue: 0,0:44:58.23,0:45:01.62,Default,,0,0,0,,chứng tỏ có người đã giúp anh ta.
Dialogue: 0,0:45:08.48,0:45:12.89,Default,,0,0,0,,Từ giờ tôi cho phép tự do sử dụng vũ khí.
Dialogue: 0,0:45:12.89,0:45:16.71,Default,,0,0,0,,khi truy bắt Baek San\Nvà Jung Yoo Gun.
Dialogue: 0,0:45:17.25,0:45:18.27,Default,,0,0,0,,Cục trưởng.
Dialogue: 0,0:45:18.27,0:45:20.62,Default,,0,0,0,,Tôi đã cho Baek San và Jung Yoo Gun\Nvào danh sách truy nã...
Dialogue: 0,0:45:20.62,0:45:24.26,Default,,0,0,0,,và yêu cầu hỗ trợ của lực\Nlượng cảnh sát và NIS.
Dialogue: 0,0:45:25.07,0:45:26.92,Default,,0,0,0,,Phó Cục trưởng,...
Dialogue: 0,0:45:28.75,0:45:32.27,Default,,0,0,0,,nếu cô đang ngầm giúp đỡ Jung Yoo Gun...
Dialogue: 0,0:45:32.27,0:45:34.47,Default,,0,0,0,,thì chấm dứt được rồi đấy.
Dialogue: 0,0:45:34.60,0:45:37.80,Default,,0,0,0,,Trước khi cô bị buộc tội phản quốc.
Dialogue: 0,0:45:55.09,0:45:58.77,Default,,0,0,0,,2 người sẽ tiếp tục bị điều tra tại NIS.
Dialogue: 0,0:45:58.77,0:46:01.32,Default,,0,0,0,,Cục trưởng đã giao tôi phụ trách việc này.
Dialogue: 0,0:46:02.25,0:46:03.61,Default,,0,0,0,,Đưa họ lên xe.
Dialogue: 0,0:46:50.99,0:46:52.65,Default,,0,0,0,,Lâu rồi không gặp.
Dialogue: 0,0:46:53.83,0:46:55.69,Default,,0,0,0,,Jung Yoo Gun.
Dialogue: 0,0:46:56.63,0:46:58.76,Default,,0,0,0,,Cô biết anh ta ở đâu chứ?
Dialogue: 0,0:47:07.97,0:47:10.66,Default,,0,0,0,,Có vẻ cô không biết.
Dialogue: 0,0:47:11.45,0:47:13.46,Default,,0,0,0,,Vậy còn Baek San?
Dialogue: 0,0:47:14.85,0:47:16.45,Default,,0,0,0,,Tôi không biết.
Dialogue: 0,0:47:17.11,0:47:19.32,Default,,0,0,0,,Vậy cũng chẳng sao.
Dialogue: 0,0:47:19.32,0:47:24.16,Default,,0,0,0,,Cô chỉ là miếng mồi để dụ\NJung Yoo Gun và Baek San.
Dialogue: 0,0:47:24.63,0:47:27.12,Default,,0,0,0,,Tôi sẽ không giết cô.
Dialogue: 0,0:47:31.25,0:47:34.23,Default,,0,0,0,,Khi biết tin cô bị bắt...
Dialogue: 0,0:47:34.23,0:47:37.81,Default,,0,0,0,,cô nghĩ Jung Yoo Gun\Nsẽ đến trước...
Dialogue: 0,0:47:37.81,0:47:43.02,Default,,0,0,0,,hay Baek San sẽ tìm đến để tránh\Ncho con trai lao vào vòng nguy hiểm?
Dialogue: 0,0:47:49.72,0:47:52.09,Default,,0,0,0,,Có lẽ sẽ chẳng ai đến.
Dialogue: 0,0:47:53.57,0:47:55.84,Default,,0,0,0,,Đội trưởng, mọi thứ đã sẵn sàng.
Dialogue: 0,0:48:25.64,0:48:26.94,Default,,0,0,0,,Có chuyện gì?
Dialogue: 0,0:48:26.94,0:48:29.22,Default,,0,0,0,,Là Yoo Joong Won.\NHắn ta đang giữ Đội trưởng Ji.
Dialogue: 0,0:48:29.22,0:48:30.28,Default,,0,0,0,,Có lần được địa điểm không?
Dialogue: 0,0:48:30.28,0:48:32.16,Default,,0,0,0,,Hắn chỉ nói sẽ gọi lại và cúp máy.
Dialogue: 0,0:48:32.16,0:48:33.26,Default,,0,0,0,,Hắn muốn nói chuyện với cô.
Dialogue: 0,0:48:33.26,0:48:35.45,Default,,0,0,0,,Cô cần kéo dài cuộc\Nđiện đàm trong 2 phút.
Dialogue: 0,0:48:35.45,0:48:37.90,Default,,0,0,0,,Như cô đã biết, dưới 2 phút\Nthì không thể dò ra vị trí.
Dialogue: 0,0:48:43.60,0:48:44.72,Default,,0,0,0,,Hệ thống dò tìm đã kích hoạt.
Dialogue: 0,0:48:45.42,0:48:48.07,Default,,0,0,0,,- Đã khởi động NRS.\N- Đồng hồ bấm giờ sẵn sàng.
Dialogue: 0,0:48:51.21,0:48:52.93,Default,,0,0,0,,Tôi là Choi Min đây.
Dialogue: 0,0:48:53.27,0:48:54.77,Default,,0,0,0,,{\i1}Yoo Joong Won đây.{\i0}
Dialogue: 0,0:48:55.37,0:48:57.99,Default,,0,0,0,,Tôi biết anh đã nhận được\Nlệnh triệu tập từ Bắc Hàn.
Dialogue: 0,0:48:57.99,0:48:59.61,Default,,0,0,0,,Kkéo dài kì nghỉ như vậy không sao chứ?
Dialogue: 0,0:48:59.61,0:49:03.27,Default,,0,0,0,,{\i1}Đại diện Kwon đã sắp xếp cho\Ntôi rồi nên cô không phải lo.{\i0}
Dialogue: 0,0:49:03.27,0:49:06.32,Default,,0,0,0,,Chứ không phải anh là người\Nđã giết Kwon Young Choon?
Dialogue: 0,0:49:07.40,0:49:09.61,Default,,0,0,0,,Tôi tin dù ai là người giết Đại diện Kwon...
Dialogue: 0,0:49:09.61,0:49:13.84,Default,,0,0,0,,cũng không phải việc của NSS.
Dialogue: 0,0:49:14.41,0:49:17.64,Default,,0,0,0,,{\i1}Tôi biết rằng mẹ anh\Nhiện vẫn ở Bắc Hàn.{\i0}
Dialogue: 0,0:49:17.64,0:49:22.85,Default,,0,0,0,,{\i1}Anh không nghĩ anh đã khiến bà\Nấy phải vào trại tập trung à?{\i0}
Dialogue: 0,0:49:25.91,0:49:27.94,Default,,0,0,0,,Choi Min.
Dialogue: 0,0:49:29.52,0:49:33.36,Default,,0,0,0,,{\i1}Tôi không nghĩ cô có tư cách nhúng\Nmũi vào việc nội bộ gia đình tôi.{\i0}
Dialogue: 0,0:49:34.79,0:49:36.87,Default,,0,0,0,,{\i1}Chúng tôi đang giữ Ji Soo Yun.{\i0}
Dialogue: 0,0:49:37.11,0:49:38.70,Default,,0,0,0,,Bằng chứng.
Dialogue: 0,0:49:38.70,0:49:40.30,Default,,0,0,0,,{\i1}Tôi sẽ gửi ảnh.{\i0}
Dialogue: 0,0:49:42.21,0:49:44.56,Default,,0,0,0,,- Đây là yêu cầu của tôi.\N- Là gì?
Dialogue: 0,0:49:44.79,0:49:49.96,Default,,0,0,0,,{\i1}Nếu cô muốn giữ mạng sống cho Ji Soo Yun,\Ntrong vòng 24 giờ phải giao ra Baek San.{\i0}
Dialogue: 0,0:49:50.30,0:49:52.11,Default,,0,0,0,,Chúng tôi cũng không biết\Nhiện ông ta đang ở đâu.
Dialogue: 0,0:49:55.57,0:49:57.62,Default,,0,0,0,,Tôi sẽ không nhắc lại đâu.
Dialogue: 0,0:49:58.01,0:50:03.83,Default,,0,0,0,,{\i1}Nói với Baek San địa điểm trao\Nđổi sẽ được đăng trên báo New Day.{\i0}
Dialogue: 0,0:50:03.83,0:50:05.51,Default,,0,0,0,,Chúng tôi cần thêm thời gian.
Dialogue: 0,0:50:05.51,0:50:07.33,Default,,0,0,0,,{\i1}Không cần lo lắng.{\i0}
Dialogue: 0,0:50:08.30,0:50:12.61,Default,,0,0,0,,Tôi chắc Jung Yoo Gun sẽ có\Ncách liên lạc với Baek San.
Dialogue: 0,0:50:12.61,0:50:14.11,Default,,0,0,0,,A lô.
Dialogue: 0,0:50:21.77,0:50:23.59,Default,,0,0,0,,Chưa dò được.
Dialogue: 0,0:50:54.08,0:50:55.39,Default,,0,0,0,,Tự nhiên đi.
Dialogue: 0,0:51:19.21,0:51:20.90,Default,,0,0,0,,Cô chắc phải đói lắm.
Dialogue: 0,0:51:46.18,0:51:47.31,Default,,0,0,0,,Vâng.
Dialogue: 0,0:51:47.42,0:51:49.63,Default,,0,0,0,,Yoo Joong Won đã bắt cóc Đội trưởng Ji.
Dialogue: 0,0:51:51.64,0:51:53.26,Default,,0,0,0,,Cô nói gì cơ?
Dialogue: 0,0:51:53.26,0:51:56.56,Default,,0,0,0,,Hắn yêu cầu giao ra Baek\NSan trong vòng 24 giờ.
Dialogue: 0,0:51:57.01,0:51:58.11,Default,,0,0,0,,{\i1}Đội trưởng Jung.{\i0}
Dialogue: 0,0:51:58.11,0:51:59.66,Default,,0,0,0,,Tôi vẫn đang nghe.
Dialogue: 0,0:51:59.94,0:52:05.75,Default,,0,0,0,,Và Cục trưởng Kang Chul Hwan đã ra lệnh tự\Ndo vũ trang khi truy bắt anh và Baek San.
Dialogue: 0,0:52:05.95,0:52:08.67,Default,,0,0,0,,Ông ta cũng đã biết\Nviệc tôi ngầm giúp anh.
Dialogue: 0,0:52:08.74,0:52:09.93,Default,,0,0,0,,Tôi hiểu rồi.
Dialogue: 0,0:52:10.36,0:52:12.85,Default,,0,0,0,,Tôi nghĩ mình không có ích gì cho anh nữa...
Dialogue: 0,0:52:12.85,0:52:17.01,Default,,0,0,0,,vì vậy chỉ còn cách dùng Kim Yun\NHwa và Baek San để tìm ra Ji Soo Yun.
Dialogue: 0,0:52:17.20,0:52:18.75,Default,,0,0,0,,Được, tôi hiểu rồi.
Dialogue: 0,0:52:27.76,0:52:30.70,Default,,0,0,0,,Có vấn đề gì sao?
Dialogue: 0,0:52:33.90,0:52:36.31,Default,,0,0,0,,Yoo Joong Won đã bắt Soo Yun.
Dialogue: 0,0:52:41.58,0:52:44.86,Default,,0,0,0,,Anh ta muốn lợi dụng anh để dụ Baek San.
Dialogue: 0,0:52:47.15,0:52:48.80,Default,,0,0,0,,Giờ anh định thế nào?
Dialogue: 0,0:52:48.80,0:52:50.87,Default,,0,0,0,,Tôi phải tìm ra Soo Yun.
Dialogue: 0,0:52:52.78,0:52:57.37,Default,,0,0,0,,Yun Hwa. Cô có thể nói tôi biết làm\Nsao để tìm ra Yoo Joong Won không?
Dialogue: 0,0:52:58.74,0:52:59.92,Default,,0,0,0,,Yun Hwa...
Dialogue: 0,0:53:01.11,0:53:04.38,Default,,0,0,0,,Tôi biết một nơi họ dùng làm căn cứ.
Dialogue: 0,0:53:04.84,0:53:06.37,Default,,0,0,0,,Là ở đâu?
Dialogue: 0,0:53:06.67,0:53:08.90,Default,,0,0,0,,Tôi chưa từng đến đó.
Dialogue: 0,0:53:10.51,0:53:14.07,Default,,0,0,0,,Nhưng tôi biết làm sao để tìm ra nó.
Dialogue: 0,0:53:22.04,0:53:25.49,Default,,0,0,0,,Đó là vị trí nơi bắt được tín\Nhiệu cuối cùng của Đội trưởng Ji.
Dialogue: 0,0:53:33.19,0:53:35.27,Default,,0,0,0,,Là nơi đó.
Dialogue: 0,0:53:37.81,0:53:40.28,Default,,0,0,0,,Những thành viên IRIS trong\Nvụ việc tại nhà thờ...
Dialogue: 0,0:53:40.83,0:53:43.68,Default,,0,0,0,,chúng đều là đặc vụ Bắc\NHàn giả danh dân tị nạn.
Dialogue: 0,0:54:32.82,0:54:36.47,Default,,0,0,0,,Đội trưởng Ji đã bị Yoo Joong Won bắt cóc.
Dialogue: 0,0:54:36.66,0:54:38.17,Default,,0,0,0,,Nhưng...
Dialogue: 0,0:54:38.31,0:54:40.68,Default,,0,0,0,,yêu cầu của hắn ta là...
Dialogue: 0,0:54:42.40,0:54:44.15,Default,,0,0,0,,Là tôi.
Dialogue: 0,0:54:46.85,0:54:49.68,Default,,0,0,0,,Đây là báo New Day ngày hôm nay.
Dialogue: 0,0:54:54.39,0:54:59.03,Default,,0,0,0,,Tôi biết là chỉ Ngài mới giải mã được.
Dialogue: 0,0:55:14.69,0:55:17.83,Default,,0,0,0,,Anh ta cho tôi 24 giờ.
Dialogue: 0,0:55:17.85,0:55:21.33,Default,,0,0,0,,Anh ta định lợi dụng việc\NJung Yoo Gun là con trai tôi.
Dialogue: 0,0:55:24.29,0:55:29.26,Default,,0,0,0,,Nếu Yoo Gun gọi, đừng trả lời.
Dialogue: 0,0:55:29.67,0:55:34.33,Default,,0,0,0,,Tôi là người... sẽ kết thúc việc này.
Dialogue: 0,0:55:53.59,0:55:58.29,Default,,0,0,0,,Đừng sợ. Không phải cho cô đâu.
Dialogue: 0,0:55:59.82,0:56:03.86,Default,,0,0,0,,Nó dành cho những vị khách sắp đến.
Dialogue: 0,0:56:03.96,0:56:09.12,Default,,0,0,0,,Nếu Baek San đến trước sẽ tiết kiệm được thời gian.
Dialogue: 0,0:57:37.68,0:57:39.79,Default,,0,0,0,,Chắc họ đã rút khỏi đây.
Dialogue: 0,0:57:57.71,0:57:59.29,Default,,0,0,0,,Ông đã quyết định chưa?
Dialogue: 0,0:57:59.55,0:58:03.56,Default,,0,0,0,,Có một câu anh nói khiến tôi rất thích.
Dialogue: 0,0:58:03.81,0:58:08.29,Default,,0,0,0,,'Những người thật sự yêu nước\Nsống ẩn mình trong bóng tối.'
Dialogue: 0,0:58:09.17,0:58:12.35,Default,,0,0,0,,Vâng, đó là sự thật.
Dialogue: 0,0:58:14.13,0:58:18.22,Default,,0,0,0,,Vậy được. Tôi sẽ trở thành\Nmột nhà yêu nước đích thực.
Dialogue: 0,0:58:19.98,0:58:22.40,Default,,0,0,0,,Nhưng anh đã đánh giá thấp tôi.
Dialogue: 0,0:58:22.40,0:58:23.86,Default,,0,0,0,,Ý ông là gì?
Dialogue: 0,0:58:23.95,0:58:30.01,Default,,0,0,0,,Anh nghĩ tôi là kẻ cơ hội\Ngiả danh ái quốc như Choi Min?
Dialogue: 0,0:58:30.51,0:58:32.89,Default,,0,0,0,,Vậy là ông từ chối lời đề nghị của tôi?
Dialogue: 0,0:58:32.98,0:58:35.93,Default,,0,0,0,,Tôi không từ chối mà cảnh cáo anh.
Dialogue: 0,0:58:36.77,0:58:41.19,Default,,0,0,0,,Số bom hạt nhân của Baek San...\Nđã nằm trong tay chúng tôi.
Dialogue: 0,0:58:46.05,0:58:47.55,Default,,0,0,0,,Thật sao?
Dialogue: 0,0:58:48.15,0:58:52.21,Default,,0,0,0,,Anh đừng tiếp tục cố gắng vô vọng mà\Nnên âm thầm cuốn gói về Bắc Hàn...
Dialogue: 0,0:58:52.46,0:58:54.82,Default,,0,0,0,,trước khi tôi tóm được anh.
Dialogue: 0,0:59:00.22,0:59:04.31,Default,,0,0,0,,Ông sẽ phải hối hận về quyết định này đấy.
Dialogue: 0,0:59:05.79,0:59:08.55,Default,,0,0,0,,Kẻ phải ân hận là anh.
Dialogue: 0,0:59:08.55,0:59:13.65,Default,,0,0,0,,Tôi sẽ khiến anh phải trả giá\Nvì đã đánh giá thấp tôi.
Dialogue: 0,0:59:14.62,0:59:16.58,Default,,0,0,0,,Để rồi xem.
Dialogue: 0,0:59:25.94,0:59:27.61,Default,,0,0,0,,Có chuyện gì vậy?
Dialogue: 0,0:59:29.98,0:59:32.56,Default,,0,0,0,,Kang Chul Hwan đã từ chối đề nghị của tôi.
Dialogue: 0,0:59:33.69,0:59:39.40,Default,,0,0,0,,Ông ta nói Baek San đã giao số bom\Nhạt nhân cho chính phủ Nam Hàn.
Dialogue: 0,0:59:42.68,0:59:44.38,Default,,0,0,0,,Xác nhận thông tin đó ngay.
Dialogue: 0,0:59:45.54,0:59:47.40,Default,,0,0,0,,Rõ.
Dialogue: 0,1:00:37.30,1:00:38.45,Default,,0,0,0,,Dừng lại!
Dialogue: 0,1:00:52.56,1:00:57.59,Default,,0,0,0,,Nói cho tôi biết về số bom hạt\Nnhân các người lấy từ Baek San.
Dialogue: 0,1:00:58.42,1:01:01.92,Default,,0,0,0,,Có vẻ cô biết nhưng không chịu nói.
Dialogue: 0,1:01:03.37,1:01:06.20,Default,,0,0,0,,Tôi chẳng có gì để nói hết.
Dialogue: 0,1:01:11.82,1:01:15.53,Default,,0,0,0,,Tôi không định dùng vũ lực\Nnhưng cô không cho tôi lựa chọn.
Dialogue: 0,1:01:16.80,1:01:19.65,Default,,0,0,0,,- Đưa cô ta đi.\N- Vâng.
Dialogue: 0,1:01:24.03,1:01:26.21,Default,,0,0,0,,Cô sẽ phải mở mồm thôi.
Dialogue: 0,1:01:45.13,1:01:46.92,Default,,0,0,0,,{\i1}Có chuyện gì vậy?{\i0}
Dialogue: 0,1:01:47.64,1:01:51.22,Default,,0,0,0,,- A lô?\N- Jung Yoo Gun đây.
Dialogue: 0,1:01:51.23,1:01:52.86,Default,,0,0,0,,Để tao nói chuyện với Yoo Joong Won.
Dialogue: 0,1:02:01.56,1:02:03.05,Default,,0,0,0,,Tăng điện áp lên.
Dialogue: 0,1:02:07.35,1:02:08.83,Default,,0,0,0,,Là Jung Yoo Gun.
Dialogue: 0,1:02:15.39,1:02:16.50,Default,,0,0,0,,Mày đang ở đâu?
Dialogue: 0,1:02:18.00,1:02:23.04,Default,,0,0,0,,Nếu đụng đến một ngón tay Soo Yun... tao sẽ\Nkhiến mày phải van xin suốt phần đời còn lại.
Dialogue: 0,1:02:23.04,1:02:25.67,Default,,0,0,0,,Nếu mày đến đây, tao sẽ thả Ji Soo Yun.
Dialogue: 0,1:02:27.29,1:02:30.56,Default,,0,0,0,,Nhưng nếu mày dùng đến\Nsự giúp đỡ của NSS...
Dialogue: 0,1:02:30.56,1:02:35.45,Default,,0,0,0,,tao sẽ tự tay kết liễu cô ta.
Dialogue: 0,1:02:36.76,1:02:38.63,Default,,0,0,0,,Tao sẽ đến một mình.
Dialogue: 0,1:02:39.07,1:02:41.22,Default,,0,0,0,,Tao sẽ gửi cho mày địa chỉ.
Dialogue: 0,1:02:42.18,1:02:43.93,Default,,0,0,0,,Mày nên nhanh lên.
Dialogue: 0,1:02:59.44,1:03:01.76,Default,,0,0,0,,Anh định một mình đến đó mà\Nkhông có kế hoạch gì sao?
Dialogue: 0,1:03:01.76,1:03:03.08,Default,,0,0,0,,Bộ muốn chết à?
Dialogue: 0,1:03:04.85,1:03:06.70,Default,,0,0,0,,Cô nên ở lại đây.
Dialogue: 0,1:03:06.90,1:03:09.63,Default,,0,0,0,,Anh nên tìm NSS để họ giúp đỡ.
Dialogue: 0,1:03:09.78,1:03:11.74,Default,,0,0,0,,Nếu đến đó một mình,\Nanh chết chắc đấy.
Dialogue: 0,1:03:11.74,1:03:13.52,Default,,0,0,0,,Yun Hwa.
Dialogue: 0,1:03:13.76,1:03:15.67,Default,,0,0,0,,Đừng lo cho tôi.
Dialogue: 0,1:03:16.44,1:03:19.09,Default,,0,0,0,,- Cô không nên dính vào chuyện này.\N- Tôi muốn thế.
Dialogue: 0,1:03:19.44,1:03:22.12,Default,,0,0,0,,Tôi cũng có việc cần\Ngiải quyết với Joong Won.
Dialogue: 0,1:03:22.12,1:03:26.59,Default,,0,0,0,,Anh không nên giết Joong Won.\NJoong Won cũng không nên giết anh.
Dialogue: 0,1:03:26.83,1:03:28.85,Default,,0,0,0,,Tôi sẽ có cách.
Dialogue: 0,1:03:33.75,1:03:36.12,Default,,0,0,0,,Anh có nghe tôi nói không đấy?
Dialogue: 0,1:03:36.12,1:03:38.12,Default,,0,0,0,,Phụ đề dịch bởi Emma - Subteam 2pi.vn
